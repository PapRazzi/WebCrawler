		Event.observe(window, 'beforeunload', function(){
			// Apple Instant Feed statistics code 
			if(enableAppleInstant == "yes"){
				 var store = new Persist.Store('FeedStats');
			 	if (store.get('resultActivity')=='true' && store.get('timeStart') !== null){
			 		var time = new Date();
					//var timeDiff = time.getTime() - store.get('HT1573');
					var timeDiff = time.getTime() - store.get('timeStart');
					var timeSpent = Math.floor(timeDiff/1000);
					var feedStats = new ACFeedStatistics();	
					// Fix for <rdar://problem/10909791> 
					if (timeDiff < 3000){
						/*var feedStats = new ACFeedStatistics();
						feedStats.updateNotRead('HT1573', store.get('position'));
						clearTimeout(setReadTimer);
						store.remove('HT1573');
						*/
						feedStats.updateNotRead('HT1573', store.get('position'), timeSpent);
						store.remove('timeStart');
						store.remove('position');
					}
					else{
						feedStats.updateRead('HT1573', store.get('position'), timeSpent);
						store.remove('timeStart');
                        store.remove('position');
					}
				}
			}
		});


	</script>
		




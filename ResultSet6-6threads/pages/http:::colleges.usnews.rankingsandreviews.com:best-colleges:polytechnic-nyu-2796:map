<script type="text/javascript">
//<![CDATA[
$('body').addClass('map-with-directions');
var map;
var gdir;
function overlayDirections()  {
	$('#step-by-step').html('');
	$('.error-marker').removeClass('invalid-address').html('');
	map = new GMap(document.getElementById("google-map")); 
	gdir  = new GDirections(map, document.getElementById("step-by-step"));
        GEvent.addListener(gdir,'load', function (){ 
            gdir.getMarker(0).getIcon().image='http://static.usnews.com/images/ah/map-icon-nursing_home.png'
            gdir.getMarker(0).getIcon().iconSize.width = 32;
            gdir.getMarker(1).getIcon().iconSize.width = 32;
            gdir.getMarker(1).getIcon().image='http://static.usnews.com/images/ah/map-icon-school.png'            
            map.addControl(new GSmallMapControl());
            map.addControl(new GMapTypeControl());
        });
        GEvent.addListener(gdir,'error', function (){ 
            drawMap();
            $('.error-marker').addClass('invalid-address').text('Please enter a valid address');
        });
        city_state = $('.college_address').text();
	toAddress = $('.college_name').text() + ', ' + $('.college_address').text();
	streetAddress = document.getElementById("start-address").value;
	city = document.getElementById("city").value;
	state = document.getElementById("state").options[document.getElementById("state").selectedIndex].value;
	zip = document.getElementById("zip-code").value;
	fromAddress = streetAddress + ", " + city + " " + state + " " + zip;
	gdir.load("from: " + fromAddress + " to: " + toAddress); 
	 
	$('#step-by-step').show();
  setTimeout(function() {
    $('#step-by-step img').each(function() {
                if ( $(this).attr('src') == 'http://maps.gstatic.com/intl/en_us/mapfiles/icon_greenA.png' ) {
                  $(this).attr('src','http://static.usnews.com/images/ah/map-icon-nursing_home.png');
                } else if ( $(this).attr('src') == 'http://maps.gstatic.com/intl/en_us/mapfiles/icon_greenB.png' ) {
                  $(this).attr('src','http://static.usnews.com/images/ah/map-icon-school.png');
                } else {
                 $(this).hide();
                }
              });
  },1000);
  $('#step-by-step').show();
}
//]]>
</script>

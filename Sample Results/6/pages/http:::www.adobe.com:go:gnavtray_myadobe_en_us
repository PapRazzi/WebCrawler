<div class="LayoutBody TextSmall Link">
	<div class="LayoutGrid-1-4">
		<div id="LayoutBody-Content">
			<div class="myoverview"><script type="text/javascript">
    $(document).ready(function(){
        $("#screenName").bind("screenNameChange",function(e){
            var screenName = $("#screenName").html();
            if(screenName != ""){
                $("#welcomeScreenNameValue").html(", "+ screenName );
            }
        })
    })
</script>

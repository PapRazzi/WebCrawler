    (function($) {
      $('.share_button_container .share').each(function() {
        $(this).click( function() {
          var shareButton = $(this);
          var shareContainer = $(this).closest(".share_button_container").find(".share_container");
          if(shareButton.hasClass("share_block"))
          {
            shareButton.removeClass("share_block");
            shareContainer.hide();
          }
          else
          {
            shareButton.addClass("share_block");
            shareContainer.show();
            shareContainer.find("input[type=text]").click(function() {
              $(this).select();
            });
          }
          });
      });
      $(document).mousedown(function(e){
        $('.share_button_container .share').each(function() {
          var shareButton = $(this);
          var shareContainer = $(this).closest(".share_button_container").find(".share_container");
          if (shareContainer.has(e.target).length === 0 && !$(e.target).hasClass("share_block"))
          {
            shareButton.removeClass("share_block");
            shareContainer.hide();
          }
        });
      });
    })(jQuery);

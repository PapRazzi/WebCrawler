<script language="javascript">
var scrollEvent = setTimeout(scrollEventFunction, 15000);

YUE.onDOMReady( function(){
    if( pageCustomEvents_2 && pageCustomEvents_2.utmInvokeSuccess  ){
    	pageCustomEvents_2.utmInvokeSuccess.subscribe( updateUtmInvoke ); 
    }
});

function updateUtmInvoke(){
	clearTimeout(scrollEvent);
	scrollEventFunction();
}

function scrollEventFunction(){
    var aTrBuyingProducts = YUD.getElementsByClassName('buyingProducts','li',get('latestBuyingLeads'));
    var numberInAView=6;
    var trCount=0;
    var brushScreen=aTrBuyingProducts.length/numberInAView;
    for(i=trCount*numberInAView; i<(trCount+1)*numberInAView ; i++){
    	YUD.setStyle(aTrBuyingProducts[i],'display','');
    }
    trCount++;
    if(trCount>=brushScreen){
    	trCount = 0;
	}
    
    function  brushByTime(){
    	YUD.setStyle(aTrBuyingProducts,'display','none');
    	for(i=trCount*numberInAView; i<(trCount+1)*numberInAView ; i++){
    		YUD.setStyle(aTrBuyingProducts[i],'display','');
    	}
    	trCount++;
    	if(trCount>=brushScreen){
    		trCount = 0;
		}
    	setTimeout(brushByTime,15000);
    }
	setTimeout(brushByTime,15000);
}
</script>

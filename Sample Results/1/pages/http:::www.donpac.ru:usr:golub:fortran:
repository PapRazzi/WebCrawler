<script type="text/javascript">

	function on_select_region_func(synonim) {
		var s = document.createElement('script');
		s.type = 'text/javascript';
		//s.async = true;
		if($('#link_' + synonim).attr('href').indexOf('paycard') >= 0){
			s.src = '/sc.jsp?reg=' + synonim;
		}else{
			s.src = '//www.rt.ru/sc.jsp?reg=' + synonim;
		}
		var loaded = function() {
			if(on_select_region_callback_func != undefined){
				on_select_region_callback_func(synonim);
			}else{
		 		window.location = $('#link_' + synonim).attr('href');
			}
		};
		if($.browser.msie){
			s.onreadystatechange = function() {
				if(this.readyState == 'complete' || this.readyState == 'loaded'){
					loaded();	
				}
			};
		}else{
			s.onload = loaded;
		}
		

		var x = document.getElementsByTagName('script')[0].parentNode;
		x.insertBefore(s, (x.hasChildNodes() ? x.childNodes[0] : null));
	}
	
	var show_region_form = function() {
		$("#show_region_form_link").trigger('click');
	};
</script>


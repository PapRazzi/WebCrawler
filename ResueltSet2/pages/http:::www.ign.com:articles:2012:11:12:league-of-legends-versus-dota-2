    <div class="shareThis addthis_toolbox addthis_default_style" addthis:url="http://www.ign.com/articles/2012/11/12/league-of-legends-versus-dota-2">
        <a class="addthis_button_facebook_like shareThis-shareCounter" fb:like:layout="button_count" fb:like:height="25"  fb:like:width="90"></a>
        <a class="addthis_button_tweet shareThis-shareCounter" tw:via="IGN"></a>
        <a class="addthis_button_google_plusone shareThis-shareCounter" g:plusone:size="medium"></a>

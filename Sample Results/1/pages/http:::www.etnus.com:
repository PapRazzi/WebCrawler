<script type="text/javascript">

	function inputFocus() {
		document.getElementById('query-input').value = '';
	}

	function inputBlur() {
		var queryInput = document.getElementById('query-input');
		if (!queryInput.value) {
			queryInput.value = 'Search';
		}
	}

	function submitQuery() {
		window.location = '/search.aspx?q='
			+ encodeURIComponent(
				document.getElementById('query-input').value);
		return false;
	}

</script>

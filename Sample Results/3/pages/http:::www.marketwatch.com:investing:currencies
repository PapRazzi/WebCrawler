    <script type="text/javascript">
		$(function() {
            //add format for currencies before binding
            $("#crossRates td.bgQuote").each(function() {
                $(this).bind("bgTick", function(e, message){
                    var element = $(this).find(".reciprocal");
                    if (message && message.b && message.b.l && (message.b.l != 0) && element.length > 0) {
                        var reciprocal = 1/message.b.l
                        element.text(reciprocal.numberFormat("#,###.0000"));
                    }
                });
            });
        });
    </script>

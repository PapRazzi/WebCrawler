	var Language_URL = new function() {
		var regex = /^https?:\/\/[^\/]+\/(language\.[a-z]+\/)?(.*?)$/;
		var match = regex.exec(document.location.href);
		this.relative_url = match[2];
		if (!this.relative_url) {
			// need this because /language.xx/ returns a 404
			this.relative_url = 'index.mhtml';
		}
	}
</script>


<script type="text/javascript">
    function signupSubmit() {
        var f = document.signupForm;
        var errors = "";
        errors += valid_email('E-Mail Address',true,100,f.email.value);
        if (errors != "") {
            alert(errors);
        } else {
            f.action = "/WileyCDA/Section/id-400490.html";
            f.submit();
        }
    }
</script>


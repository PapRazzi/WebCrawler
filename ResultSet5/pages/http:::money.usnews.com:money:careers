//CONTEXTUAL SEGMENTOR  usn_segment -> ind_chnl
var usn_segment = "widget"; //default value
var patharray = window.location.pathname.split('/');
var slashes = patharray.length - 1;
var crumbarray = ($('.breadcrumbs') != null) ? $('.breadcrumbs a') : '';

if (patharray[slashes] == "careers" && slashes == 2 || slashes == 1)
  usn_segment = "sectionfront";
if (patharray[3] == "outside-voices-careers")
  usn_segment = "ovblog";
if (patharray[3] == "slideshows")
  usn_segment = "slideshows";
if (patharray[2] == "best-jobs" & slashes > 2)
  usn_segment = "jobprofile";
if (patharray[3] == "rankings") {
  switch (patharray[4]) {
    case "best-technology-jobs":
      usn_segment = "techrankings";
      break;
    case "best-healthcare-jobs":
      usn_segment = "healthrankings";
      break;
    case "best-social-services-jobs":
      usn_segment = "socialserv";
      break;
    case "best-business-jobs":
      usn_segment = "biz";
      break;
    case "best-creative-services-jobs":
      usn_segment = "creativeserv";
      break;
  }
}

if (crumbarray.length == 3) {
  switch (crumbarray[2].innerHTML) {
    case "Technology":
      usn_segment = "techrankings";
      break;
    case "Healthcare":
      usn_segment = "healthrankings";
      break;
    case "Social Services":
      usn_segment = "socialserv";
      break;
    case "Business":
      usn_segment = "biz";
      break;
    case "Creative Services":
      usn_segment = "creativeserv";
  }
}

// SETUP
var ind_pub = "2752309712108224";
var ind_chnl = usn_segment; // optional channel variable for publisher use
var ind_el = "joblist";
var ind_pf = null; // element id and class prefix (optional)
var ind_q = null;
var jobflag = location.pathname.indexOf('best-jobs');
if (jobflag >= 0) {
    var job = location.pathname.substring(jobflag+10); 
    if (indeed_kws[job] !== undefined) {
      ind_q = indeed_kws[job];
    }
}
var ind_l = null; // optional, null for IP lookup
var ind_n = 4; // number of jobs to return
var ind_d = "http://www.indeed.com";
var ind_t = 40; // character limit for job titles
var ind_c = 30; // character limit for companies

</script>
<script type="text/javascript" src="http://usnews.indeed.com/common/js/widget.js"></script></p>


    

  </div>


    
    <!--/subwidgets-->
  



      

      


      

    
  </div>

  
  
    

  

  <div class="widget widget-even promo media-showpiece  ">


    

    
      

      

  

  

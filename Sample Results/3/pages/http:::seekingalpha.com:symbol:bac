      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">BAC</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 10,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['bac']
      });
    })();

    try { SymbolData.handle_data('BAC', {"rt_time":"11:37AM ET,02/15","last":12.06,"high":12.21,"open":12.14,"close":12.13,"low":12.04,"at":1360928263,"volume":52179972,"volume_at":1360928146,"avg3vol":167159000.0,"div_rate":0.04,"forward_pe":12.0,"payout":16.0,"trailing_pe":48.7,"div_yield":0.3,"div_grow_rate":-55.55,"payout_5y":165.0,"div_yield_5y":2.1,"chgp_1w":3.5,"chgp_4w":9.2,"chgp_52w":51.7,"beta60":2.38,"beta36":1.78,"mov_avg_10d":11.82,"mov_avg_10w":11.49,"mov_avg_200d":9.06,"short_interest":0.9,"p_short_interest_float":1.4,"liquidity":1212439100.0,"p_week_vol_shares":4.89,"sfloat":10767768829.0,"shares_held":6235603361.0,"p_shares_by_inst":57.8,"p_shares_by_insid":0.1,"net_insid_tr":30000.0,"insid_sb":211742.0,"insid_ss":181742.0,"total_revenue":22307999997.0,"revenue_growth":-13.2,"net_income":4188000000.0,"eps":0.25,"roe":1.9,"roe_ind":36.5,"roa":0.3,"int_cover":1.4,"debt_eq":1.46,"lt_debt_cap":0.6,"debt_short":30731000000.0,"debt_long":275585000000.0,"low52":6.72,"high52":12.42,"market_cap":129983622669.90001,"debt":306316000000.0,"total_enterprise":436299622669.9,"price_book":0.5911764705882354,"ev_ebitda":24.071703321046716,"last_div":0.01,"last_div_date":"02/27/13","iop":0.0,"iopp":0.0,"price_sales":5.8267716822386735,"price_tangb":0.9421875,"ev_sales":19.55798918453353,"ev_fcf":11.733414511277907,"inst_chg":538351237.0,"cf":3987888921.0499997,"fcf":37184369669.25,"debt_fcf":8.237762337364863,"rt_ts":1360928260,"chg_vs_mkt_1w":3.0,"chg_vs_mkt_4w":7.0,"chg_vs_mkt_52w":36.0,"p_close_10w":6.0,"p_close_200d":34.0}) } catch (e) { }

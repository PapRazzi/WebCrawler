<script>
 				jQuery.tablesorter.addParser({
					id: 'utf8',
					is: function(s) {
						return false;
					},
					format: function(s) {
						return s.replace(/Å/,'A').replace(/É/,'E').replace(/Î/,'I').replace(/é/,'e');
					},
					type: 'text'
				});
				
				  jQuery.tablesorter.addParser({ 
						        id: 'currency', 
						        is: function(s) { 
						            return false; 
						        }, 
						        format: function(s) { 
						            return s.toLowerCase().replace(/,/,'.'); 
						        }, 
						        type: 'numeric' 
		   		 }); 
		   		 
		   		 jQuery.tablesorter.addParser({ 
				 			id: 'tc', 
				 		        is: function(s) { 
				 			            return false; 
				 					        }, 
				 				format: function(s) { 
				 				            return s.replace(/JTC /,'00').replace(/TC /,'01'); 
				 					        }, 
				 		        type: 'numeric' 
		   		 }); 
	
		    jQuery(".sortable").tablesorter(); 

</script>

<script type="text/javascript">
    jQuery(document).ready(function(){
        jQuery("#tabs a").unbind('click').bind('click',
            function(e) {
                var jQuerytarget = jQuery(e.target),
                    jQuerytarget_tab = jQuerytarget.closest('div.tab'),
                    target_tab = e.target.id;
                jQuerytarget_tab.addClass('active').siblings().removeClass('active');

                jQuery('div' + jQuerytarget.attr('href')).siblings('div.panel').hide();
                jQuery('div' + jQuerytarget.attr('href')).show();
                jQuery.cookie('target_tab', jQuery(e.target).parents('div.tab').attr('id'));
                return false;
        });
		//Defect # 4897323 version selector changes
		var y = 'PROD_LOCAL';
		//Defect #5544938 productName having single quotes creates issues
		var prodName ="Local"; 
		if(1 != 1) {
			$("#chooseVersionLabel").html(prodName+"<span class='choose-arrow'>&nbsp;</span>");
			//fix to show the selected version as well so commnting the below line
			//$('li[value="'+y+'"]').remove();
			// Always show Choose Version even after a version has been selected.
			$("#chooseVersionLabel").html("Choose Version <span class='choose-arrow'>&nbsp;</span>");
		}
		
		// This allows the user to click outside of the Toggle on the choose menu list to close the window
		$(document).click(function() {
			$('.choose-version-list').hide();
		});
		
		
		//function for the drop down
		 $(".choose-version").click(function (e) {
        e.preventDefault();
        var sibs = $(e.target).siblings();
        $(".choose-version-list").toggle();
        return false;
        
        $(".choose-version-list li a").click(function() {
            $(".choose-version-list").hide();
        });
    });

        /* 'click' the tab stored in the cookie */
        // this isn't working for IE6
        jQuery('div#' + jQuery.cookie('target_tab') + ' a').trigger('click');
        
   //preselect the selected version
        //preselectSelectedIndex();
        
    });
    
    
    function preselectSelectedIndex()
    {
	  // Get a reference to the drop-down
      var dropdownList = document.versionSelectForm.y;
	 var isVersionSelected = false;
      // Loop through all the items
      for (i = 0; i < dropdownList.options.length; i++)
      {    
		 if (dropdownList.options[i].value == 'PROD_LOCAL')
        {
          // Item is found. Set its selected property, and exit the loop
          dropdownList.options[i].selected = "selected";
		  isVersionSelected =true;
          break;
        }
      }
		if(!isVersionSelected) {
			//create an option element and add to the drop down list
        	var op = document.createElement("option");
			alert("option element="+op);
        	dropdownList.options[dropdownList.options.length]=	new Option("Choose Version","",false,true);
		}
    }
	
	
	



    	
</script>

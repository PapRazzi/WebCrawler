<script type="text/javascript">
    (function () {
        if (typeof($) == 'undefined') return;
        var programs = [
                'bachelors',
                'mba',
                'engineering',
                'computer-information-technology',
                'education',
                'nursing'
            ],
            inputTrackerID = $('#tracker_scid'),
            path = window.location.pathname.substring(1),
            pathParts = path.split('/'),
            trackerID = inputTrackerID.val();
        if (pathParts[3] == 'rankings') {
            trackerID += '_Rankings';
        } else if ($.inArray(pathParts[2], programs) != -1) {
            trackerID += '_' + pathParts[2];
        } else if (typeof(pathParts[2]) != 'undefined') {
            trackerID += '_School';
        }
        inputTrackerID.val(trackerID);
    })();
</script>

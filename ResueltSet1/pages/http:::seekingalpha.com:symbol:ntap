      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">NTAP</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['ntap']
      });
    })();

    try { SymbolData.handle_data('NTAP', {"rt_time":"04:00PM ET,02/15","last":36.55,"high":36.61,"open":35.69,"close":35.86,"low":35.56,"volume":8517500,"volume_at":1361063419,"at":1361063419,"avg3vol":5505500.0,"c_share":5.07,"div_rate":0.0,"forward_pe":23.5,"payout":0.0,"trailing_pe":26.5,"div_yield":0.0,"payout_5y":0.0,"chgp_1w":2.2,"chgp_4w":6.4,"chgp_52w":-13.4,"beta60":1.58,"beta36":1.69,"mov_avg_10d":35.81,"mov_avg_10w":34.46,"mov_avg_200d":32.45,"short_interest":2.5,"p_short_interest_float":3.9,"liquidity":159233000.0,"p_week_vol_shares":11.11,"sfloat":369981776.0,"shares_held":331475195.0,"p_shares_by_inst":92.5,"p_shares_by_insid":0.2,"net_insid_tr":0.0,"insid_sb":82344.0,"insid_ss":82344.0,"total_revenue":6318400000.0,"revenue_growth":1.9,"net_income":512200000.0,"gross_margin":63.8,"eps":1.38,"roe":11.1,"roe_ind":76.6,"roa":5.0,"cur_ratio":1.9,"quick_ratio":1.7,"int_cover":8.2,"debt_eq":0.28,"lt_debt_cap":0.0,"debt_short":1243300000.0,"debt_long":994300000.0,"low52":26.26,"high52":46.8,"market_cap":13095276654.65,"cash":5570699388.21,"debt":2237600000.0,"total_enterprise":9762177266.439999,"price_book":2.973962571196094,"ev_ebitda":10.095322922895551,"price_sales":2.072562144633135,"price_tangb":3.972826086956522,"ev_sales":1.5450394508799694,"ev_fcf":9.112723742932415,"inst_chg":-13359242.0,"cf":798973103.6899999,"fcf":1071268869.97,"debt_fcf":2.0887380028719233,"rt_ts":1360944000,"chg_vs_mkt_1w":2.0,"chg_vs_mkt_4w":4.0,"chg_vs_mkt_52w":-22.0,"p_close_10w":6.0,"p_close_200d":13.0}) } catch (e) { }

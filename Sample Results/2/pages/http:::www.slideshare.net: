// <![CDATA[
  // homepage trackers
  slideshare_object.ga('homepage', 'pageload');
  $('a[class*=ga-track], input[class*=ga-track]').live('click', function(){
    var label = $(this).attr('class').match(/ga\-track\-(.*)/)[1];
    slideshare_object.ga('homepage', label);
  });
  $('div.newfeatures a').live('click', function(){
    slideshare_object.ga('homepage', 'latest_features');
  });
  $('ul.mainNav li a, ul.subNav li a').click(function() {
    var label = ($(this).parent().parent().hasClass('mainNav') ? 'main_nav_' : 'second_nav_') + $(this).html();
    slideshare_object.ga('homepage', label);
  });
  $('.ss-categories ul li a').click(function() {
    var label = 'category_' + $(this).html();
    slideshare_object.ga('homepage', label);
  });
  $('.topevents h2 a').click(function() {
    var label = 'spotlight_' + ($('.topevents h2 a').index(this) + 1) + '_title';
    slideshare_object.ga('homepage', label);
  });
  $('.spotlight .viewall a').click(function() {
    slideshare_object.ga('homepage', 'ssod_all');
  });
  $('.spotlight .docinfo a').click(function() {
    var label = $(this).parent().attr('id') == 'ssod_title' ? 'ssod_slideshow' : 'ssod_user';
    slideshare_object.ga('homepage', label);
  });
  $('.spotlight .thumbnails a').click(function() {
    slideshare_object.ga('homepage', 'ssod_thumbnail');
  });
  $('li a.j-facebook-share').click(function() {
    slideshare_object.ga('homepage', 'fbshare');
  });
// ]]>
</script>

    

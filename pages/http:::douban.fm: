    <script>
        Do.ready(function(){
            var UserRecord = function(){
                this.anony = true;
                this.is_pro = false;
                this.num = {};
                this.init();
            }
            UserRecord.prototype.init = function(is_pro){
                if ($('#user_play_record').length) {
                    var self = this;
                    $(['played', 'liked', 'banned']).each(function(){
                        self.num[this] = parseInt($('#rec_' + this).text());
                    })
                    this.anony = false;
                    this.is_pro = is_pro || $('.pro_icon').length > 0;
                }
            }
            UserRecord.prototype.set_num = function(cate, n){
                if(this.anony) return;
                this.num[cate] = n;
                $('#rec_' + cate).html(n);
            }
            UserRecord.prototype.increase = function(cate) {
                this.set_num(cate, this.num[cate] + 1);
            }
            UserRecord.prototype.decrease = function(cate) {
                this.set_num(cate, Math.max(this.num[cate] - 1, 0));
            }
            window.user_record = new UserRecord();
        });
    </script>

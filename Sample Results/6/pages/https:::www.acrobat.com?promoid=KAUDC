<html>
<head><link rel="canonical" href="https://www.acrobat.com/welcome/en/home.html"/></head>
<body>
<script type='text/javascript'>
if (window.location.hash.indexOf('#d=') != -1)
    window.location.replace(window.location.protocol + '//' + window.location.hostname + '/?' + window.location.hash.substr(1));
else if ("https://www.acrobat.com/welcome/en/home.html".indexOf('#') != -1 && window.location.hash != '')
	window.location.replace("https://www.acrobat.com/welcome/en/home.html&" + window.location.hash.substr(1));
else
	window.location.replace("https://www.acrobat.com/welcome/en/home.html" + window.location.hash);
</script>
</body>
</html>
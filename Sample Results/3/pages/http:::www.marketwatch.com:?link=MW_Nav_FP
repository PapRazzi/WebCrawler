	$(document).ready(function() {
		window.refreshInterval = setInterval("refresh()", 1200000);
		
		try
		{
			// for omniture tracking, immediately reset the refresh cookie to off
			document.cookie = "refresh=off";
		}
		catch(ex){} 
	});
	
    function refresh()
    {
		try
		{
			// for omniture tracking, set the refresh cookie to on to detect a page refresh
			document.cookie = "refresh=on";
		}
		catch(ex){}
		
    var videoPlaying = false;
    if (typeof dj != "undefined" && typeof dj.util != "undefined" && typeof dj.util.Video != "undefined")
      videoPlaying = dj.util.Video.isVideoPlaying();
    if (MarketWatch.LiveVideo.isVideoPlaying())
      videoPlaying = true;
    if (videoPlaying == false) {        
        clearInterval(window.refreshInterval); 
		    window.location.reload();
      }
    }

</script>





<script language=javascript>
function selectSome(){
	var count=0;
	for(var i=0;i<document.forms[0].vl.length;i++){
		if(document.forms[0].vl[i].checked){
			count++;
		}
	}
document.forms[0].fl[1].checked=(count>0);document.forms[0].fl[1 - 1].checked=(count<=0);}
</script>

<script>
// script to enable and disable checkboxes
function yschccToggle(oChkbx){
	var oContainer = document.getElementById("forf0cccb0");
	if(oContainer){
		var aCheckboxes = oContainer.getElementsByTagName("input");
		var aDivs = oContainer.getElementsByTagName("div");
		var sClassName = oChkbx.checked ? '': 'disabled';
		if(aCheckboxes){
			aCheckboxes[1].disabled = !oChkbx.checked;
			aCheckboxes[2].disabled = !oChkbx.checked;		
		}
		if(aDivs){
				aDivs[1].className =sClassName;
				aDivs[2].className =sClassName;
		}
	}
}
</script>

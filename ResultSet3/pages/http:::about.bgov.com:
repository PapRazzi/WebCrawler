                <script type="text/javascript">
                    var ooyalaLoadCount=0;
                    function receiveOoyalaEvent(playerId, eventName, eventParams){
                        if(ooyalaLoadCount==1 && eventName=="playerEmbedded"){
                            ooyalaLoadCount++;
                            if(jQuery('#heroVideo').parents('#video-container').length==0)
                            {
                            	var heroVid = jQuery('#heroVideo').parentsUntil('body');
                                if(heroVid.length==0){
                                    heroVid = jQuery('#heroVideo')[0];
                                }else{heroVid=heroVid.eq(0)[0];}
                            	heroVid.parentNode.removeChild(heroVid);
                            	document.getElementById('video-container').appendChild(heroVid);
                            }
                        }
                        else if(ooyalaLoadCount==2 && eventName=='apiReady'){
                            ooyalaLoadCount++;
                            jQuery('#video-preview').hide();
                            jQuery('#heroVideo')[0].playMovie();
                        }
                        else if(ooyalaLoadCount==3 && eventName=='playheadTimeChanged'){
                            ooyalaLoadCount++;                            
                            jQuery('#video-preview').hide();
                        }
                    }
                    (function($){

                        $('#video-preview').show();
                        $('#video-play').click(function(){
                            if(ooyalaLoadCount==0){
                                ooyalaLoadCount++;
                                $('#video-play').hide();
                                $('#video-loading').show();
                                
                                
                                var scriptTag = document.createElement( 'script' );
                                scriptTag.type = 'text/javascript';
                                scriptTag.src = 'http://player.ooyala.com/player.js?callback=receiveOoyalaEvent&playerId=heroVideo&autoplay=1&width=670&height=377&embedCode=93aGVnMzojZ9s4nS1QtjJUBzfEv4AE3q&videoPcode=J5cG060yjtn4c_ue6f98t2RIrRrd&wmode=transparent';
                                //scriptTag.innerHTML='var yuyiokj=0;';
                                
                                //var vid=document.getElementById('video-container');
                                //vid.appendChild(scriptTag);
                                if('function' == typeof __overrideWrite)
                                {document.writeln = document.write = __overrideWrite;}
                                document.getElementsByTagName('head')[0].appendChild(scriptTag);
                            }
                        });
                    }(jQuery));


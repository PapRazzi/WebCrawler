       lat: 40.714550,
       lon: -74.007124      }),

    // create a Map instance
    map = new Y.YMaps.Map({
        boundingBox: Y.one('#map'),  // place map in "map" div
        center: yahooGeo
        });

    // display the map 
    map.render(); 
    map.after("renderComplete", function(){
        map.draw(new Y.YMaps.Marker({
        geoLocation: new Y.YMaps.GeoLocation({
         lat: 40.714550,
       lon: -74.007124         }),
               label:  '<img src="http://l.yimg.com/na/global/p/globalmaps/img/star-1237.png">'
        }));
        zoom = new Y.YMaps.ControlSimpleZoom({ map: this });
        zoom.get('boundingBox').setStyles({
            top: '50px',
            right: '40px'
        });
        this.draw(zoom);
         pan = new Y.YMaps.ControlPan({ map: this });
        pan.get('boundingBox').setStyles({
            top: '10px',
            right: '6px'
        });
        this.draw(pan);
        myScale = new Y.YMaps.ControlScale({ map: this});
       myScale.get("boundingBox").setStyles({
            "bottom": "20px",
            "left": "40px"
            });
       this.draw(myScale);
  });
   // End of Map render complete
});	
		});

	})

}

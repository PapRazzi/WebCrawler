    var $= jQuery.noConflict();
$(document).ready(tigerStripe);

//pulled this into a separate function so other scripts on the page can call it
function tigerStripe() {
    //first strip the classes, so resorted tables work.
    $(".addcolor > tbody > tr").removeClass("even odd");

    $(".addcolor > tbody > tr:nth-child(even)").addClass("even");
    $(".addcolor > tbody > tr:nth-child(odd)").addClass("odd");
};

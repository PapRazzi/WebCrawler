<!-- Begin articleInfo -->

	
		
	
	





<div id="articleInfo">
	<p class="page_topper">PREVIEW</p>
	<div class="floatleft" style="width: 100%;">
	<h1>League of Legends Interview</h1>
	<h3>
	
	(<a href="/games/pc/league-of-legends/">PC</a>)
	
	</h3>
	<h4>The Warcraft connection, community influence, and Basshunter possibilities.</h4>
	</div>
	
	
	<div class="clear"></div>

	
		<div class="byline">
			By <a href="/do/my1Up?publicUserId=5685007">Daniel Perez</a> &nbsp;
			01/16/2009
		</div>
	
	
	
















<!-- Begin socialTools -->


	
	




	
	




<div id="socialBar-3172308" class="socialBar">
<ul>

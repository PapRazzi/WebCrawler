		this.validateHashKey = function(hash_key){
			var isValid = false;
			for(var x in this.previewData){
				if(x == hash_key) isValid = true;
			}
			return isValid;
		}
		this.loadThumbs = function(){
			var thumb_content = '<ul class="animate">';
			for(var x in this.previewData){
				thumb_content += ''+
				'<li>'+
				'	<div class="thumb '+x+'"></div>'+
				'</li>';
			}
			thumb_content+='</ul>';
			$('content').innerHTML = thumb_content;
			// set the container ul width
			$$('#about_us_details .thumbs_section #nav li#content ul')[0].style.width = this.thumb_image_width * this.total_number_of_thumbs + 'px';
		}
		this.navigateTo = function(leaderID){
			if (!leaderID || leaderID == '') return;
			
			// if leaderID is not a valid id, close the modal window and return
			if(!this.validateHashKey(leaderID)){
				this.hide();
				return;
			}
			
			if (!this.visible) this.show();
			this.setActive(leaderID);
			this.currentIndex = self.getPreviewDataIndex(leaderID);
			var gotoPage = Math.floor(this.currentIndex / this.number_of_thumbs_per_page);
			this.navigateToPage(gotoPage);
			this.showProfile(leaderID);
			Hash.go(leaderID);
		}
		this.setActive = function(leaderID){
			$$('#nav #content ul li').each(function(x){x.removeClassName('active');});
			$$('#nav #content ul li .'+leaderID)[0].up().addClassName('active');
		}
		this.navigateToPage = function(page){
			if(page < 0) this.currentPage = 0;
			else if(page > this.lastPage) this.currentPage = this.lastPage;
			else this.currentPage = page;

			var newOffsetX = this.currentPage * (-this.thumb_image_width * this.number_of_thumbs_per_page);
			$$('#about_us_details .thumbs_section #nav li#content ul')[0].style.left = newOffsetX + 'px';

			this.setArrowDisplayState();
		}
		this.setArrowDisplayState = function(){
			if(this.currentPage==0){
				$$('#left span.left_arrow')[0].style.display = 'none';
				$$('#right span.right_arrow')[0].style.display = '';
			}else if(this.currentPage == this.lastPage){
				$$('#left span.left_arrow')[0].style.display = '';
				$$('#right span.right_arrow')[0].style.display = 'none';
			}
		}
		this.showProfile = function(leaderID){

			if(this.active_switch == 1){
				this.active_switch = 0;
			}else{
				this.active_switch = 1;
			}
			$$('#about_us_details .details_section #pic_container_' + this.active_switch)[0].className = 'pic_container animate_opacity opacity_10 ' + leaderID;
			$('name_' + this.active_switch).innerHTML = this.previewData[leaderID].name;
			$('title_' + this.active_switch).innerHTML = this.previewData[leaderID].title;
			$('desc_' + this.active_switch).innerHTML = this.previewData[leaderID].desc;

			var twitter_info = this.previewData[leaderID].twitter;

			if(twitter_info) $('twitter_' + this.active_switch).innerHTML = '<a href="' + twitter_info.link + '" target="_blank">' + twitter_info.id + '</a>';
			else $('twitter_' + this.active_switch).innerHTML = '';

			$('name_' + this.active_switch).addClassName('opacity_10');
			$('title_' + this.active_switch).addClassName('opacity_10');
			$('desc_' + this.active_switch).addClassName('opacity_10');
			$('twitter_' + this.active_switch).addClassName('opacity_10');
			$('twitter_' + this.active_switch).style.zIndex = 10;

			this.doFade();
		}
		this.doFade = function(){
			var off = this.active_switch==1?0:1;
			$$('#about_us_details .details_section #pic_container_' + off)[0].removeClassName('opacity_10');
			$$('#about_us_details .details_section #pic_container_' + off)[0].addClassName('opacity_0');

			$('name_' + off).removeClassName('opacity_10');
			$('name_' + off).addClassName('opacity_0');

			$('title_' + off).removeClassName('opacity_10');
			$('title_' + off).addClassName('opacity_0');

			$('desc_' + off).removeClassName('opacity_10');
			$('desc_' + off).addClassName('opacity_0');

			$('twitter_' + off).removeClassName('opacity_10');
			$('twitter_' + off).addClassName('opacity_0');

			$('twitter_' + off).style.zIndex = 1;
		}

		this.setOpacity = function(element,opacity){
			element.style.opacity = opacity;
			element.style.filter = 'alpha(opacity=' + opacity * 100 + ')';
		}

		this.insertOverlay = function(){
			this.deleteOverlay();
			var overlay = document.createElement('div');
			overlay.setAttribute('id','p_overlay');
			this.setOpacity(overlay,0);
			if(this.isIE6){
				overlay.style.position = 'absolute';
				overlay.style.left = '0px';
				overlay.style.top = '0px';
				overlay.style.width = '100%';
				overlay.style.height = document.body.offsetHeight + 'px';
				overlay.style.backgroundColor = 'white';
			}
			document.getElementsByTagName('body')[0].appendChild(overlay);
			$('p_overlay').observe('click',function(){
				self.hide();
			});

			var opacity_ct = 0;
			var overlay_interval = setInterval(function(){
				self.setOpacity(overlay,opacity_ct);
				opacity_ct += 0.05;
				if(opacity_ct >= 0.8){
					clearInterval(overlay_interval);
					overlay_interval = null;
				}
			},10);
		}
		this.removeOverlay = function(){
			var overlay = $('p_overlay');
			if(overlay){
				var opacity_ct = 0.8;
				var overlay_interval = setInterval(function(){
					self.setOpacity(overlay,opacity_ct);
					opacity_ct -= 0.05;
					if(opacity_ct < 0){
						clearInterval(overlay_interval);
						overlay_interval = null;
						self.deleteOverlay();
					}
				},10);
			}
		}
		this.deleteOverlay = function(){
			if($('p_overlay')) $('p_overlay').remove();
		}

		this.show = function(){

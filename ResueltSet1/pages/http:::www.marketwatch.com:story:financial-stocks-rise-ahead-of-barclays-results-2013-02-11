		</div> <script type="text/javascript">if(typeof(window.sm)=='undefined'){ sm={}; } if(typeof(window.sm.module)=='undefined'){ sm.module={}; }sm.module.widgets = {
	
	init: function() {
		var topfundsURL="/top25funds/widget/";
 		var marketmoversURL="/marketmovers/widget/";
		
		$("#widgetForm input").focus(function() {
			$(this).removeClass("errorField");
			$(".widgetContainer .inputError").hide();
		});
		
		$("#widgetForm .focusHighlight input").focus(function(e) {
			this.select();
		});
		
		$("#widgetForm .focusHighlight input").mouseup(function(e) {
			e.preventDefault();
		});
		
		/** for sliders **/
		if($("#widgetSlider").length > 0) {
			$("#widgetSlider").myslider({
				minimum: 10,
				slide: function(val) {
					$(".widgetContainer #savings").val(val);
					$(".widgetContainer .annualSavings").text(val + "%");
				}
			});
		}		
		/** market movers **/
		if($("#marketmoversWidget").length > 0 ) {
			$.ajax({
				url:marketmoversURL,
				success:function(a){
					$("#marketmoversWidget").html(a);
				}
			});
		}
		/** top funds **/
		if ($("#topfundsWidget").length > 0) {
			$.ajax({
				url:topfundsURL,
				success:function(a){
					$("#topfundsWidget").html(a);
				}
			});
		}
	},
	isNumber: function(o) {
		return ! isNaN (o-0);
	},
	validateFields: function(actionUrl,el) {
		var isValid = true;
		var msg = "";
		var $el = $(el).parents("form");
		var $widgetCtr = $(el).parents(".widgetContainer");
		
		$el.find(".intPosVal").each(function() {
			if(!sm.module.widgets.isNumber($(this).find("input").val())) {
				$(this).find("input").addClass("errorField");
				msg = "Numerical inputs only.";
				isValid = false;
				return false;
			}
			else if(parseInt($(this).find("input").val(),10) < 0) {
				$(this).find("input").addClass("errorField");
				msg = "Positive values only.";
				isValid = false;
				return false;
			}
		});
		
		if(isValid) {
			$el.attr("action", actionUrl);
			el.parentNode.parentNode.onsubmit = null;
			$el.submit();
		}
		else{
			$widgetCtr.find(".inputError").find("span").text(msg);
			$widgetCtr.find(".inputError").show();
			$el.removeAttr("action");
			el.parentNode.parentNode.onsubmit = sm.module.widgets.submitHandler;
		}
	},
	submitHandler: function() {
		return false;
	}
};$(document).ready(function() {sm.module.widgets.init(); });/**
 * Responsibel for setting mouse events and responding to mouse events.
 * Suports: Callout on mouse move and NavigationMenu on mouse click.
 */
var WIDGET_ACTION = WIDGET_ACTION || {};
/*------------
 PUBLIC
 ------------*/
WIDGET_ACTION.init = function(main)
{
	var mainMap = $('#widgetMap');
	mainMap.mousemove(function(e) { WIDGET_ACTION.mouseMove(e); });

	WIDGET_ACTION.callout = $('<div class="widgetCallout" >');
	main.append(WIDGET_ACTION.callout);

	WIDGET_ACTION.calloutTitle = $('<div class="widgetDetails" >');
	main.append(WIDGET_ACTION.calloutTitle);

	WIDGET_ACTION.outerUpArrow = $('<div class="widgetCalloutArrow calloutArrowOuterUpw">');
	main.append(WIDGET_ACTION.outerUpArrow);

	WIDGET_ACTION.innerUpArrow = $('<div class="widgetCalloutArrow calloutArrowInnerUpw">');
	main.append(WIDGET_ACTION.innerUpArrow);

	WIDGET_ACTION.outerDownArrow = $('<div class="widgetCalloutArrow calloutArrowOuterDownw">');
	main.append(WIDGET_ACTION.outerDownArrow);

	WIDGET_ACTION.innerDownArrow = $('<div class="widgetCalloutArrow calloutArrowInnerDownw">');
	main.append(WIDGET_ACTION.innerDownArrow);

	main.mouseleave(function()
	{
		WIDGET_ACTION.callout.css('visibility', 'hidden');
		WIDGET_ACTION.calloutTitle.css('visibility', 'hidden');
		WIDGET_ACTION.outerUpArrow.css('visibility', 'hidden');
		WIDGET_ACTION.innerUpArrow.css('visibility', 'hidden');
		WIDGET_ACTION.outerDownArrow.css('visibility', 'hidden');
		WIDGET_ACTION.innerDownArrow.css('visibility', 'hidden');
	});
};

/*------------
 PRIVATE
 ------------*/
WIDGET_ACTION.BORDER = 5;
WIDGET_ACTION.ARROW_INDENT = 20;
WIDGET_ACTION.positionCallout = function(nodeInfo)
{

	if(nodeInfo === null || nodeInfo.node === null || nodeInfo.top === null)
	{
		WIDGET_ACTION.callout.css('visibility', 'hidden');
		WIDGET_ACTION.calloutTitle.css('visibility', 'hidden');
		WIDGET_ACTION.outerUpArrow.css('visibility', 'hidden');
		WIDGET_ACTION.innerUpArrow.css('visibility', 'hidden');
		WIDGET_ACTION.outerDownArrow.css('visibility', 'hidden');
		WIDGET_ACTION.innerDownArrow.css('visibility', 'hidden');
		return;
	}

	var div = nodeInfo.node.div;
	var topDiv = nodeInfo.top.div;

	WIDGET_ACTION.calloutTitle.text(nodeInfo.node.cell.getTitle());

	var h =  parseInt(topDiv.nodeHeight,10);
	var w =  parseInt(topDiv.nodeWidth,10);

	var calloutHeight = (WIDGET_ACTION.calloutTitle.outerHeight() + WIDGET_ACTION.BORDER * 2);
	WIDGET_ACTION.callout.css("height", calloutHeight + "px");

	var calloutWidth = (WIDGET_ACTION.calloutTitle.outerWidth() + WIDGET_ACTION.BORDER * 2);
	WIDGET_ACTION.callout.css("width", calloutWidth + "px");

	var outerArrow;
	var innerArrow;
	
	if(div.absoluteTop < h/2 && (div.absoluteTop + div.nodeHeight + calloutHeight / 10) < h)
	{
		var calloutTop = div.absoluteTop + div.nodeHeight * 9 / 10;
		WIDGET_ACTION.callout.css('top', calloutTop + 'px');
		WIDGET_ACTION.callout.css('bottom', 'auto');

		var calloutTitleTop = div.absoluteTop + div.nodeHeight * 9 / 10 + WIDGET_ACTION.BORDER;
		WIDGET_ACTION.calloutTitle.css('top', calloutTitleTop + 'px');
		WIDGET_ACTION.calloutTitle.css('bottom', 'auto');

		outerArrow = WIDGET_ACTION.outerUpArrow;
		innerArrow = WIDGET_ACTION.innerUpArrow;

		WIDGET_ACTION.outerDownArrow.css("visibility", "hidden");
		WIDGET_ACTION.innerDownArrow.css("visibility", "hidden");

		var arrowHeight = outerArrow.outerHeight();
		outerArrow.css('top', (calloutTop - arrowHeight) + 'px');
		outerArrow.css('bottom', 'auto');

		innerArrow.css('top', (calloutTitleTop - arrowHeight) + 'px');
		innerArrow.css('bottom', 'auto');
	}
	else if((h - div.absoluteTop + calloutHeight * 9 / 10) < h)
	{
		var calloutBottom = Math.max(0, h - div.absoluteTop - div.nodeHeight / 10);
		WIDGET_ACTION.callout.css('bottom', calloutBottom + 'px');
		WIDGET_ACTION.callout.css('top', 'auto');

		var calloutTitleBottom = Math.max(0, h - div.absoluteTop - div.nodeHeight / 10) + WIDGET_ACTION.BORDER;
		WIDGET_ACTION.calloutTitle.css('bottom', calloutTitleBottom + 'px');
		WIDGET_ACTION.calloutTitle.css('top', 'auto');

		outerArrow = WIDGET_ACTION.outerDownArrow;
		innerArrow = WIDGET_ACTION.innerDownArrow;

		WIDGET_ACTION.outerUpArrow.css("visibility", "hidden");
		WIDGET_ACTION.innerUpArrow.css("visibility", "hidden");

		arrowHeight = outerArrow.outerHeight();
		outerArrow.css('bottom', (calloutBottom - arrowHeight) + 'px');
		outerArrow.css('top', 'auto');

		innerArrow.css('bottom', (calloutTitleBottom - arrowHeight) + 'px');
		innerArrow.css('top', 'auto');
	} else
	{
		WIDGET_ACTION.callout.css('top', '0');
		WIDGET_ACTION.callout.css('bottom',  'auto');

		WIDGET_ACTION.calloutTitle.css('top', WIDGET_ACTION.BORDER + 'px');
		WIDGET_ACTION.calloutTitle.css('bottom',  'auto');

		outerArrow = WIDGET_ACTION.outerDownArrow;
		innerArrow = WIDGET_ACTION.innerDownArrow;

		WIDGET_ACTION.outerUpArrow.css("visibility", "hidden");
		WIDGET_ACTION.innerUpArrow.css("visibility", "hidden");

		outerArrow.css('top', calloutHeight + 'px');
		outerArrow.css('bottom', 'auto');

		innerArrow.css('top', (calloutHeight - WIDGET_ACTION.BORDER) + 'px');
		innerArrow.css('bottom', 'auto');
	}

	if(div.absoluteLeft < w/2 && (div.absoluteLeft + div.nodeWidth + calloutWidth * 2 / 3) < w)
	{
		
		var calloutLeft = Math.max(0, div.absoluteLeft + div.nodeWidth / 3);
		WIDGET_ACTION.callout.css('left', calloutLeft + 'px');
		WIDGET_ACTION.callout.css('right', 'auto');

		WIDGET_ACTION.calloutTitle.css('left', (Math.max(0, div.absoluteLeft + div.nodeWidth / 3) + WIDGET_ACTION.BORDER) + 'px');
		WIDGET_ACTION.calloutTitle.css('right', 'auto');

		outerArrow.css('left', (calloutLeft + WIDGET_ACTION.ARROW_INDENT) + 'px');
		outerArrow.css('right', 'auto');

		innerArrow.css('left', (calloutLeft + WIDGET_ACTION.ARROW_INDENT) + 'px');
		innerArrow.css('right', 'auto');
	}
	else if( (w - div.absoluteLeft + calloutWidth / 3) < w)
	{
		
		var calloutRight = Math.max(0, w - div.absoluteLeft - div.nodeWidth * 2 / 3);
		WIDGET_ACTION.callout.css('right', calloutRight + 'px');
		WIDGET_ACTION.callout.css('left', 'auto');

		WIDGET_ACTION.calloutTitle.css('right', (Math.max(0, w - div.absoluteLeft - div.nodeWidth * 2 / 3) + WIDGET_ACTION.BORDER) + 'px');
		WIDGET_ACTION.calloutTitle.css('left', 'auto');

		outerArrow.css('right', (calloutRight + WIDGET_ACTION.ARROW_INDENT) + 'px');
		outerArrow.css('left', 'auto');

		innerArrow.css('right', (calloutRight + WIDGET_ACTION.ARROW_INDENT) + 'px');
		innerArrow.css('left', 'auto');
	}
	else
	{
		WIDGET_ACTION.callout.css('left', '0');
		WIDGET_ACTION.callout.css('right',  'auto');

		WIDGET_ACTION.calloutTitle.css('left', WIDGET_ACTION.BORDER + 'px');
		WIDGET_ACTION.calloutTitle.css('right',  'auto');

		var arrowLeft = calloutWidth - WIDGET_ACTION.ARROW_INDENT - outerArrow.outerWidth();
		outerArrow.css('left', arrowLeft + 'px');
		outerArrow.css('right',  'auto');

		innerArrow.css('left', arrowLeft + 'px');
		innerArrow.css('right',  'auto');
	}

	WIDGET_ACTION.callout.css('visibility', 'visible');
	WIDGET_ACTION.calloutTitle.css('visibility', 'visible');
	outerArrow.css('visibility', 'visible');
	innerArrow.css('visibility', 'visible');
};

WIDGET_ACTION.previousCalloutInfo = null; 
WIDGET_ACTION.lastEvent = null;
WIDGET_ACTION.mouseMove = function(evt)
{
	WIDGET_ACTION.lastEvent = evt;

	var leafInfo = WIDGET_MAP.Node.locateLeaf(evt);
	if(leafInfo !== WIDGET_ACTION.previousCalloutInfo)
	{
		WIDGET_ACTION.previousCalloutInfo = leafInfo;
		WIDGET_ACTION.positionCallout(leafInfo);
	}
};/**
 * Represents all the information available for an item (leaf):
 * color, size, name, other...
 * and define recursize getSize(), getValue() and getColor() for each Cell element.
 */
var WIDGET_CELL = WIDGET_CELL || {};
var CONTAINER_WIDGET_CELL = function(name)
{
	this.name = name;
	this.getSize = function() { return sum(function(c){return c.getSize();}); };
	this.getValue = function() { return average(function(c){return c.getValue();}); };
	this.getColor = function() { return WIDGET_CELL.colorCalc(this.getValue()); };

	this.addChild = function(cell)
	{
		this.children.push(cell);
		return cell;
	};

	this.children = [];
	var self = this;
	function average(func)
	{
        var count = 0;
        var sum = 0;
		jQuery.each(self.children, function()
        {
            var add = func(this);
            if (! isNaN(add))
            {
                sum += add;
                count++;
            }
        });

		return sum / (Math.max(1, count));
	}

	function sum(func)
	{
		var _sum = 0;
		jQuery.each(self.children, function() { _sum += func(this); });
		return _sum;
	}
};

CONTAINER_WIDGET_CELL.prototype.getTitle = function()
{
	return WIDGET_CELL.camelCase(this.name);
};

CONTAINER_WIDGET_CELL.prototype.getNews = function(div)
{
	div.empty();
	div.css('display', 'none');
};

CONTAINER_WIDGET_CELL.prototype.getLabel = function()
{
	return this.name;
};



var WIDGET_CELL = function(data)
{
	this.name = data.name;
	this.size = parseFloat(data.size);
    this.value = parseFloat(data.value);
	if(this.size <= 0 || isNaN(this.size)) {
		return;
	}
	WIDGET_CELL.MAP[this.name] = this;

	this.calculate();
};

WIDGET_CELL.prototype.calculate = function()
{
	if(WIDGET_CELL.range === null)
	{
		WIDGET_CELL.range = {min: this.value, max: this.value, scale: Math.abs(this.value)};
	}
	else
	{
		WIDGET_CELL.minMax(this.value);
	}
};

WIDGET_CELL.MAP = {}; 

WIDGET_CELL.update = function(dataList)
{
	WIDGET_CELL.range = null;
	for(var i=0;i<dataList.length;i++)
	{
		var cell = WIDGET_CELL.MAP[dataList[i].name];
		if(cell !== null) {
			cell.update(dataList[i]);
		}
	}
};

WIDGET_CELL.prototype.update = function(updateData)
{
	this.value = parseFloat(updateData.value);
	var newSize = parseFloat(updateData.size);
	if(newSize > 0 && ! isNaN(newSize)) {
		this.size = newSize;
	}
	this.calculate();
};

WIDGET_CELL.formatDollar = function(val)
{
	return val > 1e6 ? ('$' + (val/1e6).toFixed(1) + ' bil.') :
						('$' + (val/1e3).toFixed(1) + ' mil.');
};

WIDGET_CELL.prototype.getLabel = function()
{
    return this.name;
};

WIDGET_CELL.prototype.getTitle = function()
{
	return WIDGET_CELL.camelCase(this.name) + " " + (this.value > 0 ? '+' : '') + this.value + '%';
};

WIDGET_CELL.range = null;
WIDGET_CELL.minMax = function(val)
{
	WIDGET_CELL.range.min = Math.min(WIDGET_CELL.range.min, val);
	WIDGET_CELL.range.max = Math.max(WIDGET_CELL.range.max, val);
	WIDGET_CELL.range.scale = Math.max(WIDGET_CELL.range.scale, Math.abs(val));
};

WIDGET_CELL.MID_VALUE = 0;
WIDGET_CELL.RANGE_VALUE = 4;

WIDGET_CELL.setRange = function(min, max)
{
	WIDGET_CELL.MID_VALUE = (min + max) /2;
	WIDGET_CELL.RANGE_VALUE = Math.abs(max - WIDGET_CELL.MID_VALUE);
};

WIDGET_CELL.prototype.getSize = function()
{
	return this.size;
};

WIDGET_CELL.prototype.getColor = function()
{
	return WIDGET_CELL.colorCalc(this.value);
};

WIDGET_CELL.prototype.getValue = function()
{
	return this.value;
};

WIDGET_CELL.colorCalc = function(v)
{
    if (isNaN(v)) {
        return "#C0C0C0";
	}
	var percent = Math.min(1, Math.abs(v - WIDGET_CELL.MID_VALUE) / WIDGET_CELL.RANGE_VALUE) * 0.85 + 0.15; 
	return (v >= WIDGET_CELL.MID_VALUE) ? '#' + WIDGET_CELL.getLevel(255 - 129 * percent) + WIDGET_CELL.getLevel(255 - 98 * percent) + WIDGET_CELL.getLevel(255 - 251 * percent) :
								   '#' + WIDGET_CELL.getLevel(255 - 59 * percent) + WIDGET_CELL.getLevel(255 - 233 * percent) + WIDGET_CELL.getLevel(255 - 227 * percent);
};

WIDGET_CELL.getLevel = function(v)
{
	v = Math.min(parseInt(v,10), 255);
	return WIDGET_CELL.toHex(parseInt(v / 16, 10)) + WIDGET_CELL.toHex(v % 16);
};

WIDGET_CELL.toHex = function(v)
{
	return v < 10 ? '' + v : 'ABCDEF'.charAt(v - 10);
};

WIDGET_CELL.camelCase = function(str)
{
	var s= '';
	var u = true;
	for(var i=0;i<str.length;i++)
	{
		var c = str.charAt(i);
		s += u ? c : c.toLowerCase();
		u = c == ' ';
	}
	return s;
};/**
 * Builds WIDGET_MAP.Node reresenting a Map.
 */

/*---------------
PRIVATE
-----------------*/
var WIDGET_MAP = WIDGET_MAP || {};

WIDGET_MAP.gridColors = ["white"];
WIDGET_MAP.gridWidths = [2];

WIDGET_MAP.Node = function(cell, parentNode, index)
{
	this.cell = cell;
	this.index = index;
	this.grid = index >= WIDGET_MAP.gridWidths.length ? 0 : WIDGET_MAP.gridWidths[index];

	this.isFullScreen = false;

	this.layout = new WIDGET_LAYOUT(cell.name);
	this.div = $('<div>');
	this.div.addClass('node');
	this.div.gridColor = index >= WIDGET_MAP.gridColors.length ? null : WIDGET_MAP.gridColors[index];

	var color = this.div.gridColor === null ? cell.getColor() : this.div.gridColor;

	this.div.css('backgroundColor', color);

	this.title = $('<div>');
	this.title.addClass('node_title');
	this.div.append(this.title);

	this.parent = parentNode;
	this.children = [];
};

/*---------------
PUBLIC
-----------------*/

WIDGET_MAP.makeRoot = function(parentElement, rootCell)
{
	var rootNode = new WIDGET_MAP.Node(rootCell, null/*parent*/, 0 /*index*/);
	WIDGET_MAP.currentRootNode = rootNode;
	parentElement.append(rootNode.div);

	WIDGET_MAP.ROOTSize.width = parentElement.width();
	WIDGET_MAP.ROOTSize.heigth = parentElement.height();
	return rootNode;
};

WIDGET_MAP.resize = function(parentElement, rootCell)
{
	WIDGET_MAP.ROOTSize.width = parentElement.width();
	WIDGET_MAP.ROOTSize.heigth = parentElement.height();
	WIDGET_MAP.unsetFullScreen(rootCell);
	WIDGET_MAP.currentRootNode.displayAsRoot(false);
};

WIDGET_MAP.unsetFullScreen = function(node)
{
	node.isFullScreen = false;
	jQuery.each(node.children, function( ){WIDGET_MAP.unsetFullScreen(this);});
};

/** Tells this Node to display itself as the new top level view.*/
WIDGET_MAP.Node.prototype.displayAsRoot = function(anim)
{
	var zoomDown = WIDGET_MAP.isAncestor(WIDGET_MAP.currentRootNode, this);
	if(zoomDown && this.parent !== null && (! this.parent.isFullScreen))
	{

		this.parent.maximize(false);
	}

	this.maximize(anim);

	WIDGET_MAP.currentRootNode = this;
};

WIDGET_MAP.Node.prototype.maximize = function(anim)
{
	var width = WIDGET_MAP.ROOTSize.width - WIDGET_MAP.gridWidths[this.index] * 2; 
	var height = WIDGET_MAP.ROOTSize.heigth - WIDGET_MAP.gridWidths[this.index] * 2;

	if(anim) {
		this.div.animate({
			left: '0',
			top: '0',
			width: '' + width,
			height: ''+ height
		}, 500, function() {});
	}
	this.div.css('left', WIDGET_MAP.gridWidths[this.index] + "px");
    this.div.nodeLeft = 0;
    this.div.absoluteLeft = 0;
	this.div.css('top', WIDGET_MAP.gridWidths[this.index] + "px");
    this.div.nodeTop = 0;
    this.div.absoluteTop = 0;
	this.div.width(width);
    this.div.nodeWidth = width;
	this.div.height(height);
    this.div.nodeHeight = height;
	this.isFullScreen = true;

	this.div.css('zIndex', this.index);
	this.title.css('zIndex', this.index);
	this.title.css('visibility', 'hidden');
	this.layout.setSize(this.index, this.grid, width, height);
	for(var i=0;i<this.children.length;i++)
	{
		this.children[i].display(this.layout, this.index, 0, 0, this);
		this.children[i].isFullScreen = false;
	}
};


WIDGET_MAP.Node.locateLeaf = function(mouseEvt)
{
	var ev = mouseEvt || window.event;
	var pos = {x:ev.clientX, y:ev.clientY};

	var off = WIDGET_MAP.currentRootNode.div.offset();
	pos.x -= off.left;
	pos.y -= off.top + parseInt($("body").css("border-top-width").replace(/px/,""),10);

	pos = WIDGET_MAP.adjustForScroll(pos);

	return  WIDGET_MAP.currentRootNode.findLeaf(pos.x, pos.y, 100);
};

WIDGET_MAP.isInsideMap = function(mouseEvt)
{
	var ev = mouseEvt || window.event;
	var pos = {x:ev.clientX, y:ev.clientY};

	var off = WIDGET_MAP.currentRootNode.div.offset();
	pos.x -= off.left;
	pos.y -= off.top + parseInt($("body").css("border-top-width").replace(/px/,""),10);

	return WIDGET_MAP.currentRootNode.contains(pos.x, pos.y);
};

WIDGET_MAP.Node.prototype.repaint = function()
{
	this.redoBackground();
};

WIDGET_MAP.Node.prototype.redoBackground = function()
{
	this.div.css('backgroundColor', this.div.gridColor === null ? this.cell.getColor() : this.div.gridColor);
	jQuery.each(this.children, function() { this.redoBackground();});
};

WIDGET_MAP.Node.prototype.locate = function(filter, collector)
{
	if(filter(this))
	{
		collector.push(this);
	}
	jQuery.each(this.children, function() { this.locate(filter, collector);});
};


/*---------------
PRIVATE
-----------------*/

WIDGET_MAP.ROOTSize = {};
WIDGET_MAP.currentRootNode = null;

WIDGET_MAP.isAncestor = function(target, current)
{
	if(target === null || current === null) {
		return false;
	}
	if(target === current.parent) {
		return true;
	}
	return WIDGET_MAP.isAncestor(target, current.parent);
};


WIDGET_MAP.Node.prototype.findLeaf = function(x, y, maxDepth)
{
	if(! this.contains(x, y)) {
		return null;
	}
	if(this.children.length === 0 || this.index >= maxDepth) {
		return {node: this, top: WIDGET_MAP.currentRootNode};
	}
	x -= this.div.nodeLeft;
	y -= this.div.nodeTop;
	for(var i=0;i<this.children.length;i++)
	{
		var leafInfo = this.children[i].findLeaf(x, y, maxDepth);
		if(leafInfo !== null) { return leafInfo; }
	}
	return null;
};

WIDGET_MAP.Node.prototype.contains = function(x, y)
{
	x -= this.div.nodeLeft;
	y -= this.div.nodeTop;
	return 0 <= x && x <= this.div.nodeWidth &&
	       0 <= y && y <= this.div.nodeHeight;
};

WIDGET_MAP.Node.prototype.addChild = function(cell)
{
	var child = new WIDGET_MAP.Node(cell, this /*parent*/, this.index + 1);
	this.children.push(child);

	this.layout.addChild(cell);
	this.div.append(child.div);

	return child;
};

WIDGET_MAP.Node.prototype.display = function(parentLayout, index, absLeft, absTop, topNode)
{
	var rect = parentLayout.getRect(this.cell.name);
	this.moveNode(rect, index, absLeft, absTop, topNode);
    absLeft = this.div.absoluteLeft;
    absTop = this.div.absoluteTop;
	this.layout.setSize(this.index, this.grid, rect.w, rect.h);
	for(var i=0;i<this.children.length;i++)
	{

		this.children[i].display(this.layout, index, absLeft, absTop, topNode);
	}
};

WIDGET_MAP.Node.prototype.moveNode = function(rect, index, absLeft, absTop, topNode)
{
	this.div.css('left', rect.x);
    this.div.nodeLeft = rect.x;
    this.div.absoluteLeft = absLeft + rect.x;
	this.div.css('top', rect.y);
    this.div.nodeTop = rect.y;
    this.div.absoluteTop = absTop + rect.y;
	this.div.width(rect.w);
    this.div.nodeWidth = rect.w;
	this.div.height(rect.h);
    this.div.nodeHeight = rect.h;


	this.div.css('zIndex', index);
	this.title.css('zIndex', 10);
	this.title.css('visibility', this.showTitle(topNode) ? "visible" : "hidden");
};

WIDGET_MAP.Node.prototype.showTitle = function(topNode)
{
	if (topNode.index + 1 !== this.index) {
		return false;
	}
	this.title.text(this.cell.getLabel());
	if (WIDGET_MAP.centerTitle(this.title, this.div)) {
		return true;
	}
	this.title.text(WIDGET_MAP.initials(this.cell.getLabel()));
	return WIDGET_MAP.centerTitle(this.title, this.div);
};

WIDGET_MAP.centerTitle = function(title, div)
{
	var top = parseInt((div.height() - title.height())/ 2, 10);
	if (top < 0) {
		return false;
    }
	var left = parseInt((div.width() - title.width())/ 2, 10);
	if (left < 0) {
		return false;
    }
	title.css('top', top);
	title.css("left", left);

	return true;
};

WIDGET_MAP.adjustForScroll = function(offset) {
	if( typeof( window.pageYOffset ) === 'number' ) {
		
		offset.x += window.pageXOffset;
		offset.y += window.pageYOffset;
	} else if( document.body && ( document.body.scrollLeft || document.body.scrollTop ) ) {
		
		offset.x += document.body.scrollLeft;
		offset.y += document.body.scrollTop;
	}
	else if( document.documentElement && ( document.documentElement.scrollLeft || document.documentElement.scrollTop ) ) {
		
		offset.x += document.documentElement.scrollLeft;
		offset.y += document.documentElement.scrollTop;
	}
	return offset;
};

WIDGET_MAP.initials = function(str)
{
	var result = '';
	var parts = str.split(' ');
	for (var i=0;i<parts.length;i++) {
		result += parts[i].charAt(0);
	}
	return result;
};/**
 Responsible for reading the external data and turn it into a CELL tree
*/

function widgetReadData(dataTree)
{
	if(dataTree.children)
	{
		var container = new CONTAINER_WIDGET_CELL(dataTree.name);
		for(var i=0;i<dataTree.children.length;i++)
		{
			var child = widgetReadData(dataTree.children[i]);
			if(child !== null) {
				container.addChild(child);
			}
		}
		return container;
	}
	else
	{
		
		var cell = new WIDGET_CELL(dataTree);
		return cell.size > 0 ? cell : null;
	}
}/*
 Lays out the children of a given parent. Only one level deep.
 Layout is either static from the global ARRANGEMENT map or dynamically generated from the getSize function given with each child.

 public methods:
 WIDGET_LAYOUT(parentName)  looks up ARRANGEMENT to see if a static layout is defined for this name.
 addChild(cell)    cell defines the name and getSize() function.
 setSize(index, grid, w, h) actually lays out the childrens within a [w,h] box leaving 'grid' space between children (index is for info)
 getRect(name) returns the {x,y,w,h} bounds of the named child. Must be called after 'setSize'.

 */
var WIDGET_LAYOUT = WIDGET_LAYOUT || {};

WIDGET_LAYOUT = function(parentName)
{
    var layout = (typeof(WIDGET_ARRANGEMENT) === 'undefined') ? null : WIDGET_ARRANGEMENT[parentName];
    this.staticLayout = layout === null ? null : WIDGET_LAYOUT.toBinTree(layout)[1];
    this.children = {};
};

WIDGET_LAYOUT.prototype.addChild = function(cell)
{
    this.children[cell.name] = new WIDGET_LAYOUT.Child(cell);
};

WIDGET_LAYOUT.prototype.setSize = function(index, grid, w, h)
{
    if(this.staticLayout !== null)
    {
        this.positionChildren(this.staticLayout, grid, 0, 0, w, h);
    }
    else
    {

        var list = [];
        for(var name in this.children)
        {
			if(this.children[name]) {
            	list.push(this.children[name]);
			}
        }
        if(list.length === 0) {
            return;
		}
        list.sort(function(a, b){
            return b.cell.getSize() - a.cell.getSize();
        });

        var binTree = WIDGET_LAYOUT.dynamicLayout(index, list, 0, list.length, grid, w, h);
        this.positionChildren(binTree, grid, 0, 0, w, h);
    }
};

WIDGET_LAYOUT.prototype.getRect = function(name)
{
    return this.children[name];
};


/*--------------------------------
 PRIVATE
 ----------------------------------*/
WIDGET_LAYOUT.Child = function(cell)
{
    this.cell = cell;
    this.x = 0;
    this.y = 0;
    this.w = 0;
    this.h = 0;
};

WIDGET_LAYOUT.Node = function(type, left, right)
{
    this.type = type;
    this.left = left;
    this.right = right;
};

WIDGET_LAYOUT.Node.prototype.getSize = function(children)
{
    return (this.left === null ? 0 : this.left.getSize(children)) + (this.right === null ? 0 : this.right.getSize(children));
};

WIDGET_LAYOUT.Node.prototype.encode = function()
{
    return this.type + (this.left === null ? 'X' : this.left.encode())  +
           (this.right === null ? 'X' : this.right.encode());
};

WIDGET_LAYOUT.Leaf = function(name)
{
    this.type = 'L';
    this.name = name;
};

WIDGET_LAYOUT.Leaf.prototype.getSize = function(children)
{
    return children[this.name].cell.getSize();
};

WIDGET_LAYOUT.Leaf.prototype.encode = function()
{
    return '[' + this.name + ']';
};

/** @return WIDGET_LAYOUT.Node binary layout tree from an ordered list of children.*/
WIDGET_LAYOUT.dynamicLayout = function(index, list, s, e, grid, w, h)
{

    if(s >= e - 1)
    {
        return new WIDGET_LAYOUT.Leaf(list[s].cell.name);
    }

    var i = s;
    var j = e - 1;
    var size1 = list[i].cell.getSize();
    var size2 = list[j].cell.getSize();
    while(i < j - 1)
    {
        if(size1 >= size2)
        {
            j--;
            size2 += list[j].cell.getSize();
        }
        else
        {
            i++;
            size1 += list[i].cell.getSize();
        }
    }

    if(w > h)
    {
        var w1 = parseInt((size1 * (w - grid))/(size1 + size2), 10);
        var w2 = w - grid - w1;
        return new WIDGET_LAYOUT.Node('V', w1 > 0 ? WIDGET_LAYOUT.dynamicLayout(index, list, s, j, grid, w1, h) : null,
                w2 > 0 ? WIDGET_LAYOUT.dynamicLayout(index, list, j, e, grid, w2, h) : null);
    }
    else
    {
        var h1 = parseInt((size1 * (h - grid))/(size1 + size2), 10);
        var h2 = h - grid - h1;
        return new WIDGET_LAYOUT.Node('H', h1 > 0 ? WIDGET_LAYOUT.dynamicLayout(index, list, s, j, grid, w, h1) : null ,
                h2 > 0 ? WIDGET_LAYOUT.dynamicLayout(index, list, j, e, grid, w, h2) : null);
    }
};

/** positions the (WIDGET_LAYOUT.Child) leaves of the 'node' (WIDGET_LAYOUT.Node) binary tree within the {x,y,w,h} rectangle leaving 'grid' space between children.*/
WIDGET_LAYOUT.prototype.positionChildren = function(node, grid, x, y, w, h)
{
    if(node === null) {
        return;
	}
    if(node.type === 'L')
    {
        var child = this.children[node.name];
        child.x = x;
        child.y = y;
        child.w = w;
        child.h = h;
        return;
    }

    if(node.left === null)
    {
        this.positionChildren(node.right, grid, x, y, w, h);
        return;
    }

    if(node.right === null)
    {
        this.positionChildren(node.left, grid, x, y, w, h);
        return;
    }

    var size1 = node.left.getSize(this.children);
    var size2 = node.right.getSize(this.children);

    if(node.type === 'V')
    {
        var w1 = parseInt((size1 * (w - grid))/(size1 + size2), 10);
        var w2 = w - grid - w1;
        this.positionChildren(node.left,  grid, x,             y, w1, h);
        this.positionChildren(node.right, grid, x + w1 + grid, y, w2, h);
    }
    else
    {
        var h1 = parseInt((size1 * (h - grid))/(size1 + size2), 10);
        var h2 = h - grid - h1;
        this.positionChildren(node.left,  grid, x, y,             w, h1);
        this.positionChildren(node.right, grid, x, y + h1 + grid, w, h2);
    }
};

/**
 * Builds a WIDGET_LAYOUT.Node tree from the 'encoded' string. The string format is generated by WIDGET_LAYOUT.Node.encode()
 * Leaf: [name]
 * Empty: X (used for 0 sized children that should be omitted)
 * Node: [H|V][encode left][encode right]
 */
WIDGET_LAYOUT.toBinTree = function(encoded)
{  
	if(encoded) {
	    var c = encoded.charAt(0);
	    if(c === 'X')
	    {
	        return [encoded.substring(1), null];
	    }
	
	    if(c === '[')
	    {
	        var name = encoded.substring(1, encoded.indexOf(']'));
	        return [encoded.substring(encoded.indexOf(']') + 1), new WIDGET_LAYOUT.Leaf(name)];
	    }
	
	    encoded = encoded.substring(1);
	    var leftResp = WIDGET_LAYOUT.toBinTree(encoded);
	    encoded = leftResp[0];
	    var rightResp = WIDGET_LAYOUT.toBinTree(encoded);
	    encoded = rightResp[0];
	
	    var node = new WIDGET_LAYOUT.Node(c, leftResp[1], rightResp[1]);
	
	    return [ encoded, node];
	}
	return [null,null];
};var widgetData = null;
var widgetTimeInfo = null;
var WIDGET_ARRANGEMENT = null;
var widgetMapRoot;
var widgetLastUpdateTime;
$.getJSON("http://jtools.smartmoney.com/marketspectrum/spectrumServer?action=mapData&src=marketdata&jsoncallback=?", function(rsp)
{
	widgetData = rsp.data;
	WIDGET_ARRANGEMENT = rsp.arrangement;
	widgetTimeInfo = rsp.timeInfo;
});

var widgetDataWait = setInterval("widgetCheckData()", 200);


function widgetRecur(node, cell)
{
	if(cell.children)
	{
		jQuery.each(cell.children, function() {var childNode = node.addChild(this); widgetRecur(childNode, this); });

	}
}

function widgetInit()
{
    var mainMap = $('#widgetMap');
    WIDGET_ACTION.init(mainMap);

    WIDGET_CELL.setRange(-2, 2);

	var rootCell = widgetReadData(widgetData);
	widgetMapRoot = WIDGET_MAP.makeRoot(mainMap, rootCell);
	widgetRecur(widgetMapRoot, rootCell);

	mainMap.click(function()
	{
		window.location = "http://www.smartmoney.com/map-of-the-market";
	});

    setTimeout('widgetMapRoot.displayAsRoot(false)', 200);
}

function widgetCheckData()
{
	if(widgetData !== null && WIDGET_ARRANGEMENT !== null)
	{
		clearInterval(widgetDataWait);
		$(function()
		{
			widgetInit();
		});
	}
}

function widgetRefreshAll()
{
	var timestamp = '&time=' + escape(widgetLastUpdateTime);
	$.getJSON("http://jtools.smartmoney.com/marketspectrum/spectrumServer?action=mapData&src=update" + timestamp + "&jsoncallback=?", function(rsp)
	{
		if(rsp && rsp.timeInfo)
		{
			if(rsp.timeInfo.stamp !== null) {
				widgetLastUpdateTime = rsp.timeInfo.stamp;
			}
			$("#widgetTimestamp").text("as of " + rsp.timeInfo.timestamp);
		}
		if(rsp && rsp.update && rsp.update.length > 0)
		{
			WIDGET_CELL.update(rsp.update);
			widgetMapRoot.repaint();

		}
	});
}

function getColorClass(value)
{
    return value > 0 ? "changeUp" : value < 0 ? "changeDown" : "changeFlat";
} </script>
    </div>
    <div style="float: right;">





	






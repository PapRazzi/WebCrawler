	$(document).ready(function(){
		
		$("#show-email-subscribe").click(function(){
			$(this).hide();
			$("form#email-subscribe").css('display', 'inline-block');
		});
		
		$("#plusone-facebook").hover(function(){
			$(this).children('img').attr('src', 'http://plusonemusic.net/new/wp-content/themes/plusone-white/images/facebook-grey.png');
		}, function(){
			$(this).children('img').attr('src', 'http://plusonemusic.net/new/wp-content/themes/plusone-white/images/facebook-black.png');
		});
		
		$("#plusone-twitter").hover(function(){
			$(this).children('img').attr('src', 'http://plusonemusic.net/new/wp-content/themes/plusone-white/images/twitter-grey.png');
		}, function(){
			$(this).children('img').attr('src', 'http://plusonemusic.net/new/wp-content/themes/plusone-white/images/twitter-black.png');
		});
		
		$("#show-email-subscribe").hover(function(){
			$(this).children('img').attr('src', 'http://plusonemusic.net/new/wp-content/themes/plusone-white/images/email-grey.png');
		}, function(){
			$(this).children('img').attr('src', 'http://plusonemusic.net/new/wp-content/themes/plusone-white/images/email-black.png');
		});
		
		var accordian = $(".plusoneAccordian").hrzAccordion({
			containerClass		: "container3",
			listItemClass		: "listItem3",					
			contentWrapper		: "contentWrapper3",
			contentInnerWrapper	: "contentInnerWrapper3",
			handleClass			: "accordian-handle",
			handleClassOver		: "accordian-handle-hover",
			handleClassSelected	: "accordian-handle-selected",
			openOnLoad			: 1,
			handlePosition		: 'right',
		});
		
		$(window).bind('hashchange', function() {
			console.log(accordian);
		});
		
		$("#contact-form").submit(function(){
			var values = $(this).serialize();
			$.post('./submission.php', values, function(data) {
				if (data.submitted == 'success') {
					$("#response").html("Thank you for your submission. You'll hear back from us soon.").fadeIn();
					$("#btn_send").hide();
				};
			}, 'json');
			return false;
		});
		

		$("#loading").hide();
	});
	

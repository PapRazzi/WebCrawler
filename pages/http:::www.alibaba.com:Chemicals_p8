<script type="text/javascript">
YUE.onContentReady('adWrap', checkAdView);

function checkAdView() {
  var adFocusUp = YUD.get('AD_BANNER_CATEGORY_FOCUS_UP_BLOCK');
  var adFoucsDown = YUD.get('AD_BANNER_CATEGORY_FOCUS_DOWN_BLOCK');
  var adCateHomeCommend = YUD.get('CATE_HOME_COMMEND');
  setTimeout(function() { 
    if (YUD.getStyle(adFocusUp, 'display')=='block' || YUD.getStyle(adFoucsDown, 'display')=='block' || YUD.getStyle(adCateHomeCommend, 'display')=='block') {
      YUD.setStyle(YUD.get('adWrap'), 'display', 'block');
    }
  }, 500);
}
</script>
	

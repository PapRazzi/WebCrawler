	};
	if(Y.one("#widget2")){
	var widget2 = new LocdropLocationPicker(ulwConfig2);
	widget2.setPosWidget2 = function(e){
		if(e.which == 1){
		var widgetinputid = "widget2-label";
		var widgetinputselector = "#" + widgetinputid;
		p = Y.one(widgetinputselector).getXY();
		e = parseInt(p[0]);

			        upapiExtracted  : {currentLoc : {"addr":"New York,New York,United States,10003","app":"yahoo.local_us.ups","cc":"US","city":"New York","country_conf":99,"ctry":"United States","ctx":"global","dtime":"2013-02-18T01:46:06Z","id":"id_current","input":"ip","lat":40.731391906738,"lon":-73.988403320312,"lvl":1,"raw":"216.165.95.76","sc":"NY","state":"New York","state_conf":80,"town_conf":75,"type":"physical","woe":12761335,"zip":"10003","zip_conf":1},defaultLoc : null},

	widget.setPendingYrbCall(false);
	widget.setYrbContainer(response);
}
YUI().use("node", "event", "substitute", "overlay", function(Y){
	Y.on("domready", function (){ 
		var ulwConfig = {
		divID  : "widget",
		appID  : "universal_location_widget__v2",
		ctx  : "search",
		callback : locChanged,
		crumb  : "ZWpHazAvQno4NDY-",
//yqlHost  : "gamma.locdrop.query.yahoo.com",
		yqlTimeout : null,
		upapiExtracted  : {currentLoc : {"addr":"New York,New York,United States,10003","app":"yahoo.local_us.ups","cc":"US","city":"New York","country_conf":99,"ctry":"United States","ctx":"global","dtime":"2013-02-18T01:46:56Z","id":"id_current","input":"ip","lat":40.731391906738,"lon":-73.988403320312,"lvl":1,"raw":"216.165.95.78","sc":"NY","state":"New York","state_conf":80,"town_conf":75,"type":"physical","woe":12761335,"zip":"10003","zip_conf":1},defaultLoc : null},
		locationLevel : "city",
		overlayAnchor : "input_search_test",
		locale   : null,
		yrbHost  : null,
		yrbPath  : null,
		minWidth : null,
		anchorText : "Choose a location",
		placeFinderGflags : true,
		minWidth : 307,
		customInput : true,
		positionXY : null
	};

	var widget = new LocdropLocationPicker(ulwConfig);
	widget.init();

	Y.on("keypress",function(e){if(e.keyCode == 13)e.preventDefault();},"#yls-csz");

	var widgetinputid = "yls-csz";
	var widgetinputselector = "#" + widgetinputid;
	p = Y.one(widgetinputselector).getXY();
	e = parseInt(p[0]+142);
	d = p[1] + 25;

	widget.setPositionXY([e,d]);
	Y.on("mouseup", function(e) {
		if(!widget.clickedInside(e, widgetinputid) && e.target.get("className") != "ghosttext" && e.which == 1) {
			widget.close(); 
		}
	}, document, widget);
	widget.keyup = function (A){
		if(this.state == "closed") this.open(A);
		this.handleInputEvents(A);
		if(Y.all("#"+this.lwBodyId+" li").size() == 0 && A.keyCode!=13) 
			Y.one(".yui3-overlay-content, .lw-loading-msg").setStyle("border-width","0px");
		else  Y.one(".yui3-overlay-content, .lw-loading-msg").setStyle("border-width","1px");
	};
	widget.clickOpen = function(W){

		var widgetinputid = "yls-csz";
		var widgetinputselector = "#" + widgetinputid;
		p = Y.one(widgetinputselector).getXY();
		e = parseInt(p[0]);
		d = parseInt(p[1] + 25);

		this.setPositionXY([e,d]);
		if(Y.one(".yui3-overlay-content, .lw-loading-msg"))
		Y.one(".yui3-overlay-content, .lw-loading-msg").setStyle("border-width","1px");
		if(this.state == "closed"||this.state == "open"||this.state == "reopen")
			this.open(W);
		else
			this.close();
	}
	Y.on("focus", Y.bind(widget.clickOpen, widget), widgetinputselector);
	Y.on("keyup", Y.bind(widget.keyup, widget), widgetinputselector); 
	Y.on("click", Y.bind(widget.clickOpen, widget), widgetinputselector);

	function locChanged(widget) {
		var location = widget.loc;
		var widgetinputid = "#yls-csz";
		Y.one(widgetinputid).set("value",location.addr);
		Y.one(".yls-gl-srchbtn.yls-gl-palette1").focus();
	}
// start of second locdrop integration

		var ulwConfig2 = {
		divID  : "widget2",
		appID  : "universal_location_widget__v2",
		ctx  : "search",
		callback : locChanged2,
		crumb  : "ZWpHazAvQno4NDY-",
//yqlHost  : "gamma.locdrop.query.yahoo.com",
		yqlTimeout : null,
		upapiExtracted  : {currentLoc : {"addr":"New York,New York,United States,10003","app":"yahoo.local_us.ups","cc":"US","city":"New York","country_conf":99,"ctry":"United States","ctx":"global","dtime":"2013-02-18T01:46:56Z","id":"id_current","input":"ip","lat":40.731391906738,"lon":-73.988403320312,"lvl":1,"raw":"216.165.95.78","sc":"NY","state":"New York","state_conf":80,"town_conf":75,"type":"physical","woe":12761335,"zip":"10003","zip_conf":1},defaultLoc : null},

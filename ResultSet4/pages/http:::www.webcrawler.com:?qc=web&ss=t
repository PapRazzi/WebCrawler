    /**Google Analytics PageView Tracking **/
    var _gaq = _gaq || [];

    function gaPageViewTracking(segment, clickType, clickPosition, domainValue) {
        _gaq.push(['_setAccount', 'UA-1764423-1']);
        _gaq.push(['_setCustomVar', 1, 'Segment', segment, 3]);
        _gaq.push(['_setCustomVar', 2, 'ClickType', clickType, 3]);
        _gaq.push(['_setCustomVar', 3, 'ClickPosition', clickPosition, 3]);
        _gaq.push(['_setCustomVar', 4, 'ClickDomain', domainValue, 3]);
        _gaq.push(['_trackPageview', "/" + segment + "/clicks/" + clickType + "/" + clickPosition + "/" + domainValue]);
    }

    (function () { var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true; ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s); })();

<script language="JavaScript" type="text/javascript">

	   					
var login = window.ggsContactlogin;
								
if(login == false){
	document.getElementById('login-btn').onclick = popUpXman;
}else{
	document.getElementById('login-btn').onclick = user_down;
}
	      									


function popUpXman(){
          var xmanjsUrl = "https://login.alibaba.com/xman/xman.js?display=p&pd=alibaba&style=l&modal=n&delay=true&title=a&login=b&lbutton=c&jbutton=a";
          if(!AE.xman_join_login){
             YAHOO.util.Get.script(xmanjsUrl, {onSuccess: 
                function(){
                   AE.xman_join_login.boxPopup();
                }
             });
          }else{
             AE.xman_join_login.boxPopup();
             }
       }
       
       
       function xman_callback(){
                        dmtrack.clickstat('http://tracelog.www.alibaba.com/null.gif',{th_page:'index',th_nav:'userguide'});
						AE.xman_join_login.boxClose();
						var confirmSample = new AE.widget.popup({
							title : '',
							content : 'Which language do you prefer?',
							size : [320, 0],
							style : 'notice'
						});
						confirmSample.setButton({
							'Yes' : {
								title : 'Thai',
								className : 'buttonSkinA M',
								isDefault : true,
								event : {
									click : function(el, arg){
										window.location.href = 'http://us.download.alibaba.com/download.do?appId=c20120001&p=ZnA9MjAxMjA1MjFfVXNlcl9HdWlkZV9USF9BbGliYWJhX2NvbV9UaGFpX0xhbmd1YWdlLnBkZiZmbj1Vc2VyX0d1aWRlX1RIX0FsaWJhYmFfY29tX1RoYWlfTGFuZ3VhZ2UucGRm';
										this.hide();
									}
								}
							},
							'No' : {
								title : 'English',
								className : 'buttonSkinB M',
								event : {
									click : function(el, arg){
										window.location.href = 'http://us.download.alibaba.com/download.do?appId=c20120001&p=ZnA9MjAxMjA1MjFfVXNlcl9HdWlkZV9USF9BbGliYWJhX2NvbS5wZGYmZm49VXNlcl9HdWlkZV9USF9BbGliYWJhX2NvbS5wZGY%3D';
										this.hide();
									}
								}
							}
						});
						
						confirmSample.show();
                        
	    	}
			function user_down(){
                        dmtrack.clickstat('http://tracelog.www.alibaba.com/null.gif',{th_page:'index',th_nav:'userguide'});
						var confirmSample = new AE.widget.popup({
							title : '',
							content : 'Which language do you prefer?',
							size : [320, 0],
							style : 'notice'
						});
						confirmSample.setButton({
							'Yes' : {
								title : 'Thai',
								className : 'buttonSkinA M',
								isDefault : true,
								event : {
									click : function(el, arg){
										window.location.href = 'http://us.download.alibaba.com/download.do?appId=c20120001&p=ZnA9MjAxMjA1MjFfVXNlcl9HdWlkZV9USF9BbGliYWJhX2NvbV9UaGFpX0xhbmd1YWdlLnBkZiZmbj1Vc2VyX0d1aWRlX1RIX0FsaWJhYmFfY29tX1RoYWlfTGFuZ3VhZ2UucGRm';
										this.hide();
									}
								}
							},
							'No' : {
								title : 'English',
								className : 'buttonSkinB M',
								event : {
									click : function(el, arg){
										window.location.href = 'http://us.download.alibaba.com/download.do?appId=c20120001&p=ZnA9MjAxMjA1MjFfVXNlcl9HdWlkZV9USF9BbGliYWJhX2NvbS5wZGYmZm49VXNlcl9HdWlkZV9USF9BbGliYWJhX2NvbS5wZGY%3D';
										this.hide();
									}
								}

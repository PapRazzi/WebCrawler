    (function(){
      if (is_users_cookie_exists()){
        var img_el = $("user_image_st");
        var img_src = user_picture(readCookie("user_id"),"small");
        img_el.select("img")[0].src = img_src;
        var textarea = $("stock_talk_textarea");
        if (isAuthor()){
          img_el.addClassName("author");
          textarea.addClassName("author");
          $('st_textarea').addClassName("author");
        }
        img_el.href = getUserUri();
      }  
    }());
  </script>


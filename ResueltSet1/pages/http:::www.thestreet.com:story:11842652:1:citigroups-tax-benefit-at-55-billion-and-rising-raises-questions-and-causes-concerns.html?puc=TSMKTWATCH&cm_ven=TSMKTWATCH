$(document).ready(function() {

	$('.ticker').mouseover(function(){
        $('.ticker').removeClass('active');
        if  ($(this).attr('id') != "TNX" ){
            $(this).addClass('ticker active');
            var src = "http://www.thestreet.idmanagedsolutions.com/charts/common/quote.chart?SYMBOL_US=" + $(this).attr('id') + "&WIDTH=338&TIME_SPAN=1D&XAXIS_LABEL=1";
            $('#miniChartImg').attr('src', src);
        }
	 });

});

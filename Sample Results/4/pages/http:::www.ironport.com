                    <!-- c17v1 DHTML tabs component starts here -->
                    <div class="clb c17v1">
                      <div class="tab" id="overview"><a href="#overview">Overview</a></div>
                      <div class="tab" id="faqs"><a href="#faqs">FAQs</a></div>
                      <div class="tab" id="sales"><a href="#sales">Sales and Support</a></div>
                      <div class="tab" id="training"><a href="#training">Training</a></div>
                      <div class="tab" id="partners"><a href="#partners">Partners</a></div>

    <!-- Banner -->
    <div id="phcontent_0_pnlBanner" class="bannercontainer masthead clearfix">
		
        <span id="phcontent_0_lblBanner" class="banner">
                    
                    <a title="UofAHome" class="sitelink" href="http://www.ualberta.ca/"></a>
                
                </span>
        
        <h1 id="sitetitle" class="sitetitle">
            <a id="phcontent_0_hypHeadingLink" href="http://www.ualberta.ca/">UofAHome</a></h1>
        <!-- Audience Navigation -->
        <nav id="phcontent_0_navAudience">
            
                    <ul>
                        
                    <li class="navitem">
                        <a class="topnav" href="http://www.prospective.ualberta.ca/">Prospective Students</a>
                    </li>
                
                    <li class="navitem">
                        <a class="topnav" href="http://www.students.ualberta.ca/">Current Students</a>
                    </li>
                
                    <li class="navitem">
                        <a class="topnav" href="/">Faculty & Staff</a>
                    </li>
                
                    <li class="navitem last">
                        <a class="topnav" href="http://www.alumni.ualberta.ca/">Alumni & Friends</a>
                    </li>
                

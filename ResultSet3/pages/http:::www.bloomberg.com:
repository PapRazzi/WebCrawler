    //<![CDATA[
    if (reg_enabled) {
            document.write("<scr" + "ipt type='text/javascript' src='http://cdn.gotraffic.net/r/v/20130131_145519/javascripts/breg-min.js'>" + "</scr" + "ipt>");
            $(document).ready(function() {
            if (BLOOMBERG.user){ BLOOMBERG.user.init({"enable_tracking_cookies":true,"u":"https://login.bloomberg.com"}); }
        });
    }
    if (q_enabled && reg_enabled) {
            document.write("<scr" + "ipt type='text/javascript' src='http://cdn.gotraffic.net/q/v/20121113_141442/javascripts/q-min.js'>" + "</scr" + "ipt>");
            $(document).ready(function() {
            if (BLOOMBERG.queue){ BLOOMBERG.queue.init({"url":"https://login.bloomberg.com/q"}); }
        });
    }
        //]]>
</script>
<script type='text/javascript'>
  //<![CDATA[
    (function($) {
      var methods = {
        init: function(options) {
          return this.each(function() {
            var $this = $(this),
                data = $this.data('videoCassette');
    
            if (! data) {
              $this.data('videoCassette', {
                player: null,
                intention: null,
                cassette: $this.find('.cassette'),
                playing: null,
                load_player: ooyala_lazy_load
              });
              data = $this.data('videoCassette');
            }
    
            $this.find(".poster").click(function(e) {
              $this.videoCassette('loadPlayer');
              $this.videoCassette('playNextTrack', true);
            });
    
            data.cassette.find('.title').click(function(e) {
              e.preventDefault();
              $this.videoCassette('loadPlayer');
              $this.videoCassette('playTitle', this, true);
            });
    
            $this.find(".share").click( function() {
              var shareButton = $(this);
              var shareContainer = $(this).closest(".track").find(".share_container");
              if(shareButton.hasClass("share_block"))
              {
                shareButton.removeClass("share_block");
                shareContainer.hide();
              }
              else
              {
                shareButton.addClass("share_block");
                shareContainer.show();
                shareContainer.find("input[type=text]").click(function() {
                  $(this).select();
                });
              }
            });
    
            // bind event tracking,
            // thumbnail click/video playing binding done with player event handler playStarted
            $this.find('h3 a').bindTracking('Module Title');
            $this.find('a.title').bindTracking('Video Headline');
            //$this.find('object').bindTracking('Video Thumbnail');
            $this.find('.more a').bindTracking('More Link');
            $this.find(".share").bindTracking('Share');
            $.each(['twitter', 'facebook', 'linkedin', 'googleplus'], function(idx, val){
              $this.find('.social_share_icons .' + val + '_icon a').bindTracking('Share ' + val);
            });
            $this.find("input[name=permanent_url]").bindTracking('Copy Link');
    
          });
        },
        loadPlayer: function() {
          var $this = $(this),
              data = $this.data('videoCassette');
          if (data.load_player) {
            // Not able to use jquery to build SCRIPT element and append to body
            // due to a bug in the Ooyala player. This can be refactored if the
            // bug is resolved.
            $this.find(".poster").html('<div class="loading"></div>');
            var script = document.createElement("script");
            script.setAttribute("src", ooyala_bg_script);
            $("body")[0].appendChild(script);
          } else {
            $this.find(".poster").hide();
          }
        },
        popOutUrlForCurrentTitle: function(startTime) {
          var $this = $(this);
          var data = $this.data('videoCassette');
          return data.playing.find('.title').attr('data-popout-url') + startTime;
        },
        adStarted: function() {
          var $this = $(this);
          var data = $this.data('videoCassette');
          if(data.cassette.find('.track.playing').length == 0)
            $this.videoCassette('playStarted', true);
        },
        playStarted: function() {
          var $this = $(this);
          var data = $this.data('videoCassette');
          if (data.player == null || data.playing !== null) {
            return;
          }
    
          $this.videoCassette('playNextTrack', true);
        },
        playerReady: function(thePlayer) {
          var f, a;
          var $this = $(this);
          var data = $this.data('videoCassette');
    
          data.player = thePlayer;
          if (data.load_player) {
            data.load_player = false;
            $this.find(".poster").hide();
          }
    
          if (data.intention !== null) {
            f = data.intention.f;
            a = data.intention.a;
            data.intention = null;
            if ( a == null) {
              a = [];
            }
            a = Array.prototype.slice.call(a);
            a.unshift(f);
            $this.videoCassette.apply(this,a);
          }
        },
        playTitle: function(elem, markCassette) {
          var $this = $(this);
          var data = $this.data('videoCassette');
          var player = data.player;
          var $elem = $(elem);
          var embedCode = null;
          var trackToPlay = null;
    
          if (player !== null) {
            embedCode = $elem.attr('data-embed-code');
            trackToPlay = $elem.closest('.track');
            data.cassette.find('.track').removeClass('playing');
            data.cassette.find('.track').removeClass('selected');
            trackToPlay.addClass('playing');
            data.playing = trackToPlay;
            if (markCassette) {
              data.cassette.find('.track').removeClass('mark');
              trackToPlay.addClass('mark');
            }
    
            if(player.getEmbedCode === undefined || player.getEmbedCode() != embedCode){
              player.setEmbedCode(embedCode);
              data.intention = {f: 'startPlay', a: null};
            }
            else{
              player.playMovie();
            }
          } else {
            data.intention = {f: 'playTitle', a: arguments};
          }
        },
        startPlay: function() {
          var $this = $(this);
          var data = $this.data('videoCassette');
          var player = data.player;
          if (player !== null) {
            player.playMovie();
          }
        },
        stopPlay: function() {
          var $this = $(this);
          var data = $this.data('videoCassette');
          var player = data.player;
          if (player !== null) {
            player.pauseMovie();
          }
        },
        playNextTrack: function(mark) {
          var $this = $(this);
          var data = $this.data('videoCassette');
          var next = [];
          if (typeof(mark) === 'undefined') {
            mark = false;
          }
    
          if (data.playing == null || data.playing.next().length == 0) {
            // go to the first one
            next = data.cassette.find('.track').first().not('.mark');
          } else {
            // grab the next one that is not the mark
            next = data.playing.next().not('.mark');
          }
    
          if (data.playing != null) {
            data.playing.removeClass('playing');
          }
    
          if (next.length == 0) {
            data.playing = null;
            $this.find('.credit').show();
            return;
          } else {
            $this.videoCassette('playTitle', next.find('.title'), mark);
          }
        },
    
        populateLiveTvSchedule: function(){
          var $this = $(this);
          var times = $this.find(".tv_schedule .schedule_details .schedule_time");
          var descs = $this.find(".tv_schedule .schedule_details .schedule_desc");
          $.ajax({
            type: "GET",
            url: "/rapi/tv/schedule/" + $this.find(".tv_schedule_region").html() + '/' + (times.length + 1) + '/',
            success: function(data){
              if(data instanceof Array && data.length > 0)
              {
                $this.find("ol.livestream li.schedule_desc a").html(data[0]["title"]+'<span class="camera"></span>');
                for(var i = 0; i < times.length; ++i)
                {
                  $(times[i]).html(data[i+1]['start_time']);
                  $(descs[i]).html(data[i+1]['title']);
                }
              }
            },
            complete: function(){
              setTimeout(function(){$this.videoCassette('populateLiveTvSchedule')}, 2*60*1000);
            }
          });
        }
      };
    
      $.fn.videoCassette = function(method) {
        if (methods[method]) {
          return methods[method].apply(this, Array.prototype.slice.call(arguments, 1));
        } else if (typeof(method) === 'object' || ! method) {
          return methods.init.apply(this, arguments);
        } else {
          $.error("Method " + method + " does not exist on jQuery.videoCassette");
        }
      };
    })(jQuery);
  //]]>
</script>

	<script>
	$(function() {
		var cache = {},
			lastXhr;
		$( "#f_keywords" ).autocomplete({
			minLength: 2,
select: function(event, ui) { 
         $(event.target).val(ui.item.value);
         $('#sf').submit();
         return false;
    },

			source: function( request, response ) {
				var term = request.term;
				if ( term in cache ) {
					response( cache[ term ] );
					return;
				}

				lastXhr = $.getJSON( "/pred_txt.php", request, function( data, status, xhr ) {
					cache[ term ] = data;
					if ( xhr === lastXhr ) {
						response( data );
					}
				});
			}
		});
	});
	</script>



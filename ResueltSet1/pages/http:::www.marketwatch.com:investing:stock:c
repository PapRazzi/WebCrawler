    $(document).ready(function() {
    $("div.foreverblock").each(function() {
    var doctype = $(this).data("doctype");
    if (typeof(doctype) == "undefined") {
    doctype = $(this).attr("data-doctype");
    }
    var nb = new MarketWatch.News.NewsBox(this, doctype, 'C', 'US', '');
    nb.setNewsSpark(Spark.Shared._Investing_HeadlineItem.RenderView);
    var thunk = this;
    if (typeof (BlueGrass) != "undefined" && $(thunk).find(".bgChannel").text() != "") {
    BlueGrass.Runtime.openConnection({}, function(connection, type) {
    if (type == "connect") {
    connection.subscribe($(thunk).find(".bgChannel").text(), nb.bluegrassCallback);
    nb.setNewsSpark(Spark.Shared._Investing_HeadlineItem.RenderView);
    }
    });
    }
    });
    });
  </script>
            
        <script type="text/javascript">
            // Resizing Alacra images
            function resizeImage(el, maxwidth, maxheight) {
                var width = $(el).width();
                var height = $(el).height();
                var ratio = 0;

                if (width > maxwidth) {
                    ratio = maxwidth / width;
                    $(el).css('width', maxwidth).css('height', height * ratio);
                    width = width * ratio;
                    height = height * ratio;
                }

                if (height > maxheight) {
                    ratio = maxheight / height;
                    $(el).css('width', width * ratio).css('height', maxheight);
                }
            }

            $('.alacraimgcontainer img').each(function() {
                var maxheight = 42;
                var maxwidth = 100;
                if ($(this).height() > maxheight || $(this).width() > maxwidth) {
                    resizeImage(this, maxwidth, maxheight);
                }
                $(this).show();
            });

            // Intelligently wrapping Alacra text
            var parentHeight = 57;
            var looseMaxChars = 200;
            $('.alacraheadline p').each(function() {
                var element = $(this);
                if (element.text().length > looseMaxChars) {
                    var trimString = element.text();
                    element.text(trimString.slice(0, looseMaxChars));
                }

                while (element.height() > parentHeight) {
                    var oldString = $.trim(element.text());
                    var reversedOldString = oldString.split('').reverse().join('');
                    var index = reversedOldString.indexOf(' ');
                    var newString = oldString.slice(0, oldString.length - index);
                    newString = $.trim(newString) + '…';
                    element.text(newString);
                }
            });
        </script>
    


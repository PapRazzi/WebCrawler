        document.write("<div st_btn style='display:none'><fieldset class='choose_username_stocktalk' id='choose_username_row'><input type='text' class='text nick' name='nick' id='input_nick' title='Please choose username' value='Please choose username' maxlength='80' onfocus='SA.Utils.Expansion.Input(this); this.clearAll();'/></fieldset></div>");
      }
    </script>

	init: function() {
		var topfundsURL="/top25funds/widget/";
 		var marketmoversURL="/marketmovers/widget/";
		
		$("#widgetForm input").focus(function() {
			$(this).removeClass("errorField");
			$(".widgetContainer .inputError").hide();
		});
		
		$("#widgetForm .focusHighlight input").focus(function(e) {
			this.select();
		});
		
		$("#widgetForm .focusHighlight input").mouseup(function(e) {
			e.preventDefault();
		});
		
		/** for sliders **/
		if($("#widgetSlider").length > 0) {
			$("#widgetSlider").myslider({
				minimum: 10,
				slide: function(val) {
					$(".widgetContainer #savings").val(val);
					$(".widgetContainer .annualSavings").text(val + "%");
				}
			});
		}		
		/** market movers **/
		if($("#marketmoversWidget").length > 0 ) {
			$.ajax({
				url:marketmoversURL,
				success:function(a){
					$("#marketmoversWidget").html(a);
				}
			});
		}
		/** top funds **/
		if ($("#topfundsWidget").length > 0) {
			$.ajax({
				url:topfundsURL,
				success:function(a){
					$("#topfundsWidget").html(a);
				}
			});
		}
	},
	isNumber: function(o) {
		return ! isNaN (o-0);
	},
	validateFields: function(actionUrl,el) {
		var isValid = true;
		var msg = "";
		var $el = $(el).parents("form");
		var $widgetCtr = $(el).parents(".widgetContainer");
		
		$el.find(".intPosVal").each(function() {
			if(!sm.module.widgets.isNumber($(this).find("input").val())) {
				$(this).find("input").addClass("errorField");
				msg = "Numerical inputs only.";
				isValid = false;
				return false;
			}
			else if(parseInt($(this).find("input").val(),10) < 0) {
				$(this).find("input").addClass("errorField");
				msg = "Positive values only.";
				isValid = false;
				return false;
			}
		});
		
		if(isValid) {
			$el.attr("action", actionUrl);
			el.parentNode.parentNode.onsubmit = null;
			$el.submit();
		}
		else{
			$widgetCtr.find(".inputError").find("span").text(msg);
			$widgetCtr.find(".inputError").show();
			$el.removeAttr("action");
			el.parentNode.parentNode.onsubmit = sm.module.widgets.submitHandler;
		}
	},
	submitHandler: function() {
		return false;
	}
};$(document).ready(function() {sm.module.widgets.init(); }); </script>

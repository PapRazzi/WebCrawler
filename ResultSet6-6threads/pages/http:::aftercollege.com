        $.ajax({
            url : '/ajax/activityhandler.ashx?action=signup_map',
            cache : 'false',
            dataType : 'json',
            success : function(data){
                if(typeof(data) == "object"){
                    $('#map').show();
                    $(function (){
                        var latlng = new google.maps.LatLng(37.0625, -95.677068);     
                        var myOptions = {       
                            zoom: 2,       
                            center: latlng,  
                            disableDefaultUI: true,     
                            mapTypeId: google.maps.MapTypeId.ROADMAP     
                        };
                        var map = new google.maps.Map(document.getElementById("map"),myOptions);
                        var markers = new Array();
                        var result = data.message;
                        loadMarkers(map,result);
                    });
                }
            }
        });
            
        function loadMarkers(map,messages){
            var i = 0, length = messages.length - 1;
            var markers = [];
            var infoBoxes = [];
            var width = 125;
            var height = 22;
            var minTime = 1500;
            var interval = 5000;
            (function iterator(){
                    if(markers.length > 0 ){
                        //markers[0].setMap(null);
                        //infoBoxes[0].close();
                    }
                    markers = [];
                    infoBoxes = [];
                    var min = -125;
                    var max = -66;
                    var skew = ((max - (parseFloat(messages[i % length].lng)))/(max - min));
                    var offset = (skew < .5) ? (-1 * ((.5 - skew) * (width))) : ((skew - .5) * (width));
                    var pixelOffset = (-1 * (width/2)) + offset;
                    pixelOffset = (pixelOffset < (-1* width)) ? (-1 * width) : ((pixelOffset > 0) ? 0 : pixelOffset);
                    var pinColor = messages[i % length].tags[0] == "signup" ? "yellow" : "green";
                    var pinImage = new google.maps.MarkerImage("https://c292481.ssl.cf1.rackcdn.com/redesign/" + pinColor + "_pin.png",
                        new google.maps.Size(21, 34),
                        new google.maps.Point(0,0),
                        new google.maps.Point(10, 34));
                    var marker = new google.maps.Marker({
                        position : new google.maps.LatLng(messages[i % length].lat,messages[i % length].lng),
                        map: map,
                        icon : pinImage,
                        animation: google.maps.Animation.DROP,
                        title : messages[i % length].lat + "," + messages[i % length].lng
                    });
                    markers.push(marker);
                    var infoMessage = messages[i % length].tags[0] == "signup" ? "New Member" : "Job Application";
                    var infowindow = new InfoBox({
                        content : infoMessage,
                        pixelOffset: new google.maps.Size(pixelOffset, 0),
                        boxStyle : {
                            border : "1px solid #ccc",
                            textAlign : "center",
                            width : width + "px",
                            height : height + "px",
                            fontSize : "12px",
                            background : "#fff",
                            color : "#575A5C",
                            fontWeight : "bold"
                        },
                        closeBoxURL : "",
                        disableAutoPan : true
                    });
                    infoBoxes.push(infowindow);
                    infowindow.open(map,marker);
                    setTimeout(function(){marker.setMap(null);},minTime + interval);
                    ++i;
                    var diff = Math.abs(messages[((i % length) + 1)].postedon - messages[(i % length)].postedon);
                    var timeout = (((diff * 100) % interval) + minTime)
                    setTimeout(function(){infowindow.close();},timeout);
                    setTimeout(iterator,timeout);
                }
            )();
        }
        
    });
</script>




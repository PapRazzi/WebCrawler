<script type="text/javascript">
var msgCrawl="HRUâ€™s online Individual Development Plan (IDP) is here!&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &#8212; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;New Job Aid for Retirement Specialists located on the Resource Page&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &#8212; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;HRU registered users reach the 18,000 mark!&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &#8212; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Does your Agency have a course to share on HRU.Gov? Let us know at hru@opm.gov&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &#8212; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;";
var crawl_width=0;
var divNews_Width=0;

function pageLoaded() {
	divNews_Width=document.getElementById("divNews").clientWidth;
	crawl_width = document.getElementById("news_crawl").offsetWidth;
	while (document.getElementById("news_crawl").offsetWidth < divNews_Width + crawl_width) {
		document.getElementById("news_crawl").innerHTML += msgCrawl;
	};
}

function kd(e){
	var kn;
	kn = e.keyCode;
	
	if (document.getElementById("Email")) {
		if (document.getElementById("Email").value == "") {
		} else if (document.getElementById("password").value == "") {
		} else if (kn == 13) {
			login();
		};
	};
}
function login() {
	if (document.getElementById("Email").value == "") {
		alert("Please enter your email address and password.");
		document.getElementById("Email").focus();
	} else if (document.getElementById("password").value == "") {
		alert("Please enter your password.");
		document.getElementById("password").focus();
	} else {
		document.pageForm.submit();
	};
};
</script>


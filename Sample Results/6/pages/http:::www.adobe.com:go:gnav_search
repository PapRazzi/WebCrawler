<!-- HEAD ADOBE SCRIPTS --><!-- $Id: //depot/projects/dylan/releases/rc_13_1/docroot/lib/com.adobe.search.results.ssi#1 $ -->
<link href="http://wwwimages.adobe.com/www.adobe.com/lib/com.adobe/template/search/screen.css" type="text/css" rel="stylesheet" media="screen" />
<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/lib/com.adobe/module/ProductSelector.js"></script>
<!-- $Id: //depot/projects/dylan/releases/rc_13_1/docroot/lib/com.adobe.search.results.ssi#1 $ -->
<script type="text/javascript" src="/lib/com.adobe/lib/jquery-1.2.6.min.js"></script>
<script type="text/javascript" src="/lib/spry/Spry_1_6_1_022408/includes_minified/SpryData.js"></script>
<script type="text/javascript" src="/lib/spry/Spry_1_6_1_022408/includes_minified/SpryJSONDataSet.js"></script>
<script type="text/javascript" src="/lib/spry/Spry_1_6_1_022408/includes_minified/SpryAutoSuggest.js"></script>
<script type="text/javascript">
	Spry.Data.JSONDataSet.prototype.loadObjectDataIntoDataSet = function(jsonObj) {
        this.notifyObservers("onPreLoad");
 
        if (jsonObj == null) jsonObj = "null";
		
        var rs = Spry.Data.JSONDataSet.flattenDataIntoRecordSet(jsonObj, Spry.Data.Region.processDataRefString(null, this.path, this.dataSetsForDataRefStrings), this.pathIsObjectOfArrays);
 
        this.flattenSubPaths(rs, this.subPaths);
 
        this.doc = null;
        this.docObj = jsonObj;
        this.data = rs.data;
        this.dataHash = rs.dataHash;
        this.dataWasLoaded = true;
 
        this.syncColumnTypesToData();
 
        this.disableNotifications();
        this.filterAndSortData();
        this.enableNotifications();
 
        this.notifyObservers("onPostLoad");
        this.notifyObservers("onDataChanged");		
	};
	// SO JQUERY AND PROTOTYPE DON'T CLASH
	var $j = jQuery.noConflict(); 
	// WATCH KEYPRESS EVENT CODES FOR SPRY AUTO SUGGEST
	var grabInput = function(event) {
		// tab or ESC hit
		if(event.keyCode==9 || event.keyCode==27) { return; }
		if((event.keyCode >= 0 &&  event.keyCode <= 90) || event.type=="click") {
			if(event.keyCode == 40 || event.keyCode == 38) { return; }					
			var query = event.element().value;
			if(event.keyCode == 13 && !query.empty()) {
				$('search-text').stopObserving();
				$('searchTextArea').stopObserving();
				$('searchform').stopObserving();
				return;
			} 
			if(query=="") return;
			
			var ionURL = "http://community.adobe.com/help/rm/sugg.html?supressErrors=true&o=json&jsonp=true&q="+query+"&jsoncallback=?";
			var ossURL = "http://content.atomz.com/autocomplete/sp10/04/3b/7b/?max_results=200&jsonp=true&callback=preTermSuggCallbackFunction&query="+query;
			if (query.length > 1){
			$j.ajax({
				url: ossURL,
				dataType : "jsonp",
				jsonp : "d",
				jsonpCallback: preTermSuggCallbackFunction
			});
		  }
		}
	}
	
	var dsProductsJson = new Spry.Data.JSONDataSet(null, { "path" : "queries" });
	
	Event.observe(window, "load", function() {
		if(!$('suggestMenu')) {
			$('search-text').observe('keypress',function(e) {
				if(e.keyCode == 13) {
					window.setTimeout("getURL()", 10);	
				}
			});	
		} else {
			$('search-text').observe('keydown',grabInput);
			$('search-text').observe('keyup',grabInput);
			$('search-text').observe('click',grabInput);
		}
		
		var temp = window.location.search;
		var nonEmptySearch = /term/.test(temp);
		var _term = window.location.search.sub(/\?.*term=(.*)/,"#{1}").split('&')[0] || "";
		if(!_term.empty() && nonEmptySearch) {
			_term = _term.replace(/\+/g," ");
			$("search-text").value = decodeURIComponent(_term);			
		}
	});
	
	var preTermSuggCallbackFunction = function(obj){
		var query = $('search-text').value;
		var oParsedResponse = obj;
		var origDisplayResults = 10;
		var matches = []; // our real output
		
		// Iterate all the results
		for (var i = 0; i < oParsedResponse.length; ++i)
          	{
			
				// Keep only the ones which start with the requested query
				// (i.e. the index of the match is 0--the beginning)
			  if (oParsedResponse[i].toLowerCase().search(query.toLowerCase())== 0)
			  {     
					  matches[matches.length] = oParsedResponse[i];

			  }
			  if (matches.length >= origDisplayResults)
			  {
					  break;
			  }
          	}
		
		//if beginning words are not sufficient then look for inside words 
		if (matches.length < 10) {
		for (var i = 0; i < oParsedResponse.length; ++i)
          	{
			
				// Keep only the ones which start with the requested query
				// (i.e. the index of the match is 0--the beginning)
			  if (oParsedResponse[i].toLowerCase().search(query.toLowerCase()) > 0)
			  {     
					  matches[matches.length] = oParsedResponse[i];

			  }
			  if (matches.length >= origDisplayResults)
			  {
					  break;
			  }
          	}
		}	
         termSuggCallbackFunction(matches);
	};
	 
	var termSuggCallbackFunction = function(obj){
		var term = {
			"queries" : obj
		}		
		dsProductsJson.loadObjectDataIntoDataSet(term);
	};

	function init() {
		DWRUtil.useLoadingMessage();
		DWREngine._errorHandler =  errorHandler;
	}		
	

	function getURL(key) { 
		var collection;
		var archived = "";
		// GRAB TEXT AND HIDDEN INPUT VALUES
		var term=encodeURIComponent(document.searchform.searchterm.value);
		var loc=encodeURIComponent(document.searchform.loc.value);
		var cat=encodeURIComponent(document.searchform.cat.value)
		// ARCHIVED FOR SUPPORT DOCS
		if($("include-archived")) { archived = ($('include-archived').checked) ? "true" : ""; }
		// CREATE QUERY STRING TO SEND TO SEARCH
		term = (term != "") ? "&term=" + term : "";	
		loc = (loc != "") ? "loc=" + loc : "";
		cat = (cat != "") ? "&cat=" + cat : "";
		archived = (archived != "") ? "&archived=" + archived : "";	
		collection = loc+term+cat+archived;
		// IF PS ENABLED, ADD PRODUCT NAME IN SEARCH COLLECTION
		if (_product_selector_flag != "off") {
			var category=encodeURIComponent(document.searchform.searchin.value);
			var categoryIndex=document.searchform.searchin.selectedIndex;
			category = (category != "" && categoryIndex > 0) ? "&product=" + category : "";
			collection = collection+category; 
		}
		document.searchform.action = '?'+collection;
		document.searchform.submit(); 
		return false;
		   
	}
	
	document.observe('dom:loaded', function() {
		
		
		// WATCH ARCHIVED IF EXISTS (SUPPORT ONLY)
		if($("include-archived")) {
			$("include-archived").observe("click",function(e){
				$("include-archived").value = (this.checked) ? "true" : "";
				getURL();						
			});
		}												
		// SUBMIT LINKS
		var sLinks = $$('#searching-within a');
		// ROLL OVER LINKS AND WATCH CLICK
		sLinks.each(function(li) {			
			Event.observe(li,"click", function(event) {
				event.stop();
				if(event.target.href.empty()) return;
				var target = event.target.id;
				// CHECK FOR COMMUNITY HELP CLICK
				var isCommunityHelp = (target=="community-help");
				// GRAB TEXT AND HIDDEN INPUT VALUES
				var locale=encodeURIComponent(document.searchform.loc.value);
				var term = encodeURIComponent(document.searchform.searchterm.value);
				// IF PS ENABLED, ADD PRODUCT NAME IN SEARCH COLLECTION
				if (_product_selector_flag != "off"){ 
					var category = encodeURIComponent(document.searchform.searchin.value);
					category = (category!="all-categories") ? category : "";
					var qCat  = (isCommunityHelp) ? "&l=" : "&product=";
				}
				// TOGGLE QUERY PARAM NAMES FOR SEARCH GROUPS
				var qTerm = (isCommunityHelp) ? "?q=" : "&term=";
				var qLoc  = (isCommunityHelp) ? "&lr=" : "&loc=";
				var query = qTerm + term;			
				if(isCommunityHelp) {
					query += qLoc +  locale;
					query += "&hl=" + locale;
					if (_product_selector_flag != "off"){
						query += (category.empty()) ? "&l=0" : qCat + category + "_product_adobelr";
					} else { 
						query  += "&l=0";
					}
				} else {
					query += qLoc + locale;
					if (_product_selector_flag != "off"){
						query += (category.empty()) ? "" : qCat + category;
					}
				}
				window.location.href=(event.target+query)
			});
		});
	});
</script>

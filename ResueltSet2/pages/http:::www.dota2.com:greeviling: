			{
				var strMenus = '#languageSelector';
				
				$( strMenus ).each(
					function()
					{
						$( this ).hover(
							function ()
							{
								//show its submenu
								$( this ).find( 'div' ).fadeIn( 'fast' );
							},
							function ()
							{
								//hide its submenu
								$( this ).find( 'div' ).hide();
							}
						);
					}
				);
			}

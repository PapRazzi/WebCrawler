function categorySearch(iForm){
	if(trim(iForm.SearchText.value) == ''){
		alert('Please input a search term.');
		return false;
	}
	if(iForm.SearchText.value.length>50){
		alert('please input a search term less than 50 characters.');
		return false;
	}
	var searchURL = 'http://www.alibaba.com' +
									'/trade/search?SearchText=' + escape(iForm.SearchText.value) +
									'&IndexArea=product_en&CatId=' + '7';
	window.location = searchURL;
	return false;
}
for(var i=0,j=6; i<j; i++){
    var overkShowConfig = [];
    var overkShowInstance = [];
    overkShowConfig[i]={
    	targetId:"ovs_"+i,
    	contentId:"ovs_content_"+i,
    	showDelayTime:200,
    	hiddenDelayTime:200,
    	excursion:[get("ovs_"+i).offsetWidth+5,0]
	};
	overkShowInstance[i] = new AE.widget.overShow();
	overkShowInstance[i].init(overkShowConfig[i]);
}

//add by colin
//获取父级元素的一级子元素
var fnGetChildNode = function (oParent, name) {
	var aChildNode = [] ;
	var oNodes = oParent.getElementsByTagName(name) ;
	for (var i in oNodes) {
		if (oNodes[i].parentNode==oParent){
			aChildNode.push(oNodes[i]) ;
		}
	}
	return aChildNode ;
} ;

var fnSlideTab = function (oLis) {
	var lastActive = 0 ;
	for (var i=0, k;k= oLis[i] ;i++ ) {
		k.index = i ;
		k.onmouseover = function () {
			var _self = this;
			this.runSlide = setTimeout(function(){
				oLis[lastActive].className = '' ;
				oLis[_self.index].className = 'active' ;
				var oChildNode = fnGetChildNode(_self.parentNode.parentNode, 'div') ;
				oChildNode[lastActive].className = (oChildNode[lastActive].className.indexOf('dblock')>-1)?oChildNode[lastActive].className.replace('dblock','dnone'):oChildNode[lastActive].className+' dnone' ;
				oChildNode[_self.index].className = (oChildNode[lastActive].className.indexOf('dnone')>-1)?oChildNode[lastActive].className.replace('dnone','dblock'):oChildNode[lastActive].className+' dblock' ;
				lastActive = _self.index ;
			}, 100)

		} ;
		k.onmouseout = function(){
			clearTimeout(this.runSlide);
		}
	}
} ;

(function () {
	var oSlideTabs = YAHOO.util.Dom.getElementsByClassName('slideHeader','ul');
	for (var i=0, k;k= oSlideTabs[i] ;i++ ) {
		fnSlideTab(k.getElementsByTagName('li')) ;
	}
})();

</script>




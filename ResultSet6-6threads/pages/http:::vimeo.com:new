    window.addEvent('domready', function() {
                                    
        function SlideShow(elements, speed){
            var i = 0;

            setInterval(function() {
                elements[i].removeClass('active');
                i++;
                if (i >= elements.length) {
                    i = 0;
                }
                elements[i].addClass('active');
            }, speed);
        }

        var fig1 = $$('img.fig1');
        var fig2 = $$('img.fig2');
        var fig3 = $$('img.fig3');

        SlideShow(fig1, 3000);
        SlideShow(fig2, 1800);
        SlideShow(fig3, 4000);
    


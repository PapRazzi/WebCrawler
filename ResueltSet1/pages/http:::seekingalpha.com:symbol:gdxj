      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">GDXJ</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['gdxj']
      });
    })();

    try { SymbolData.handle_data('GDXJ', {"rt_time":"04:00PM ET,02/15","last":17.22,"high":17.52,"open":17.45,"close":17.7,"low":17.07,"volume":5787500,"volume_at":1361063419,"at":1361063419,"avg3vol":2968600.0,"div_rate":0.74,"div_yield":4.3,"chgp_1w":-7.0,"chgp_4w":-16.0,"chgp_52w":-38.1,"beta60":0.9,"beta36":0.9,"mov_avg_10d":18.19,"mov_avg_10w":19.68,"mov_avg_200d":21.01,"short_interest":1.1,"liquidity":42160000.0,"p_week_vol_shares":13.75,"shares_held":0.0,"p_shares_by_inst":0.0,"p_shares_by_insid":0.0,"net_insid_tr":0.0,"insid_sb":0.0,"insid_ss":0.0,"low52":17.07,"high52":29.89,"market_cap":2322978000.0,"total_enterprise":2322978000.0,"last_div":0.75,"last_div_date":"12/24/12","iop":-0.46199999999999997,"iopp":-38.11881188118812,"inst_chg":0.0,"rt_ts":1360944000,"chg_vs_mkt_1w":-7.0,"chg_vs_mkt_4w":-18.0,"chg_vs_mkt_52w":-45.0,"p_close_10w":-13.0,"p_close_200d":-18.0}) } catch (e) { }
  </script>

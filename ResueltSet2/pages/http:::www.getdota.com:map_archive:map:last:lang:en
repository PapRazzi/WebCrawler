<iframe id="map-upload" src="" style="display:none;"></iframe>

<script language="javascript">	
	var mirror_timer = new Array(3);
	mirror_timer[1] = 0;
	mirror_timer[2] = 0;
	
	
	$(function() {
		bindMenuActions(1);
		bindMenuActions(2);
		SelectMirror(1, 0);
		SelectMirror(2, 0);
	});
	
	function SelectMirror(mirror_nr, mirror_id){
		$('#input_mirror'+mirror_nr).val(mirror_id);
		$('#mirror'+mirror_nr).html($('#mirror_name'+mirror_nr+'_'+mirror_id).html());
		$('#mirrors-list'+mirror_nr).hide();
	}
	
	function ShowMirrorsList(mirror_nr){
		if($('#mirrors-list'+mirror_nr).is(':visible'))
			$('#mirrors-list'+mirror_nr).hide();
		else{
			clearTimeout(mirror_timer[mirror_nr]);
			mirror_timer[mirror_nr] = setTimeout('hideMenus()', 1500);
			$('#mirrors-list'+mirror_nr).show();
		}
	}
	
	function hideMirrorsListByTime(mirror_nr){
		clearTimeout(mirror_timer[mirror_nr]);
		mirror_timer[mirror_nr] = setTimeout('hideMenus()', 50);
	}
	
	function hideMenus(){
		$('#mirrors-list1').hide();
		$('#mirrors-list2').hide();
	}
	
	function bindMenuActions(mirror_nr){
		$('#mirrors-list'+mirror_nr).mouseout( function() { hideMirrorsListByTime(mirror_nr); } );	
		$('#mirrors-list'+mirror_nr).mouseover( function() { clearTimeout(mirror_timer[mirror_nr]); } );
	}
	
	function DownloadMap(mirror_nr)
	{
		as_zip = 0;
		if($('#as_zip'+mirror_nr).attr("checked")) as_zip = 1;
		$.post("http://www.getdota.com/app/getmap/", 
			{ 
				mirror_id: $('#input_mirror'+mirror_nr).val(),
				mirror_nr: mirror_nr,
				file_name: $('#input_file_name'+mirror_nr).val(),
				as_zip: as_zip,
				language: $('#input_language'+mirror_nr).val(),
				language_id: $('#input_language_id1').val(),
				map_id: $('#input_map_id'+mirror_nr).val()
			}, 
			function(data){
				//$('#map-upload').attr('src', data);
				if(data != '')document.location.href = data; else alert("This map isn't avaliable on this mirror");
			}
		);	
	}
</script>					</td>
				</tr>
				</table>					

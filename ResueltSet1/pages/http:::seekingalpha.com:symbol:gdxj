    document.write('<scr'+'ipt src="http://static.cdn-seekingalpha.com/account/cookie/'+readCookie('user_cookie_key')+'"></scri'+'pt>');
  } else if (readCookie('user_id')) {
    document.write('<scr'+'ipt src="http://seekingalpha.com/account/no_cookie_key"></scri'+'pt>');
  }


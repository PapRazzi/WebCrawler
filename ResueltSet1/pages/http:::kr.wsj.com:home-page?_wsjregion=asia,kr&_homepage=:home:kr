<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script>

<script type="text/javascript">
// gutterAd Vanilla JS
(function (window, $, config) {

    'use strict';

    $ = $ || false;
    if (!config) {
        config = {
            env: 'dev'
        };
    }

    if (config.env === 'dev') {

        var gutterAd = {

            init: function () {
                var self = this,
                    stickyDiv = self.getDiv('sticky'),
                    fullWideDiv = self.getDiv('fullwide'),
                    contentWideDiv = self.getDiv('contentwide'),
                    footer = document.getElementById('footerWrapper'),
                    footerPageWide = self.getDiv('footer'),
                    stickyDivOffsetTop = stickyDiv.offsetTop,
                    footerOffset = self.getDimension(footer),
                    docHeight = self.getDocHeight();
console.log(contentWideDiv);
                self.updateNodeClasses(footer, fullWideDiv, footerPageWide, contentWideDiv);
                self.scroll(stickyDiv, stickyDivOffsetTop, self);

            },
            updateNodeClasses: function (footer, fullWideDiv, footerPageWide, contentWideDiv) {

                if (footer && fullWideDiv && footerPageWide && contentWideDiv) {
                    if (footer.className.indexOf(' gutter-ad-true') === -1) {
                        footer.className = footer.className += ' gutter-ad-true';
                    }
                    if (contentWideDiv.className.indexOf(' gutter-ad-true') === -1) {
                        contentWideDiv.className = contentWideDiv.className += ' gutter-ad-true';
                    }
                    if (footer.parentNode.className.indexOf(' gutter-ad-true') === -1) {
                        footer.parentNode.className = footer.parentNode.className += ' gutter-ad-true';
                    }
                    if (fullWideDiv.className.indexOf(' gutter-ad-true') === -1) {
                        fullWideDiv.className = fullWideDiv.className += ' gutter-ad-true';
                    }
                    if (footerPageWide.className.indexOf(' gutter-ad-true') === -1) {
                        footerPageWide.className = footerPageWide.className += ' gutter-ad-true';
                    }
                }
            },
            getWindowHeight: function () {
                var B = document.body,
                    D = document.documentElement;
                return Math.min(D.clientHeight, B.clientHeight);
            },
            throttle: function (ms, callback) {
                var timer;

                return function() {
                    var self = this, args = arguments;
                    clearTimeout(timer);
                    timer = setTimeout(function() {
                        callback.apply(self, args);
                    }, ms);
                };
            },
            scroll: function (stickyDiv, stickyDivOffsetTop, self) {

                var stuck = false;

                var scrollInit = function () {
                    var windowScroll = (window.scrollY) ? window.scrollY : document.getElementsByTagName('html').item(0).scrollTop;

                    if (windowScroll > stickyDivOffsetTop) {
                        if (!stuck) {
                            stickyDiv.className = stickyDiv.className += ' fixed-sponsor-scrolling';
                            stuck = true;
                        }
                    } else {
                        stickyDiv.className = stickyDiv.className.replace(/\b\sfixed-sponsor-scrolling\b/, '');
                        stuck = false;
                    }
                };

                if (navigator.userAgent.indexOf('MSIE') !== -1) {
                    window.onscroll = self.throttle(15, scrollInit);
                } else {
                    window.onscroll = scrollInit;
                }

            },
            getDiv: function (node) {

                var divs = document.getElementsByTagName('div'),
                    i = 0,
                    nodeEl = node,
                    div;

                for (i; i < divs.length; i += 1) {
                    if ((divs[i].getAttribute('class') === 'gutter-ad-wrapper') && nodeEl === 'sticky') {
                        div = divs[i];
                    } else if ((divs[i].className.indexOf('fullwide') !== -1) && nodeEl === 'fullwide') {
                        div = divs[i];
                    } else if ((divs[i].className.indexOf('contentwide') !== -1) && nodeEl === 'contentwide') {
                        div = divs[i];
                    } else if ((divs[i].parentNode.parentNode.className.indexOf('pagewide') !== -1) && nodeEl === 'footer') {
                        div = divs[i].parentNode.parentNode;
                    }
                }
                return div;
            },
            getDimension: function (elmID) {

                var elmHeight, elmHeightNum, elmMargin, elmMarginNum, elmPadding, elmPaddingNum,
                    elm = elmID;

                if (document.all) { // IE
                    elmHeight = elm.currentStyle.height + 'px';
                    elmHeightNum = parseInt(elmHeight, 10);
                    elmMargin = parseInt(elm.currentStyle.marginTop, 10) + parseInt(elm.currentStyle.marginBottom, 10) + "px";
                    elmMarginNum = parseInt(elmMargin, 10);
                    elmPadding = parseInt(elm.currentStyle.paddingTop, 10) + parseInt(elm.currentStyle.paddingBottom, 10) + "px";
                    elmPaddingNum = parseInt(elmPadding, 10);
                } else { // Mozilla
                    elmHeight = document.defaultView.getComputedStyle(elm, '').getPropertyValue('height');
                    elmHeightNum = parseInt(elmHeight, 10);
                    elmMargin = parseInt(document.defaultView.getComputedStyle(elm, '').getPropertyValue('margin-top')) + parseInt(document.defaultView.getComputedStyle(elm, '').getPropertyValue('margin-bottom')) + "px";
                    elmMarginNum = parseInt(elmMargin, 10);
                    elmPadding = parseInt(document.defaultView.getComputedStyle(elm, '').getPropertyValue('padding-top')) + parseInt(document.defaultView.getComputedStyle(elm, '').getPropertyValue('padding-bottom')) + "px";
                    elmPaddingNum = parseInt(elmPadding, 10);
                }
                return (elmHeightNum + elmMarginNum + elmPaddingNum);
            },
            getDocHeight: function () {

                var d = document;
                return Math.max(
                  Math.max(d.body.scrollHeight, d.documentElement.scrollHeight),
                  Math.max(d.body.offsetHeight, d.documentElement.offsetHeight),
                  Math.max(d.body.clientHeight, d.documentElement.clientHeight));
            }
        };

        return gutterAd.init();

    } else if (config.env === 'prototype' && $) {

        var stickyDiv = $('.gutter-ad-wrapper'),
            footer = $('#footerWrapper'),
            stickyDivOffsetTop = $(stickyDiv).offset().top,
            docHeight = $(document).height();

        $(footer).addClass('gutter-ad-true');
        $('body > .fullwide').addClass('gutter-ad-true');
        $('.fullwide .pagewide').addClass('gutter-ad-true');
        $('.contentwide').addClass('gutter-ad-true');

        $(window).scroll(function (docHeight) {
            var windowHeight = $(window).height(),
                windowScroll = $(window).scrollTop(),
                scrollBottom = docHeight - windowHeight - windowScroll;

            if (windowScroll > stickyDivOffsetTop) {
                $(stickyDiv).addClass('fixed-sponsor-scrolling');
            } else {
                $(stickyDiv).removeClass('fixed-sponsor-scrolling');
            }

        });
    }
}(window, jQuery, {
    env: 'prototype' // dev | prototype
}));

</script>

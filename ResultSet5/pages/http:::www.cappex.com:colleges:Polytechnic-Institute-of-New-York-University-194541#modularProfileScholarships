<script type="text/javascript">

 var mapLat = 40.694371;
 var mapLng = -73.986774;

 if (false) {

 //The following code will run if the zipcode and radius was passed
 //circle will be added to the map

 var zipLat = 37.787766;
 var zipLng = -122.400766;
 var circle;

 MQA.EventUtil.observe(window, 'load', function() {
 var options = {
 elt:document.getElementById('map_canvas'), /*ID of element on the page where you want the map added*/
 zoom:7, /*initial zoom level of the map*/
 mtype:'map', /*map type (map)*/
 bestFitMargin:0, /*margin offset from the map viewport when applying a bestfit on shapes*/
 zoomOnDoubleClick:true /*zoom in when double-clicking on map*/
 };

 /*Construct an instance of MQA.TileMap with the options object*/
 window.map = new MQA.TileMap(options);
 //set map size to value that fits inside #tripRadiusMap
 map.setSize(new MQA.Size(560, 350));

 //define default circle with no center point and radius of 0
 MQA.withModule('shapes', function(location) {
 circle = new MQA.CircleOverlay();
 circle.radiusUnit = 'MI'; //units MI or KM
 circle.radius = 0; //radius of circle
 circle.borderWidth = 0; //no border
 circle.fillColor = '#3399FF'; //color of circle
 circle.shapePoints = [zipLat, zipLng]; //sets center of map
 circle.fillColorAlpha = 0.5; //opacity or transparency

 });

 //add zoom controls
 MQA.withModule('smallzoom', function() {
 map.addControl(
 new MQA.SmallZoom(),
 new MQA.MapCornerPlacement(MQA.MapCorner.TOP_LEFT, new MQA.Size(5, 5))
 );
 });

 // map view / satellite view / etc
 MQA.withModule('viewoptions', function() {
 map.addControl(
 new MQA.ViewOptions()
 );
 });

 MQA.withModule('geocoder', function() {

 //Executes the geocode service with the postal code
 map.geocodeAndAddLocations({postalCode:''});

 //Override the POI construction function to exclude the pin in the map.
 //The pin will not allow us to center the map on the circle
 MQA.Geocoder.constructPOI = function(location) {
 zipLat = location.latLng.lat; //obtain lat and long from geocoder
 zipLng = location.latLng.lng;
 circle.radius = radius; //update circle radius
 circle.shapePoints = [zipLat, zipLng]; //update circle center point
 map.addShape(circle);
 map.setCenter({lat:zipLat,lng:zipLng}); //center map on point

 /*
 set proper zoom level around circle

 at this time, mapquest did not support bestFit for circles,
 so we must create a rectangle surrounding the circle
 MQA.RectLL is created with MQA.LatLng arguments derived from circle.shapPoints
 first argument for RectLL is upperLeft latlong object and second is lowerRight latlong object
 links to mapquest documentation:
 http://developer.mapquest.com/content/documentation/ApiDocumentation/53/JavaScript/TKitDevRef/files/mqa-object-js.html#MQA.RectLL
 http://developer.mapquest.com/content/documentation/ApiDocumentation/53/JavaScript/TKitDevRef/files/mqa-tilemap-js.html#MQA.TileMap.zoomToRect
 link to forum, which is source for following line of code:
 http://developer.mapquest.com/web/products/forums/-/message_boards/view_message/267158;jsessionid=6226BAC9267705B53540AE5892D5E16A?_19_threadView=tree
 */
 map.zoomToRect(new MQA.RectLL(new MQA.LatLng(circle.shapePoints[2], circle.shapePoints[3]), new MQA.LatLng(circle.shapePoints[0] - Math.abs(circle.shapePoints[0] - circle.shapePoints[2]), circle.shapePoints[1] + Math.abs(circle.shapePoints[1] - circle.shapePoints[3]))));

 }

 });
 });

 }
 else if (mapLat && mapLng) { //this runs if there was not a zip radius, but the mapLat and mapLng are (aka: on the modularProfile including quickFactsWithMap

 MQA.EventUtil.observe(window, 'load', function() {
 var options = {
 elt:document.getElementById('map_canvas'), /*ID of element on the page where you want the map added*/
 zoom:8, /*initial zoom level of the map*/
 latLng:{lat:mapLat, lng:mapLng}, /*center of map in latitude/longitude */
 mtype:'map', /*map type (map)*/
 bestFitMargin:0, /*margin offset from the map viewport when applying a bestfit on shapes*/
 zoomOnDoubleClick:true /*zoom in when double-clicking on map*/
 };

 /*Construct an instance of MQA.TileMap with the options object*/
 window.map = new MQA.TileMap(options);

 // zoom controls
 MQA.withModule('smallzoom', function() {
 map.addControl(
 new MQA.SmallZoom(),
 new MQA.MapCornerPlacement(MQA.MapCorner.TOP_LEFT, new MQA.Size(5, 5))
 );
 });

 // map view / satellite view / etc
 MQA.withModule('viewoptions', function() {

 map.addControl(
 new MQA.ViewOptions()
 );

 });

 // frustrating

 var markerIcon = new MQA.Icon("/img/icons/map/capMarkerBlank.png", 44, 37);

 var marker = new MQA.Poi({lat:mapLat, lng:mapLng});
 marker.setIcon(markerIcon);
 map.addShape(marker);

 });

 } else {

 $('mapMsg').set('html', 'Couldn&apos;t find map data');
 $('map_canvas').set("html", "<img src='/img/content/disabledMap.png'>");

 }
 </script>

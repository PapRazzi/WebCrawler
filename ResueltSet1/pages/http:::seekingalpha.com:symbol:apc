    <div class="title_tab">
      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">APC</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['apc']
      });
    })();

    try { SymbolData.handle_data('APC', {"rt_time":"04:00PM ET,02/15","last":82.08,"high":83.83,"open":83.69,"close":84.0,"low":81.69,"volume":4173600,"volume_at":1361063490,"at":1361063490,"avg3vol":3093700.0,"c_share":5.07,"div_rate":0.36,"forward_pe":20.6,"payout":8.0,"trailing_pe":17.3,"div_yield":0.4,"payout_5y":7.0,"div_yield_5y":0.6,"chgp_1w":-2.8,"chgp_4w":5.8,"chgp_52w":-6.8,"beta60":1.49,"beta36":2.28,"mov_avg_10d":83.25,"mov_avg_10w":78.38,"mov_avg_200d":70.96,"short_interest":1.7,"p_short_interest_float":1.1,"liquidity":282242300.0,"p_week_vol_shares":2.56,"sfloat":504592401.0,"shares_held":418565688.0,"p_shares_by_inst":83.7,"p_shares_by_insid":0.4,"net_insid_tr":171962.0,"insid_sb":330752.0,"insid_ss":158790.0,"total_revenue":13411000000.0,"revenue_growth":-4.1,"net_income":2391000000.0,"gross_margin":41.3,"eps":4.74,"roe":11.9,"roe_ind":98.3,"roa":3.4,"cur_ratio":1.5,"quick_ratio":1.1,"int_cover":4.3,"debt_eq":0.69,"lt_debt_cap":0.4,"debt_short":0.0,"debt_long":13269000000.0,"low52":56.42,"high52":88.28,"market_cap":41020237188.0,"cash":2533779270.75,"debt":13269000000.0,"total_enterprise":51755457917.25,"price_book":2.0112717471208037,"ev_ebitda":11.906017464285714,"last_div":0.09,"last_div_date":"03/11/13","iop":0.0,"iopp":0.0,"price_sales":3.0587008566102454,"price_tangb":2.8013651877133103,"ev_sales":3.85917962249273,"ev_fcf":-15.95697773971839,"inst_chg":14031740.0,"cf":5677264796.0,"fcf":-3243437370.25,"debt_fcf":-4.091030128008065,"rt_ts":1360944000,"chg_vs_mkt_1w":-3.0,"chg_vs_mkt_4w":3.0,"chg_vs_mkt_52w":-17.0,"p_close_10w":5.0,"p_close_200d":16.0}) } catch (e) { }

  <script type="text/javascript">
/*global window*/
if (window.jQuery !== undefined) {
    window.jQuery(function ($) {
        "use strict";
        /* If user is logged in, put the markup from
           http://www.usnews.com/mws_cin/grad_law_premium_rankings
           after this widget.

           If user is anonymous, show this widget.

           We target "this widget" primarily by class attribute,
           but class attribute is not necessarily unique.
           So, we also manipulate the class attribute after processing
           and grab the first matching widget in case a few duplicate
           class attributes are on the page.
           Note that if the user is logged in and the ajax-fetched widget
           contains the class attribute we're hooking on, all bets are off.
        */

        var html_class = 'youre-not-seeing-all-the-data-we-have',
            $widget = $('.widget.' + html_class + ':not(.processed):first'),
            is_premium = (window.location.hostname.indexOf('premium') !== -1);
        if (is_premium) {
            $.ajax({
                cache: true,
                url: 'http://www.usnews.com/mws_cin/grad_law_premium_rankings',
                dataType: 'jsonp',
                context: $widget,
                success: function (markup) {
                    this.after(markup);
                }
            });
        } else {
            $widget.show();
        }
        $widget.addClass('processed');
    });
}
  </script>

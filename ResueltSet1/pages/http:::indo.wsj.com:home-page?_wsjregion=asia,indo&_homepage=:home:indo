      fwDiv.className = fwDiv.className + " subType-unsubscribed sectionType-none sectionType-unsub-none";
    } else {
      fwDiv.className = fwDiv.className + " subType-unsubscribed";
    }
  }

  if (dl.hash.indexOf("printMode") > -1) {
    try {
      var head = d.getElementsByTagName('head')[0];
      var link = document.createElement('link');
      link.rel = 'stylesheet';
      link.href = '/css/wsjprint.css';
      link.type = 'text/css';
      head.appendChild(link);
    } catch (e) {
      d.write('<link rel="stylesheet" href="/css/wsjprint.css" type="text/css"/>');
    }
  }
}());

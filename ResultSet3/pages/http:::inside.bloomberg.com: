(function($){
    var defaultSearchText = 'Search Posts    ';
    var searchBox = $('#searchform input');
    searchBox.blur(function(){
        if(searchBox.val()=='' || searchBox.val()==defaultSearchText)
        {searchBox.addClass('ghost-text');searchBox.val(defaultSearchText);}
        else{searchBox.removeClass('ghost-text');}        
    });
    $(document).ready(function(){
        if(searchBox.val()=='' || searchBox.val()==defaultSearchText)

	<!-- Custom Script Content Block -->
	<script type="text/javascript">
function openPressBox(pressBoxId, openLinkId, closeLinkID) {
	document.getElementById(pressBoxId).style.height = 'auto';
	document.getElementById(closeLinkID).style.display = 'none';
	document.getElementById(openLinkId).style.display = 'inline';
}


function closePressBox(pressBoxId, openLinkId, closeLinkID) { 
	document.getElementById(pressBoxId).style.height = '';
	document.getElementById(openLinkId).style.display = 'inline';
	document.getElementById(closeLinkID).style.display = 'none';
}

var menuLinkIDs = new Array('pressLink','newsLink','awardsLink');

function swapMenuLink(clickLinkID) {
	for(var i=0;i<menuLinkIDs.length;i++) {
		if (menuLinkIDs[i]!=clickLinkID.id) {
			if (document.getElementById(menuLinkIDs[i])) {
				document.getElementById(menuLinkIDs[i]).className='';
			}
		} else {
			if (document.getElementById(clickLinkID.id)) {
				document.getElementById(clickLinkID.id).className='selected'; 
			}
		}
	}
}
</script>
	<!-- END Custom Script Content Block -->

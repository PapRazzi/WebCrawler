      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">LNG</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['lng']
      });
    })();

    try { SymbolData.handle_data('LNG', {"rt_time":"04:00PM ET,02/15","last":21.74,"high":22.06,"open":22.06,"close":21.82,"low":21.42,"volume":2378400,"volume_at":1361063419,"at":1361063419,"avg3vol":3762600.0,"c_share":0.96,"div_rate":0.0,"forward_pe":-8.4,"payout":0.0,"div_yield":0.0,"payout_5y":0.0,"chgp_1w":-1.8,"chgp_4w":6.5,"chgp_52w":55.3,"beta60":0.88,"beta36":3.82,"mov_avg_10d":21.8,"mov_avg_10w":19.98,"mov_avg_200d":16.16,"short_interest":4.8,"p_short_interest_float":8.1,"liquidity":79842000.0,"p_week_vol_shares":5.21,"sfloat":213386404.0,"shares_held":164660077.0,"p_shares_by_inst":73.7,"p_shares_by_insid":11.2,"net_insid_tr":-946638.0,"insid_sb":8542.0,"insid_ss":955180.0,"total_revenue":271390000.0,"revenue_growth":-8.7,"net_income":-296265000.0,"gross_margin":69.1,"eps":-2.04,"roa":-6.8,"cur_ratio":4.6,"quick_ratio":1.5,"debt_eq":3.85,"lt_debt_cap":0.8,"debt_short":0.0,"debt_long":2295939000.0,"low52":10.51,"high52":22.32,"market_cap":4855462102.0199995,"cash":214408630.07999998,"debt":2295939000.0,"total_enterprise":6936992471.94,"price_book":8.142322097378276,"ev_ebitda":192.2509899936258,"price_sales":17.89108700401636,"price_tangb":9.452173913043477,"ev_sales":25.56097303489443,"ev_fcf":-6.8263538661980485,"inst_chg":49088087.0,"cf":-207708360.39000002,"fcf":-1016207569.65,"debt_fcf":-2.2593208991650813,"rt_ts":1360944000,"chg_vs_mkt_1w":-2.0,"chg_vs_mkt_4w":4.0,"chg_vs_mkt_52w":39.0,"p_close_10w":9.0,"p_close_200d":35.0}) } catch (e) { }

	var addLocdrop = function(){
		Y.one(".yls-br-get-directions").setStyle("z-index","99");
		Y.one(".directions_btn").setStyle("display","none");
		var ulwConfig2 = {
			        divID		: "widget2",
			        appID		: "universal_location_widget__v2",
			        ctx		: "search",
			        callback	: locChanged2,
			        crumb           : "UTNHRE5weGtuQW8-",
				//yqlHost         : "gamma.locdrop.query.yahoo.com",
			        yqlTimeout      : null,
			        upapiExtracted  : {currentLoc : {"addr":"New York,New York,United States,10003","app":"yahoo.local_us.ups","cc":"US","city":"New York","country_conf":99,"ctry":"United States","ctx":"global","dtime":"2013-02-18T01:46:58Z","id":"id_current","input":"ip","lat":40.731391906738,"lon":-73.988403320312,"lvl":1,"raw":"216.165.95.78","sc":"NY","state":"New York","state_conf":80,"town_conf":75,"type":"physical","woe":12761335,"zip":"10003","zip_conf":1},defaultLoc : null},
			        locationLevel   : "city",
				overlayAnchor	: "q",
			        locale 		: null,
			        yrbHost		: null,
			        yrbPath		: null,
				minWidth        : null,
				anchorText      : "Choose a location",
				placeFinderGflags : true,
			        customInput	: true,
			        positionXY      : null
	};
	var widget2 = new LocdropLocationPicker(ulwConfig2);
	Y.one("#q").set("autocomplete","off");
	widget2.setPosWidget2 = function(W){
		var widgetinputid = "q";
		var widgetinputselector = "#" + widgetinputid;
		p = Y.one(widgetinputselector).getXY();
		e = parseInt(p[0]);
		d = parseInt(p[1] + 25);
		this.setPositionXY([e,d]);
		if(this.state == "closed"||this.state == "open"||this.state == "reopen")
		this.open(W);
		
	}
	Y.on("mouseup", function(e) {
		if(!widget2.clickedInside(e, "#q") && e.which == 1) {
			widget2.close(); 
          	}
        }, document, widget2);
	widget2.keyup = function (A){
		if(this.state == "closed") this.open(A);
		this.handleInputEvents(A);
		if(Y.all("#"+this.lwBodyId+" li").size() == 0 && A.keyCode!=13) 
		Y.one(".yui3-overlay-content, .lw-loading-msg").setStyle("border-width","0px");
		else  Y.one(".yui3-overlay-content, .lw-loading-msg").setStyle("border-width","1px");
	};
	widget2.init();
	Y.on("click",Y.bind(widget2.setPosWidget2,widget2),"#q");
	Y.on("keypress",function(e){if(e.keyCode == 13)e.preventDefault();},"#q");
	Y.on("focus", Y.bind(widget2.setPosWidget2, widget2), "#q");
	Y.on("keyup", Y.bind(widget2.keyup, widget2), "#q");	
	}


	function locChanged2(widget){

		var location = widget.loc;
		var widgetinputid = "#q";
		Y.one(widgetinputid).set("value",location.addr);
		Y.one(widgetinputid).setStyle("color","black").setStyle("font-style","normal");
		Y.one(".button.location_btn_go").focus();
	}




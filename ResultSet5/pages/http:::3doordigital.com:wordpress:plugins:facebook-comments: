<script type="text/javascript">

$('#collapseOne').collapse("hide");

$('.carousel').carousel({
  interval: 7000
})
$('#carousel-nav a').click(function(q){
    q.preventDefault();
    targetSlide = $(this).attr('data-to')-1;
    $('#myCarousel').carousel(targetSlide);
});

$('#myCarousel').bind('slid', function() {
    // Get currently selected item
    var item = $('#myCarousel .carousel-inner .item.active');

    // Deactivate all nav links
    $('#carousel-nav a').removeClass('active');

    // Index is 1-based, use this to activate the nav link based on slide
    var index = item.index() + 1;
    $('#carousel-nav a:nth-child(' + index + ')').addClass('active');
});
</script>
<script>
$(function () {
	var msie6 = $.browser == 'msie' && $.browser.version < 7;
		if (!msie6) {
		var top = $('.headline_area').offset().top - parseFloat($('.headline_area').css('margin-top').replace(/auto/, 0));
		$(window).scroll(function (event) {
			// what the y position of the scroll is
			var y = $(this).scrollTop();
			// whether that's below the form
			if (y >= top) {
				// if so, ad the fixed class
				$('.headline_area').addClass('gofixed');
			} else {
				// otherwise remove it
				$('.headline_area').removeClass('gofixed');
			}
		});
		}  
	});
</script>

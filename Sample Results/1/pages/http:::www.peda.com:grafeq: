<script type="text/javascript"><!--
keep_menu = false;
showing_menu = 0;
active_timer = false;

function ShowMenu(menu)
{
 if (!document.getElementById) return;

 keep_menu = true;
 if (showing_menu == menu) return;
 HideMenu(true);

 var menu_items_object = document.getElementById(menu);
 var object = document.getElementById(menu + "-mt");

 var x_offset = -6;
 var y_offset = object.offsetHeight + 5;

 if (object.offsetParent)
   {
    while (object.offsetParent)
     {
      x_offset += object.offsetLeft; y_offset += object.offsetTop;
      object = object.offsetParent;
     }
   } else { x_offset += object.x; y_offset += object.y; }

 menu_items_object.style.top = y_offset + "px";
 menu_items_object.style.left = x_offset + "px";
 menu_items_object.style.visibility = "visible";

 showing_menu = menu;
}

function HideMenu(force)
{
 if (!document.getElementById) return;
 if ((keep_menu && !force) || !showing_menu) return;

 var menu_items_object = document.getElementById(showing_menu);
 menu_items_object.style.visibility = "hidden";

 showing_menu = false;
}

function HideMenuCallback(force)
{
active_timer = false;
HideMenu(false);
}

function HideMenuSoon(menu)
{
 keep_menu = false;

 if (active_timer) clearTimeout(hide_timeout);

 active_timer = true;
 hide_timeout = setTimeout("HideMenuCallback();",500);
}
//--></script>

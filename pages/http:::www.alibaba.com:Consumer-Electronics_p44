					<script type="text/javascript">
    function gotoAdvancedSearch(){
       var keyword = YAHOO.util.Dom.get('SearchTextIdx').value;
          url = 'http://www.alibaba.com/trade/advancesearch?advancedSearchText=';
       if((/^search (?:products|suppliers|buyers)$/i).test(YAHOO.lang.trim(keyword))){
          keyword = '';
       }
	   keyword = encodeURIComponent(keyword).replace(/%20/g, '+');
       window.location.href = url+keyword;
    }
</script>

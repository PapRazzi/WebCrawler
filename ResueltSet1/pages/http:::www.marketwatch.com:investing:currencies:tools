  <script type="text/javascript">
    $(document).ready(function() {
    $("div.foreverblock").each(function() {
    var doctype = $(this).data("doctype");
    if (typeof(doctype) == "undefined") {
    doctype = $(this).attr("data-doctype");
    }
    var nb = new MarketWatch.News.NewsBox(this, doctype, '', '', 'Currencies');
    nb.setNewsSpark(Spark.Shared._Investing_HeadlineItem.RenderView);
    var thunk = this;
    if (typeof (BlueGrass) != "undefined" && $(thunk).find(".bgChannel").text() != "") {
    BlueGrass.Runtime.openConnection({}, function(connection, type) {
    if (type == "connect") {
    connection.subscribe($(thunk).find(".bgChannel").text(), nb.bluegrassCallback);
    nb.setNewsSpark(Spark.Shared._Investing_HeadlineItem.RenderView);
    }
    });
    }
    });
    });
  </script>

	         $(document).ready(function() {
				
        //CONTROLLO FLASH per ipad	
        var ms = null;
        var tablet = DetectTierTablet();
        var smartphone = DetectTierIphone();
        //se mobile device
        if(tablet == true || smartphone === true){
        	var     ms_text= null,
                    ms_text = $("#text").attr("class");
            $("div#swf_farfalle").remove();
            $("div#corpo_pagina_img").fadeIn("slow").css('display','table');
        }
        //se no ipad
        else{
            $(document).ready(function() {
                // alert("ms is " + ms);
                var ms_flash = $("#animation").attr("class");
                    
                    $("div#swf_farfalle").delay(ms_flash).fadeOut("slow", function () {
                         $("div#corpo_pagina_img").css('display','table');
                    })
            });
     	 }

	
			});

      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">TGT</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['tgt']
      });
    })();

    try { SymbolData.handle_data('TGT', {"rt_time":"04:00PM ET,02/15","last":61.71,"high":62.6,"open":62.37,"close":63.09,"low":60.74,"volume":14614400,"volume_at":1361063419,"at":1361063419,"avg3vol":5030100.0,"c_share":1.03,"div_rate":1.44,"forward_pe":12.8,"payout":28.0,"trailing_pe":13.7,"div_yield":2.3,"div_grow_rate":19.32,"payout_5y":21.0,"div_yield_5y":1.7,"chgp_1w":-1.2,"chgp_4w":0.3,"chgp_52w":17.2,"beta60":0.93,"beta36":0.54,"mov_avg_10d":62.36,"mov_avg_10w":61.03,"mov_avg_200d":61.08,"short_interest":2.9,"p_short_interest_float":2.1,"liquidity":415923200.0,"p_week_vol_shares":4.87,"sfloat":661011099.0,"shares_held":558975078.0,"p_shares_by_inst":85.6,"p_shares_by_insid":0.3,"net_insid_tr":128586.0,"insid_sb":365685.0,"insid_ss":237099.0,"total_revenue":71863000000.0,"revenue_growth":4.1,"net_income":3019000000.0,"gross_margin":31.0,"eps":4.5,"roe":18.5,"roe_ind":83.3,"roa":6.1,"cur_ratio":1.2,"quick_ratio":0.1,"int_cover":7.8,"debt_eq":1.13,"lt_debt_cap":0.5,"debt_short":4028000000.0,"debt_long":14526000000.0,"low52":52.25,"high52":65.8,"market_cap":40160524028.46,"cash":1470318258.78,"debt":18554000000.0,"total_enterprise":57244205769.68,"price_book":2.4556307202546757,"ev_ebitda":7.7210960034637175,"last_div":0.36,"last_div_date":"02/15/13","iop":0.0,"iopp":0.0,"price_sales":0.558848420306138,"price_tangb":2.4585657370517926,"ev_sales":0.7965741169959506,"ev_fcf":28.37435615794446,"inst_chg":13663203.0,"cf":5186831575.22,"fcf":2017462720.6000001,"debt_fcf":9.196700296143256,"rt_ts":1360944000,"chg_vs_mkt_1w":-1.0,"chg_vs_mkt_4w":-2.0,"chg_vs_mkt_52w":5.0,"p_close_10w":1.0,"p_close_200d":1.0}) } catch (e) { }

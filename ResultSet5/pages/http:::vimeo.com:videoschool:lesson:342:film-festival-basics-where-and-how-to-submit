                var translation = typeof this.dict[key] != 'object' ? this.dict[key] : (plural ? this.dict[key].plural : this.dict[key].singular);
                if (typeof replacements === 'object') {
                    translation = this.substitute(translation, replacements);
                }
                return translation;

<script type="text/javascript">
    function addSearch(url) {
        if (window.external != undefined && window.external != null) {
            if (null != url) {
                var currentUrl = "http://" + window.location.host + url;
                window.external.AddSearchProvider(currentUrl);
            }

            return false;
        }
    }
</script>

 PAGE.add('singleBarGraphWidget', function() {
 // add tooltip functionality
 $('.segment').jqTip();

 function comafy(str) {
 return str.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
 }

 // define color object & count vars
 // NOTE - colors array may eventually be a passed var with help from some JSP
 var colors = ["#62E68C", "#90BAE1", "#DD7C7C", "#EBA763"], count = 0;

 // animate graph on load
 $('.segment').each(function(index) {
 // percent determines width
 widthMod = parseFloat($(this).data('segment-percent'));

 // set legend colors
 $('.singleBarGraphLegend span').eq(count).css('color', colors[count]);

 $(this).css("background-color", colors[count]);

 $(this).css({
 'width': widthMod + "%",
 'opacity': 1,
 'display': 'table-cell',
 'float': 'none',
 'height': 'auto'
 });

 // animate the graph on page load
 /*
 $(this).animate({
 'width': widthMod + "%",
 'opacity': 1
 }, 1000, function() {
 // a little hackish really, but
 // doing this means when/if animated, the segments
 // will be FAR easier to contain
 $('.segment').css({
 'display': 'table-cell',
 'float': 'none',
 'height': 'auto'
 });
 */
 content = $(this).width() > 50 ? comafy($(this).data('segment-value')) : '';
 $(this).html(content);
 $(this).attr('data-jqtipcontent', comafy($(this).data('segment-value')) + " " + $(this).data('segment-label'));
 //});

 count ++;
 });

 /* dpl 6.17.12 - removing animation per jace - but commenting out because we may revisit
 // mouse over/out calls
 $('.segment').on('mouseover', function() {
 $('.segment').stop();
 num = $('.segment').length - 1;
 per = 50 / num;

 $('.segment').not($(this)).animate({
 width: per + "%"
 }, 300);

 $(this).animate({
 width: 50 + '%'
 }, 1000);

 });

 $('.segment').on('mouseout', function() {
 $('.segment').stop();
 $('.segment').each(function(index) {

 widthMod = parseFloat($(this).data('segment-percent'));

 $(this).animate({
 width: widthMod + "%"
 }, 1000);
 });

 });
 */
 });

 // we don't want to wait for the page to finish before we launch the
 // graph, so executing it here (and it will be remove from the page load queue
 PAGE.execute('singleBarGraphWidget');
 </script>

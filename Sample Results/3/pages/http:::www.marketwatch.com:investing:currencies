      {
      MarketWatch.Investing.Converter.Setup('/investing/currencies/convert');
      });

      $('#converter input').numeric({allow:"."});

      var f = document.getElementById("fromCurrencyDDL");
      var t = document.getElementById("toCurrencyDDL");
      
      setSelectedIndex(f, 'USD');
      setSelectedIndex(t, 'EURUSD');

      function setSelectedIndex(s, v) {
      for ( var i = 0; i < s.options.length; i++ ) {
        if ( s.options[i].value == v ) {
            s.options[i].selected = true;
            return;
              }
          }
      }

    </script>

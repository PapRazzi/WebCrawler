<script type="text/javascript" charset="utf-8">

function answer(answer_url, skipping) {
  answering = true
  var lastAnswer = $('lastAnswer')
  if (lastAnswer) {
    lastAnswer.innerHTML = "<h1>Saving Your Response</h1>" + 
      '<div class="content" style="padding:4px"><span style="padding:4px">saving...</span><img alt="Loading" src="http://s.gr-assets.com/assets/loading-9404bf7c15db1e6aae360bad4c671139.gif" /></div>';
  }
  if (!skipping) {
    get_next_question();
  }
  if (in_batch) {
    answer_url += "&in_batch=" + in_batch;
  }
  new Ajax.Updater('answer', answer_url, {evalScripts:true, onComplete:function() {
    answering = false
    if (skipping) {
      get_next_question();
    }
  }});

  //urchinTracker('/trivia'); 
  if(window.pageTracker) {
    pageTracker._trackPageview('/trivia'); 
  }
}

var adUpdaters = [];

function get_next_question() {
  var qid = question_batch.pop();
  var questionDiv = $('question');
  if (qid) {
    if (questionDiv) {
      questionDiv.innerHTML = '<div id="questionContent" class="bigBox">' +
        '<div class="h1Container">' +
        '<h1 class="brownBackground">' +
        'Loading the Next Question' +
        '</h1>' +
        '</div>' +
        '<div class="bigBoxBody"><div class="bigBoxContent">' +
        'loading the next question...<br/>' +
        '<img alt="Loading" src="http://s.gr-assets.com/assets/loading-9404bf7c15db1e6aae360bad4c671139.gif" /><br/><br/><br/><br/>' +
        '</div></div>' +
        '<div class="bigBoxBottom"/>' +
        '</div>'
    }
    var qid_index = question_url_base.indexOf('QID');
    var question_url = question_url_base.substr(0, qid_index) + qid + question_url_base.substr(qid_index + 3);
    if (in_batch) {
      if (question_url_base.indexOf('?') != -1) {
        question_url += "&";
      } else {
        question_url += "?";
      }
      question_url += "in_batch=" + in_batch;
    }
    new Ajax.Updater('question', question_url, {
      evalScripts:true, 
      on404: function () {
        $('question').innerHTML = '<div id="questionContent" class="bigBox">' +
          '<div class="h1Container">' +
          '<h1 class="brownBackground">' +
          'loading the next question' +
          '</h1>' +
          '</div>' +
          '<div class="bigBoxBody"><div class="bigBoxContent">' +
          'question in batch doesn\'t exist anymore loading another..<br/>' +
          '<img alt="Loading" src="http://s.gr-assets.com/assets/loading-9404bf7c15db1e6aae360bad4c671139.gif" />' +
          '</div></div>' +
          '<div class="bigBoxBottom"/></div>';
        setTimeout("get_next_question()", 1000);
      },
      onComplete: function() {
        AdIFrameRotator.rotators.invoke('update');
      }
    });
  } else {
    if (answering) {
      $('question').innerHTML = '<div id="questionContent" class="bigBox">' +
        '<div class="h1Container">' +
        '<h1 class="brownBackground">' +
        'Loading the Next Question' +
        '</h1>' +
        '</div>' +
        '<div class="bigBoxBody"><div class="bigBoxContent">' +
        'You just hit the end of the batch. Loading more questions...<br/>' +
        '<img alt="Loading" src="http://s.gr-assets.com/assets/loading-9404bf7c15db1e6aae360bad4c671139.gif" />' +
        '</div></div>' +
        '<div class="bigBoxBottom"/>' +
        '</div>'
      setTimeout("get_next_question()", 200);
      return false;
    }
   new Ajax.Updater('question', "/trivia/load_batch", {evalScripts:true});
  }
  return false;
}
</script>


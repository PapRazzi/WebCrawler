      <script language="JavaScript" type="text/javascript" charset="UTF-8">
		/*$("#stop").click(function () {
			  $("#player").attr("data", "");
			});*/
		$('.stop').click(stopmusic);
		$('.start').click(playmusic);
		$('.playnext').click(gonext);
		$('.playprev').click(goprev);

		function stopmusic() {
						$("#musica").text('');
						$('li.stop').removeClass('active');
						$('li.stop').addClass('no_active');
						$('li.start').removeClass('no_active');
						$('li.start').addClass('active');
						$('ul.music_list li').removeClass('active');
						$('ul.music_list li img').attr("src", "iPad/img/music_to_play.png");
						}
		function playmusic() {
						var $curr =  $('#music li.play');
						var $toplay =  $('#music li.play').attr('title');
						$("#musica").html(function() {
						  return '<object id="player" height="0" width="0" data="' + $toplay + '"></object>';
						});
						$('li.stop').removeClass('no_active');
						$('li.stop').addClass('active');
						$('li.start').removeClass('active');
						$('li.start').addClass('no_active');
						$('ul.music_list li.playsong').eq($curr.index()).addClass('active')
						$('ul.music_list li.active img').attr("src", "iPad/img/music_playing.gif");
						}
		function gonext() {
						var $curr =  $('#music li.play');
						var $next =  (!$curr.next().length)? $('#music li').first() : $curr.next();
						$next.addClass('play');
						$curr.removeClass('play');
						var $toplay =  $('#music li.play').attr('title');
						$("#musica").html(function() {
						  return '<object id="player" height="0" width="0" data="' + $toplay + '"></object>';
						});
						$('ul.music_list li').removeClass('active')
						$('ul.music_list li img').attr("src", "iPad/img/music_to_play.png");
						$('ul.music_list li.playsong').eq($next.index()).addClass('active')
						$('ul.music_list li.active img').attr("src", "iPad/img/music_playing.gif");
						$('li.stop').removeClass('no_active');
						$('li.stop').addClass('active');
						$('li.start').removeClass('active');
						$('li.start').addClass('no_active');
						}
		function goprev() {
						var $curr =  $('#music li.play');
						var $next =  (!$curr.prev().length)? $('#music li').last() : $curr.prev();
						$next.addClass('play');
						$curr.removeClass('play');
						var $toplay =  $('#music li.play').attr('title');
						$("#musica").html(function() {
						  return '<object id="player" height="0" width="0" data="' + $toplay + '"></object>';
						});
						$('ul.music_list li').removeClass('active')
						$('ul.music_list li img').attr("src", "iPad/img/music_to_play.png");
						$('ul.music_list li.playsong').eq($next.index()).addClass('active')
						$('ul.music_list li.active img').attr("src", "iPad/img/music_playing.gif");
						$('li.stop').removeClass('no_active');
						$('li.stop').addClass('active');
						$('li.start').removeClass('active');
						$('li.start').addClass('no_active');
						}
			
		</script>
      <span id="musica">
        <object id="player" height="0" width="0" data="audio/1_LUIGI_CINQUE-SECRET.mp3"></object>
      </span>

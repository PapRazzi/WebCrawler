  <li realy_st='true' poster='657461' st_id="2486278new" class="single_stocktalk origin" subject_id="2486278" id="li_2486278new" style='width:312px !important'>
    <a href='/author/kinetic-investments' class="user_image author"><img src='http://static3.cdn-seekingalpha.com/images/users_profile/000/657/461/small_pic.png?1350495823'/></a>

<script type="text/javascript">
    //<![CDATA[
	$(document).ready(function() {
		var hoverOverZillow = false
		$(top).focus();
		$(top).blur(processIframeClick);

		$('#zillow #zillow-container').hover(function(){
			hoverOverZillow = true;
		}, function(){
			hoverOverZillow = false
			$('top').focus();
		});

		function processIframeClick(){
			//console.log("top blurrred.");
			if(hoverOverZillow){
				BLOOMBERG.tracker.EVENTTRACK.record('Module Link Tracking', 'Zillow', this.href);
			}
		}

		var cookie_val = BLOOMBERG.util.Cookie.get("zillow_test");
		if(cookie_val === "hide"){
			$('#zillow').hide();
		}
	});
    //]]>
</script>



    <script>
    Do(function(){
    var lnkHelp = $('#fm-footer .lnks a');
    lnkHelp.click(function(e){
        var el = $(e.currentTarget),
        p = el.parent(),
        panel = $('#fm-lnks-panel');

      p.toggleClass('panel-active');
      panel.toggle();

      if ($("#fm-section2").length) {
          // fix ie z-index bug.
          if ($.browser.msie &&
            p.hasClass('panel-active')) {
              $('#fm-section2').css('zIndex', -1);
          } else {
              $('#fm-section2').css('zIndex', 30);
          }
      }
      return false;
    });
    $('html').click(function(){
      lnkHelp.parent().removeClass('panel-active');
      $('#fm-lnks-panel').hide();
      // fix ie z-index bug.
      if ($.browser.msie) {
        $('#fm-section2').css('zIndex', 30);
      }
    });

    });
    </script>




		<script type="text/javascript">
		/*tab切换*/
	YUE.on('thtab', 'mouseover',
	function(e) {
		var tar = YUE.getTarget(e);
		if (tar && tar.id) {
			switch (tar.id) {
			case 'tabBuyer':
				buyerTab(tar.id);
			case 'tabSupplier':
				userTab(tar.id);
				break;

			}
		}
	});
	function userTab(id) {
		if (!YUD.hasClass(id, 'current')) {
			var config = {
				tabBuyer: {
					id: 'tabSupplier',
					switchDisplay: 'none'
				},
				tabSupplier: {
					id: 'tabBuyer',
					switchDisplay: 'block'
				},
				tabBuyerCls: '',
					tabSupplierCls: 'lanEN'

			};
			var o = config[id];
			YUD.removeClass(o.id, 'current');
			YUD.addClass(id, 'current');

			get(o.id + 'Content' + config[o.id + "Cls"]).style.display = "none";
			get(id + 'Content' + config[id + "Cls"]).style.display = "block";

		}

	}
	function buyerTab(id) {

	}

	</script>


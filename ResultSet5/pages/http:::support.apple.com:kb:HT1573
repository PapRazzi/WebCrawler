    $('articlecontent').select('img').each(function(element) {
        if(element.width > 640 ){
                  var imageDiv = document.createElement('div');
                  imageDiv.setStyle({ 'display':'block', 'width':'660px', 'overflow':'auto', 'border':'1px solid #dedede','margin-right': '15px'});
                  element.parentNode.replaceChild(imageDiv, element);
                  imageDiv.appendChild(element);

              }
      });

    $('articlecontent').select('table').each(function(element) {

        if(element.width > 660 || element.offsetWidth>660){
                 var tableDiv = document.createElement('div');
                 tableDiv.setAttribute("class","kbtablecontainer");
                 element.setAttribute("style","margin-bottom:0px !important");      
                 element.parentNode.replaceChild(tableDiv, element);
                 tableDiv.appendChild(element);
                 }
      });
    </script>
	

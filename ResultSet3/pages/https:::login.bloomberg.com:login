    $(document).ready(function() {
      $('#new_user_session').submit(function(e) {
        var form = this;
        if (!e.isDefaultPrevented()) {
          e.preventDefault();
          BLOOMBERG.user.ga.events.record("Sign In", "Submitted", "bloomberg");
          setTimeout(function() {
            form.submit();
          }, 200);
        }
      });
    });
  </script>


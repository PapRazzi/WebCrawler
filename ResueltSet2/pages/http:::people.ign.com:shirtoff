<script type="text/javascript">
    var increment = parseInt(20) + 1;

    var startIndex = parseInt(0) + 1;
    /*if(increment>displayTotal) {
        startIndex = displayTotal;
    }*/
    var destinationPath;
    var currentType = "all";

    $("#moreUpdates").click(function () {
        _gaq.push(['_trackPageview', '/home']);
        $("#moreUpdates").css("display", "none");
        $(".leftButtonEdge").css("display", "none");
        $(".rightButtonEdge").css("display", "none");
        //maybe keep track of total results and decide to display more based on that
        //have to keep track of type as well
        /*    $.getJSON("http://"+document.domain+"/proxy?dest="+endpointBase+"activities/"+pageOwnerId+"/@self%3FsortOrder%3Dascending%26startIndex%3D"+startIndex+"%26count%3D11",function(json) {
        $.each(json.entry, function(i,item){
            if ( i == increment - 1 ) {
                $("#moreUpdates").css("display","block");
                $(".leftButtonEdge").css("display","block");
                $(".rightButtonEdge").css("display","block");
                return false;
            }
            ign.social.activityItem.create(startIndex,item.actorLinks[0].uri, item.actorLinks[1].uri, item.actorTitle,item.activityObjects[0].objectTitle,item.created);
            startIndex++;
        });
    }); */
        //going to call getactivities instead
        if (location.pathname.toString().indexOf("/home") == 0) {
            destinationPath = location.pathname;
        } else {
            destinationPath = location.pathname + "/profile";
        }
        ign.social.generic.error(location.pathname);
        $.get("http://" + location.host + destinationPath + "/getactivities?start=" + startIndex + "&type=" + currentType, function (data) {
            $("#activityList").append(data);
            startIndex = startIndex + increment;
            if (isMore) {
                $("#moreUpdates").css("display", "block");
                $(".leftButtonEdge").css("display", "block");
                $(".rightButtonEdge").css("display", "block");
            }
        });
    });
    $("document").ready(function () {
        $(".showall").click(function () {
            currentType = "all";
            _gaq.push(['_trackPageview', '/home']);
            $(".showgames").removeClass("activitySortSelected");
            $(".showpeople").removeClass("activitySortSelected");
            $(".showblog").removeClass("activitySortSelected");
            $(".showall").addClass("activitySortSelected");
            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=all", function (data) {
                $("#activityList").html(data);
            });
        });
        $(".showgames").click(function () {
            currentType = "MEDIA_ITEM";
            _gaq.push(['_trackPageview', '/home']);
            $(".showall").removeClass("activitySortSelected");
            $(".showpeople").removeClass("activitySortSelected");
            $(".showblog").removeClass("activitySortSelected");
            $(".showgames").addClass("activitySortSelected");
            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=MEDIA_ITEM", function (data) {
                $("#activityList").html(data);
                if (data.indexOf("yougotnofriendssucka") > -1) {
                    $("#moreUpdates").css("display", "none");
                    $(".leftButtonEdge").css("display", "none");
                    $(".rightButtonEdge").css("display", "none");
                }
            });
        });
        $(".showpeople").click(function () {
            currentType = "PERSON";
            _gaq.push(['_trackPageview', '/home']);
            $(".showgames").removeClass("activitySortSelected");
            $(".showall").removeClass("activitySortSelected");
            $(".showblog").removeClass("activitySortSelected");
            $(".showpeople").addClass("activitySortSelected");
            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=PERSON", function (data) {
                $("#activityList").html(data);
                if (data.indexOf("yougotnofriendssucka") > -1) {
                    $("#moreUpdates").css("display", "none");
                    $(".leftButtonEdge").css("display", "none");
                    $(".rightButtonEdge").css("display", "none");
                }
            });
            //$.getJSON("http://"+document.domain+"/proxy?dest="+endpointBase+"activities/"+pageOwnerId+"/@self%3FfilterBy%3DactorType%26filterValue%3DPERSON%26sortOrder%3Dascending%26startIndex%3D0%26count%3D11",function(json) {
        });
        $(".showblog").click(function () {
            currentType = "BLOG_ENTRY";
            _gaq.push(['_trackPageview', '/home']);
            $(".showgames").removeClass("activitySortSelected");
            $(".showall").removeClass("activitySortSelected");
            $(".showpeople").removeClass("activitySortSelected");
            $(".showblog").addClass("activitySortSelected");

            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=BLOG_ENTRY", function (data) {
                $("#activityList").html(data);
                if (data.indexOf("yougotnofriendssucka") > -1) {
                    $("#moreUpdates").css("display", "none");
                    $(".leftButtonEdge").css("display", "none");
                    $(".rightButtonEdge").css("display", "none");
                }

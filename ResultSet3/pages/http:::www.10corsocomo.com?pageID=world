			var back_cookie = readCookie('set_bk_cookie');
			if (back_cookie) {
				$.backstretch(back_cookie, {fade: 2000});
			}
			else {
				$.backstretch("wallpapers/wallpaper_1.jpg", {fade: 2000});
			}
			$("#wall1").click(function (e) {
			e.preventDefault();
			  $.backstretch("wallpapers/wallpaper_1.jpg", {fade: 2000});
			   createCookie('set_bk_cookie', "wallpapers/wallpaper_1.jpg",0);
			});
			$("#wall2").click(function (e) {

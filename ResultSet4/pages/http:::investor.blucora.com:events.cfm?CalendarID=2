<script type="text/javascript">
	jQuery(document).ready(function($)
	{
		// Hide all the tooltips
		$("#irToolbar li").each(function() {
			$("a strong", this).css("opacity", "0");
		});
		
		$("#irToolbar li").hover(function() { // Mouse over
			$(this)
				.stop().fadeTo(300, 1)
				.siblings().stop().fadeTo(300, 0.3)
	
			$("a strong", this)
				.stop()
				.animate({
					opacity: 1,
					top: "-30px"
				}, 300);
			
		}, function() { // Mouse out
			$(this)
				.stop().fadeTo(300, 1)
				.siblings().stop().fadeTo(300, 1)
				
			$("a strong", this)
				.stop()
				.animate({
					opacity: 0,
					top: "-1px"
				}, 300);
		});
	});
</script>
	
	
	
	

	
	

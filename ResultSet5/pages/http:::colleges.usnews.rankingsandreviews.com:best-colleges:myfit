        $(document).ready(function() {
            $(".details").hide();
            $(".expandedProfiles").show();
            $(".menuWrap ul li").click(function() {
                $(".menuWrap ul li").removeClass("active");
                $(this).addClass("active");
            });
            $("#expandedProfiles").click(function() {
                $(".details").hide();
                $(".expandedProfiles").show();
            });
            $("#satScores").click(function() {
                $(".details").hide();
                $(".satScores").show();
            });
            $("#financialAid").click(function() {
                $(".details").hide();
                $(".financialAid").show();
            });
            $("#campusLife").click(function() {
                $(".details").hide();
                $(".campusLife").show();
            });
            $("#myFit").click(function() {
                $(".details").hide();
                $(".myFit").show();
            });
            $("#advancedSearch").click(function() {
                $(".details").hide();
                $(".advancedSearch").show();
            });
        });
    </script>

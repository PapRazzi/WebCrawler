    <script type="text/javascript" language="javascript">
    $.fn.equalHeights = function(px) {
        $(this).each(function(){
            var currentTallest = 0;
            $(this).children().each(function(i){
                if ($(this).height() > currentTallest)
                    currentTallest = $(this).height();
            });
            // for ie6, set height since min-height isn't supported
            if ($.browser.msie && $.browser.version == 6.0)
                $(this).children().css({'height': currentTallest});
            $(this).children().css({'min-height': currentTallest});
        });
        return this;
    };
    
    $("document").ready(function() {
        // tiptip for all badges
        $("span.as_badge").tipTip({edgeOffset: 10});
        
        // tipTip for the chat with sales link
        $("li.chat_with_sales a").tipTip({maxWidth: "250px", edgeOffset: -20}).attr('title', '');
        $("li.chat_with_sales a img").attr('title', '');
        
        // tipTip for image popups
        $('a.as_img_tip').each(function() { $(this).tipTip({content: "<img src='" + this.href + "' width='400px'>", maxWidth: "420px", edgeOffset: -50 }); })
        $('img.as_img_tip').each(function() { $(this).tipTip({content: "<img src='" + this.src + "' width='400px'>", maxWidth: "420px", edgeOffset: -50 }); })
        
        // expandable images
        $('.as_expandable').wrap('<div class="as_expandable_wrapper" title="Click to expand/shrink this image" />');
        $('.as_expandable_wrapper').tipTip({edgeOffset: 10, defaultPosition: "top"});
        $('.as_expandable').click( function() {
          $(this).toggleClass("as_fullwidth").attr("style", "");
        });
        
        // general tiptips
        $('.tiptip').tipTip({defaultPosition: "top"});
        
        
        cookie_json = $.cookie("SSO_DATA");
        if (cookie_json) {
          cookie = eval('(' + cookie_json + ')');
          $("div#as_user_block div.slideout_container div.as_user_name").html(cookie['fullname']);
          $("div#as_user_block div.slideout_container div.as_user_email").html(cookie['email']);
          $("div#as_user_block div.slideout_container div.as_signout").html('<a class="awesome" href="https://account.activestate.com/signout/?next=' + escape(location.href) + '">Sign Out</a>');
          $("div#as_account_nav ul").prepend('<li class="leaf first activestate-user"><a href="https://account.activestate.com" id="profile_nav" class="user_toggle">' + cookie['email'] + '</a></li>');	    
        } 
        else {
          $("div#as_account_nav ul").prepend('<li class="leaf sign_in"><a href="https://account.activestate.com/signin/?next=' + escape(location.href) + '" id="signin_nav">Sign In</a></li>');
        }
        
        function top_nav_slider_toggle(slider_element_selector) {
			var slider_element = $(slider_element_selector);
			// if the slider element is visible, hide it
			if (slider_element.is(":visible")) {
				$('#slideout_close').fadeToggle('normal', function() {
						slider_element.slideToggle();
					});
			}
			// otherwise, hide any visible sliders and show the requested one
			else {
				if ($('.top_nav_slideout').is(":visible")) {
					$('#slideout_close').fadeOut('normal', function() {
						$('.top_nav_slideout:visible').slideToggle('normal', function() {
							slider_element.slideToggle();
							$('#slideout_close').fadeToggle();
						});
					});
				}
				else {
					slider_element.slideToggle('normal', function() {
						$('#slideout_close').fadeToggle();
					});
				}
			}
		}
		
		$('#slideout_close').click(function(event) {
			event.preventDefault();
			$(this).fadeToggle('normal', function() {
				$('.top_nav_slideout:visible').slideToggle();
			});
		});
		
		$('.search_toggle').click(function(event) {
			event.preventDefault();
			if (!$('.page-search').length) { // don't want to toggle the search box on search result pages
				top_nav_slider_toggle('#as_search_block');
				$('#as_search_block #edit-search-theme-form-1').focus();
			}
		})
		
		$('.contact_toggle').click(function(event) {
			event.preventDefault();
			if (!$('.page-company-contact-us').length) { // don't want to toggle the contact us box on the contact us page
				top_nav_slider_toggle('#as_contact_block');
			}
		})
		
		$('.user_toggle').click(function(event) {
			event.preventDefault();
			top_nav_slider_toggle('#as_user_block');
		})
        
        var recent_tweets = $("#recent_tweets").length; // easy test for element existence
        if (recent_tweets) {
          $("#recent_tweets").tweet({
              username: "activestate",
              join_text: "auto",
              avatar_size: 0,
              count: 100,
              auto_join_text_default: "",
              auto_join_text_ed: "",
              auto_join_text_ing: "",
              auto_join_text_reply: "",
              auto_join_text_url: "",
              loading_text: "loading tweets..."
          });
        }
        
        $('#main').equalHeights();
        
        // prettify any code blocks
        $(function() {
          if (typeof prettyPrint == 'function') {
            prettyPrint();
          }
        });
        
        // set the marketo PPC tracking cookie
        var as_ppc = $.url.param("gclid");
        
        // we only want the search keyword if this is coming in via a PPC campaign
        if (as_ppc) {
          
          var refer = document.referrer;
          var searchString;
          var searchEngine;
          var excludedReferrers = [ "activestate.com" ];
        
          if ( (refer != undefined) && (refer != "") ) {
          
            // get the domain of the referring website -- http://[[this-thing.com]]/
            var referrerDomain =
                refer.substr(refer.indexOf("\/\/") + 2,
                             refer.indexOf("\/",8) - refer.indexOf("\/\/") - 2).toLowerCase();
        
            var excludedDomainFound = false;
            var i = 0;
        
            // search the excluded domain list to see if the referrer domain is on it
            while ( (i < excludedReferrers.length) && !excludedDomainFound) {
              var thisExcludedDomain = excludedReferrers[i].toLowerCase();
              excludedDomainFound = (referrerDomain.indexOf(thisExcludedDomain) != -1);
              i++;
            }
        
              // only if the referrer isn't in our excluded domain list...
            if( !excludedDomainFound ) {
        
              var searchEngines = [
                   { name: "Yahoo", url: /\.yahoo\.co/i, query: "p" },
                   { name: "Google", url: /\.google\./i, query: "q" },
                   { name: "Microsoft Live", url: /\.live\.com/i, query: "q" },
                   { name: "MSN Search", url: /search\.msn\./i, query: "q" },
                   { name: "AOL", url: /\.aol\./i, query: "query" },
                   { name: "Bing", url: /\.bing\.com/i, query: "q" },
                   { name: "Ask", url: /\.ask\.com/i, query: "q" }
                 ];
        
              // find the referring search engine (if any)
              i = 0;
              while (i < searchEngines.length) {
                if (refer.match(searchEngines[i].url)) {
                  searchEngine = searchEngines[i].name;
                  searchString = $.url.setUrl(refer).param(searchEngines[i].query);
                  break;
                }
                i++;
              }
        
              // If no search engine is found, this person probably used a less
              // popular one.  Use the referring doman, then guess the query parameter
              if (i == searchEngines.length) {
        
                searchEngine = referrerDomain;
      
                var queries = ["q","p","query"];
                var i = 0;
                while ((i < queries.length) && (searchString == undefined)) {
                  searchString = $.url.setUrl(refer).param(queries[i]);
                  i++;
                }
      
                // no search strings found -- use this text instead.
                if (searchString == undefined) {
                  searchString = "None";
                }
              }
            }
            
            $.cookie('mktoPPCKeyword', searchString, {expires: 730, path: '\/', domain: 'activestate.com'} );
            
          }
        }
    });
	  
</script>
    
<!-- attach upsell function -->
<script type="text/javascript">
// <![CDATA[
function attach_upsell(nodeid) {
$.getJSON("https://store.activestate.com/json/catalog?callback=?", function(data) { 
    
    $.each(data, function(i, item) {
    
    if (item.nid == nodeid ) {
    
    var is_upsell  = 0;
    var upsell_url = "https://store.activestate.com/as/upsell/" + nodeid;
    
    // see if there's an upgrade for the product & pop up a shadowbox if there is
    is_upsell = item.has_upsell;
      
    if (is_upsell) {
      var cartlink_id = "cartlink_" + nodeid;
      $("#" + cartlink_id).click(function() {
        Shadowbox.open({
          content: upsell_url,
          player: "iframe",
          width: "600px",
          height: "550px"
        });
        return false;
      });
    }
    
    }
})
})
}
// ]]>
</script>



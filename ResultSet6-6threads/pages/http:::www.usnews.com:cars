	<script type="text/javascript">
		$(document).ready(function(){
			if (!$('#used-zip').val()){
				$('#used_submit').attr('disabled','disabled');
			}
		    $('#used-zip').keyup( function(evt){
		    	enableBtn();
	        });		
	
		    $('#used-zip').blur( function(evt){
	        	enableBtn();				
	        });		         				
		});
						
		function enableBtn(){
			var passed = true;
			var zipRE=/^\d{5}$/;
			if($('#used-zip').val().search(zipRE)==-1){
				passed = false;
			}
			if(passed){
				$('#used_submit').removeAttr('disabled');
			} else {
				$('#used_submit').attr('disabled','disabled');
			}
		}
	</script>

			
		
		
			
			

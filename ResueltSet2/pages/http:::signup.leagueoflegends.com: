<script type="text/javascript">

var trailerPlayer = null;

$(function () {
	$('#trailer_dialog').appendTo('body');
	
	$('#trailer_player .close_button, #trailer_player_background').click(function () {
		$('#flowplayer').html('');
		$('#trailer_dialog').hide();
		return false;
	});

	$('#trailer_button').click(function () {
		$('#trailer_dialog').show();

		trailerPlayer = flowplayer("flowplayer", "/flash/flowplayer.commercial-3.2.1.swf", {
			key: '#$22fc9fde88b7ad2c184',
			logo: null,
			onBegin: function(clip) {
				$('#trailer_player ul').find('a.active').removeClass('active');
				$('#trailer_player ul a:eq(' + clip.index + ')').addClass('active');
			},
			playlist: ["https:\/\/riot-web-static.s3.amazonaws.com\/video\/CG_trailer.flv"]		});

		return false;
	});

	var trailerLinks = $('#trailer_player ul a');
	
	for(var i = 0; i < trailerLinks.length; i++) {
		var link = trailerLinks[i];

		with ({index: i}) {
			$(link).click(function () {
				var href = $(this).attr('href');

				if (href && href != '#') {
					if (!$(this).hasClass('active')) {
						trailerPlayer.play(index);
					}
				}

				return false;
			});
		}
	}
});


  <script>
var ggsChannel = (function() {
	
	var bannerCon = get("banner-con");
	var banners = bannerCon.getElementsByTagName("li");
	var cursor = 0;
	var next = YUD.getElementsByClassName("J-next")[0];
	var prev = YUD.getElementsByClassName("J-prev")[0];
	var circles = get("scroll-nav").getElementsByTagName("a");
	var topnav = get("top-nav");
	var bannerScroll ;
		
	//banner轮换
	var bigBanner = function() {
		YUE.on(next,'click',function() {
			clearInterval(bannerScroll);
			bannerSwitch(1);
			setupScroll();
		});	
		YUE.on(prev,'click',function() {
			clearInterval(bannerScroll);
			bannerSwitch(-1);
			setupScroll();
		});		
		YUE.on(circles,"mouseover",function() {
			clearInterval(bannerScroll);
			var d = parseInt(this.id.substr(11))-cursor;
			bannerSwitch(d);
		});
		YUE.on(circles,"mouseout",function() {
			setupScroll();
		});
		setupScroll();
	};
	
	var setupScroll = function() {
		bannerScroll = setInterval(function() {
			if(cursor == banners.length -1) {
				cursor = -1;	
			}	
			bannerSwitch(1);
		},5000);	
	}
	
	//banner轮换轮换效果
	var bannerSwitch = function(d) {
		cursor = cursor + d;	
		for(var i=0; i<banners.length;i++) {
			if(i != cursor) {
				YUD.addClass(banners[i],"hidden");	
				YUD.removeClass(get('scroll-nav_'+i),"current");
			} else {
				YUD.removeClass(banners[i],"hidden");
				YUD.addClass(get('scroll-nav_'+i),"current");
			}
		}
		next.style.display = cursor >= banners.length -1 ? "none" : "block";	
		prev.style.display = cursor <= 0 ? "none" : "block";
	};
	
	//圆角导航切换
	var initialTop = function() {
		if(window.screen.height < 620) {
			get("top-nav").style.top = window.screen.height * 0.77 + "px";
		}
	};
	
	var ieMode=document.documentMode; 
	var isIE=!!window.ActiveXObject; 
	var isIE6=isIE&&!window.XMLHttpRequest; 
	var isIE7=isIE&&!isIE6&&!ieMode||ieMode==7; 
	
	//顶部导航
	var topNav = function() {

			
			var scrollTop = document.body.scrollTop || document.documentElement.scrollTop ;
			var heightVal = window.innerHeight < 683 ? parseInt(window.innerHeight) - 63  : 683;
			
			if(isIE6 || isIE7) {
				
				if(scrollTop > 620 ) {
					topnav.style.top = scrollTop+"px";	
				} else {
					topnav.style.top = heightVal + "px";	
				} 
				
			} else {				
				
				if(scrollTop > 620) {
					topnav.style.position = "fixed";	
					topnav.style.top = 0;	
				} else {
					if( scrollTop + heightVal > 620){
						topnav.style.position = "absolute";
						topnav.style.top = 620 + "px";	
					} else {
						topnav.style.position = "fixed";	
						topnav.style.top = heightVal + "px";	
					}
				} 
			
			}
							
	} ;

	window.onscroll = topNav ;
	window.onresize = topNav ;


        //滚动效果
	var vScroll = function(start,end,top,cell) {



		top = top == undefined ? document.body.scrollTop || document.documentElement.scrollTop : top ;
		cell = cell == undefined ? parseInt((end-top-start)/100) : cell;

		if(Math.abs(start+top - end) >= Math.abs(cell)) {
			if(Math.abs(cell) < 0.1) {
				return ;
			}
			window.scrollTo(0,top+start+cell);
			setTimeout(function() {
				if(top+start+cell != end) {
					vScroll(start+cell,end,top,cell);
				}
			},1);
		}
	}
	
	//合作者信息切换
	var partnerSwitch = function() {
		var partnerTags = YUD.getElementsByClassName("partner-region")[0].getElementsByTagName("li");
		var partnerInfos = YUD.getElementsByClassName("partner-info");
		var country = location.search.slice(9);
		switch (country)
		{
			case 'korea' :
				var coun = "kr";
				break;
			case 'brazil' :
				var coun = "bz";
				break;
			case 'malaysia' :
				var coun = "my";
				break;
			case 'peru' :
				var coun = "pr";
				break;
			case 'turkey' :
				var coun = "tk";
				break;
			case 'vietnam' :
				var coun = "vt";
				break;
			default: break;
		
		}
		if(coun){
			YUD.addClass(partnerInfos,"hidden");
			YUD.removeClass(YUD.getElementsByClassName(coun+"-box")[0],"hidden");
			YUD.removeClass(partnerTags,"current");
			YUD.addClass(get(coun),"current");
			var end = YUD.getY('partners');
			vScroll(0,end);
		}
		YUE.on(partnerTags,"click",function() {
			YUD.addClass(partnerInfos,"hidden");
			YUD.removeClass(YUD.getElementsByClassName(this.id+"-box")[0],"hidden");
			YUD.removeClass(partnerTags,"current");
			YUD.addClass(this,"current");
		}) ;
	} ;

	return {
		init : function() {
			bigBanner();
			topNav();
			partnerSwitch();
			initialTop();
            window.vScroll = vScroll ;
		}	
	}	
	
})();

ggsChannel.init();

</script>
</html><!--  IntlCmsPageId=200017039  -->
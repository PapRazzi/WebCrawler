var cacheList = new Array([]);
function getGroupData(groupCategoryID,element){
if ($.inArray(groupCategoryID,cacheList) == -1) {
$.ajax({
type: "POST",
url: currentBaseURL +'/index.cfm?action=act_categoryGroups_details_ajax&categoryGroupID=' + groupCategoryID,
dataType: "html",
success: function(response){
$(element).find(".sub-catList").html(response);
$(element).find(".sub-catList").removeClass('sub-catList_loader');
cacheList.push(groupCategoryID);
},
error: function(xhr, ajaxOptions, thrownError) {
}
})
}
}

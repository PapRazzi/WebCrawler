<script type="text/javascript">
		$(function() {
			
			$('.flexslider').flexslider({
				animation: "slide",
        touch: false,
				slideDirection: "horizontal",
				animationDuration: 400,
				directionNav: false,
				pauseOnHover: true,
	      useCSS: false
			}).data('flexslider').controlNav.each(
				function()
				{
					// for each controlNav item we update the aria-label and text to match
					// the the first h2 element within its corresponding tabpanel 
					var label = $('#'+$(this).attr('aria-controls')).find('h2:first').text();
					if (label.length) $(this).text(label).attr({'title':label,'aria-label':label});
				});
        $('.flexslider').find('a').bind('focus',function(){
        $('.flexslider').scrollLeft(0).scrollTop(0);
			});
			
			// on resize, correct heights of rotator slides so that they are all the same
			$(this).resize(function(e) {
				var $slides = $(".slides > li"),
						$slide,
						commonheight = 0;
				$slides.height('auto');
				$slides.each(function(){
					$slide=$(this);
					if($slide.height()>commonheight)
						commonheight = $slide.height();
				}).height(commonheight);
			}).resize();
		});
	</script> 
<!--
HOME PAGE RSS FEED PROCESSING
Derived from Google JSAPI Feeds example. Licensed under Apache license 2.0 (http://www.apache.org/licenses/LICENSE-2.0.html).
-->

<script src="http://www.google.com/jsapi?key=AIzaSyA5m1Nc8ws2BbmPRwKu5gFradvD_hgq6G0" type="text/javascript"></script>
<script type="text/javascript">  
  google.load("feeds", "1");
  
  // Callback function, for when a feed is loaded.
  function feedLoaded(result) {
    if (!result.error) {
      // Grab the container we will put the results into
      var container = document.getElementById("rssoutput");
      container.innerHTML = '';
      
      // Loop through the feeds, putting each entry in its own object
      for (var i = 0; i < result.feed.entries.length; i++) {
        var entry = result.feed.entries[i];
        
        // Create all tags for container inner HTML
        var row = document.createElement("li");
        var link = document.createElement("a");
        var dateSpan = document.createElement("span");
        var titleSpan = document.createElement("span");
        
        // Clean up the entry link in the RSS feed
        var entryLink = entry.link.replace("ec2-174-129-28-157.compute-1.amazonaws.com","blog.brackets.io");
        entryLink.replace("174.129.28.157","blog.brackets.io");
		
		// Clean up the encoded characters in the RSS feed
        var entryTitle = entry.title.replace("&#8230;","...");
		
        // Assign attributes for all tags
        if (entryLink.indexOf("typekit.com") > -1) row.setAttribute("class","typekit");
        else if (entryLink.indexOf("phonegap.com") > -1) row.setAttribute("class","phonegap");
        else if (entryLink.indexOf("brackets.io") > -1) row.setAttribute("class","brackets");
        else row.setAttribute("class","adobe");
             
        link.setAttribute("href",entryLink);
        dateSpan.setAttribute("class","date");
        titleSpan.setAttribute("class","title");
        
        // Append the tag contents from inside out
        var entryDate= new Date(entry.publishedDate);
        dateSpan.appendChild(document.createTextNode(formatDate(entryDate)));
        titleSpan.appendChild(document.createTextNode(entryTitle));   
        link.appendChild(dateSpan);
        link.appendChild(document.createTextNode(" "));
        link.appendChild(titleSpan);
        row.appendChild(link);
        
        container.appendChild(row);
      }
    }
  }
  
  function OnLoad() {
    // Create a feed instance
    var feed = new google.feeds.Feed("http://feeds.feedburner.com/adobeandhtml");
    feed.setNumEntries(10);
    
    // Calling load sends the request off.  It requires a callback function.
    feed.load(feedLoaded);
  }
  
  // For format Sep 24
  function formatDate(obj) {
    var month=new Array();
    month[0]="Jan";
    month[1]="Feb";
    month[2]="Mar";
    month[3]="Apr";
    month[4]="May";
    month[5]="Jun";
    month[6]="Jul";
    month[7]="Aug";
    month[8]="Sep";
    month[9]="Oct";
    month[10]="Nov";
    month[11]="Dec";
    var m = month[obj.getMonth()];   
    return m + " " + obj.getDate();
  }
  
  google.setOnLoadCallback(OnLoad);
</script>

<script type="text/javascript">
var ONVIDEO = 1;
function updateVideo(URL, key)
{


	// object = '<object width="575" height="385"><param name="movie" value="'+URL+'&autoplay=0&rel=0&fs=1"></param><param name="allowFullScreen" value="true"></param><embed src="'+URL+'&autoplay=0&rel=0&fs=1" type="application/x-shockwave-flash" width="575" height="385" allowfullscreen="true"></embed></object>'
	var so = new SWFObject("/php/modules/player/player.swf", "sotester", "575", "431", "9", "#FF6600");
	so.addVariable("file", "http://www.youtube.com/v/"+URL);
	so.addParam("allowfullscreen", "true");
	so.addParam("allowscriptaccess", "always");
	so.addParam("autoplay", "1");
	// so.addVariable("type", "video");
	so.addVariable('autostart','true');
	so.addParam('bgcolor', '#000000');
	so.addVariable("aboutlink", "http://www.playdota.com");
	so.addVariable("abouttext", "PlayDota Videos");
	so.addVariable("skin", "/php/modules/player/skin.swf");
	so.addVariable("image", "http://img.youtube.com/vi/URL/0.jpg");
	so.write("video");

	updateModerator(key);

	document.getElementById('permlink').innerHTML = "(<a href='http://www.playdota.com/videos/"+key+"'>Permanent link</a>)";
}



var dotaNav = '';
window.onload = function() {

	dotaNav = new Navigation('video', 12, 5342);
updateVideo('Z_NhOy2KE-s', 2911);
};

</script>




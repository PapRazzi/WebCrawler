<script>
  function switchPortfolioTabs(curr_id, curr_filter){
    var tabs = ['headlines_all','headlines_pro_articles','headlines_in_focus_articles','headlines_articles','headlines_transcripts','headlines_news'];
    var obj = $(curr_id);
    $('portfolio_filters').childElements().each(function(filter){
      if (filter == curr_filter) {
        filter.className = "filter_by_issue choose_this";
      }
      else if (filter.className !="gray") {
        filter.className = "filter_by_issue";
      }
    })

    tabs.each(function(tab_id){
      if ($(tab_id).id == obj.id && obj.className == "section") {
        $(tab_id).className = "section selected";
      } else if ($(tab_id).id != obj.id) {
        $(tab_id).className = "section";
      }
    })
    pageTracker._trackEvent('Portfolio Tracking', 'Click Content', curr_id);
    var my_location = "";
    if (location.pathname.match(/account\/portfolio/)){
      my_location = "portfolio";
    } else if (location.pathname.match(/symbol\//)){
      my_location = "symbol";
    }
    my_location += (my_location != "" ? ", " : "") + "headlines";
    Mone.event('tab_clicks', my_location, curr_id, {data: {pro_status: (typeof(user_cookie) != "undefined" ? user_cookie.user_pro_status : 0)}});
    if ($('go_nopro_event') && curr_id == "headlines_pro_articles"){
      Mone.event('tab_clicks', my_location, 'no_pro_articles');
    }
  }

  function portfolioPagination(type, page, direction){
    if ($(type + '_show_more')) {
      $(type + '_show_more').remove();
      $('show-more-preloader').style.display = "";
    }
    if (page == 0) return;
    new Ajax.Updater({success: 'headlines_'+type}, '/account/ajax_headlines_content', {
      parameters: { type: type, page: page, slugs: 'aig', is_symbol_page : true},
      insertion: 'bottom',
      onComplete: function(){$('show-more-preloader').style.display = "none";}
    });
    if (window.pageTracker) pageTracker._trackEvent("Portfolio Tracking", 'Pagination', type+" - "+direction);
  }

    $('headline_all_tab').onclick();
</script>

<script type="text/javascript"> 
function Mo(id,obj) {
	document.getElementById(id).bgColor = 'yellow';
	if( obj) 
		obj.bgColor = 'orange';
}
function Mu(id,obj) {
	document.getElementById(id).bgColor = '#ffffff';
	if( obj )
		obj.bgColor = '#ffffff';
}
 
function TMo(obj) {
	obj.bgColor = 'yellow';
	id = obj.id.replace('cell','tool');
	document.getElementById(id).bgColor = 'orange';
}
 
function TMu(obj) {
	obj.bgColor = '#ffffff';
	id = obj.id.replace('cell','tool');	
	document.getElementById(id).bgColor = '#ffffff';
	
}
</script>


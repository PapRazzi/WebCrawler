<script type="text/javascript">
var hide=false;
var auto=false;

change_main_width();
var fb_tabs=jQuery( "#right_bar_tabs" ).tabs({
	cookie: {
		expires: 90,
		path: '/'
	},
	select: function(event, ui) {
		if(ui.index==0){
			if(jQuery("#right_bar_stock_list li").length==0){
				jQuery("#right_bar_stock_list").load("/modules/rightbar/ajax_stock_list.php?portid="+jQuery('#select_portfolio').val());
			}
		}
		if(ui.index==1){
			if(jQuery("#right_bar_guru_list li").length==0){
				jQuery("#right_bar_guru_list").load("/modules/rightbar/ajax_guru_list.php?type="+jQuery('#select_gurutype').val());
			}
		}
	}
});

if(jQuery('#right_bar_stock_list li.current').length>0){
	var t=jQuery('#right_bar_stock_list li.current').position().top-220;
	jQuery('#right_bar_stocks').scrollTop(t);
}
if(jQuery('#right_bar_guru_list li.current').length>0){
	var t=jQuery('#right_bar_guru_list li.current').last().position().top-220;
	jQuery('#right_bar_gurus').scrollTop(t);
}


var ieVersion = getInternetExplorerVersion();
if (ieVersion == 7 || ieVersion == 8) {
    document.body.onresize = function () {
    	change_main_width();
    };
}
jQuery(window).resize(function(){
	change_main_width();
});


jQuery("#select_portfolio").change(function(){
	jQuery("#right_bar_stock_list").css('opacity', 0.4);
	jQuery(this).after("<img src='http://static.gurufocus.com/images/indicator.gif' id='right_bar_indicator' />");
	jQuery("#right_bar_stock_list").load("/modules/rightbar/ajax_stock_list.php?portid="+jQuery(this).val(), function(){
		jQuery("#right_bar_indicator").remove();
		jQuery("#right_bar_stock_list").css('opacity', 1);
		jQuery('#right_bar_stocks').scrollTop(0);
	});
	SetCookie("focusbar_portfolio", jQuery(this).val());
});

jQuery("#select_gurutype").change(function(){
	jQuery("#right_bar_guru_list").css('opacity', 0.4);
	jQuery(this).after("<img src='http://static.gurufocus.com/images/indicator.gif' id='right_bar_indicator' />");
	jQuery("#right_bar_guru_list").load("/modules/rightbar/ajax_guru_list.php?type="+jQuery(this).val(), function(){
		jQuery("#right_bar_indicator").remove();
		jQuery("#right_bar_guru_list").css('opacity', 1);
		jQuery('#right_bar_gurus').scrollTop(0);
	});
	SetCookie("focusbar_gurutype", jQuery(this).val());
});


jQuery('#right_bar_link_hide').click(function(){
	jQuery('#right_bar').hide();
	jQuery('#right_bar_show').show();
	SetCookie("focusbar_hide", "1");
	hide=true;
	change_main_width();
});

jQuery('#right_bar_link_show').click(function(){
	jQuery('#right_bar_show').hide();
	jQuery('#right_bar').show();
	SetCookie("focusbar_hide", "0");
	hide=false;
	if(!auto){
		change_main_width();
	}
	if(jQuery("#right_bar_stock_list li").length==0){
		jQuery("#right_bar_stock_list").load("/modules/rightbar/ajax_stock_list.php?portid="+jQuery('#select_portfolio').val());
	}
	if(jQuery("#right_bar_guru_list li").length==0){
		jQuery("#right_bar_guru_list").load("/modules/rightbar/ajax_guru_list.php?type="+jQuery('#select_gurutype').val());
	}
});


jQuery(function(){
	change_main_width();

					if(jQuery("#right_bar_stock_list li").length==0){
			jQuery("#right_bar_stock_list").load("/modules/rightbar/ajax_stock_list.php?portid="+jQuery('#select_portfolio').val());
		}
					});


function change_main_width(){
	var w=jQuery(window).width()-jQuery("#right_bar").width();
	if(w>1000){
		auto=false;
		if(!hide){
			jQuery("#right_bar").show();
			jQuery('#right_bar_show').hide();
			jQuery("#_temp_style").html("<style>div{max-width: "+w+"px !important;} div.timeline-band, div.timeline-band-inner, div.timeline-band-layer, div.timeline-ether-bg{max-width: 2910px !important;}</style>");
		}
		else
		{
			jQuery("#_temp_style").html("");
		}
	}
	else{
		auto=true;
		jQuery("#right_bar").hide();
		jQuery('#right_bar_show').show();
		jQuery("#_temp_style").html("");
	}

	jQuery(".right_bar_content").height(jQuery(window).height()-122-jQuery("#right_bar_bottom").height());
}


function getInternetExplorerVersion()
//Returns the version of Internet Explorer or a -1
//(indicating the use of another browser).
{
var rv = -1; // Return value assumes failure.
if (navigator.appName == 'Microsoft Internet Explorer')
{
 var ua = navigator.userAgent;
 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
 if (re.exec(ua) != null)
   rv = parseFloat( RegExp.$1 );
}
return rv;
}

function SetCookie(name,value)
{
    jQuery.cookie(name, value, {expires: 90, path: '/'});
}
function delCookie(name)
{
	jQuery.cookie(name, null, {expires: 90, path: '/'});
}

var delay_in_mins=5;
function keepAlive(){
	var url=(""+location.href).replace("&CMD=","&CMD=STARTPAGE&CMD=");
        document.getElementById('keepalive').src=url;
        var g=setTimeout("keepAlive()",1000*60*delay_in_mins);
        }
var g=setTimeout("keepAlive()",1000*60*delay_in_mins);
</script>



<script type="text/javascript">
function getRandom(nMin, nMax){
	var rnd = Math.random();
	return nMin + Math.floor((nMax - nMin + 1) * rnd);
}
AE.webUser.create();
if(!AE.webUser.isFirstIn()){
	var txt = 'text1.gif';
	if(YAHOO.lang.isObject(adsControlData)){
		if(adsControlData.productNum > 0){
			txt = 'bannertext/1/text' + getRandom(1, 11) + '.gif';
		}else{
			txt = 'bannertext/0/text' + getRandom(1, 8) + '.gif';
		}
	}
    if(AE.webUser.getServiceType()=='cgs'){
 document.write('<a class="hugeBanner" href="http://buyer.alibaba.com?tracelog=bsd_sell"><img src="http://i02.i.aliimg.com/images/cms/upload/cgs/615x151_big_buyer_091117.jpg" border="0" style="margin:0;padding:0" alt="Big Buyers &ndash; Matching you with quality trade partners" /></a>');
}else{
	document.write(
		'<form id="quickPost" action="http://us.my.alibaba.com/product/post_product.htm" method="get">'+
			'<input type="hidden" id="tracelog" name="tracelog" value="sellnow_banner_product" />'+
			'<div id="for_sell_banner" style="height: 139px; border: 6px solid #d1e6f3; background: url(http://img.alibaba.com/images/cms/upload/banner/promotion/bg.gif);">'+
				'<div class="textWrap" style="background:url(http://img.alibaba.com/images/cms/upload/banner/promotion/'+txt+') left bottom no-repeat;"></div>'+
				'<div class="title">Display your Product now!</div>'+
				'<div class="cnt clearfix">'+
					'<input id="product_name" class="canBeClear" type="text" maxlength="128" style="width: 389px;" value="Enter your Product Name here" name="productName" tabindex="1"/>'+
					'<img src="http://i02.i.aliimg.com/images/cms/upload/banner/promotion/btn.gif" style="cursor:pointer" onclick="doSubmit()" class="J-continue" />'+
				'</div>'+
			'</div>'+
		'</form>'
	);
    }
}else{
 document.write('<a class="hugeBanner" href="http://www.alibaba.com/activities/posting_trade_show.html?tracelog=posting_ts_apr09"><img src="http://i02.i.aliimg.com/images/cms/upload/others/posting_trade_show/620x150_post_trade_show_090402.jpg " border="0" style="margin:0;padding:0" alt="Buyers like more choice, Suppliers like more buyers!" /></a>');
}
function doSubmit(){
	var get = YAHOO.util.Dom.get;
	if(get('product_name').value === 'Enter your Product Name here'){
		get('product_name').value = '';
	}
	 if(!checkIsLogin()){
           popUp(this);
        }else{
            get('quickPost').submit();
        }
}
function clearOnClick(){
	this.value='';YAHOO.util.Event.removeListener(this, 'click');
	YAHOO.util.Dom.setStyle(this, 'color', '#000000');
}
function clearOnSubmit(f){
	if(YAHOO.lang.trim(f.productName.value) == 'Enter your Product Name here'){
		f.productName.value = '';
	}
	f.submit();
}
if(YAHOO.util.Dom.get('product_name')){YAHOO.util.Event.on('product_name', 'click', clearOnClick);}

var jumpUrl = '';
	
function checkIsLogin() {
	
	var isLoginCookie = AE.bom.getCookie('xman_us_t');
	if(isLoginCookie && isLoginCookie.indexOf('sign=y') !== -1) {
	   return true ;
	}
	return false ;
};

function buildUrl(linkHref) {
	jumpUrl = linkHref;
};

function capturePop(link) {
	YUE.on(link,'click',function(event) {
		event.preventDefault();
		if(link.href !== undefined) {
			buildUrl(link.href);
		}
		popUp(this);
	});
};
	
function popUp(id) {
	if(!checkIsLogin()) {
	var xmanjsUrl = "https://login.alibaba.com/xman/xman.js?display=p&pd=alibaba&style=b&modal=n&join=a&login=a&tab=l&back=n&title=a&link=a&lbutton=a&jbutton=a";
		if(!AE.xman_join_login){
			YAHOO.util.Get.script(xmanjsUrl, {onSuccess:function(){
				try{
					AE.xman_join_login.boxPopup(id); 
				}catch(e){ } }
			});
		}else{
			try{
				AE.xman_join_login.boxPopup(id);
			}catch(e){}
		}
	}else {
		xman_callback(null,id);
	}
};

function xman_callback(obj,id) { 	
	if(jumpUrl == ''){
		window.location.reload();
	}
	window.location.href = jumpUrl;
}	


</script><!--skylight-for_sell_banner.html -end -->	</div>

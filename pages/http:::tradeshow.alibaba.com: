<div id="tradeshowbody" style="width:760px;">
	
	<script language="javascript">try{document.domain="alibaba.com";}catch(E){}</script>
	<script language="javascript">

     	 var withinActionUrl = '$searchBarView.WithinActionUrl';
	 
	 	 var withinResult = false;
	 
	 	 function beforeSubmit(SearchBarForm){
	 
	 var oFormIndexArea = document.getElementById("IndexAreaOptionIdx");	
		var sv = oFormIndexArea.options[oFormIndexArea.selectedIndex].value;
	 		  	if(trim(SearchBarForm.SearchText.value) == '' && sv != 'ts_en'){
	    	alert('Please input a search term.');
	    	return false;
	  	}
	  	if(SearchBarForm.SearchText.value.length>50)
	  	{
	  		alert('please input a search term less than 50 characters.');
	  		return false;
	  	}
			
			if(sv=='ts_en' && get('srchLocationIdx').value.isEmpty() && get('srchIndustryIdx').value.isEmpty() && (SearchBarForm.SearchText.value.trim().isEmpty() || SearchBarForm.SearchText.value.trim() == 'Trade Shows')){
				alert('Please select at least one condition.');
				return false;
			}

      		 	if(SearchBarForm.IndexArea.tagName.toUpperCase() == 'SELECT'){
			
	    		    	var selectedText = SearchBarForm.IndexArea.options[SearchBarForm.IndexArea.selectedIndex].text;
			var selectedValue = SearchBarForm.IndexArea.options[SearchBarForm.IndexArea.selectedIndex].value;
			var actionUrl = "";

						SearchBarForm.Type.value = "";			
			
        		    	if(selectedText == 'Buying Leads'){
	      		SearchBarForm.Type.value = "BUY";
				actionUrl = "http://importer.alibaba.com/buyeroffers";
	    	} 
			else if(selectedText == 'Selling Leads'){ 
    	      	SearchBarForm.Type.value = "SALE,AGENT,MISC";
				actionUrl = "http://www.alibaba.com/trade/search";
    	    } 
			else if(selectedValue == 'ts_en'){
				actionUrl = "http://tradeshow.alibaba.com/search";
				tradeShowSearch();
			}
			else {
				actionUrl = "http://www.alibaba.com/trade/search";
    	    }
			SearchBarForm.action = actionUrl
	  	}
		
	  		  	if(withinResult && withinActionUrl != ''){ SearchBarForm.action = withinActionUrl; }
		if(checkText(SearchBarForm.SearchText.value))
		{
			alert('Sorry, the information you filled in is not in English. Please input the information in English instead.');
			return false;
		}
		//SearchBarForm.submit();
		
		if (SearchBarForm.SearchText.value.trim().isEmpty() || SearchBarForm.SearchText.value.trim() == 'Trade Shows') {
			SearchBarForm.SearchText.value = '';
			SearchBarForm.keyword.value = '';			
		}
		
	  	return true;
	 }
	 
	 function   checkText(str)   
      {   
          for(i=0;i<str.length;i++){   
          if(str.charCodeAt(i)>256)
		  return true;   
		}
		return false;

      }   
	 
				function cateChange(selectObject){
		  if (selectObject==null) return ;
		   if (selectObject.selectedIndex<0) return ;
			var selectOption=selectObject.options[selectObject.selectedIndex];
			if (selectOption.value==""){
			   for(var i=0;i<selectObject.options.length;i++){
			       if (selectObject.options[i].value=='0'){
				      selectObject.selectedIndex=i;
					  break;
				   }
			   }
			}
		}
		
				function tradeShowSearch(){
		
				var form = document.forms["SearchBarForm"];
				var oIndustrySelect = form.elements["srchIndustryIdx"];
				var oLocationSelect = form.elements["srchLocationIdx"];
				var oSearchText = form.elements["SearchTextIdx"];
				
				var industry = oIndustrySelect.options[oIndustrySelect.selectedIndex].value;

				form.elements["keywordIdx"].value = oSearchText.value;
				form.elements["locationIdx"].value = oLocationSelect.options[oLocationSelect.selectedIndex].value;				
				form.elements["industry"].value = industry;
		}
		
				function loadSearchItemsIdx(){
		var typeSel = get('IndexAreaOptionIdx');
			for(i=0;i<typeSel.length;i++){
			typeSel[i].selected=false;
		}
		typeSel[4].selected=true;
				searchTypeChange();
		}
		
		//setTimeout(loadSearchItemsIdx,1000);
		//YUE.on(window,'load',loadSearchItemsIdx);
		
				function searchTypeChange(){

			var oSelect1 = document.getElementById("srchIndustryIdx");
			var oSelect2 = document.getElementById("srchLocationIdx");
			var oSearchText = document.getElementById("SearchTextIdx");
			var oCountrySel = get('refineSearchCountry_id');
			var oFormIndexArea = document.getElementById("IndexAreaOptionIdx");
    			
    		if(!oFormIndexArea){ return; }
    			
    		var sv = oFormIndexArea.options[oFormIndexArea.selectedIndex].value;
			
			var isHome = "true";
			

				if(sv == 'ts_en'){
    				oSelect1.style.display = '';
    				oSelect2.style.display = '';
    				oCountrySel.style.display = 'none';
    				oSearchText.size = 30;
    			}else{
    				oSelect1.style.display = 'none';
    				oSelect2.style.display = 'none';
    				oCountrySel.style.display = '';
    				oSearchText.size = 55;
    			}

			
		}
		
		function SelectCountry(obj){
	
	   	  if(obj.value == "0"){
	    document.location = "http://www.alibaba.com/trade/advancesearch";
	    return;
	  }
	  
	  }
			
		function printYearMonthListIdx(){
			var monthNames = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
			var oSelect = document.getElementById("srchYearMonthIdx");
			var now = new Date();
			var year = now.getFullYear();
			var month = now.getMonth();
			var idx = 1;
			for(i = month; i<12; i++, idx++){
				oOption = oSelect.options[idx];
				if(i == month){
					oOption.innerHTML = monthNames[i] + " >> " + year;
					oOption.value = year + "|" + (i + 1) ;
				}else{
					oOption.innerHTML = monthNames[i];
					oOption.value = year + "|" + (i + 1) ;	
				}
			}
			year += 1;
			for(i = 0; i<month; i++, idx++){
				oOption = oSelect.options[idx];
				if(i == 0){
					oOption.innerHTML = monthNames[i] + " >> " + year;
					oOption.value = year + "|" + (i + 1) ;
				}else{
					oOption.innerHTML = monthNames[i];
					oOption.value = year + "|" + (i + 1) ;
				}
			}

			return oSelect;
		}
		function changeSearchType(sType,obj){
			var typeSel = get('IndexAreaOptionIdx');
			for(i=0;i<4;i++){
			typeSel[i].selected=false;
		}
		var oLi=obj.parentNode.parentNode.getElementsByTagName('li');
		YUD.removeClass(oLi,"current")
		obj.parentNode.className="current";
			switch(sType){
				case("product"):{
					typeSel[0].selected=true;
					break;
					}
				case("buy"):{
					typeSel[1].selected=true;
					break;
					}
				case("sell"):{
					typeSel[2].selected=true;
					break;
					}
				case("company"):{
					typeSel[3].selected=true;
					break;
					}
				case("ts"):{
					typeSel[4].selected=true;
					break;
					}
				}
				searchTypeChange();
			}
	  </script>


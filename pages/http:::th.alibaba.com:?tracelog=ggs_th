AE.namespace("AE.widget");
AE.namespace("AE.bom");
AE.bom = {
    isGecko: YAHOO.env.ua.gecko > 0,
    isIE: YAHOO.env.ua.ie > 0,
    isIE6: YAHOO.env.ua.ie == 6,
    isIE7: YAHOO.env.ua.ie == 7,
    isIE8: YAHOO.env.ua.ie == 8,
    isOpera: YAHOO.env.ua.opera > 0,
    isSafari: navigator.userAgent.toLowerCase().indexOf("safari") != -1,
    getCookie: function(b) {
        var a = document.cookie.match("(?:^|;)\\s*" + b + "=([^;]*)");
        return a ? unescape(a[1]) : ""
    },
    setCookie: function(c, a, e, b, f) {
        a = escape(a);
        a += b ? "; domain=" + b: "";
        a += f ? "; path=" + f: "";
        if (e) {
            var d = new Date;
            d.setTime(d.getTime() + e * 86400000);
            a += "; expires=" + d.toGMTString()
        }
        document.cookie = c + "=" + a
    },
    removeCookie: function(a) {
        this.setCookie(a, "", -1)
    }
};

YL.isEnglish = function(b) {
    var a = /[^\x00-\x80]/g;
    if (a.test(b)) {
        return false
    } else {
        return true
    }
};
YL.isAscii = function(b) {
    var a = /[^\x00-\xFF]/g;
    if (a.test(b)) {
        return false
    } else {
        return true
    }
};
var trim = YL.trim;




AE.namespace("widget");
AE.widget.overShow = AE.widget.overShow ||
function() {
    var o = this;
    var n = {
        targetId: "overShowTargetId",
        positionId: false,
        contentId: "overShowContentId",
        showDelayTime: 200,
        hiddenDelayTime: 200,
        excursion: [0, 0],
        needMask: false,
        needXY: true,
        closeBtnClass: "close-button"
    };
    var d;
    var m = false;
    var a, h, e, g;
    var b = false;
    var c = true,
    l = false,
    f = false;
    o.hold = function() {
        f = true
    };
    o.setFree = function() {
        f = false
    };
    o.afterShow = new YAHOO.util.CustomEvent("afterShow", o);
    o.afterShowDelay = new YAHOO.util.CustomEvent("afterShowDelay", o);
    o.afterHidden = new YAHOO.util.CustomEvent("afterHidden", o);
    o.afterHiddenDelay = new YAHOO.util.CustomEvent("afterHiddenDelay", o);
    o.init = function(r) {
        if (m) {
            return false
        }
        d = YL.merge(n, r);
        a = get(d.targetId);
        h = d.positionId ? get(d.positionId) : false;
        e = get(d.contentId);
        YUE.on(a, "mouseover", o.showDelay);
        YUE.on(e, "mouseover",
        function(t) {
            if (!a || a == undefined) {
                return
            }
            o.afterShowDelay.fire(a);
            if (b) {
                clearTimeout(b)
            }
            b = setTimeout(function() {
                o.showDirectly()
            },
            d.showDelayTime)
        });
        YUE.on(a, "mouseout", o.hiddenDelay);
        YUE.on(e, "mouseout", o.hiddenDelay);
        YUE.on(a, "mouseover",
        function() {
            c = false
        });
        YUE.on(e, "mouseover",
        function() {
            c = false
        });
        YUE.on(a, "mouseout",
        function() {
            c = true
        });
        YUE.on(e, "mouseout",
        function() {
            c = true
        });
        if (d.needMask) {
            g = document.createElement("iframe");
            g.className = "maskIframe";
            g.style.display = "none";
            if (AE.bom.isIE6 && location.protocol == "https:") {
                g.src = globalImgServer + "/js/blank.html"
            }
            g.style.zIndex = YUD.getStyle(e, "zIndex") - 1;
            g.style.top = "0px";
            g.style.left = "0px";
            g.frameBorder = 0;
            if (e && e.parentNode) {
                e.parentNode.appendChild(g)
            }
        }
        var s = YUD.getElementsByClassName(d.closeBtnClass);
        if (s) {
            YUE.on(s, "click",
            function() {
                c = true;
                o.hiddenDirectly()
            })
        }
    };
    o.showDelay = function(r) {
        if (a != this) {
            c = true;
            o.hiddenDirectly()
        }
        a = this;
        o.afterShowDelay.fire(a);
        if (b) {
            clearTimeout(b)
        }
        b = setTimeout(function() {
            o.showDirectly()
        },
        d.showDelayTime)
    };
    o.showDirectly = function() {
        if (l || f) {
            return
        }
        e.style.visibility = "hidden";
        e.style.display = "";
        if (d.needXY) {
            var r = YUD.getXY(h || a);
            q(a, d.excursion);
            r[0] += d.excursion[0];
            r[1] += d.excursion[1]
        }
        e.style.visibility = "visible";
        e.style.display = "";
        if (d.needXY) {
            YUD.setXY(e, r)
        }
        o.afterShow.fire(a);
        l = true;
        if (d.needMask) {
            g.style.display = "";
            g.style.width = e.offsetWidth + "px";
            g.style.height = e.offsetHeight + "px";
            if (d.needXY) {
                YUD.setXY(g, r)
            }
            g.style.visibility = "visible"
        }
    };
    o.hiddenDelay = function(r) {
        o.afterHiddenDelay.fire();
        if (f == true) {
            return
        }
        if (b) {
            clearTimeout(b)
        }
        b = setTimeout(o.hiddenDirectly, d.hiddenDelayTime)
    };
    o.hiddenDirectly = function() {
        if (!c) {
            return false
        }
        o.afterHidden.fire();
        e.style.display = "none";
        if (d.needMask) {
            g.style.display = "none"
        }
        l = false
    };
    var q = function(s, r) {
        if (typeof(r[0]) == "string") {
            if (r[0] == "auto") { (p()) ? r[0] = "right": r[0] = "left"
            }
            switch (r[0]) {
            case "center":
                r[0] = parseInt(h ? (h.offsetWidth - e.offsetWidth) / 2 : (s.offsetWidth - e.offsetWidth) / 2);
                break;
            case "right":
                r[0] = ((h ? h.offsetWidth: s.offsetWidth) - e.offsetWidth);
                break;
            default:
                r[0] = 0
            }
        }
        if (typeof(r[1]) == "string") {
            switch (r[1]) {
            case "center":
                r[1] = parseInt(h ? (h.offsetHeight - e.offsetHeight) / 2 : (s.offsetHeight - e.offsetHeight) / 2);
                break;
            case "bottom":
                r[1] = h ? h.offsetHeight: s.offsetHeight;
                break;
            default:
                r[1] = 0
            }
        }
        return r
    };
    var p = function() {
        return document.body.offsetWidth < (YUD.getX(h || a) + e.offsetWidth)
    }
};




AE.namespace("AE.app");
AE.app.categories = function(a) {
    this.config = {
        rootTarget: get("categories"),
        subTarget: get("categories-subs"),
        afterLoadClass: "after-load-subs",
        itemCurrentClass: "current",
        afterShowSubClass: "after-show-sub",
        afterShowNextItemClass: "active-border-color",
        showDelayTime: 50,
        hiddenDelayTime: 50,
        excursion: [184, 0]
    };
    this.temp = {
        rootItems: [],
        subItems: {}
    };
    this.onAfterShow = new YAHOO.util.CustomEvent("onAfterShow", this);
    this.onAfterHide = new YAHOO.util.CustomEvent("onAfterHide", this);
    return this
};
AE.app.categories.prototype = {
    afterShow: function(f, e, d) {
        var b = this,
        c = this.config,
        a;
        this.onAfterShow.fire(this);
        if ((d.subItem.offsetHeight + YUD.getY(d.subItem)) > (YUD.getViewportHeight() + YUD.getDocumentScrollTop())) {
            a = YUD.getY(d.subItem) + YUD.getViewportHeight() + YUD.getDocumentScrollTop() - (d.subItem.offsetHeight + YUD.getY(d.subItem));
            if ((a + d.subItem.offsetHeight) < (YUD.getY(d.subItem) + d.rootItem.offsetHeight + 3)) {
                a += (YUD.getY(d.subItem) + d.rootItem.offsetHeight + 3) - (a + d.subItem.offsetHeight)
            }
            if (a > 110) {
                YUD.setY(d.subItem, a)
            }
        }
        if (c.afterShowSubClass) {
            YUD.addClass(c.rootTarget, c.afterShowSubClass)
        }
        YUD.addClass(d.nextItem, c.afterShowNextItemClass);
        YUD.addClass(d.rootItem.parentNode, c.itemCurrentClass)
    },
    afterHidden: function(e, d, c) {
        var a = this;
        var b = this.config;
        this.onAfterHide.fire(this);
        if (b.afterShowSubClass) {
            YUD.removeClass(b.rootTarget, b.afterShowSubClass)
        }
        YUD.removeClass(c.nextItem, b.afterShowNextItemClass);
        YUD.removeClass(c.rootItem.parentNode, b.itemCurrentClass)
    },
    init: function(n) {
        this.config = YL.merge(this.config, n || {});
        var l = this,
        d = this.config;
        var g = d.subTarget.getElementsByTagName("div");
        for (var f = 0; f < g.length; f++) {
            var c = g[f];
            l.temp.subItems[trim(c.getAttribute("rel")).toUpperCase()] = c
        }
        var h = d.rootTarget.getElementsByTagName("a");
        for (var f = 0; f < h.length; f++) {
            var o = h[f];
            var m = trim((o.textContent ? o.textContent: o.innerText)).toUpperCase();
            if (l.temp.subItems[m]) {
                var e = o.overShow = new AE.widget.overShow();
                var c = this.temp.subItems[m];
                var a = h[f + 1] ? h[f + 1] : null;
                e.afterShow.subscribe(l.afterShow, {
                    rootItem: o,
                    subItem: c,
                    nextItem: a
                },
                l);
                e.afterHidden.subscribe(l.afterHidden, {
                    rootItem: o,
                    subItem: c,
                    nextItem: a
                },
                l);
                var b = d.excursion;
                e.init({
                    targetId: o,
                    contentId: c,
                    showDelayTime: d.showDelayTime,
                    hiddenDelayTime: d.hiddenDelayTime,
                    excursion: b
                })
            }
            l.temp.rootItems.push(o)
        }
        YUD.addClass(d.rootTarget, d.afterLoadClass);
        if (AE.bom.isIE) {
            CollectGarbage()
        }
        return l
    }
};

try {
    var cateControl = get("cate-father").categories = (new AE.app.categories()).init({
        excursion: [AE.bom.isIE6 ? 167 : 168, 0]
    });
    cateControl.onAfterShow.subscribe(function() {
        autoSuggest.UIController.hideList()
    })
} catch(err) {}
</script>



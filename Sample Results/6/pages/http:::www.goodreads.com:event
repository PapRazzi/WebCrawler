<script type="text/javascript" charset="utf-8">
  function checkCountry() {
    country = document.eventForm.search_country_code.value;
    if (country && country != "US") {
      Element.hide('distance_from_postal_code');
    }
    if (country && country == "US") {
      Element.show('distance_from_postal_code');
    }
  }
  Event.observe(window, 'load', function(e) {
    checkCountry();
  });
</script>

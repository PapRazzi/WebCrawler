	<script>
		YUE.onDOMReady(function(){
			//控制语言选择菜单的显示
			YUE.on(document, 'click', function(ev){
				var el = YUE.getTarget(ev);
				if(el.id === 'change_lang' || YUD.isAncestor('change_lang', el)){
					YUD.setStyle('lang_select', 'display', 'block');
				}
				else {
					YUD.setStyle('lang_select', 'display', 'none');
				}
			});
		});
		//语言切换
		var lang = {
			_current: 'en', //当前语言，初始需判断IP
			lang_select: 'lang_select', //语言选择控件
			id_list: ['supplier_services', 'success_story'], //语言对应控件
			choosen: {}, //选中的语言
			change: function(){ //切换语言
				var that = this;
				YUE.onDOMReady(function(){					
					var lang_type = that.choosen.choosen,
						lang_desc = that.choosen[lang_type]['lang_desc'];
					if(lang_type && lang_type !== that._current){ //相同的语言，中止切换
						for( var i = 0; i < that.id_list.length; i++ ){
							var tmpId = that.id_list[i] + '_';
							YUD.setStyle(tmpId + that._current, 'display', 'none');
							YUD.setStyle(tmpId + lang_type, 'display', 'block');
						}
						that._current = lang_type;
						YUD.get('current_lang').innerHTML = lang_desc;
						YUD.setStyle(that.lang_select, 'display', 'none');
					}
				});
			},
			_getChoosen: function(obj){ //获取当前选中的语言
				if(!obj){
					return false;
				}
				var tag = obj.tagName,
					el = obj,
					lang_type = el.name; //当前选中的语言的类型
				if(tag.toLowerCase() !== 'a'){
					return false;
				}
				this.choosen['choosen'] = lang_type;
				if(!this.choosen[lang_type]){ //保存选中的语言
					this.choosen[lang_type] = {};
					this.choosen[lang_type]['element'] = el; //选中的语言的控件
					this.choosen[lang_type]['lang_desc'] = el.innerHTML;
				}
				return true;
			},
			bind: function(){ //绑定选择语言事件
				var that = this;
				YUE.onDOMReady(function(){
					YUE.on(that.lang_select, 'click', function(ev){
						var el = YUE.getTarget(ev);
						if(that._getChoosen(el)){
							that.change();
						}
					});
				});
			}
		};
		lang.bind(); //语言切换事件绑定
		//判断IP，切换语言
		lang.choosen = {
			choosen: 'en',
			en: {
				lang_desc: 'EN'
			},
			cn: {
				lang_desc: '简体'
			},
			tw: {
				lang_desc: '繁體'
			}
		};
		if(adsControlData.ipRegion){
			if(adsControlData.ipRegion === 'China'){
				lang.choosen.choosen = 'cn';
			}
			else if(adsControlData.ipRegion === 'Hong Kong' || adsControlData.ipRegion === 'Taiwan' || adsControlData.ipRegion === 'Macau'){
				lang.choosen.choosen = 'tw';
			}
		}
		lang.change();
	</script><!--skylight-sell/supplier_services_lang.html -end -->		</div>

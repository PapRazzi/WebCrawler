      required: true,
      rangelength: [4, 20],
      checkUserName:true
      }
      },
      errorPlacement: function(error, element) { }
      });
      $("#registerForm #username").focus();

      if(MarketWatch.Member && MarketWatch.Member.init) {
				MarketWatch.Member.init();
			}
		});

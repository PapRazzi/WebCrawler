}, getSubtopic: 
	function(parentNode, index) {
		var retVal = null; 
		var i = 0; 

		for (topicId in parentNode.subtopics) { 
			if (i == index) {
				retVal = parentNode.subtopics[topicId];
				break; 
			} 
			i++; 
		} 

		return retVal; 
	}
};



if(!width&&!height&&!adSize){var iframe=jQuery(el).parent().find('iframe');if(iframe.length>0){width=jQuery(iframe[0]).width();height=jQuery(iframe[0]).height();adSize=''+width+'x'+height;ad_sizes.push(adSize);}}
if(!width&&!height&&!adSize){var a=jQuery(el).parent().find('a');if(a.length>0){width=jQuery(a[0]).children('img').width();height=jQuery(a[0]).children('img').height();adSize=''+width+'x'+height;ad_sizes.push(adSize);}}});for(var i=0;i<ad_sizes.length;i++){var key='adSize'+(i+1);bk_data[key]=ad_sizes[i];}
var bk_adzones=[];if(typeof(ad_zones)!='undefined'&&ad_zones.length>0){var j=0;for(var i=0;i<ad_zones.length;i++){if(jQuery.inArray(ad_zones[i],bk_adzones)==-1){bk_adzones.push(ad_zones[i]);var key=(j==0)?'adZone':'adZone'+(j+1);j++;bk_data[key]=ad_zones[i];}}}
var bk_adsections=[];if(typeof(ad_sections)!='undefined'&&ad_sections.length>0){var j=0;for(var i=0;i<ad_sections.length;i++){if(jQuery.inArray(ad_sections[i],bk_adsections)==-1){bk_adsections.push(ad_sections[i]);var key=(j==0)?'adSection':'adSection'+(j+1);j++;bk_data[key]=ad_sections[i];}}}
blueKai.blueKai.bk_track(true,bk_data);},5000);});

</script>



<script type='text/javascript'>
var amzn_wdgt={widget:'MyFavorites'};
amzn_wdgt.tag='bloombergco00-20';
amzn_wdgt.columns='1';
amzn_wdgt.rows='3';
amzn_wdgt.width='300';

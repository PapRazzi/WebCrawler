$(this).find(".sub-catList").css( "top", "-30px" )
/* positioning */
var offset = $(this).find(".sub-catList").offset();
var listHeight = $(this).find(".sub-catList").height();
var scrolltop = $(window).scrollTop();
var browserHeight = $(window).height();
var movePosition = (browserHeight+scrolltop) - (offset.top+listHeight) - 60;
if ( (browserHeight+scrolltop) < (offset.top+listHeight)) {
if (offset.top + movePosition < 0) {
$(this).find(".sub-catList").css( "top", -offset.top - 20 + scrolltop + "px" )
} else {
$(this).find(".sub-catList").css( "top", movePosition  + "px" )
}
} else if (offset.top + movePosition < 0) {
$(this).find(".sub-catList").css( "top", -offset.top - 20 + "px" )
}
}, function() {
$(this).find(".sub-catList").removeClass('showSubMenu');
})
});

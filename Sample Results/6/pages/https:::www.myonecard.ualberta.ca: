<script language="javascript" type="text/javascript">
    var defNav = 'login';

    // called on document load
    $(document).ready(
        function() {
            // select one or more elements to be overlay triggers
            $('#overlayAccountNo').dialog({ width:260, height:182, closeOnEscape: false, autoOpen: false, draggable:false, resizable:false, modal:true, position:'center', title:'Report My ONEcard Lost', close: function(event, ui) { $('#divOnHoldError').hide(); $('#divHoldButtons').show(); $('#divOnHoldSuccess').hide(); }});
        }
    );
    // call when hash mark changes #deposit #login
    $.address.change(
    function(event) {
        var nav             = (event.parameters['nav'] == null ? defNav : unescape(event.parameters['nav']));

        if (!(nav == 'deposit' || nav == 'login')) {
            nav = defNav;
        }
        _gaq.push(['_trackPageview', '/Home/' + nav]);
        // The one to fade in
        var tabFadeIn  = nav;
        //remomve selected link otherwise
        $('#ulMyONEcard a').removeClass('here');
        $('#lnkLeft' + tabFadeIn).addClass('here');
        $('#txtheader').html(PageTitle(tabFadeIn));
        $.address.title(PageTitle(tabFadeIn));

        // initial load no fade
        if ($('#nav ul li a[class=current]').length == 0 ) {
            $('#lnk' + tabFadeIn).addClass('current');
            $('#div' + tabFadeIn).show();
            // Focus on correct textbox

            if (tabFadeIn == 'deposit') {
                $('#Deposit_DepositAmount').focus();

            }
            else if (tabFadeIn == 'login') {
                if ($('#inpCCID').length>0) {
                    $('#inpCCID').focus();
                }
                else {
                    $('#inpPINAccount').focus();
                }
            }
            return;
        }
       
        // find correct one to fade out
        var tabFadeOut = $('#nav ul li a[class=current]').attr('title');
        $('#div' + tabFadeOut).fadeTo(110,0,
        function() {
            $('#div' + tabFadeOut).hide();
            $('#div' + tabFadeIn).show();
            $('#div' + tabFadeIn).fadeTo(1,0,
            function() {
                $('#lnk' + tabFadeOut).removeClass();
                $('#lnk' + tabFadeIn).addClass('current');
                $('#div' + tabFadeIn).show(0);
                $('#div' + tabFadeIn).fadeTo(110,1,
                function() {
                    // side menu check
                    
                    // change the side menu links
                    if (tabFadeIn == 'deposit') {
                        $('#Deposit_DepositAmount').focus();

                    }
                    else if (tabFadeIn == 'login') {
                        if ($('#inpCCID').length>0) {
                            $('#inpCCID').focus();
                        }
                        else {
                            $('#inpPINAccount').focus();
                        }
                    }
                    
                });
                
            });
            
        });        
    });
    function PageTitle(action) {
        switch(action) {
            case 'deposit':
                return 'Credit Card Deposit';
            case 'login':
                return 'Login';
        }
    }
    // called on document load
    $(document).ready(
        function() {
            // clear creditcard number
            $('#inpCardNum').val('');
            // restrict input to float
            $('#Deposit_DepositAmount').numeric({ allow: "." });
            // restrict input to number
            $('#inpCardNum').numeric();
            
            $('#Deposit_AccountId').numeric();
            // focus on first text box
            $('#butLogin').attr("disabled", false);
        }
    );
    // called when user tries to login
    function chkFormLogin() {
        // initially check for empty fields
        if ($('#inpCCID').val().length==0) {
            $('#divErrorLogin div').html('CCID is required!')
            $('#divErrorLogin').show();
            return false;
        }
        // check for empty password 
        if ($('#inpPASS').val().length==0) {
            $('#divErrorLogin div').html('Password is required!')
            $('#divErrorLogin').show();
            return false;
        }
        // lock the buttons
        $('#butLogin').attr("disabled", true);
        // hide error
        $('#divErrorLogin').hide();
        //show animation
        $('#butLogin_ANI').show();
        //submit form
        $('#frmLogin').submit();
        return true;
        
    }
    // User presses Cancel button
    function cancelDeposit() {
        $('#Deposit_DepositAmount').val('');
        $('#inpCardNum').val('');
        $('#Deposit_ExpiryMonth').val('0');
        $('#Deposit_ExpiryYear').val('0');
        $('#Deposit_CardType').val('0');
        $('#Deposit_AccountId').val('');
        $("[name='Deposit.Account']").removeAttr("checked");
        $('#Deposit_Email').val('');
        $('#divDepositError').hide();
        $('#frmDeposit [class=txtvalr]').attr('class','txtval');
        $.address.value('/?nav=login');
    }
</script>

<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-19580476-2']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>

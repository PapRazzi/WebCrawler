<script LANGUE="javaScript">
function makeArray()    {
    this[0] = makeArray.arguments.length;
    for (i = 0; i<makeArray.arguments.length; i++)
        this[i+1] = makeArray.arguments[i];
}

var daysofweek = new makeArray('Sun.',
                               'Mon.',
                               'Tues.',
                               'Wed.',
                               'Thurs.',
                               'Fri.',
                               'Sat.');

var monthsofyear = new makeArray('January',
                                 'February',
                                 'March',
                                 'April',
                                 'May',
                                 'June',
                                 'July',
                                 'August',
                                 'September',
                                 'October',
                                 'November',
                                 'December');

function Nths(day) { 
    if (day == 1 || day == 21 || day == 31) return '';
    if (day == 2 || day == 22) return '';
    if (day == 3 || day == 23) return '';
    return '';
}

function DayOfWeek(day,month,year) {
    var a = Math.floor((14 - month)/12);
    var y = year - a;
    var m = month + 12*a - 2;
    var d = (day + y + Math.floor(y/4) - Math.floor(y/100) +
             Math.floor(y/400) + Math.floor((31*m)/12)) % 7;
    return d + 1;
}

function FullDate(day,month,year) {
    return monthsofyear[month] +' '+ day + Nths(day);
//    return daysofweek[DayOfWeek(day,month,year)] +', '+ monthsofyear[month] +', '+ day + Nths(day);
//    return daysofweek[DayOfWeek(day,month,year)] +', '+ day + Nths(day) +', '+ monthsofyear[month] +', '+ year;
}
</script>

<script type='text/javascript'>
	var needCheckService = 0;
	var needCheckHI = 0;
	var needCheckTVIN = 0;
	
	var dontCheckHover = false;
	
	function highlightServices() {
		dontCheckHover = true;

	  if (needCheckService > 0 && needCheckService == 5) {
	    try {
	      if ($("#dtel3").val() == "0" && get_element("dtelImg", 'wtb').length == 0)
	        $(".dtel2").click();
	    } catch (ee) {
	    }
	  }
	
	  if (needCheckTVIN > 0 && 7 == 5) {
	    try {
	      if ($("#dtel3").val() == "0" && get_element("dtelImg", 'wtb').length == 0)
	        $(".dtel2").click();
	    } catch (ee) {
	    }
	  }
	  if (needCheckTVIN > 0 && 6 == 5) {
	    try {
	      if ($("#dtel3").val() == "0" && get_element("dtelImg", 'wtb').length == 0)
	        $(".dtel2").click();
	    } catch (ee) {
	    }
	  }
	
	  if (needCheckHI > 0 && 6 == 5) {
	    try {
	      if ($("#dtel3").val() == "0" && get_element("dtelImg", 'wtb').length == 0)
	        $(".dtel2").click();
	    } catch (ee) {
	    }
	  }
	
	  				$("#netImg").attr({src:"/img/hk/pX.png"});
	  				$("#netImg").css("cursor", "default");
	  				$(".net,.net2").css("cursor", "default");

	  				$("#tvImg").attr({src:"/img/hk/pX.png"});
	  				$("#tvImg").css("cursor", "default");
	  				$(".tv,.tv2").css("cursor", "default");

	  				$("#mobImg").attr({src:"/img/hk/pX.png"});
	  				$("#mobImg").css("cursor", "default");
	  				$(".mob,.mob2").css("cursor", "default");

	
	  if (needCheckTVIN > 0 && $("#comments").val() != "ТвИн") {
	    $("#comments").val("ТвИн");
	    $("#da").click();
	  }
	  if (needCheckHI > 0) {
	    $("#comments").val('Хочу подключить тарифный план «Домашний Интернет» ');
	    $("#da").click();
	  }
	  dontCheckHover = false;
	}
	
	$(document).ready(function () {
	
	
	  var docH = $(window).height();
	  var FH = $('#forma').height();
	  var marg = (docH - FH) / 2;
	  if (marg < 0) {
	    marg = 10;
	  }
	  var bs = (docH - knop) / 2;
	  var docWidth = $(window).width();
	  var blockWidth = $('#forma').width();
	  var half = ((docWidth - blockWidth) / 2) + blockWidth;
	  var kupitx = 0 - half + $("#button_slide").width() / 2;
	  var knop = $("#button_slide").height();
	
	
	  function resizer() {
	    docH = $(window).height();
	    FH = $('#forma').height();
	    docWidth = $(window).width();
	    marg = (docH - FH) / 2;
	    if (marg < 0) {
	      marg = 10;
	    }
	    bs = (docH - knop) / 2;
	    $("#button_slide").css('top', '' + bs + 'px');
	    $('#forma').css('margin-top', '' + marg + 'px');
	    $("#kupit").css("width", "" + docWidth + "px");
	    $("#kupit").css("height", "" + docH + "px");
	  }
	
	  resizer();
	
	  $(window).resize(function () {
	    resizer();
	  });
	
	  function uehat() {
	    $("#kupit").animate({right:'-=' + half}, 500,
	      function () {
	        $("#kupit").hide();
	        $("#kupit").css("width", "0px");
	        $("#kupit").css("height", "0px");
	        $("#button_slide").show();
	
	      });
	  }
	
	  var yaGoalParams = {region:'moscow'};
	
	  function goalOpenWTB() {
	  
	    //google
	    try {
	      _gaq.push(['_trackEvent', 'hochu_moscow', 'zakaz_zvonka_moscow']);
	    } catch (e) {
	    }
	    //yandex
	    try {
	      yaCounter12151939.reachGoal('hochu_kupit', yaGoalParams);
	    } catch (e) {
	    }
	  
	  }
	
	  function goalZhduZvonka() {
	  
	    //google
	    try {
	      _gaq.push(['_trackEvent', 'gdu_moscow', 'gduzvonka_moscow']);
	    } catch (e) {
	    }
	    //yandex
	    try {
	      yaCounter12151939.reachGoal('zhdu_zvonka', yaGoalParams);
	    } catch (e) {
	    }
	  
	  }
	
	  $("#kupit").css('right', '' + kupitx + 'px');
	  $("#write2").hide();
	  $("#kupit").hide();
	  $(".error2").hide();
	  $(".error3").hide();
	  $('.spasibo').hide();
	  $('.send_error').hide();
	
	  $("#button_slide").click(function () {
	    $("#button_slide").hide();
	    $("#kupit").show();
	    resizer();
	    save_stat_full(0, 0, 0);
	    goalOpenWTB();
	    var cnt = $('span.checkbox');
	    for (var i = 0; i < cnt.length; i++) {
	      var id = cnt[i].nextSibling.id;
	      cnt[i].onclick = function (x) {
	        return function () {
	          set_cookie(x, document.getElementById(x).checked, 'wtb');
	        };
	      }(id);
	    }
	    init_cookie('wtb');
	    $("#kupit").animate({right:'+=' + half}, 500, highlightServices());
	  });
	  $("#closeF").click(function () {
	      uehat();
	    }
	  );

    // Отлов нависания курсора над формой и срабатывание закрытия по клику вне формы
    $('#HK_form').hover(
        function(){
            $("#kupit").unbind("click", uehat);
        },
        function(){
            $("#kupit").bind("click", uehat);
        }
     );
    $("#city").click(function(){
        $("#kupit").unbind("click", uehat);
    });
    $("#city20").click(function(){
        $("#kupit").unbind("click", uehat);
    });

	  $("#da").click(function () {
	    $("#write2").show("slow");
	    $("#write").animate({height:'+=' + 200}, 500);
	    $("#forma").css("margin-top", "30px");
	    $("#da").hide();
	  });
	
	  $('#basic-modal').click(function () { // переход ко второму окну либо вывод ошибки
	
	    var telVal = $('#zvonit').val().replace(/^\s+/g, "");
	    var err = '';
	
	    if (telVal.length == 0) {
	      err = 'Пожалуйста, укажите номер телефона';
	
	    } else {
	      var reg1 = new RegExp('^((8|[+]?7)[- ]?)?([(]?[0-9]{3}[)]?[- ]?){1}([0-9]{1}[- ]?){6}[0-9]{1}$');
	      var reg2 = new RegExp('^((8|[+]?7)[- ]?)?([(]?[0-9]{4}[)]?[- ]?){1}([0-9]{1}[- ]?){5}[0-9]{1}$');
	      var reg3 = new RegExp('^((8|[+]?7)[- ]?)?([(]?[0-9]{5}[)]?[- ]?){1}([0-9]{1}[- ]?){4}[0-9]{1}$');
	      if (!reg1.test(telVal) && !reg2.test(telVal) && !reg3.test(telVal)) {
	        err = 'Пожалуйста, укажите номер в формате +7ХХХХХХХХХХ';
	      }
	    }
	
	    if (err.length > 0) {
	      $("#error2p").html(err);
	      $(".error2").show();
	      $("#zvonit").addClass("error1");
	      $("#zvonit").removeClass("hk_text");
	      return;
	    }
	    else {
	
	      var b1 = $("#mob3").val();
	      var b2 = $("#net3").val();
	      var b3 = $("#tv3").val();
	      var b4 = $("#dtel3").val();
	
	
	      if ((b1 == '1') || (b2 == '1') || (b3 == '1') || (b4 == '1')) {
	
	        save_stat_full(0, 0, 1);
	        goalZhduZvonka();
	        //document.wtbForm.submit();
	        function request_error(err) {
	          $("#kupit").animate({right:'-=' + half}, 500,
	            function () {
	              $("#kupit").hide();
	              $("#kupit").css("width", "0px");
	              $("#kupit").css("height", "0px");
	              $('.send_error').show();
	              logProxyError("wanttobuy.jsp", "$(document).ready()", err);
	              docH = $(window).height();
	              var sp = $('.bmc').height();
	              sp = (docH - sp) / 2;
                $('.bmc').css('margin', '' + sp + 'px 0 75% 0');
	
	            });
	        }
	
	        // делаем запрос на сторонний сервер через прокси
	        try {
	          var requestData = {
	            url: 'https://espo.rt.ru:8445/ajax/callback/saveCallback_ajax.jsp',
	            fio:$('#fio').val(),
	            zvonit:$('#zvonit').val(),
	            time:'с ' + $('#city').val() + ' до ' + $('#city20').val(),
	            mob:$('#mob3').val(),
	            net:$('#net3').val(),
	            tv:$('#tv3').val(),
	            dtel:$('#dtel3').val(),
	            adres:$('#adres').val(),
	            comments:$('#comments').val(),
	            haveMob:$('#haveMob').attr("checked") == "checked" ? '1' : '',
	            haveNet:$('#haveNet').attr("checked") == "checked" ? '1' : '',
	            haveTv:$('#haveTv').attr("checked") == "checked" ? '1' : '',
	            haveTel:$('#haveTel').attr("checked") == "checked" ? '1' : '',
	            haveOther:$('#haveOther').attr("checked") == "checked" ? '1' : '',
	            region:'77',
	            channel:'4' //константа: идентификатор канала послупления заявок "сайт"
	          };
	          (function (requestData) {
	            $.post(
	              '/pages/proxy.jsp',
	              requestData,
	              function (data) {
	                try {
	                  if (data == null || data == '')
	                    request_error("data is empty");
	                  else {
	                    var cont = eval("(" + data + ")");
	
	                    if (cont.isError == 0 && cont.callbackOrderNumber != 'null') {
	                      $("#kupit").animate({right:'-=' + half}, 500,
	                        function () {
	                          $("#kupit").hide();
	                          $("#kupit").css("width", "0px");
	                          $("#kupit").css("height", "0px");
	                          $('.spasibo').show();
	
	                          docH = $(window).height();
	                          var sp = $('.bmc').height();
	                          sp = (docH - sp) / 2;
	                          $('.bmc').css('margin', '' + sp + 'px auto auto auto');
	
	                        });
	                    } else {
	                      request_error("cont.isError: " + cont.isError + ', cont.callbackOrderNumber' + cont.callbackOrderNumber);
	                    }
	                  }
	                } catch (error) {
	                  request_error(error);
	                }
	              }
	            );
	          })(requestData);
	        } catch (error) {
	          request_error(error);
	        }
	
	      } else {
	        $(".alarm").css("border", "1px #990000 solid");
	        $(".error3").show();
	      }
	    }
	
	
	    $(".alarm").click(function () {
	      $(".alarm").css("border", "1px #fff solid");
	      $(".error3").hide();
	    });
	
	
	  });
	  $("#zvonit").select().keydown(function () {
	    $("#zvonit").addClass("hk_text");
	    $("#zvonit").removeClass("error1");
	    $(".error2").hide();
	  });
	
	
	  $(".modalCloseImg").click(function () { // скрывает 2 окно при клике по кнопке закрыть
	    $("#button_slide").show();
	    $('.spasibo').hide();
	    $('.send_error').hide();
	  });//
	  $(".btnz").click(function () { // скрывает 2 окно при клике по кнопке ок
	    $("#button_slide").show();
	    $('.spasibo').hide();
	    $('.send_error').hide();
	  });//
	  $(".spasibo").click(function () { // скрывает 2 окно при клике мимо формы
	    var chk_hover2 = $('.bmc').is(':hover');
	    var bm_hover = $('#basic-modal').is(':hover');
	    if (chk_hover2 == false) {
	      if (bm_hover == false) {
	        $('.spasibo').hide();
	        $("#button_slide").show();
	      }
	    }
	  });
	  $(".send_error").click(function () { // скрывает 2 окно при клике мимо формы
	    var chk_hover2 = $('.bmc').is(':hover');
	    var bm_hover = $('#basic-modal').is(':hover');
	    if (chk_hover2 == false) {
	      if (bm_hover == false) {
	        $('.send_error').hide();
	        $("#button_slide").show();
	      }
	    }
	  });
	
	
	  // чекбоксы
	  
	  $("#dtelImg,.dtel2,.dtel ").click(function () {
	    try {
	      if ($("#dtelImg").attr("src") == '/img/hk/p1.png') {
	        $("#dtelImg").attr({src:"/img/hk/p0.png"});
	        $("#dtel3").val("0");
	        delete_element("dtelImg", 'wtb');
	      }
	      else if ($("#dtelImg").attr("src") == '/img/hk/p0.png') {
	        $("#dtelImg").attr({src:"/img/hk/p1.png"});
	        $("#dtel3").val("1");
	        set_cookie("dtelImg", true, 'wtb');
									}
								} catch (e) {
									alert(e);
								}
							});
	
	  $("#netImg,.net2,.net ").click(function () {
	    try {
	      if ($("#netImg").attr("src") == '/img/hk/p1.png') {
	        $("#netImg").attr({src:"/img/hk/p0.png"});
	        $("#net3").val("0");
	        delete_element("netImg", 'wtb');
	      }
	      else if ($("#netImg").attr("src") == '/img/hk/p0.png') {
	        $("#netImg").attr({src:"/img/hk/p1.png"});
	        $("#net3").val("1");
	        set_cookie("netImg", true, 'wtb');
									}
								} catch (e) {
									alert(e);
								}
							});
	
	  $("#tvImg,.tv2,.tv ").click(function () {
	    try {
	      if ($("#tvImg").attr("src") == '/img/hk/p1.png') {
	        $("#tvImg").attr({src:"/img/hk/p0.png"});
	        $("#tv3").val("0");
	        delete_element("tvImg", 'wtb');
	      }
	      else if ($("#tvImg").attr("src") == '/img/hk/p0.png') {
	        $("#tvImg").attr({src:"/img/hk/p1.png"});
	        $("#tv3").val("1");
	        set_cookie("tvImg", true, 'wtb');
									}
								} catch (e) {
									alert(e);
								}
							});
	
	  $("#mobImg,.mob2,.mob ").click(function () {
	    try {
	      if ($("#mobImg").attr("src") == '/img/hk/p1.png') {
	        $("#mobImg").attr({src:"/img/hk/p0.png"});
	        $("#mob3").val("0");
	        delete_element("mobImg", 'wtb');
	      }
	      else if ($("#mobImg").attr("src") == '/img/hk/p0.png') {
	        $("#mobImg").attr({src:"/img/hk/p1.png"});
	        $("#mob3").val("1");
	        set_cookie("mobImg", true, 'wtb');
									}
								} catch (e) {
									alert(e);
								}
							});
	
		});
		function checkHave(q) {
			if ($('#' + q).attr('checked') == "checked") {
				delete_element(q, 'wtb');
			} else {
				set_cookie(q, true, 'wtb');
			}
			$('#' + q).click();
		}
</script>


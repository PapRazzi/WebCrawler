	$(document).ready(function(){
		$('ul.ukn-u-tab-nav li:first').addClass('ukn-active');
		$('ul.ukn-u-tab-nav li a').click(function(e) {
			e.preventDefault();
			var tabSelect = $(this).attr('href');
			$('.ukn-u-tab-nav li').removeClass('ukn-active');
			$(this).parent('li').addClass('ukn-active');
			$('.ukn-u-tab-content > li').removeClass('ukn-active').hide();
			$('.ukn-u-tab-content').find('#'+tabSelect).addClass('ukn-active').show();
		});
	});

  <div class="gc-social">
    
    <span class='signed-out'>
      <a href="/setup?state=%2F%2B%2Fplugins%2F%2B1button%2F" id="loginLink" class="button">Sign in</a>
    </span>
    
  </div>

  <form class="gc-search" action="/search/results" method="GET">
    <div class="searchbox">
      
      
      <input type="text" name="q" class="q" id="q" value="" placeholder='Search' autocomplete="off" />
    </div>
    <button class="button button-blue">
      <img src="/_static/images/search.png" alt="Search"/>
    </button>
    
  </form>

</header>


<script>
  var a=window.devsite||{};window.devsite=a;a.location=window.location;a.setTimeout=function(){return window.setTimeout.apply(window,arguments)};a.clearTimeout=function(){window.clearTimeout.apply(window,arguments)};a.localInit=function(){a.search.init()};a.search={KEY_ESC:27,fadeTime:a.KD_FADE_SHORT,keyDelay:300};
a.search.init=function(){a.search.initResultHandlers();var f=$(".gc-search");f.submit(function(){if(window.devsite.geo){var b=window.devsite.geo.lat+","+window.devsite.geo.lon,b=$('<input type="hidden" name="l"/>').val(b);f.append(b)}});var e=$("#q");e.unbind("keyup click");e.bind("keyup click",function(b){a.search.timeout&&a.clearTimeout(a.search.timeout);$(this).val()&&b.keyCode!=a.search.KEY_ESC?a.search.timeout=a.setTimeout(function(){a.search.updateResults()},a.search.keyDelay):a.search.removeAutocomplete()});
var e=$(".searchbox").children(".button"),d=$("#restrict input");(d.is(":checked")||0==d.length&&0!=e.length)&&a.search.grow();$("body").delegate("#restrict input","click",function(){$(this).is(":checked")?a.search.grow($("#restrict input").val()):a.search.shrink();a.search.updateResults();$("#q").focus()})};
a.search.initResultHandlers=function(){$(".search-results .result").unbind("click");$(".search-results .result").click(function(){var f={type:$(this).data("type"),url:$(this).data("url"),hl:$(this).data("hl"),q:$(this).data("q")};$.post("/search/go",f,function(e){e&&(a.location.href=e)});return!1})};a.search.fadeOutIfExists=function(f,e,d){f&&f.length?f.fadeOut(e,d):d()};a.search.removeAutocomplete=function(){var f=$(".search-autocomplete");f.remove();a.search.lastQ=null};
a.search.progressDot=function(f){function e(){c+=1;100>c&&b.animate({opacity:"toggle"},a.KD_FADE_LONG,e)}var d=6,b=f.parent().find(".progress");b.remove();b=$("<div></div>").addClass("progress");b.css({position:"absolute","border-radius":d+"px",border:d+"px solid #3D9400",right:d+"px",top:f.parent().height()/2-d+"px",width:0,height:0});f.parent().append(b);var c=0;return f={start:function(){$.fx.off||e()},clear:function(){b.remove()},error:function(){b.stop();b.css({opacity:1,border:d+"px solid #DD4B39"})}}};
a.search.grow=function(f){var e=$("#q"),d=$(".searchbox"),b=$(".gc-search").children("button"),c=d.children(".button");if(f){var c=e.val(),g=$(".results").data("query-match");g&&e.val(c.substr(g.length+1));c=$("<span></span>");c.addClass("button button-blue mini");c.text($("#restrict").find("span").text());c.data("query-match",g);d.prepend(c);g=$("<input />");g.attr({id:"search_project",type:"hidden",name:"p",value:f});$(".gc-search").append(g)}c.append($("<pre>  X</pre>"));g=$(".searchbox").width();
f=c.outerWidth(!0);var k=d.outerWidth(!0)+f;d.css("width",k+"px");e.css("width",g+"px");e=b.offset();e.left+=f+10;b.offset(e);b=function(){a.search.shrink();$("#q").focus()};c.click(b)};
a.search.shrink=function(){var f=$("#q"),e=$(".searchbox"),d=e.children("input"),b=e.children(".button"),c=b.data("query-match");c&&d.val($.trim(c)+" "+d.val());d=$(".gc-search").children("button");$("#restrict input").removeAttr("checked");$("#search_project").remove();c=b.outerWidth(!0);b.remove();b=d.offset();b.left-=c+10;e.css("width","");f.css("width","100%");d.offset(b)};
a.search.updateResults=function(){var f=$(".searchbox"),e=$("#q"),d=$("#search_project"),b={q:$.trim(e.val()||"")};window.devsite.geo&&(b.l=window.devsite.geo.lat+","+window.devsite.geo.lon);d&&d.val()&&(b.p=escape(d.val()));if(!a.search.lastQ||!(a.search.lastQ.q==b.q&&a.search.lastQ.p==b.p)){a.search.lastQ=b;a.search.xhr&&a.search.xhr.abort();var c=$(".search-results");b.n=c.data("n")||4;var g=a.search.progressDot(e);g.start();a.search.xhr=$.get("/search/results",b,function(d){g.clear();d&&($("#what-q").text(b.q),
c=$(".search-results"),a.search.fadeOutIfExists(c,a.search.fadeTime,function(){c.empty();if($.trim(d).length){if(!c.length){c=$("<div></div>").addClass("search-results search-autocomplete").data("n",4);$("body").append(c);var g=f.offset();c.css({position:"absolute",top:g.top+f.outerHeight()+3,left:g.left})}c.append(d);var g=$("#search-next"),h=c.children(".results").data("c");g.length?h?(g.attr("href","/search/results?c="+encodeURIComponent(h)+"&q="+encodeURIComponent(b.q)),g.show()):g.hide():$("#search-full").show();
$("body").bind("click.searchAutocomplete",function(b){!$.contains(c.get(0),b.target)&&(!c.is(b.target)&&!e.is(b.target))&&c.hasClass("search-autocomplete")&&(a.search.removeAutocomplete(),$(this).unbind("click.searchAutocomplete"))});a.search.initResultHandlers();c.fadeIn(a.search.fadeTime)}}))});a.search.xhr.error(function(){g.error()})}};

  devsite.search.init();

  if (window.jQuery) {
    $(document).ready(function() {
      var loginLink = $('#loginLink');
      if (loginLink.length > 0) {
        loginLink.click(devsite.devsite.login);
      }
    });
  }
</script>

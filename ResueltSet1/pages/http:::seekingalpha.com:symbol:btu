      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">BTU</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['btu']
      });
    })();

    try { SymbolData.handle_data('BTU', {"rt_time":"04:00PM ET,02/15","last":24.4,"high":25.01,"open":24.96,"close":25.01,"low":24.16,"volume":5939700,"volume_at":1361063450,"at":1361063450,"avg3vol":6529200.0,"c_share":2.41,"div_rate":0.34,"forward_pe":12.4,"payout":9.0,"trailing_pe":6.5,"div_yield":1.4,"div_grow_rate":8.23,"payout_5y":10.0,"div_yield_5y":0.9,"chgp_1w":2.8,"chgp_4w":-6.0,"chgp_52w":-32.5,"beta60":1.38,"beta36":1.75,"mov_avg_10d":24.1,"mov_avg_10w":25.76,"mov_avg_200d":24.65,"short_interest":1.6,"p_short_interest_float":4.7,"liquidity":83355500.0,"p_week_vol_shares":11.89,"sfloat":241340300.0,"shares_held":217524031.0,"p_shares_by_inst":81.1,"p_shares_by_insid":0.4,"net_insid_tr":172364.0,"insid_sb":199439.0,"insid_ss":27075.0,"total_revenue":7930900000.0,"net_income":957700000.0,"gross_margin":29.2,"eps":3.77,"roe":19.2,"roe_ind":94.6,"roa":3.7,"cur_ratio":1.5,"quick_ratio":0.7,"int_cover":3.6,"debt_eq":1.07,"lt_debt_cap":0.5,"debt_short":47800000.0,"debt_long":6205100000.0,"low52":18.78,"high52":37.0,"market_cap":6548616814.0,"cash":646810103.35,"debt":6252900000.0,"total_enterprise":12154706710.65,"price_book":1.1015801354401806,"ev_ebitda":5.692271208097223,"last_div":0.085,"last_div_date":"02/05/13","iop":0.0,"iopp":0.0,"price_sales":0.8257091646597485,"price_tangb":1.099099099099099,"ev_sales":1.532575963717863,"ev_fcf":-5.80617509682322,"inst_chg":11142209.0,"cf":1247994597.75,"fcf":-2093410293.0,"debt_fcf":-2.9869443275924508,"rt_ts":1360944000,"chg_vs_mkt_1w":3.0,"chg_vs_mkt_4w":-8.0,"chg_vs_mkt_52w":-40.0,"p_close_10w":-5.0,"p_close_200d":-1.0}) } catch (e) { }

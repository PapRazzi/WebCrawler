function ads_runSkin(){
	/* load the skin or the toggle skin (cookied) */
	if(typeof ads_togSkinCookie==="undefined" || ads_togSkinCookie==null){ads_skinFile = "http://i.i.com.com/cnwk.1d/Ads/942/10/Statefarm_CNET_Skin_2-11.jpg";}
	else{ads_skinFile = ads_togSkinCookie[1] + "skin" + ads_togSkinCookie[0] + ".jpg";}
	
	/*building style tag based upon site id*/
	if(typeof(ads_useExtIds)!="undefined"){
		if(ads_skin_masterPageWrapper.find(ads_siteId)){ ads_skinDiv="masterPageWrapper"; ads_skinStyles="#masterPageWrapper { background:"+ads_skinBkgClr+" url('"+ads_skinFile+"') no-repeat top center !important; }";}
		else if(ads_skin_mantleSkin.find(ads_siteId)){ ads_skinDiv="mantle_skin"; ads_skinStyles="#mantle_skin { background:"+ads_skinBkgClr+" url('"+ads_skinFile+"') no-repeat top center; }"; }
		else if(ads_skin_mantleSkinImp.find(ads_siteId)){ ads_skinDiv="mantle_skin"; ads_skinStyles="#mantle_skin { background:"+ads_skinBkgClr+" url('"+ads_skinFile+"') no-repeat top center !important; }"; }
		else if(ads_skin_rbSkin.find(ads_siteId)){ ads_skinDiv="rb_skin"; ads_skinStyles=".rb_skin { background:"+ads_skinBkgClr+" url('"+ads_skinFile+"') no-repeat top center; }";}
		else{ ads_skinStyles="body { background:"+ads_skinBkgClr+" url('"+ads_skinFile+"') no-repeat top center fixed; }"; }
	}
	
	/*building style tag for the head*/
	if(ads_inFIF){
		var ads_headTag=window.parent.document.getElementsByTagName("head")[0];
		if(window.parent.document.getElementById("ads_skinStyles")){var ads_skinStyleTag=window.parent.document.getElementById("ads_skinStyles");}
		else{var ads_skinStyleTag=window.parent.document.createElement("style");}
	}
	else{
		var ads_headTag=document.getElementsByTagName("head")[0];
		if(document.getElementById("ads_skinStyles")){var ads_skinStyleTag=document.getElementById("ads_skinStyles");}
		else{var ads_skinStyleTag=document.createElement("style");}
	}
	
	/*original background properties*/
	if(ads_inFIF){if(!window.parent.document.getElementById("ads_origSkin")){ads_buildOrigSkinTag();}}
	else{if(!document.getElementById("ads_origSkin")){ads_buildOrigSkinTag();}}
	function ads_buildOrigSkinTag(){
		if(ads_inFIF){var ads_origBkgTag=window.parent.document.createElement("script");}else{var ads_origBkgTag=document.createElement("script");}ads_origBkgTag.setAttribute("type","text/javascript"); ads_origBkgTag.setAttribute("id","ads_origSkin"); ads_headTag.appendChild(ads_origBkgTag);
		var ads_origBkgImageA = ads_getStyle(ads_skinDiv,"background-image","backgroundImage");if(ads_origBkgImageA){ads_origBkgImageA=ads_origBkgImageA.replace(/'/g,"\"");}
		ads_origBkgProperties="var ads_origBkgImage='"+ads_origBkgImageA+"';\n"; ads_origBkgProperties+="var ads_origBkgAttachment='"+ads_getStyle(ads_skinDiv,"background-attachment","backgroundAttachment")+"';\n";ads_origBkgProperties+="var ads_origBkgColor='"+ads_getStyle(ads_skinDiv,"background-color","backgroundColor")+"';\n";ads_origBkgProperties+="var ads_origBkgPosition='"+ads_getStyle(ads_skinDiv,"background-position","backgroundPosition")+"';\n";ads_origBkgProperties+="var ads_origBkgRepeat='"+ads_getStyle(ads_skinDiv,"background-repeat","backgroundRepeat")+"';";
		if(!window.ActiveXObject){ads_origBkgTag.text=ads_origBkgProperties;}else{ads_origBkgTag.text=ads_origBkgProperties;}
	}	
		
	/*creating style tag for skin styles*/
	ads_skinStyleTag.setAttribute("type","text/css"); ads_skinStyleTag.setAttribute("id","ads_skinStyles"); ads_headTag.appendChild(ads_skinStyleTag);
	if(!window.ActiveXObject){ads_skinStyleTag.innerHTML=ads_skinStyles;}else{ads_skinStyleTag.styleSheet.cssText=ads_skinStyles;}
		
	/*skin clicks*/
	if(ads_skinClickURL){ads_addEventListener(window,"load",ads_buildSkinClicks);}else{ads_addEventListener(window,"load",ads_clearSkinClicks);}
}
function ads_buildSkinClicks(){if(ads_inFIF){var elem=window.parent.document.getElementsByTagName("*");}else{var elem=document.getElementsByTagName("*");}for(var i=0;i<elem.length;i++){if(elem[i].className=="skinClick"){ads_skinClickEls.push(elem[i]);}}for(var i in ads_skinClickEls){ads_skinClickEls[i].innerHTML='<a href="'+ads_skinClickURL+'" target="_blank" rel="nofollow" style="width:100%; height:100%; display:block;"></a>';}}
function ads_clearSkinClicks(){if(ads_inFIF){var elem=window.parent.document.getElementsByTagName("*");}else{var elem=document.getElementsByTagName("*");}for(var i=0;i<elem.length;i++){if(elem[i].className=="skinClick"){ads_skinClickEls.push(elem[i]);}}for(var i in ads_skinClickEls){ads_skinClickEls[i].innerHTML='';}}
function ads_readCookie(name){var nameEQ=name+"=";if(ads_inFIF){var ca=window.parent.document.cookie.split(";");}else{var ca=document.cookie.split(";");}for(var i=0;i<ca.length;i++){var c=ca[i];while(c.charAt(0)==" ")c=c.substring(1,c.length);if(c.indexOf(nameEQ)==0){var v=c.substring(nameEQ.length,c.length);var va=v.split("|");return va;}}return null;}
function ads_addEventListener(obj,type,fn){if(obj.addEventListener){obj.addEventListener(type,fn,false);}else if(obj.attachEvent){obj["e"+type+fn]=fn;obj[type+fn]=function(){obj["e"+type+fn](window.event);};obj.attachEvent("on"+type,obj[type+fn]);}}
function ads_getStyle(el,styleProp,ieStyleProp){var y="";if(ads_inFIF){if(el!="body"){var x=window.parent.document.getElementById(el);if(x!=null){if(x.currentStyle){var y=x.currentStyle[ieStyleProp];}else if(window.getComputedStyle){var y=window.parent.document.defaultView.getComputedStyle(x,null).getPropertyValue(styleProp);}}}else{if(window.parent.document.body.currentStyle){var y=window.parent.document.body.currentStyle[ieStyleProp];}else if(parent.window.getComputedStyle){var y=window.parent.document.defaultView.getComputedStyle(document.body,null).getPropertyValue(styleProp);}}}else{if(el!="body"){var x=document.getElementById(el);if(x!=null){if(x.currentStyle){var y=x.currentStyle[ieStyleProp];}else if(window.getComputedStyle){var y=document.defaultView.getComputedStyle(x,null).getPropertyValue(styleProp);}}}else{if(document.body.currentStyle){var y=document.body.currentStyle[ieStyleProp];}else if(window.getComputedStyle){var y=document.defaultView.getComputedStyle(document.body,null).getPropertyValue(styleProp);}}}return y;}
</script>

	if(document.getElementById('rtcol') != null){
		document.getElementById('rtcol').style.position="relative";
		document.getElementById('rtcol').style.width="191px";
		document.getElementById('rtcol').style.backgroundColor="#fff";
		document.getElementById('rtcol').style.left="";
		document.getElementById('rtcol').style.cssFloat="left";
		document.getElementById('rtcol').style.styleFloat="left";
	}

	//if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
	//	var ffversion=new Number(RegExp.$1) // capture x.x portion and store as a number
	//	if (ffversion>=3){
	//		if(document.getElementById('quoteHeader'))document.getElementById('quoteHeader').style.height="3200px";
	//	}
	//}

}

//WSJ - TRowe - Specific Page Centering Logic
if(window.location.href.indexOf('jsc/article') == -1 && (window.location.href.indexOf('trowe') != -1 || pID == '8_0001')){

	var tableTags = document.getElementsByTagName('table');
	for(var qa=0; qa<=tableTags.length; qa++){
	 if(tableTags[qa] && tableTags[qa].width == "100%"){
	  if(tableTags[qa].parentNode && tableTags[qa].parentNode.width != "50%"){
		  tableTags[qa].style.backgroundColor = 'white';
		  tableTags[qa].style.margin = '0px auto';
		  tableTags[qa].style.width = '990px';
	  }else{
	  	  tableTags[qa].parentNode.style.paddingLeft="10px";
		  break;
	  }

	 }
	}

	var isIEChk = ((dojo.isIE) ? true : false);

	var imgTags = document.getElementsByTagName('img');

	if(!isIEChk){

	for(var zz=0; zz<=imgTags.length; zz++){
	 if(imgTags[zz].width == "15"){
	  imgTags[zz].style.width = '0px';
	  break;
	 }
	}

	}else{

	for(var zz=0; zz<=imgTags.length; zz++){
	 if(imgTags[zz]){
		 if(imgTags[zz].src.indexOf('b.gif') != -1){
			  imgTags[zz].style.width = '0px';
		  }
	 }
	}


	for(var zz=0; zz<=divTags.length; zz++){

		 if(divTags[zz] && divTags[zz].style.width == "14px"){
				  divTags[zz].style.width = '10px';
				  break;
		 }

	}

	}
}


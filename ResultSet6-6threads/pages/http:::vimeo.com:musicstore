<script id="pagination_tmpl" type="text/html">
    <ol>
    <% for (var i = 0; i < maxPages; i++) { %>
        <% if (i == 0 && curPage > 1) { %>
            <li class="prev"><a href="#music_data">Prev</a></li>
        <% } %>

        <% if (startIndex != 0 && i == 2) { %>
            <li class="dots">&hellip;</li>
        <% } %>

        <% if (i == 0 || ((startIndex != 0 && i < startIndex + 4 && i >= startIndex - 5) || (startIndex == 0 && i < 10)) || i == maxPages -1) { %>
            <li data-index="<%= i %>">
                <a href="#music_data" class="<%= curPage == i+1 ? 'selected' : '' %>">
                    <%= i+1 %>
                </a>
            </li>
        <% } %>

        <% if ((startIndex != 0 && i == maxPages - 2) || (startIndex == 0 && i == 10 && maxPages > 10)) { %>
            <li class="dots">&hellip;</li>
        <% } %>

        <% if (i == maxPages - 1 && curPage <= maxPages - 1) { %>
            <li class="next"><a href="#music_data">Next</a></li>
        <% } %>
    <% } %>
    </ol>
</script>


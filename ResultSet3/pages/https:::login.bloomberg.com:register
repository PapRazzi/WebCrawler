<script type="text/javascript">
        var pageTrackingVariables = pageTrackingVariables || [];
    var bb_unescape_str = (typeof decodeURIComponent != "undefined" ? decodeURIComponent : unescape);
    var bb_userid = BLOOMBERG.user.cookie.getCookieValue('bdfpc');
    var bb_bregid = bb_unescape_str(BLOOMBERG.user.cookie.getCookieValue('breg')).split(":")[0];
    var alerts_cmpid = BLOOMBERG.user.alerts.getCampaignId();
    var alerts_widgets_bucket = BLOOMBERG.user.alerts.getModulePlacementBucket();

    pageTrackingVariables.push({name:'bb_userid',    value:bb_userid,               scope:1});
    pageTrackingVariables.push({name:'bb_bregid',    value:bb_bregid,               scope:1});
    pageTrackingVariables.push({name:'alerts_widgets_bucket',    value:alerts_widgets_bucket,  scope:1});
    pageTrackingVariables.push({name:'al_cmpid',     value:alerts_cmpid,            scope:3});
</script>

    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount',"UA-11413116-30"]);
        for(var i=0;i<pageTrackingVariables.length;i++){
                    if (i >= 5){ break; }
            _gaq.push(['_setCustomVar',  i+1,  pageTrackingVariables[i].name, pageTrackingVariables[i].value,  pageTrackingVariables[i].scope]);
        }
        _gaq.push(['_trackPageview']);
        (function() {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>

<script type="text/javascript">
    if (BLOOMBERG && BLOOMBERG.user && BLOOMBERG.user.comscore){
        var comScoreTrackingParams = {};
        for(var i=0;i<pageTrackingVariables.length; i++){
            comScoreTrackingParams[pageTrackingVariables[i].name] = pageTrackingVariables[i].value;
        }
        BLOOMBERG.user.comscore.track(comScoreTrackingParams);
    }
    $("select option").each(function(){
        if ($(this).text().length > 0){
            $(this).attr("title", $(this).text());
        }
    });
</script>

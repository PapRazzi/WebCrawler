            PageVars.data.sections = "reviews|Laptops";
            PageVars.data.page_type = 'category index';
            PageVars.data.page_type_id = '2001';
        </script> <!-- End SiteCatalyst --> </div> <!--[if lt IE 9]> <noscript><div id="rbFooter" section="ftr" class="footerTag"></noscript>

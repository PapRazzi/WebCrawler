    <script>
    $(document).ready(function(){
        $("a[id='note_tooltip']").each(function(){
            $(this).tooltip();
        });
    })
    Douban.init_rss_edit = function(o){
        $('#show_b').show();
        $('#show_b').click(function(){
            $('#form_rss').show();
            $(this).hide();
            return false;
        });
        $('#cancel_b').click(function(){
            $('#form_rss').hide();
            $('#show_b').show();
            return false;
        });
        $('#form_rss').submit(function(){
            var d = {
                'uid':$('#uid').attr('value'),
                'rss_type':$('input:checked').attr('value')
                };
            $.post_withck('/j/note/rss_edit', d, function(r){
                var r = eval('('+r+')');
                if (r.ok){
                    $('#type_word').html(r.type);
                }
                $('#form_rss').hide();
                $('#show_b').show();
            });
            return false;
        });
    }
</script>


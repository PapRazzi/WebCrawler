						function validQuery()
						{
							var theQuery = document.searchForm.q.value;
							theQuery = theQuery.replace( /[^a-zA-Z0-9]/g, "" );
							if(theQuery.length > 0){
								return true;
							}else{
								alert('Please enter keywords to search.');
								document.searchForm.q.focus();
								return false;
							}
						}
					
</script>

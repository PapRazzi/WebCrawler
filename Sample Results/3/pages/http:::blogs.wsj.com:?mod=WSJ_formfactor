	  var tracking;
	  var proomni = new dj.blogs.util.omni(function(){ 
      tracking = new dj.blogs.Tracking(this);
      tracking.blogName    = 'Wsjblogsblog';
      tracking.title       = 'home';
      tracking.section     = 'Blogs Main';
      tracking.subsection  = 'Blogs Main';
      tracking.ad_zone     = 'blog_';
      tracking.publishTime = '2009-07-22 20:38';
      tracking.pageType    = 'home';
      tracking.articlePrivilege = '';
      tracking.blogPrivilege    = 'sub';
      tracking.articleAuthor = '';
      tracking.articleID = '';
      tracking.blogLanguage = 'english';


      tracking.firePixel();   });
</script>

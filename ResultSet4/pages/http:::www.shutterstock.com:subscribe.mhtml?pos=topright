<script type="text/javascript">


$$('.product-container,.license-group-container,.continue-button,.gray-box,.rounded-box-container').each( function(element) {
	var dimensions = element.getDimensions();

	$w('width height').each( function(dimension) {
		if (dimensions[dimension] % 2 != 0) {
			
			element.childElements().each( function(descendant) {

				if (dimension == 'width' && descendant.getStyle('right') == '-1px') {
					descendant.style.right = '-2px';

				} else if (dimension == 'height' && descendant.getStyle('bottom') == '-1px') {
					descendant.style.bottom = '-2px';
				}
			} );
		}
	} );
} );
</script>



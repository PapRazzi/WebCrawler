function navDoAction(email) {
	var EM = email;
	var NewsletterKey = document.getElementById('navNewsletterSignup').value;
	var navRegistrationWebsite = document.getElementById('navRegistrationWebsite').value;
	var businessUnit = document.getElementById('businessUnit').value; 
	var NewsletterSignup = document.getElementById('NewsletterSignup').value;  

	var datavars = "NewsletterKey="+ NewsletterKey + "&RegistrationWebsite="+ navRegistrationWebsite + "&EM=" + EM + "&businessUnit="+businessUnit+"&NewsletterSignup="+NewsletterSignup;

	
	jQuery.post("/subscribenewsletter",
					{
						NewsletterKey : NewsletterKey,
						RegistrationWebsite : navRegistrationWebsite,
						businessUnit : businessUnit,
						NewsletterSignup: NewsletterSignup,
						EM : EM
					},
					function(data) {
						jQuery("#nl_response_message").show();
						jQuery("#nl_response_message").html(data);
						//jQuery("#navnewslettermailboxID").hide();
						jQuery("#navnewslettermailboxID").remove(); 
					});
}

function navnews_textclick() {
	var objTXT = document.getElementById('navnewsletteremailID');
	var objErrTXT = document.getElementById('navIDnewsletterbox_error');
	var objContentBox = document.getElementById('navnewemailbox_content');
	var empty_string = /^\s*$/; //create RegExp object for re-use

	//navnewemailbox_content  navnewslettermailboxID
	if (empty_string.test(objTXT.value))
	{
		objErrTXT.innerHTML='';
		objErrTXT.innerHTML="* Enter Email Address";
		objTXT.value='';
		objTXT.focus();
	}
	else if(validate(objTXT.value))
	{
		objErrTXT.innerHTML='';
		objErrTXT.innerHTML="* Invalid Email Address";
		objTXT.value='';
		objTXT.focus();
	} else {
		objErrTXT.style.display='none';
		navDoAction(objTXT.value);
	}
     //return false;
}
function navnews_mailclick() {
    document.getElementById('navnewsletteremailID').value='';
}
function validate(email) {
    var reg = /^([A-Za-z0-9_\-\.]{2,})+\@([A-Za-z0-9_\-\.]{2,})+\.([A-Za-z]{2,4})$/;
    if(reg.test(email) == false)
       return true;
    else
         return false;
}
function navshowtext(){
	if(document.getElementById('navnewsletteremailID').value == 'Email address'){
		document.getElementById('navnewsletteremailID').value='';
	}
}

function keypress(e){
	if(e.which == 13){
		navnews_textclick();
		return false;
	}
}

</script>

      document.getElementById('amount').value = '';


      //auto clear amount
      setInterval(function(){
      MarketWatch.Investing.Converter.Convert(
      $('#converter select[name=fromCurrency] option:selected').val(),
      $('#converter select[name=toCurrency] option:selected').val(),
      $('#converter input').val(), 0);

      }, 5000);


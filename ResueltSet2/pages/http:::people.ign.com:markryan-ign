<script type="text/javascript">
$(".initialComment").live("focus",function() {
    if($(this).css("height")=="15px") {
        $(this).css("height","45px");
        $(this).val("");
    }
});

$(".initialComment").live("keyup",function() {
    maxHeight = 50;
    minHeight = 2;
    var myActivityId = "";
    var formIdAddition = "";
    if(myActivityId=="") {
        myActivityId = this.id.toString().replace("myComment", "");
        formIdAddition = myActivityId;
    }
    if($(this).val()=="") {
        $("#btnMakeComment"+formIdAddition).addClass("btnDisabled");
        $("#btnMakeComment"+formIdAddition).removeClass("btnEnabled");
        $("#myComment"+formIdAddition).removeClass("initialCommentTyping");
    } else {
        $("#btnMakeComment"+formIdAddition).removeClass("btnDisabled");
        $("#btnMakeComment"+formIdAddition).addClass("btnEnabled");
        $("#myComment"+formIdAddition).addClass("initialCommentTyping");
    }
});
$(".btnMakeComment").live("click",function() {

    var myActivityId = "";
    var formIdAddition = "";
    var destinationUrlPrefix = "activity";
    var inActivityStream = false;
    if(myActivityId=="") {
        inActivityStream = true;
        myActivityId = this.id.toString().replace("btnMakeComment", "");
        formIdAddition = myActivityId;
    }
    if ($("#btnMakeComment"+formIdAddition).hasClass("btnDisabled"))
    {
        return;
    }

    textEntered = $("#myComment"+formIdAddition).val();
    if(textEntered.toString().length>1024) {
        alert('Too much text, you can enter up to 1,024 characters');
        return false;
    }
    if(textEntered!="Write a reply..." && textEntered!="") {
        if(window.location.toString().indexOf("/activity?id=")==-1) {
            destinationUrlPrefix = window.location.toString()+"/activity";
        }
        if(window.location.toString().indexOf("my.ign.com")>-1) {
            destinationUrlPrefix = "/activity";
        }
        $("#myComment"+formIdAddition).css("background-color","#EAEAEA");
        $("#btnMakeComment"+formIdAddition).html("Replying...");
        $("#btnMakeComment"+formIdAddition).addClass("btnDisabled");
        $("#btnMakeComment"+formIdAddition).removeClass("btnEnabled");
        $.post(destinationUrlPrefix+"/postcomment", { id: myActivityId, comment: encodeURIComponent($("#myComment"+formIdAddition).val()) },
            function(data) {

              if(data.match(/Bad activity id/gi)){
                $("#commentsOn"+myActivityId).find('#commentList').slideUp('fast');
                $("#commentsOn"+myActivityId).append('<div style="margin-left:10px;padding:3px;border:1px solid #f40;background:#fa0">Oops! There was an error please try again later.<\/div>');
                return;
              }

                //refresh the target area
                if(inActivityStream) {
                    $.get(destinationUrlPrefix+"/justcomments?id="+myActivityId+"&ownerid="+1088083+"&userid="+0, function(data) {
                        $("#myComment"+formIdAddition).val("");
                        $("#commentsOn"+myActivityId).html(data);
                        $("#btnMakeComment"+formIdAddition).html("Reply");
                    });
                } else {
                    $.get(destinationUrlPrefix+"/allcomments?id="+myActivityId+"&ownerid="+1088083+"&userid="+0, function(data) {
                        $(".arrowIconz").remove();
                        $("#myComment").val("");
                        $("#commentList").html(data);
                        $("#btnMakeComment").html("Reply");
                    });
                }

                ign.social.drones.fireEvent('Social Comment', '0', '1088083');

            }
        );
    }
});
</script>    <div class="leftButtonEdge"></div>
    <p id="moreUpdates">More updates</p>

    <div class="rightButtonEdge"></div>
</div>

<script type="text/javascript">
    var increment = parseInt(20) + 1;

    var startIndex = parseInt(63) + 1;
    /*if(increment>displayTotal) {
        startIndex = displayTotal;
    }*/
    var destinationPath;
    var currentType = "all";

    $("#moreUpdates").click(function () {
        _gaq.push(['_trackPageview', '/home']);
        $("#moreUpdates").css("display", "none");
        $(".leftButtonEdge").css("display", "none");
        $(".rightButtonEdge").css("display", "none");
        //maybe keep track of total results and decide to display more based on that
        //have to keep track of type as well
        /*    $.getJSON("http://"+document.domain+"/proxy?dest="+endpointBase+"activities/"+pageOwnerId+"/@self%3FsortOrder%3Dascending%26startIndex%3D"+startIndex+"%26count%3D11",function(json) {
        $.each(json.entry, function(i,item){
            if ( i == increment - 1 ) {
                $("#moreUpdates").css("display","block");
                $(".leftButtonEdge").css("display","block");
                $(".rightButtonEdge").css("display","block");
                return false;
            }
            ign.social.activityItem.create(startIndex,item.actorLinks[0].uri, item.actorLinks[1].uri, item.actorTitle,item.activityObjects[0].objectTitle,item.created);
            startIndex++;
        });
    }); */
        //going to call getactivities instead
        if (location.pathname.toString().indexOf("/home") == 0) {
            destinationPath = location.pathname;
        } else {
            destinationPath = location.pathname + "/profile";
        }
        ign.social.generic.error(location.pathname);
        $.get("http://" + location.host + destinationPath + "/getactivities?start=" + startIndex + "&type=" + currentType, function (data) {
            $("#activityList").append(data);
            startIndex = startIndex + increment;
            if (isMore) {
                $("#moreUpdates").css("display", "block");
                $(".leftButtonEdge").css("display", "block");
                $(".rightButtonEdge").css("display", "block");
            }
        });
    });
    $("document").ready(function () {
        $(".showall").click(function () {
            currentType = "all";
            _gaq.push(['_trackPageview', '/home']);
            $(".showgames").removeClass("activitySortSelected");
            $(".showpeople").removeClass("activitySortSelected");
            $(".showblog").removeClass("activitySortSelected");
            $(".showall").addClass("activitySortSelected");
            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=all", function (data) {
                $("#activityList").html(data);
            });
        });
        $(".showgames").click(function () {
            currentType = "MEDIA_ITEM";
            _gaq.push(['_trackPageview', '/home']);
            $(".showall").removeClass("activitySortSelected");
            $(".showpeople").removeClass("activitySortSelected");
            $(".showblog").removeClass("activitySortSelected");
            $(".showgames").addClass("activitySortSelected");
            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=MEDIA_ITEM", function (data) {
                $("#activityList").html(data);
                if (data.indexOf("yougotnofriendssucka") > -1) {
                    $("#moreUpdates").css("display", "none");
                    $(".leftButtonEdge").css("display", "none");
                    $(".rightButtonEdge").css("display", "none");
                }
            });
        });
        $(".showpeople").click(function () {
            currentType = "PERSON";
            _gaq.push(['_trackPageview', '/home']);
            $(".showgames").removeClass("activitySortSelected");
            $(".showall").removeClass("activitySortSelected");
            $(".showblog").removeClass("activitySortSelected");
            $(".showpeople").addClass("activitySortSelected");
            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=PERSON", function (data) {
                $("#activityList").html(data);
                if (data.indexOf("yougotnofriendssucka") > -1) {
                    $("#moreUpdates").css("display", "none");
                    $(".leftButtonEdge").css("display", "none");
                    $(".rightButtonEdge").css("display", "none");
                }
            });
            //$.getJSON("http://"+document.domain+"/proxy?dest="+endpointBase+"activities/"+pageOwnerId+"/@self%3FfilterBy%3DactorType%26filterValue%3DPERSON%26sortOrder%3Dascending%26startIndex%3D0%26count%3D11",function(json) {
        });
        $(".showblog").click(function () {
            currentType = "BLOG_ENTRY";
            _gaq.push(['_trackPageview', '/home']);
            $(".showgames").removeClass("activitySortSelected");
            $(".showall").removeClass("activitySortSelected");
            $(".showpeople").removeClass("activitySortSelected");
            $(".showblog").addClass("activitySortSelected");

            $.get("http://" + location.host + location.pathname + "/getactivities?start=0&type=BLOG_ENTRY", function (data) {
                $("#activityList").html(data);
                if (data.indexOf("yougotnofriendssucka") > -1) {
                    $("#moreUpdates").css("display", "none");

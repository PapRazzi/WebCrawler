  <div class="content"><b>JUMP TO: </b>
<script language="Javascript">
$(function() {
    $("#month-submit").hide();
    $("#blog-submit").hide();
    $("#blog-changer select").change(function() {
        window.location = $("#blog-changer select option:selected").val();
    })
    $("#month-changer select").change(function() {
        window.location = $("#month-changer select option:selected").val();
    })
});
function writeMonthOptions()
{
    var theDate = new Date();
    var currYear = 2013;
    var currYear = theDate.getFullYear();    
    var endYear = 2008;
    var currMonth = theDate.getMonth();
    if (currMonth == 0) { currMonth = 1; } 
    cal_months = [' ', 'January', 'February', 'March', 'April', 'May', 'June', 'July', 'August','September', 'October', 'November', 'December'];
    for (yearCounter = currYear; yearCounter > endYear; yearCounter--)
    {
        for (monthCounter = currMonth; monthCounter > 0; monthCounter--) {
            if (monthCounter < 10) { monthTmp = '0'; } else { monthTmp = ''; } 
            document.write('<option value="/blog/' + yearCounter + '/' + monthTmp + monthCounter + '">' + cal_months[monthCounter] + ' ' + yearCounter + '</option>');
        }
        currMonth = 12;
    }
}
</script>
<form id="month-changer" action="" method="post">
    <select id="blog-jump-nav" name="month-nav">
            <script language="javascript">
                document.write('<option>A Specific Month</option>');
                writeMonthOptions();
                document.write('<input type="submit" value="Go" id="month-submit" />');
            </script>
    </select>
</form>

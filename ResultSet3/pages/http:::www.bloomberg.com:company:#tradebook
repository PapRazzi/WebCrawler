    <script type="text/javascript">
		/**
		 * Google Feed API
		 */
		feedsLoaded = false;
		google.load("feeds", "1");
		google.setOnLoadCallback(initFeeds);
		function initFeeds() {
			feedsLoaded = true;
		}
		/**
		 * Document Load
		 */
		$(document).ready(function () {
			marketTimeline.init();
			metricPerSec.init();
			// init our app
			var google_to = 0;
			var apploader = setInterval(function(){
				if(feedsLoaded || google_to==50){
					window.app = AppController.load();
					clearInterval(apploader);
				}
				// timeout for google feed api
				google_to++;
			}, 100);
		});
	</script>


  <script type="text/javascript">
    function updateCommentCount(count) {
      $("#sharetools .bubblecaption a").empty().text(count).append(
          $("<span/>", { text: count == 1 ? " Comment" : " Comments"})
      );
      $("div.skip-to-comments .comment-count").empty().html("This Story has <span>" + count + "</span>" + (count == 1 ? " Comment" : " Comments"));
        if (count == 1) {
            $("div.skip-to-comments a.skip-to-comment-link").empty().html("Join the conversation");
        }
        else if (count > 1) {
            $("div.skip-to-comments div a.skip-to-comment-link").empty().html("Read them &amp; join the conversation");
        }
    }
    
    function trackCommentShare(data) {
        var s = s_gi('djglobal,djmarketwatch');
        s.prop45 = 'ugc_livefyrecommentshared';
        void(s.tl(true, 'o', 'ugc_livefyrecommentshared'));
    }


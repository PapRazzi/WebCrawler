                $(document).ready(function () {
                    $("#city-sel-btn").fancybox();
                });
            </script>
            <input id="selected_region" type="hidden" name="selected_region">

            <div id="region_list" style="height: 0"></div>
            <script type="text/javascript">
                var on_select_region_callback_func;

                var init_region_form = function(container, region_list, callback) {
	                if (region_list != undefined && region_list.length > 0){
		                $('a[id*=link_]').each(function(){
			                var str = this.id.replace('link_', '');
			                if (jQuery.inArray(str, region_list) >= 0) {
				                $(this).show();
				                $('#lk_'+str).hide();
			                } else {
				                $(this).hide();
				                $('#lk_'+str).show();
			                }

		                })
	                }
	                on_select_region_callback_func = callback;
                }

                init_region_form('region_list', []);
            </script>
            
            <form action="/search" name="qseach" id="qseach" onsubmit="return checkSearch();">
                <input name="request_id" type="hidden" value="0"> <input name="page" type="hidden" value="1"> <input
                    type="text" name="text" class="string" placeholder="Поиск" value="Поиск"
                    onfocus="javascript: if (this.value == 'Поиск') this.value = '';">
                <button>
                    <b class="visuallyhidden">Найти</b>
                </button>
            </form>

            <script type="text/javascript">
                function checkSearch() {
                    var tt = document.qseach.text;
                    if (tt.value == 'Поиск')
                        tt.value = '';
                    if (tt.value.replace(/^\s+/g, "") == "") {
                        alert("Укажите запрос для поиска");
                        return false;
                    }
                    return true;
                }
            </script>


<script type="text/javascript">
    (function(){
        function adjust_overlay_position(field_obj) {
            var overlay_id = field_obj.attr("data-overlay-id");
            var arrow = $("#" + overlay_id + " .popup_arrow");
            var overlay = $("#" + overlay_id);
            var arrow_width = arrow.outerWidth(false);
            var arrow_height = arrow.outerHeight(false);
            arrow.css("left", -arrow_width);
            arrow.css("top", (overlay.outerHeight(false) - arrow_height)/2);
            overlay.
                    css("left", field_obj.offset().left + field_obj.outerWidth(false) + arrow_width).
                    css("top", field_obj.offset().top + (field_obj.outerHeight(false) - overlay.outerHeight(false))/2);
        }
        $("#user_email, #user_password").focus(function () {
            var overlay_id = $(this).attr("data-overlay-id");
            var overlay = $("#" + overlay_id);
            adjust_overlay_position($(this));
            overlay.show();
        });
        var hide_overlay_timers = {};
        $("#user_email, #user_password").blur(function () {
            var overlay_id = $(this).attr("data-overlay-id");
            if (hide_overlay_timers[$(this).css("id")]){
                clearTimeout(hide_overlay_timers[$(this).css("id")]);
            }
            hide_overlay_timers[$(this).css("id")] = setTimeout(function(){
                $("#" + overlay_id).hide();
            }, 250);
        });

        var config = {
            'newsletter-share-the-view' : 'Bloomberg View',
            'newsletter-politics' : 'Political Capital'
        };
        function bb_show_message(){
            var email_field = $("#user_email");
            var field_container = email_field.closest(".field");
            var email = email_field.val();
            if (email !== undefined && email.length > 0) {
                $.ajax({
                    url: '/user/lists/' + email,
                    success: function(data) {
                        if (data.length > 0 && config[data[0]] !== undefined) {
                            field_container.addClass("with_info");
                            var content = "You provided this email when you signed up for the " + config[data[0]] + " newsletter. Complete your registration for access to additional features."
                            field_container.find(".info_content").html(content);
                        } else {
                            field_container.removeClass("with_info");
                        }
                        adjust_overlay_position($("#user_password"));
                    },
                    error: function(data) {
                        field_container.removeClass("with_info");
                        adjust_overlay_position($("#user_password"));
                    }
                });
            } else {
                field_container.removeClass("with_info");
                adjust_overlay_position($("#user_password"));
            }
        }
        $(function(){
            bb_show_message();
        });
        $("#user_email").blur(function(){
            bb_show_message();

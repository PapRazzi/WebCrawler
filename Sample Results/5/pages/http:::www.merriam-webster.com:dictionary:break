<script type="text/javascript">
// <--
// This function performs a reference search.
function doReferenceSearch (the_form)
	{	document.location = '/' + the_form.ref.value + '/' + the_form.word.value;
		return false;
	};

// This function performs a reference switch.
function doReferenceSwitch (ref)
        {       //var search_box = document.getElementById('search_box_terms');
                //search_box.autocomplete.cachedResponse = [];
                //search_box.focus();
				if ( document.getElementById('search_box_terms').value.replace(/\s+/, '') != '' )
					{	document.location = '/' + document.getElementById('ref').value + '/' + encodeURIComponent(document.getElementById('search_box_terms').value);
					};
                return false;
        };

// Set it up.
jQuery(function()
	{	// This callback handles all suggestion clicks.
		var onAutocompleteSelect = function(value, data)
			{	// $('#selection').html('<img src="\/global\/flags\/small\/' + data + '.png" alt="" \/> ' + value);
				// alert(data);
				document.location = "/" + document.getElementById('ref').value + "/" + value;
			};

		// Autocomplete options.
		var options =
			{	serviceUrl: '/autocomplete',
				width: 434,
				delimiter: /(,|;)\s*/,
				onSelect: onAutocompleteSelect,
				deferRequestBy: 0, //miliseconds
				params: { ref: function() { var ids= {'dictionary' : 1, 'thesaurus' : 2, 'spanish' : 3, 'medical' : 4, 'concise' : 5 }; return ids[document.getElementById('ref').value] } },
				noCache: false // set to true, to disable caching
			};

		// Set up the autocomplete field.
		a1 = $('#search_box_terms').autocomplete(options);				
		//document.getElementById('search_box_terms').autocomplete = a1;
	});

// -->
</script>
					<div class="definition">
					<script type="text/javascript">

// This method updates the status of the saved words display.
function updateMySavedWordsDisplay (data)
	{	if ( typeof(FB) != 'undefined' && typeof(FB.curr_user) !== 'undefined' && FB.curr_user.access_token != null )
			{	var script	= document.createElement('script');
				script.type	= 'text/javascript';
				script.src	= 'http://stats.merriam-webster.com/my-saved-words/app.php?action=words.in_list&word=/dictionary/break[1]&success=onUpdateMySavedWords&failure=onMySavedWordsError&uid=' + escape(FB.curr_user.uid) + '&access_token=' + escape(FB.curr_user.access_token);
				document.getElementById('my-saved-words').appendChild(script);		
			}
		else
			{	updateMySavedWords('my-saved-words', -1, '');
			};
	};

// This function handles the action of adding a word
function onAddedToMySavedWords (data)
	{	updateMySavedWords('my-saved-words', 1, data.content.word);
	};

// This function handles words which couldn't be savd.
function onMySavedWordsAddError (data)
	{	alert("Sorry, your word wasn't saved. Please try reloading this page and saving the word again.");		
	};

// This function handles all My Saved Words error messages.
function onMySavedWordsError (data)
	{	updateMySavedWords('my-saved-words', -1, data.content.word);
	};

// This function updates the My Saved Words content.
function onUpdateMySavedWords (data)
	{	updateMySavedWords('my-saved-words', data.social_id == 'null' ? -1 : (data.content.exists == 1 ? 1 : 0), data.content.word);		
	};

// This function saves a word to the My Saved Words list.
function saveToMySavedWords ()
	{	var node	= document.createElement('script');
		node.type	= 'text/javascript';
		node.src	= 'http://stats.merriam-webster.com/my-saved-words/app.php?action=words.save&word=/dictionary/break[1]&success=onAddedToMySavedWords&failure=onMySavedWordsAddError&uid=' + escape(FB.curr_user.uid) + '&access_token=' + escape(FB.curr_user.access_token);		
		//alert(node.src);
		document.getElementById('my-saved-words').appendChild(node);					

		// Make the cookie "permanent."
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + (400 * 40));
		var c_value=escape('yes') + "; expires="+exdate.toUTCString();
		document.cookie='my_saved_words_seen' + "=" + c_value;

		// Track the click.
		var url = 'http://www.merriam-webster.com/my-saved-words/dictionary/starclick/save/break[1]';
		_gaq.push(['_trackPageview', url]);

		return;
	};

// This function saves a word to the My Saved Words list.
function deleteFromMySavedWords (container, word)
	{	if ( FB.curr_user.uid == null ) return;
		var node	= document.createElement('script');
		node.type	= 'text/javascript';
		node.src	= 'http://stats.merriam-webster.com/my-saved-words/app.php?action=words.delete&word[]=' + word + '&success=updateMySavedWordsDisplay&failure=updateMySavedWordsDisplay&uid=' + escape(FB.curr_user.uid) + '&access_token=' + escape(FB.curr_user.access_token)
		container.appendChild(node);

		// Track the click.
		var url = 'http://www.merriam-webster.com/my-saved-words/dictionary/starclick/unsave/break[1]';
		_gaq.push(['_trackPageview', url]);

		return;
	};

// This method updates the display of the My Saved Words view.
function updateMySavedWords (id, which, word)
	{	var no_homograph_tip = !document.getElementById('homograph-tool-tip');

		// The word is already saved.
		if ( which == 1 )
			{	document.getElementById(id).innerHTML = '';

				var node		= document.createElement('a');
				node.word		= word;
				node.className	= 'my_saved_words_link';
				node.href		= "/my-saved-words/manage-list.htm";
				node.style.display = 'block';
				node.style.padding = '0 0 0 0';	
				node.style.margin = '0 0 0 -4px';
				node.onclick	= function () { return my_words_logged_in(function() { document.location = '/my-saved-words/manage-list.htm'; }); };
				node.onmouseover = function () { var elm = document.getElementById('popularity-info'); if ( elm ) { elm.style.display = 'none'; if ( document.getElementById('homograph-tool-tip') ) {	document.getElementById('homograph-tool-tip').style.display = 'none'; }; };};
				node.innerHTML	= 'Saved';
				document.getElementById(id).appendChild(node);

				node		= document.createElement('a');
				node.className	= 'my_saved_words_icon';
				node.word		= word;
				node.href		= '#';
				node.onclick	= function () { deleteFromMySavedWords(this.parentNode, this.word); return false; };
				node.onmouseover = function () { var elm = document.getElementById('popularity-info'); if ( elm ) { elm.style.display = 'none'; if ( document.getElementById('homograph-tool-tip') ) {	document.getElementById('homograph-tool-tip').style.display = 'none'; }; };};
				node.innerHTML	= '<img src="/images/my-saved-words/saved.gif" />';				
				node.style.display = 'block';
				node.style.margin = '2px 0 0 0';				
				node.style.width = '40px;'												
				document.getElementById(id).appendChild(node);			
				
				// Make the cookie "permanent."
				var exdate=new Date();
				exdate.setDate(exdate.getDate() + (400 * 40));
				var c_value=escape('yes') + "; expires="+exdate.toUTCString();
				document.cookie='my_saved_words_seen' + "=" + c_value;
			}

		// Show the login bubble.
		else if ( which == -1 )
			{	document.getElementById(id).innerHTML = '';

				node			= document.createElement('a');				
				node.className	= 'my_saved_words_link';
				node.word		= word;
				node.href		= '#';
				node.innerHTML	 = 'Save';
				node.onclick	= function () { showFBLogin(saveToMySavedWords); return false; };
				node.onmouseover = function () { document.getElementById('my_saved_words_notice').style.display = 'block'; var elm = document.getElementById('popularity-info'); if ( elm ) { elm.style.display = 'none'; }; if ( document.getElementById('homograph-tool-tip') ) {	document.getElementById('homograph-tool-tip').style.display = 'none'; }; };
				node.style.display = 'block';
				document.getElementById(id).appendChild(node);

				node		= document.createElement('a');
				node.className	= 'my_saved_words_icon';
				node.word		= word;
				node.href		= word;
				node.onclick	= function () { showFBLogin(saveToMySavedWords); return false; };
				node.onmouseover = function () { document.getElementById('my_saved_words_notice').style.display = 'block'; var elm = document.getElementById('popularity-info'); if ( elm ) { elm.style.display = 'none'; }; if ( document.getElementById('homograph-tool-tip') ) {	document.getElementById('homograph-tool-tip').style.display = 'none'; }; };
				node.innerHTML	= '<img src="/images/my-saved-words/save.gif" />';	
				node.style.display = 'block';
				node.style.margin = '-2px 0 1px 4px';
				node.style.padding = '0 0 0 0';
				node.style.width = '40px';
				document.getElementById(id).appendChild(node);				

				node			= document.createElement('div');
				node.className	= 'notice';
				node.id			= 'my_saved_words_notice';
				node.innerHTML	= '';
				if ( no_homograph_tip && !/(^|;\s*)my_saved_words_seen=/.test(document.cookie) )
					{	var exdate=new Date();
						exdate.setDate(exdate.getDate() + 7);
						var c_value=escape('yes') + "; expires="+exdate.toUTCString();
						document.cookie='my_saved_words_seen' + "=" + c_value;
						node.style.display = 'block';						
					}
				else
					{	node.onmouseout = function () { this.style.display = 'none'; };
					};
				node.innerHTML = '<a id="my_saved_words_close_button" href="#" onclick="document.getElementById(\'my_saved_words_notice\').clear(); this.parentNode.style.display = \'none\'; return false;" class="close-window" style="position: absolute; margin-left: 263px; margin-top: -6px;"><img src="http://stats.merriam-webster.com/images/popularity/close-window.jpg" /></a>';
				
				node.innerHTML	+= '<strong>Save this word to your Favorites.</strong><br />If you\'re logged into Facebook, you\'re ready to go.';
				node.clear = function () 
					{	/**

						// Removed 08-31-2012 to place close button back in per request.
						if ( document.getElementById('my_saved_words_close_button') ) 
							{ document.getElementById('my_saved_words_close_button').style.display = 'none'; 
							}; 
						this.onmouseout = function () 
							{ this.style.display = 'none'; 
							}; 
						**/
					};
				document.getElementById(id).appendChild(node);
			}

		// The user hasn't saved this word yet.
		else
			{	document.getElementById(id).innerHTML = '';

				node			= document.createElement('a');
				node.className	= 'my_saved_words_link';
				node.word		= word;
				node.href		= '#';
				node.onclick	= function () { saveToMySavedWords(); return false; };
				node.onmouseover = function () { document.getElementById('my_saved_words_notice').style.display = 'block'; var elm = document.getElementById('popularity-info'); if ( elm ) { elm.style.display = 'none'; }; };
				node.innerHTML	 = 'Save';
				node.style.display = 'block';
				document.getElementById(id).appendChild(node);

				node			= document.createElement('a');
				node.word		= word;
				node.href		= '#';
				node.className	= 'my_saved_words_icon';
				node.onclick	= function () { saveToMySavedWords(); return false; };
				node.innerHTML	= '<img src="/images/my-saved-words/save.gif" />';	
				node.onmouseover = function () { document.getElementById('my_saved_words_notice').style.display = 'block'; var elm = document.getElementById('popularity-info'); if ( elm ) { elm.style.display = 'none'; }; if ( document.getElementById('homograph-tool-tip') ) {	document.getElementById('homograph-tool-tip').style.display = 'none'; }; };
				node.style.display = 'block';
				node.style.margin = '-2px 0 1px 0';	
				node.style.width = '40px';
				document.getElementById(id).appendChild(node);

				node			= document.createElement('div');
				node.id			= 'my_saved_words_notice';
				node.className	= 'notice';
				node.innerHTML	= '';						
				if ( no_homograph_tip && !/(^|;\s*)my_saved_words_seen=/.test(document.cookie) )
					{	var exdate=new Date();
						exdate.setDate(exdate.getDate() + 7);
						var c_value=escape('yes') + "; expires="+exdate.toUTCString();
						document.cookie='my_saved_words_seen' + "=" + c_value;
						node.style.display = 'block';						
						node.innerHTML = '<a id="my_saved_words_close_button" href="#" onclick="document.getElementById(\'my_saved_words_notice\').clear(); this.parentNode.style.display = \'none\'; return false;" class="close-window" style="position: absolute; margin-left: 263px; margin-top: -6px;"><img src="http://stats.merriam-webster.com/images/popularity/close-window.jpg" /></a>';
					}
				else
					{	node.onmouseout = function () { this.style.display = 'none'; };
					};
				node.innerHTML += '<strong>Save this word to your Favorites.</strong><br />If you\'re logged into Facebook, you\'re ready to go.';
				node.clear = function () { if ( document.getElementById('my_saved_words_close_button') ) { document.getElementById('my_saved_words_close_button').style.display = 'none'; }; this.onmouseout = function () { this.style.display = 'none'; }; };
				document.getElementById(id).appendChild(node);		
			};		
	};	

</script>

<script>
function showPopup(e) {
	if(this.id) {
		var hp;
		var childrenEl = YAHOO.util.Dom.getChildren(this.id);
		if(childrenEl.length > 0) {
			var context = this.id;
			for(var i=0; i < childrenEl.length; i++) {
				if(YAHOO.util.Dom.hasClass(childrenEl[i], "zoom") == false) {
					context = childrenEl[i];
				}
			}
			hp = document.getElementById(this.id + "_large");
			YAHOO.yhelp.mpImageOverlay1.setBody(hp.innerHTML);
			YAHOO.yhelp.mpImageOverlay1.cfg.setProperty("context", [context, "tl","tr"]);
			YAHOO.yhelp.mpImageOverlay1.render();
			YAHOO.yhelp.mpImageOverlay1.show();
		}
	}
}
function hidePopup(e) {
	YAHOO.yhelp.mpImageOverlay1.hide();
}
function mpImageInit() {
	var mpImages = YAHOO.util.Dom.getElementsByClassName('mpImage', 'a');
	if(mpImages.length > 0) {
		YAHOO.util.Event.on(mpImages, 'mouseover', showPopup);
		YAHOO.util.Event.on(mpImages, 'mouseout', hidePopup);
		YAHOO.yhelp.mpImageOverlay1 = new YAHOO.widget.Overlay("mpImageOverlay1", { visible:false } );
		YAHOO.yhelp.mpImageOverlay1.render(document.body);
	}
}
YAHOO.util.Event.addListener(window, "load", mpImageInit);

</script>							</div><!-- id="suprprop" class="indx" -->

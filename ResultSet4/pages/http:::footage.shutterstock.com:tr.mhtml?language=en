	<script>
	$(function(){

		var player = new MediaElementPlayer('video', {
			loop: true,
			enableAutosize: true,
			enableKeyboard: false
		});
		
		// Make video click through to featured gallery if it is available
		var montage = $('#montage');
		var galleryUrl = montage.data('gallery');
		if (galleryUrl) {
			montage.css('cursor', 'pointer');
			montage.on('click', function() {
				location.href = location.protocol + "//" + location.hostname + galleryUrl;
			});
		}

	});
	</script>

	<input type="hidden" id="feedUrl" value="http://blog.thomsonreuters.com/index.php/feed" />
	<input type="hidden" id="dateFormat" value="EEE, dd MMM yyyy HH:mm:ss zzz" />
	<input type="hidden" id="numArticles" value="3" />
	<input type="hidden" id="displayType" value="Singleton" />
	<input type="hidden" id="descrToggle" value="Yes" />
	<input type="hidden" id="label" value="The Knowledge Effect Blog" />	
	<div class="rss_singleton">
		<h3>The Knowledge Effect Blog</h3>
		<div id="rssItems"></div>
	</div>
<script type="text/javascript">
jQuery(document).ready(function() {
	var url = jQuery('#feedUrl').val();
	var dateFmt = jQuery('#dateFormat').val();
	var numArts = jQuery('#numArticles').val();
	jQuery.get('/rss.fetch.action', {feedUrl: url, dateFormat: dateFmt, numArticles: numArts}, function(data) { onRSSComplete(data); });
});
function onRSSComplete(data) {
	var descrTog = jQuery('#descrToggle').val();
	var items = data.rssItems;
	var html = '<ul>';
	jQuery.each(items, function() {
		var date = "";
		if ('Relative' === 'Relative') {
			var timeSince = (new Date()).getTime() - this.pubDate;
			if (timeSince >= 31536000000)
				date = parseInt(timeSince / 31536000000) + ' Years Ago';
			else if (timeSince >= 2592000000)
				date = parseInt(timeSince / 2592000000) + ' Months Ago';
			else if (timeSince >= 604800000)
				date = parseInt(timeSince / 604800000) + ' Weeks Ago';
			else if (timeSince >= 86400000)
				date = parseInt(timeSince / 86400000) + ' Days Ago';
			else if (timeSince >= 3600000)
				date = parseInt(timeSince / 3600000) + ' Hours Ago';
			else if (timeSince >= 60000)
				date = parseInt(timeSince / 60000) + ' Minutes Ago';
			else if (timeSince >= 1000)
				date = parseInt(timeSince / 1000) + ' Seconds Ago';
			if (date.indexOf('1 ') === 0)
				date = date.replace('s Ago', ' Ago');
		} else {
			var d = new Date(this.pubDate);
			date = Date.parse((d.getMonth() + 1) + ' ' + d.getDate() + ' ' + d.getFullYear()).toString("dd MMM yyyy");
		}
		html+= '<li><p><a href="' + this.link + '" target="blank">' + this.title + '</a> - <span class="date">' + date + '</span>';
		if (descrTog == 'Yes') {
			var summary = jQuery.trim(this.description);
			var length = parseInt('45');
			if (!isNaN(length))
				summary = summary.substring(0, length) + '...';
			html+= '<br/><span class="summary">' + summary + '</span>';
		}
		html+= '</p></li>';
	});
	html+= '</ul>';
	jQuery('#rssItems').html(html);
	jQuery(".rss_horizontal ul li:nth-child(3n+1)").addClass("clear");
}
</script></div>
</div>

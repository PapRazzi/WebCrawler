<script type="text/javascript" src="http://img.alibaba.com/js/app/intelligentSearch.js" charset="iso-8859-1"></script>
<script language="javascript">
		var intelSearchConfig = {
		url: 'http://connectkeyword.alibaba.com/lenoIframeJson.htm?iframe_delete=true',
		inputElId: 'SearchTextIdx',
		listCssClass: 'intelligentList',
		listCssStyle: 'left:0px;top:'+(AE.bom.isIE?26:22)+'px;width:400px;z-index:777',
		returnVarName: 'intelSearchData',
		keyWordClass: 'boldKeword',
		isNeedSubmit: false
	}
	var controler = new AE.widget.intelligentSearch();
	controler.init(intelSearchConfig);
	
		var searchClickHint = function(input){
		var hintClass = "unable";
		var hintAttr = "hint";
		
		//run
		YUD.addClass(input,hintClass);
		input.value = input.getAttribute(hintAttr);
		
		//event
		YUE.on(input,"click",function(ev){
			if(!YUD.hasClass(this,"unable"))return;
			this.value = "";YUD.removeClass(this,"unable");
		})
		YUE.on(input,"keydown",function(ev){
			if(!YUD.hasClass(this,"unable"))return;
			this.value = "";YUD.removeClass(this,"unable");
		})
		YUE.on(input,"blur",function(ev){
			if(this.value == ""){
				YUD.addClass(this,"unable");
				this.value = this.getAttribute("hint");
			}
		})
		YUE.onDOMReady(function(){
			if(input.createTextRange){
				var rng = input.createTextRange();
				if(input.hasFocus)rng.moveStart('character',input.value.length); 
				rng.collapse(true);
				rng.select();
			}else if(input.setSelectionRange){
				var rng = input.setSelectionRange(0,0);
			}
			input.focus();
		})
	}
	
	searchClickHint(get("SearchTextIdx"));
	</script>

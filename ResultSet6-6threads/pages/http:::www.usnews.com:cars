<script type="text/javascript">
$(document).ready(function(){

	var make=document.getElementById("ow_make");
	var model=document.getElementById("ow_model");

	$("#ow_make").change(function(){
		populateOwnerModelDropdown(make,model);	
		if(	$("form.owners-widget select#ow_make").val() == "ALL BRANDS" ){
			$("#owner-submit").attr("disabled", "disabled");
		}
	});

	$("#ow_model").change(function(){
		$("#owner-submit").removeAttr("disabled")	
	});	
	
	$("#owner-submit").click(function(){
		$('form.owners-widget').attr('action',$("form.owners-widget select#ow_model").val());
		$('form.owners-widget').submit();
	});
	
});
</script>
	



<script>
  var page_url = 'http://seekingalpha.com/symbol/ntap';
  var currTab = null;
  var page = 0

  SeekingAlpha.Initializer.onDOMLoad(function(){
    var pa = {slugs:"ntap",subjects:5};
    var insertStockTalks = function(response) {
      if (response.total_pages > 0) $('more_stocktalk_link').style.display = 'block';
      var con = $('ajax_st_result');
      if (con){
        con.innerHTML = '';
        new Insertion.Top($('ajax_st_result'),'<div class="list" id="stocktalk_list">'+response.html+'</div>');
      }
    }
    var onFailed = function() {
      var con = $('ajax_st_result');
      if (con){
        con.innerHTML = '';
        new Insertion.Top($('ajax_st_result'),'<div class="list" id="stocktalk_list"></div>');
      }
      var stBlock = $('block_stocktalks');
      if (stBlock) stBlock.style.display = "none";
    }
    SA.Pages.Quote.init();
      create_ad('symbol_headline_dart',2, 't', true);
      $('auhtorpick_tabs').parentNode.className = "tab_quote_headlines";
      currTab = "tab_quote_headlines";
    Event.observe(window, 'scroll', SA.Pages.Quote.check_ad);
  });

  function create_ad(unit, tile, pos, notSticky){
    if ($(unit) && $(unit).innerHTML == ""){
      notSticky = (notSticky === undefined) ? false : true;
      NEW_AD_DELTA = 0;
      ad_src = 'http://ad.doubleclick.net/adj/sek.quotes/stocks-non-refresh;sz=300x250;x=x;tile=TILE;d=quotes;ms=1;pos=POS;t=stocks-non-refresh;qp=true;t=technology;s=ntap;'+dart_my_vocation_and_profiles()+'ord='+ord+'?';
      ad_src = ad_src.sub("TILE", tile).sub("POS", pos);
      SA.Utils.Ads.add({el:$(unit), HadCreated:false, size:'300x250', delta:NEW_AD_DELTA, info:ad_src, NotSticky: notSticky});
      SA.Utils.Ads.run();
    }
  }

  /* Navigation Functions */
  function add_selected_icon(row_id) {
    if ($(row_id)){
      $(row_id).className = 'selected';
    }
  }

  function trackNavLinkEvent() {
    try {
      var category = this.parentNode.parentNode.select('span.various_nav_title')[0].innerHTML;
      trackLink(this,'Quote Page Tracking', category, this.innerHTML);
    } catch(e) {}
  }

  $$('.various_nav a').each(function(link) {
    if (/^\/|seekingalpha\.com/i.test(link.href)) {
      try {
        var source = ('qp_nav_' + link.innerHTML).toLowerCase().replace(/&amp;/ig, '').replace(/\W+/g, '_');
        link.setAttribute('sasource', source);
      } catch(e) {}
    } else link.observe('click', trackNavLinkEvent);
  });
</script>
    <script src="http://static.cdn-seekingalpha.com/javascripts/jspell_packaged.js?1360856395" type="text/javascript"></script>
<link href="http://static3.cdn-seekingalpha.com/stylesheets/footer_packaged.css?1360856422" media="screen" rel="stylesheet" type="text/css" /><script type="text/javascript">
  if(typeof (authors) == "undefined" || typeof (symbols) == "undefined")
    load_remote_script("http://static2.cdn-seekingalpha.com/javascripts/sa.js?1360666218");
  if(is_users_cookie_exists() && !cookieExists('tops') && typeof(tops) == "undefined")
    load_remote_script("http://static.cdn-seekingalpha.com/javascripts/tops.js?1360664180");
  var mone_params = {};
  SeekingAlpha.Initializer.AddAfterLoad(function(){SA.Performance.Utils.loadPrintStyle('http://static1.cdn-seekingalpha.com/stylesheets/print.css')});

  if (SA.Data.User.loggedIn() && SA.Data.User.email().match(/@seekingalpha.com/)){
    try{$('user_settings_pop').children[0].insert("<li class='mt10'><a href='javascript:void(0)' onclick='switch_status()'>PRO ON / OFF</a></li>");}catch(e){}
  }

  function switch_status(){
    new Ajax.Request('/authentication/get_menu', {
      asynchronous:true,
      method:'get',
      onComplete: function(transport) {
        eval(transport.responseJSON.code);
      }
    });
  }
</script>

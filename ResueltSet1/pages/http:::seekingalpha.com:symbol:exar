      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">EXAR</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['exar']
      });
    })();

    try { SymbolData.handle_data('EXAR', {"rt_time":"04:00PM ET,02/15","last":11.13,"high":11.43,"open":11.43,"close":11.32,"low":10.96,"volume":245800,"volume_at":1361063490,"at":1361063490,"avg3vol":156600.0,"c_share":0.33,"div_rate":0.0,"forward_pe":74.2,"payout":0.0,"div_yield":0.0,"payout_5y":0.0,"chgp_1w":2.4,"chgp_4w":21.5,"chgp_52w":59.0,"beta60":0.65,"beta36":0.88,"mov_avg_10d":10.92,"mov_avg_10w":9.5,"mov_avg_200d":8.32,"short_interest":3.7,"p_short_interest_float":2.1,"liquidity":1047000.0,"p_week_vol_shares":1.56,"sfloat":31207171.0,"shares_held":33654686.0,"p_shares_by_inst":73.2,"p_shares_by_insid":2.9,"net_insid_tr":-257594.0,"insid_sb":90040.0,"insid_ss":347634.0,"total_revenue":118661000.0,"revenue_growth":-11.6,"net_income":-20351000.0,"gross_margin":52.7,"eps":-0.45,"roa":-7.4,"cur_ratio":6.5,"quick_ratio":5.9,"debt_eq":0.02,"lt_debt_cap":0.0,"debt_short":3138000.0,"debt_long":0.0,"low52":6.63,"high52":11.44,"market_cap":514169627.16,"cash":201028921.56,"debt":3138000.0,"total_enterprise":316278705.6,"price_book":2.217131474103586,"ev_ebitda":88.9923200900394,"price_sales":4.3330970340718515,"price_tangb":2.31875,"ev_sales":2.6653972712180076,"ev_fcf":-228.21145703553233,"inst_chg":-969790.0,"cf":-7391477.12,"fcf":-1385901.96,"debt_fcf":-2.2642294264451435,"rt_ts":1360944000,"chg_vs_mkt_1w":2.0,"chg_vs_mkt_4w":19.0,"chg_vs_mkt_52w":42.0,"p_close_10w":17.0,"p_close_200d":34.0}) } catch (e) { }

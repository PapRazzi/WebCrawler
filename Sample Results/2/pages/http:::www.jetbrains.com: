<script type="text/javascript">
$(document).ready(function() {
    $('#menu_products').hide();
    $('#jb_nav .products a').click(
        function(){
            var productsView = $('#menu_products');
            if (productsView.is(":visible")) {
                setTimeout(function() {
                    $('#jb_nav .products').removeClass('open');
                }, 1000);
                productsView.stop(false, true).slideUp(500);
            } else {
                $(this).parent().addClass('open');
                productsView.stop(false, true).slideDown(500);
            }
            return false;
        }
    );
    $('#menu_products .close').click(
        function(){
            setTimeout(function() {
                $('#jb_nav .products').removeClass('open');
            }, 1000);
            $('#menu_products').stop(false, true).slideUp(500);
        }
    );

        if ($("nav ul.nav").children().length > 0) {
        var nav = $("nav");
        $(window).scroll(function() {
        if ($(window).scrollTop() > 154  /*&& (navigator.userAgent.indexOf('iPhone') == -1 && navigator.userAgent.indexOf('iPad') == -1 && navigator.userAgent.indexOf('Blackberry') == -1 && navigator.userAgent.indexOf('Android') == -1)*/) {
            nav.addClass('fixed-menu');
            /*$("nav").stop().animate({top: 0}, 100);*/
        } else {
            nav.removeClass('fixed-menu');
            /*$("nav").stop().animate({top: 0}, 100);*/
        }
    });
    }
    
});
</script>                     <script type="text/javascript" src="js2/jquery/ui.core.js"></script>              <script type="text/javascript" src="js2/jquery/ui.widget.js"></script>              <script type="text/javascript" src="js2/jquery/ui.rcarousel.min.js"></script>              <script type="text/javascript" src="js2/jquery/countdown.js"></script>              <script type="text/javascript" src="js2/cookie.js"></script>              <script type="text/javascript">
        var ui = { set: {} };
        $(document).ready(function() {
            function generatePages() {
                var _total, i, _link;
                _total = $( "#carousel" ).rcarousel( "getTotalPages" );
                for ( i = 0; i < _total; i++ ) {
                    _link = $( "<a href='#'></a>" );

                    $(_link)
                        .bind("click", {page: i},
                            function( event ) {
                                $( "#carousel" ).rcarousel( "goToPage", event.data.page );
                                event.preventDefault();
                            }
                        )
                        .addClass( "bullet off" )
                        .appendTo( "#pages" );
                }
                // mark first page as active
                $( "a:eq(0)", "#pages" )
                    .removeClass( "off" )
                    .addClass( "on" )
            }

            function pageLoaded( event, data ) {
                $( "a.on", "#pages" )
                    .removeClass( "on" );
                $( "a", "#pages" )
                    .eq( data.page )
                    .addClass( "on" );
            }

            $("#carousel").rcarousel({
                visible: 1,
                step: 1,
                speed: 500,
                interval: 10,
                auto: {enabled: true},
                width: 960,
                height: 573,
                start: generatePages,
                pageLoaded: pageLoaded
            });
            var products_content = $('#menu_products.create');
            $('.all_products a').toggle(
            function(){
                    $(this).parent().addClass('open');
                    products_content.stop(false, true).slideDown(500,
                        function() {
                            if($(document).scrollTop() < (products_content.position().top + products_content.height())- $(window).height()) {
                                $("html, body").animate({ scrollTop: (products_content.position().top + products_content.height())- $(window).height() }, "slow");
                            }
                        }
                    );


                },
                function(){
                    $(this).parent().removeClass('open');
                    products_content.stop(false, true).slideUp(500);
                }
            );


        });

    </script>     
        

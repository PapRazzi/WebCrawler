        {
            var p=a[i].split('=');
            if (p.length != 2) continue;
            b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
        }
        return b;
    })(window.location.search.substr(1).split('&'));
    
    qs = qs || {};
    if(!qs.hasOwnProperty('alcmpid'))
    {
        var alcmpid = BLOOMBERG.util.Cookie.get('alcmpid');
        if(alcmpid!=null){ qs['alcmpid'] = alcmpid; }
    }
    if(qs.hasOwnProperty('alcmpid') && qs['alcmpid'] != ''){
        BLOOMBERG.util.Cookie.remove('alcmpid', '/', 'bloomberg.com');
        BLOOMBERG.util.Cookie.setEndOfSession('alcmpid', qs['alcmpid'], '/', 'bloomberg.com');
        _gaq.push(['_setCustomVar', 2, 'al_cmpid', qs['alcmpid'], 3]);
    }
</script>

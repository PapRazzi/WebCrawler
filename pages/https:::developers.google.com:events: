p.registration.Timer=function(a,b,d){this.timeLeft_=a;this.timeoutCallback_=b;this.dangerTime_=d;var e=this;this.timer_=window.setInterval(function(){e.tick_()},1E3);this.tick_();this.dangerTime_&&this.timeLeft_<=this.dangerTime_&&this.addDangerStyle_();$("#countdown-timer").html(p.registration.Timer.formatTime_(this.timeLeft_))};p.registration.Timer.prototype.stop=function(){window.clearInterval(this.timer_);$("#countdown-timer").removeClass("danger-timer");$("#countdown-timer").addClass("stopped-timer")};

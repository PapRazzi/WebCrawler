<script type="text/javascript">
    $('#socialbuttons > div').hover(
        function () {
            $(this).find('.countBox').show();

            if ($(this).hasClass('twitter') && $(this).find('.countBox').children().length == 2) {
                $(this).find('.countBox').prepend('<a href="https://twitter.com/share" class="twitter-share-button" data-lang="en" data-count="vertical">Tweet</a><script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="https://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</scr' + 'ipt>');
            }
            else if ($(this).hasClass('facebook')) {
                $(this).find('.countBox').css('visibility', 'visible');
            }
            else if ($(this).hasClass('google') && $(this).find('.countBox').children().length == 2) {
                $(this).find('.countBox').prepend('<div class="g-plusone" data-href="http://www.marketwatch.com/story/financial-stocks-rise-ahead-of-barclays-results-2013-02-11" data-size="tall"></div><script type="text/javascript">(function() { var po = document.createElement("script"); po.type = "text/javascript"; po.async = true; po.src = "https://apis.google.com/js/plusone.js"; var s = document.getElementsByTagName("script")[0]; s.parentNode.insertBefore(po, s); })();</scr' + 'ipt>');
            }
        },
        function () {
            if ($(this).hasClass('facebook')) {
                $(this).find('.countBox').css('visibility', 'hidden');
            }
            else {
                $(this).find('.countBox').hide();
            }
        }
    );
</script>








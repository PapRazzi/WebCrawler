<script type="text/javascript">
  $(function(){
    var down = false;
    $(".download-button").mousedown(function(){
      if (!down) {
        $(".download-label-box").addClass('download-label-box-down');
        $(".download-label-box").removeClass('download-label-box-up');
        down = true;
      }
    }).mouseup(function(){
      if (down) {
        $(".download-label-box").addClass('download-label-box-up');
        $(".download-label-box").removeClass('download-label-box-down');
        down = false;
      }
    });

    var hovering = false;
    $(".download-button").hover(function(){
      if (!hovering) {
        $(".download-free").css('text-decoration', 'underline');
        hovering = true;
      }
    }, function(){
      if (hovering) {
        $(".download-free").css('text-decoration', 'none');
        hovering = false;        
      }
    })

  });
</script>

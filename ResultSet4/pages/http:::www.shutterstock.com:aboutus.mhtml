					var leaderID = this.down().className.split(' ')[1];
					self.navigateTo(leaderID);
				});
				x.observe('mouseover',function(){
					x.down().addClassName('opacity_10');
				});
				x.observe('mouseout',function(){
					x.down().removeClassName('opacity_10');
				});
			});

			$$('#about_us_details .navigation em#previous, #about_us_details .navigation .left_arrow, #about_us_details .navigation em#next, #about_us_details .navigation .right_arrow').each(function(x){
				x.setOpacity(0.5);
				x.observe('mouseover',function(){
					if(x.hasClassName('ab_prev')){
						$$('.ab_prev').each(function(el){
							el.setOpacity(1);
						});
					}
					if(x.hasClassName('ab_next')){
						$$('.ab_next').each(function(el){
							el.setOpacity(1);
						});
					}
				});
				x.observe('mouseout',function(){
					if(x.hasClassName('ab_prev')){
						$$('.ab_prev').each(function(el){
							el.setOpacity(0.5);
						});
					}
					if(x.hasClassName('ab_next')){
						$$('.ab_next').each(function(el){
							el.setOpacity(0.5);
						});
					}
				});
				x.observe('click',function(){
					if(x.hasClassName('ab_prev')){
						self.navigateToPrevious();
					}
					if(x.hasClassName('ab_next')){
						self.navigateToNext();
					}
				});
			});
			$$('#about_us_details .close_btn_dark')[0].observe('click',function(){
				self.hide();
			});
			this.addKeyEventListeners();
		}
		this.navigateToPrevious = function(){
			--this.currentIndex;
			if(this.currentIndex < 0) this.currentIndex = this.total_number_of_thumbs -1;
			var dataID = this.getPreviewDataID(this.currentIndex);
			this.navigateTo(dataID);
		}
		this.navigateToNext = function(){
			++this.currentIndex;
			if(this.currentIndex >= this.total_number_of_thumbs) this.currentIndex = 0;
			var dataID = this.getPreviewDataID(this.currentIndex);
			this.navigateTo(dataID);
		}
		this.addKeyEventListeners = function(){
			document.onkeydown = self.checkKeycode;
		}
		this.checkKeycode = function(e){
			var keycode;
			if (window.event) keycode = window.event.keyCode;
			else if (e) keycode = e.which;
			if(self.visible){
				if(keycode == 37) self.navigateToPrevious();
				if(keycode == 39) self.navigateToNext();
				if(keycode == 27) self.hide();
			}
		}


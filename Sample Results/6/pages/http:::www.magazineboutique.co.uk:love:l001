<script type="text/javascript">

function parseQS(key){ 
    var qs = window.location.search.substring(1).split("&"); 
    for (var i=0;i<qs.length;i++){ 
        var kvpair = qs[i].split("="); 
        if (kvpair[0] == key) return kvpair[1]; 
    } 
}

var sPath = window.location.pathname;
var sScriptName = sPath.substring(sPath.lastIndexOf('/') + 1);
var sProtocol = window.location.protocol;
var cat = "";

switch(sScriptName){

    case "mpurchase.asp":
        switch(parseQS("m")){
            case "842":
                //Easy Living Landing Page
                cat="easyl950";
                break
            case "85":
                //CN Traveller Landing Page
                cat="cntra473";
                break
            case "469":
                //Glamour Landing Page
                cat="glamo576";
                break
            default: 
                cat=""
        }
        break
        
    case "mpurchase2.asp":
    	cat='direc059'
        break
        
    case "mpurchase3.asp":
    	cat='direc741'
        break
        
    case "mpurchase4.asp":
    	cat='direc277'
        break

    case "mpurchase5.asp":
    	cat='succe126'
        break

    default: cat=""
}

if(cat!=""){
    var axel = Math.random()+"";
    var a = axel * 10000000000000;
    document.write('<IFRAME SRC="' + sProtocol + '//fls.uk.doubleclick.net/activityi;src=2124570;type=offer420;cat=' + cat + ';ord='+ a + '?" WIDTH=1 HEIGHT=1 FRAMEBORDER=0></IFRAME>');
}
</script>


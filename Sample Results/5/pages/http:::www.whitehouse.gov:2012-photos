<script type="text/javascript">
$(document).ready(function(){
    $('.show-hide').hide();
    $( '.toggle-text').click(function(){
        $(this).next('.question-text').toggleClass('show-less');
        $(this).next('.show-hide').slideToggle('fast');
        return true;
    });
    $( '.click-img').click(function(){
       $(this).prev('.question-text').toggleClass('show-less');
        $(this).prev('.show-hide').slideToggle('fast');
        return true;
    });
  var nav = $('.nav-wrap');
  $(nav).insertAfter('#page');
   $('.nav-wrap').css('display' , 'block');
$(document).scroll(function(){
  if ($(document).scrollTop() > 145){
    $('.nav-wrap').css('position' , 'fixed').css('top' , '0');
  } else {
    $('.nav-wrap').css('position' , 'absolute').css('top' , '145px');
  };
})
$('a').click(function() {
    var elementClicked = $(this).attr("href");
    var destination = $(elementClicked).offset().top;
    if (elementClicked === '#intro'){
      $("html:not(:animated),body:not(:animated)").animate({ scrollTop: destination-175}, 500 );
    } else{
      $("html:not(:animated),body:not(:animated)").animate({ scrollTop: destination-20}, 500 );
    }
    //return false;
});
});
</script>

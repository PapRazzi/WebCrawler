	<script language="JavaScript">
	
	var defaultBtn = 'Submit';
	var elmCount = 0;

	/** submit form with default command button */
	function defaultSubmit() {
		LoginSubmit(defaultBtn);
	}

	/**
	 * submit form with given button value
	 *
	 * @param value of button
	 */
	function LoginSubmit(value) {
		aggSubmit();
		var hiddenFrm = document.forms['Login'];

		if (hiddenFrm != null) {
			hiddenFrm.elements['IDButton'].value = value;
			hiddenFrm.submit();
		}
	}
	
	function focusLoginField(e) {
		if (document.getElementById('IDToken1')) {
			document.getElementById('IDToken1').focus();
		}
	}
	
	
	window.onload = focusLoginField;
	</script>

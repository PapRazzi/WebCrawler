        function getParameterByName(name) {
    var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
    return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
}

var affiliate_id = getParameterByName('aff');

if (affiliate_id) {
    var qs = 'affiliate=' + affiliate_id +
             '&x-at=' + (getParameterByName('x-at') || 'undefined');
    document.write('<img src="http://store.uniblue.com/278/cookie?' + qs + '" width=0 height=0 />');
} 
    

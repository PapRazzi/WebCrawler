				function fnPopClick(e) {
                	YAHOO.util.Event.preventDefault(e);
    				document.getElementById("latsttabcontent").style.display = 'none';
    				document.getElementById("poptabcontent").style.display = 'block';
    				document.getElementById("popparent").setAttribute("class", "selected");
    				document.getElementById("latparent").setAttribute("class", "");
    			}
    			function fnLatestClick(e) {
    				YAHOO.util.Event.preventDefault(e);
    				document.getElementById("poptabcontent").style.display = 'none';
    				document.getElementById("latsttabcontent").style.display = 'block';
    				document.getElementById("latparent").setAttribute("class", "selected");
    				document.getElementById("popparent").setAttribute("class", "");
    			}
				YAHOO.util.Event.addListener("populartab1", 'click', fnPopClick);
				YAHOO.util.Event.addListener("latesttab1", 'click', fnLatestClick);
			

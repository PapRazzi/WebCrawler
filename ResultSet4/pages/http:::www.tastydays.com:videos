<img id="bgbody" class="bg-body" src="http://www.tastydays.com/wp-content/uploads/2012/08/tomato-background.jpg" alt="image description" /><script language="javascript" type="text/javascript">
$(document).ready(function() {


function getEventTarget(e) {
  var targ;
  if (!e) var e = window.event;
  if (e.target) targ = e.target;
  else if (e.srcElement) targ = e.srcElement;
  if (targ.nodeType == 3) // defeat Safari bug
    targ = targ.parentNode;
  return targ;
}


  $(document).click( function (e) {
    var target = getEventTarget(e);
var bg = 'NO';

    if (target.id == 'header') {
       bg = 'YES';
       }
    if (target.id == 'main') {
       bg = 'YES';
       }
    if (target.id == 'wrapper') {
       bg = 'YES';
       }
     if (target.id == 'bgbody') {
       bg = 'YES';
       }
     if (target.id == 'body_actual') {
       bg = 'YES';
       }
     if (target.id == 'bpage') {
       bg = 'YES';
       }
//alert(target.id);
    if (bg == "YES") {
                 window.open('http://www.recipebridge.com/r/tomato-recipes','_blank');
            //alert('background clicked');
    }
  });


});
</script>






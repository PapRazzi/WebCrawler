data-expanded-footer="&lt;div class=&quot;js-tweet-details-fixer tweet-details-fixer&quot;&gt;
          &lt;div class=&quot;cards-media-container &quot;&gt;&lt;div data-card-url=&quot;https://t.co/AKvLCan2&quot; data-card-type=&quot;summary&quot; class=&quot;cards-base cards-summary &quot; data-element-context=&quot;platform_summary_card&quot;&gt;
  &lt;div class=&quot;cards-content clearfix&quot;&gt;
    &lt;h3&gt;
      &lt;a target=&quot;_blank&quot; class=&quot;twitter-timeline-link&quot; href=&quot;https://t.co/AKvLCan2&quot;&gt;GET friends/list&lt;/a&gt;
    &lt;/h3&gt;
    &lt;p class=&quot;article&quot;&gt;
      &lt;a target=&quot;_blank&quot; class=&quot;twitter-timeline-link&quot; href=&quot;https://t.co/AKvLCan2&quot;&gt;Returns a cursored collection of user objects for every user the specified user is following (otherwise known as their &amp;quot;friends&amp;quot;).&lt;/a&gt;
    &lt;/p&gt;
&lt;div class=&quot;source&quot;&gt;&lt;a class=&quot;account-group js-action-profile js-user-profile-link&quot; href=&quot;/twitterapi&quot; data-user-id=&quot;6253282&quot;&gt;

if(pID == '5_5036')
{
  if(gup("subscription")!=null && gup("subscription")!="" ){
	var x=document.getElementsByName("edition");
	 for (i=0;i<x.length;i++)
	 {
	  if (x[i].value.toLowerCase()==gup("subscription").toLowerCase())
	   {
		x[i].checked = true;
	   }
	 }
	}
}

function gup(name)
{
  name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
  var regexS = "[\\?&]"+name+"=([^&#]*)";
  var regex = new RegExp( regexS );
  var results = regex.exec( window.location.href );
  if( results == null )
    return "";
  else
    return results[1];
}


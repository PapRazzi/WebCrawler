<script>
	var gSearchField = null;
	function FilterByTags()
	{
		gSearchField.ClearIfDefaultValue();

	    var searchTextInput = $( 'workshopSearchText' );
	    searchTextInput.value = v_trim( searchTextInput.value );
	    if ( searchTextInput.value.length < 3 && searchTextInput.value.length != 0 )
        {
			alert( 'The search text must be at least 3 characters long.' );
		}
        else
        {
    		var form = document.getElementById( 'TagsFilterForm' );
	    	form.submit();
        }
	}

	function SelectSortMethod( url )
	{
		HideMenu( $('sortselect'), $('sortselect_options') );

		window.location = url;
	}

	function ToggleSectionVisibility( browseBar, elementID )
	{
		if ( $(elementID).visible() )
		{
			$(browseBar).className = "rightSectionBrowseBar";
			$(elementID).hide();
			SetCookie( 'workshopBrowseSectionVisibility' + elementID, "false", 10, '/workshop' );
		}
		else
		{
			$(browseBar).className = "rightSectionBrowseBar toggled";
			$(elementID).show();
			SetCookie( 'workshopBrowseSectionVisibility' + elementID, "true", 10, '/workshop' );
		}
	}
</script>

      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">C</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 10,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['c']
      });
    })();

    try { SymbolData.handle_data('C', {"rt_time":"11:35AM ET,02/15","last":44.12,"high":44.51,"open":44.33,"close":44.32,"low":44.03,"at":1360928165,"volume":8365685,"volume_at":1360928146,"avg3vol":35549500.0,"div_rate":0.04,"forward_pe":9.5,"payout":2.0,"trailing_pe":18.0,"div_yield":0.1,"div_grow_rate":-75.69,"payout_5y":0.0,"div_yield_5y":2.4,"chgp_1w":3.1,"chgp_4w":5.6,"chgp_52w":33.7,"beta60":2.59,"beta36":1.92,"mov_avg_10d":43.04,"mov_avg_10w":41.22,"mov_avg_200d":33.32,"short_interest":0.9,"p_short_interest_float":1.1,"liquidity":1457407800.0,"p_week_vol_shares":3.04,"sfloat":2926213109.0,"shares_held":2092246318.0,"p_shares_by_inst":64.7,"p_shares_by_insid":2.1,"net_insid_tr":-444377.0,"insid_sb":38905.0,"insid_ss":483282.0,"total_revenue":27627999997.0,"revenue_growth":-8.7,"net_income":7541000000.0,"eps":2.44,"roe":4.2,"roe_ind":80.8,"roa":0.4,"int_cover":1.4,"debt_eq":1.72,"lt_debt_cap":0.6,"debt_short":52027000000.0,"debt_long":239463000000.0,"low52":24.61,"high52":44.68,"market_cap":129382813284.0,"debt":291490000000.0,"total_enterprise":420872813284.0,"price_book":0.693819782984746,"ev_ebitda":15.160578268043679,"last_div":0.01,"last_div_date":"01/31/13","iop":0.0,"iopp":0.0,"price_sales":4.683032188289022,"price_tangb":0.8468330134357005,"ev_sales":15.233560638833817,"ev_fcf":18.78522615917718,"inst_chg":297521537.0,"cf":7536578199.0,"fcf":22404458148.0,"debt_fcf":13.01035704922954,"rt_ts":1360928158,"chg_vs_mkt_1w":3.0,"chg_vs_mkt_4w":3.0,"chg_vs_mkt_52w":20.0,"p_close_10w":7.0,"p_close_200d":32.0}) } catch (e) { }

<script>
  var set_timeout_submit = null;
  function run_submit(){
    get_disabled();
    remove_children('[select_name=dashboard]','[select_name=theme]');
    remove_children('[select_name=sectors]','[select_name=industry]');
    var params = [];
    $$("[name=filters]").each(function(filter){
      if (filter.value != "") params.push(filter.value);
    });
    var href = "/articles";
    if (params.size() > 0) href = href+"?filters="+params.join(",");
    location.href = href; 
  }

  function get_disabled(){
    var _temp = [];
    $$("[name=filters]")[0].select("option").each(function(opt){ if (opt.getAttribute("disabled_list") && opt.selected) _temp.push(opt.getAttribute("disabled_list"));});
    $$("[name=filters]").each(function(sel){
      if (sel.getAttribute("disabled_list") && sel.value != "") {_temp.push(sel.getAttribute("disabled_list"));}; 
    });
    de_choices(_temp.join(",").split(","));
  }
  
  function de_choices(disable_tags){
    $$("[name=filters]").each(function(select){
      if (select.getAttribute("select_name") && disable_tags.indexOf(select.getAttribute("select_name")) != -1) 
        select.disabled = 1; 
      else 
        select.disabled = 0;
      select.select("option").each(function(opt){ if (disable_tags.indexOf(opt.value) != -1 && opt.value != "") opt.disabled = 1; else opt.disabled = 0;});
    });
    
    reset_child('[select_name=sectors]','[select_name=industry]');
    reset_child('[select_name=dashboard]','[select_name=theme]');
  }
  
  function set_filters(){
    var params = getURLParam('filters') ? getURLParam('filters').split(",") : ""
    if (params != "")
      $$("[name=filters]").each(function(select) {
        select.select("option").each(function(option) { if (params.indexOf(option.value) != -1) {option.selected = "1"; select.style.background = "none repeat scroll 0 0 #fff8bb";select.addClassName("LA_selected"); select.style.height="19px";select.style.color="#2d2d2d"}});
      });
  }
  
  function reset_child(parent,child){
    $$(child)[0].select('option').each(function(opt){if (opt.selected) child_parent = opt.getAttribute("parent"); });
    if ($$(parent)[0].value != "" && $$(parent)[0].value != child_parent) $$(child)[0].select('option')[0].selected = "1";
  }
  
  function remove_children(parent,child){
    var sector = $$(parent)[0].value;
    if (sector != "") $$(child)[0].select('option').each(function(opt){ if (opt.getAttribute("parent") != sector && opt.value != "") opt.remove() ;});      
  }
  
  //set_filters(); 
  //get_disabled();
  //remove_children('[select_name=dashboard]','[select_name=theme]');
  //remove_children('[select_name=sectors]','[select_name=industry]');
  init_menus();
  set_new_filters();
  $$('#articles-menu a').each(function(e){
    e.observe('click', select_from_dropdown.bindAsEventListener(e));
  })
  
  
  
  //////////////// new menu functions
  
  function select_from_dropdown(e){
    var el = Event.element(e);
    if (el.up().hasClassName('disabled_value')) return false;
    
    var top_input = $(el.getAttribute('parent_input'));
    
    var setText = el.innerHTML.unescapeHTML();
    if (top_input.id == "sectors_input" && el.up().getAttribute('parent')) {
      var parend_id = el.up().getAttribute('parent');
      setText = $('sector_' + parend_id).innerHTML.unescapeHTML() + ": " + setText;
    }
    top_input.value = setText;
    top_input.setAttribute("selected", el.up().getAttribute('value'));
    top_input.style.background = (setText == "All") ? "" : "#FFF8BB";

    set_disabled();
    submit_search();
    return false;
  }
  
  function select_element_from_dropdown(el){
    if (el.parentNode.hasClassName('disabled_value')) return false;
    
    var top_input = $(el.getAttribute('parent_input'));
    
    var setText = el.innerHTML.unescapeHTML();
    if (top_input.id == "sectors_input" && el.parentNode.getAttribute('parent')) {
      var parend_id = el.parentNode.getAttribute('parent');
      setText = $('sector_' + parend_id).innerHTML.unescapeHTML() + ": " + setText;
    }
    top_input.value = setText;
    top_input.setAttribute("selected", el.parentNode.getAttribute('value'));
    top_input.className = (setText == "All") ? "" : "selected";
    set_disabled();
    return false;
  }
  
  function set_disabled(){
    var _temp = [];
    var _selected = [];
    $$("#articles-menu input").each(function(box){if (box.getAttribute("selected")){_selected.push(box.getAttribute("selected"))}})
    $$("#articles-menu li").each(function(opt){
      if (opt.getAttribute("disabled_list") && _selected.indexOf(opt.getAttribute("value")) != -1 ){
        _temp.push(opt.getAttribute("disabled_list"));
      }
      });
    var disable_tags = _temp.join(",").split(",");
    $$("#articles-menu li").each(function(select){
      var val = 0;
      if (select.getAttribute("select_name") && disable_tags.indexOf(select.getAttribute("select_name")) != -1){
        select.addClassName('disabled');
      } else {
        select.removeClassName('disabled');
      }
      
      if (select.getAttribute("value") && disable_tags.indexOf(select.getAttribute("value")) != -1){
        select.addClassName('disabled_value');
      } else {
        select.removeClassName('disabled_value');
      }
    });    
  }
  
  function init_menus(){
    $$("#articles-menu input[type=text]").each(function(el){
      el.value = "All";
      el.removeAttribute("selected");
    })
  }
  
  function set_new_filters(){
    var params = getURLParam('filters') ? getURLParam('filters').split(",") : "";
    if (params != "") {
      params.each(function(value) {
        var parent = $$("#articles-menu li[value=" + value + "]")[0];
        var child = null;
        if (parent) child = parent.firstChild;
        if (child) select_element_from_dropdown(child);
      });
    }
  }
  
  function submit_search(add_filter) {
    var params = [];

    $$("#articles-menu input[type=text]").each(function(filter){
      if (filter.value != "All") params.push(filter.getAttribute('selected'));
    });
    if (!(add_filter === undefined)) {
      params.push(add_filter);
    } else {
      params.push(get_current_page());
    }

    var href = "/articles";
    if (params.size() > 0) href = href+"?filters="+params.join(",");
    if (['articles', 'news', 'transcripts'].indexOf(add_filter) != -1){
      SA.Data.Cookies.set('_sasource', 'latest_articles_tab_'+add_filter, 0.0007);
    }
    location.href = href;     
  }
  
  function get_current_page() {
    try {
      if (location.search.split("=")[1].split(",").indexOf('news') != -1) return 'news';
      if (location.search.split("=")[1].split(",").indexOf('transcripts') != -1) return 'transcripts';
    } catch(e){}
    return 'articles';
  }

  function select_news(action) {
    if (action != "off") {
      $$("li[select_name=dashboard]")[0].addClassName('disabled');
      $$("li[select_name=highlight]")[0].addClassName('disabled');
    } else {
      $$("li[select_name=dashboard]")[0].removeClassName('disabled');
      $$("li[select_name=highlight]")[0].removeClassName('disabled');     
    }
  }
  
  function button_on(el){
   el.className = "menu_drop_on"; 
  }
  
  function buttons_off(){
    $$('#articles-menu input[type=button]').each(function(el){
	    el.className = "menu_drop_off";
	  })
  }
  
  function closeDropMenu(el){
    if (el.hasClassName('menu_drop_on')){
      el.parentNode.removeClassName('hover');
      Event.stop(event);
      buttons_off();
    }
  }
</script>
        </div>

  var aColumns = new Array("right", "left");

  for (var i=aColumns.length-1; i>=0; --i) {
    var dHiddenContent = document.getElementById(aColumns[i] + 'colhidden');
    var dBTFContentPlaceholder = document.getElementById(aColumns[i] + 'colbtf');
    if (dHiddenContent != null && dBTFContentPlaceholder != null) {
      var htmlContent = dHiddenContent.innerHTML;
      dBTFContentPlaceholder.innerHTML = htmlContent;
      dBTFContentPlaceholder.style.visibility = 'visible';
      dBTFContentPlaceholder.style.display = 'block';

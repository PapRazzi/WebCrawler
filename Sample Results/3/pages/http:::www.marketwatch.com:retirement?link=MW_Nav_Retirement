    <div id="retirementhowto" class="section">
        <div class="headerx">
            <h2>How-To Guides</h2>
        </div>
        
        <div class="fancyselect">
            <p class="selected">Choose a topic</p>
            <div class="options">                <h3>Investing</h3>
                <ul>
                                    <li>
                    <a href="/story/how-to-invest-for-retirement-2012-11-13" class="headline">How to invest for retirement</a>
                </li>

                </ul>
                <h3>Health Care</h3>
                <ul>
                                    <li>

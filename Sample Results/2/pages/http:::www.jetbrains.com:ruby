<script type="text/javascript">
$(document).ready(function() {
    $('#menu_products').hide();
    $('#jb_nav .products a').click(
        function(){
            var productsView = $('#menu_products');
            if (productsView.is(":visible")) {
                setTimeout(function() {
                    $('#jb_nav .products').removeClass('open');
                }, 1000);
                productsView.stop(false, true).slideUp(500);
            } else {
                $(this).parent().addClass('open');
                productsView.stop(false, true).slideDown(500);
            }
            return false;
        }
    );
    $('#menu_products .close').click(
        function(){
            setTimeout(function() {
                $('#jb_nav .products').removeClass('open');
            }, 1000);
            $('#menu_products').stop(false, true).slideUp(500);
        }
    );

});
</script>         <link rel="stylesheet" type="text/css" media="screen" href="styleset1.css"/>  <link rel="stylesheet" type="text/css" media="print" href="../01_css/print.css"/>  <link rel="Shortcut Icon" href="../favicon.ico" type="image/x-icon"/>              <script type="text/javascript" src="../js2/checkOS.js"></script>                      <script type="text/javascript" src="../js2/cookie.js"></script>                      <script type="text/javascript" src="../js2/button_download.js"></script>                      <script type="text/javascript" src="../js2/archer_medium.font.js"></script>                      <script type="text/javascript" src="../js2/jquery/fancybox.js"></script>                      <script type="text/javascript">

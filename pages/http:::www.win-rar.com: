<script type="text/javascript">
$('#sr_language_menu_select').change(
  function() {
    $('select option:selected').each(function () {
      _gaq.push(['_trackEvent','Misc', 'language changed', 'changed to ' + $(this).text()]);
    });
  }
)

$('#searchfield').keypress(function(e){
      var code = null;
      code = (e.keyCode ? e.keyCode : e.which);
      if(code == 13){
        _gaq.push(['_trackEvent','Misc', 'search used', $('#searchfield').attr('value')]);
       }
});

$('ul#linklist > li > a').click(
    function(){
        _gaq.push(['_trackEvent','Outbound Links', $(this).text()]);
}
)

</script>






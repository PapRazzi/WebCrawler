          $$('.rta_bottom_lnk')[2].hide();
          btn_margin += 100;
          Mone.event("propromotion","article","1 blue button shown");
          twobuttons = false;
        }
        $('related_promo_container_rel').removeClassName('apos_e');
        $('related_promo_container_rel').addClassName('apos_propromotion');
        new_unit = "<a href='http://seekingalpha.com/account/pro' sasource='propromotion'><div class='propromotion_button fr mt7' style='margin-right:"+btn_margin+"px'></div></a>";
        $('related_promo_container_rel').innerHTML += new_unit;
        if (twobuttons) Mone.event("propromotion","article","2 buttons shown");
        /* The abtest itself */
        var hours = 24;
        if (SA.Pages.Article.pro_article()){
          hours = 24;
        }
        if (SA.Pages.Article.alpha_rich_article()){
          hours = 72;
        }

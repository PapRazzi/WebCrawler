<script type="text/javascript" charset="utf-8">
	$(function() {
		
		confoo.session.tag = "";
		var url = "/en/2013/sessions";
		
		var $tags = $('#content_wrapper div.tag-selector span.tag');
				$sessions = $('div.schedule_day td'),
				$btnWishlist = $('<a id="btnWishlist" href="#" class="btnWishlist" title="Wish List">Wish List</a>');
		
		$tags.filterByTag({
			setTag: function(sessions, tag) {
				$sessions.filter(':has(span.tag.'+tag+')')
					.addClass(tag)
					.addClass('tag');
			},
			unsetTag: function(sessions, tag) {
				$sessions.removeClass(tag).removeClass('tag');
			}
		});
		
							
			$('div.schedule_day td').mouseover(function() {
				$(this).find('div.tags').append($btnWishlist);
				var session_id = $(this).attr('id').replace('session_', '');
				$btnWishlist.attr('href', '/en/wishlist/'+session_id);
			});
				
		$('#tag-selector').scrollFollow({
			speed: 200,
			killSwitch: 'follow',
			onText: 'x',
			offText: '+'
		});
		$('#follow').click();
		
	});
</script>		</div>

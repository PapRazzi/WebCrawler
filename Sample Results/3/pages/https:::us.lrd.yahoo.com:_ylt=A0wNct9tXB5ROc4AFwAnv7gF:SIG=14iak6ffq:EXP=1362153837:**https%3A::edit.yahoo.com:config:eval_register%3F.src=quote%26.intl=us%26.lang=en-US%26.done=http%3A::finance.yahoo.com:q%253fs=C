function loadScriptSync () {
    if(arguments.length < 1) return false;
    var args = Array.prototype.slice.call(arguments);
    var node = document.createElement("script");
    node.type = 'text/javascript';
    node.loaded = false;
    node.src = args.shift();
    if (node.addEventListener) {
        node.addEventListener("load", function (e) {
            if(!e) e = window.event;
            var loadingNode = e.target || e.srcElement;
            if(!loadingNode.loaded) {
                loadingNode.loaded = true;
                args.length > 0 && loadScriptSync.apply(loadingNode, args);
            }
        }, false);
    } else if(node.attachEvent) {
        node.attachEvent('onreadystatechange', function (e) {
            if(!e) e = window.event;
            var loadingNode = e.target || e.srcElement;
            if ((loadingNode.readyState === 'complete' || loadingNode.readyState === 'loaded') && !loadingNode.loaded) {
                loadingNode.loaded = true;
                args.length > 0 && loadScriptSync.apply(loadingNode, args);
            }
        });
    }
    document.body.appendChild(node);
    node = null;
}


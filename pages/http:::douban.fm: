Do.ready('fm-bannerad', function(){
  initBannerAd({
    ad_source: '#fm-banner-ads',
    alt_slot: '#ft-ads-slot',
    shift_cb: function(e){
      if (e === 'hide') {
        $('#fm-section3').css('padding-bottom', 0);
        return;
      }
      $('#fm-section3').css('padding-bottom', e === 'banner'? '145px' : 0);
    }
  });
});

Do.ready(function() {
    window.show_login_for_usr_from_subject = function(){
        var update_captcha = function () {
            $.get('/j/new_captcha', function (captcha_id) {
                    $('form.pop_win_login_form input[name="captcha_id"]').val(captcha_id);
                    var img_url = '/misc/captcha?size=m&id=' + captcha_id;
                    $('form.pop_win_login_form img.captcha').attr('src', img_url);
                }, 'json');
        };

        $.fn.loginFormCheck = function(){
            // 验证邮箱格式
            $('input.pop_email').blur(function(){
                var reg = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/,
                    email = $.trim($(this).val());
                if(email !== '' && !reg.test(email)){
                    $(this).siblings('span.error').show();
                }
            }).focus(function(){
                $('span.error').hide();
            });
            $('form.pop_win_login_form').submit(function(){
                if($.trim($('input.pop_email').val()) === ''){
                    $('div.spec span.error').show();
                }
                if($('span.error:visible').length){
                    return false;
                }
            });
        };

        // 遮罩
        $('body').append($('<div class="overlay"></div>'));
        $('div.overlay').css({'width': $(document).width() + 32, 'height': $(document).height(), 'display': 'block'});

        var context = location.href.split('context=')[1];
        $.get('/j/misc/login_form',
            {start_radio_from_subject: context, redir: '/?context=' + context},
            function(r){
                var dlg = dui.Dialog({
                          width: 555,
                          title: '登录',
                          iframe: 1,
                          content: r
                    }).open(),
                node = dlg.node;
                update_captcha();
                // fix dlg
                node.find('form.pop_win_login_form').prepend(node.find('.login_notice_from_subject'));
                node.find('.hd h3').replaceWith(node.find('.bd h3'));
                node.find('h3 a, form a').attr('target', '_blank');
                node.find('form').loginFormCheck();
                node.find('form.pop_win_login_form img.captcha').click(function () {
                    update_captcha();
                });
                dlg.update();
                $('.dui-dialog-close').attr('href', 'http://douban.fm')
                    .unbind('click').click(function () {
                        location.href = "/";
                    });
            });
        $(window).bind(window.consts.LOGIN_EVENT, function () {
            location.reload();
        });
        return false;
    };

        function open_win(url, title, width, height) {
    var offset = $('body').offset(),
        w = $(window),
        left = parseInt(offset.left +
            ((w.width() - width) / 2), 10),
        top = parseInt(offset.top +
            ((w.height() - height) / 2.5), 10),
        features = (
            'toolbar=0,status=0,resizable=1,' +
            'width=' + width + ',height=' + height +
            ',left=' + left + ',top=' + top ),
        win = window.open(url, title, features);
    if (win) {
        win.focus();
        if (typeof __pop_win !== 'undefined'){
            pop_win.close();
        }
    } else {
        location.href = res;
    }
}

$("#login .sina").click(function (e) {
    e.preventDefault();
    var portal = $(this),
        is_loading = portal.data('loading');
    if (typeof is_loading === 'undefined' || !is_loading) {
        if (!window.auth_url) {
            portal.data('loading', true);
            $.ajax({
                url: '/partner/sina/login',
                success: function (res) {
                            portal.data('loading', false);
                            if (res === 'logged in') {
                                location.reload(true); 
                            } else if (res === 'urlerror') {
                                alert('网络连接超时了, 请稍后重试 :(');
                            } else {
                                window.auth_url = res;
                            }
                        },
                async: false
            });
        }
        if (window.auth_url) {
            open_win(window.auth_url, '用户授权', 600, 400);
        }
    }
    return false;
});

$("#login .msn").click(function () {
    open_win($(this).attr('href'), '用户授权', 465, 375);
    return false;
});

});
</script>


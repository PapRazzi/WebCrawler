</div><script>(function($){$.extend({jGFeed:function(url,fnk,num,key){if(url==null){return false;}var gurl="http://ajax.googleapis.com/ajax/services/feed/load?v=1.0&callback=?&q="+url;if(num!=null){gurl+="&num="+num;}if(key!=null){gurl+="&key="+key;}$.getJSON(gurl,function(data){if(typeof fnk=="function"){fnk.call(this,data.responseData.feed);}else{return false;}});}});})(jQuery);
        $.jGFeed('http://www.ualbertablog.ca/feeds/posts/default?alt=rss',
          function(feeds){
            // Check for errors
            if(!feeds){
              $('.colloquy-blog ul').append('<li>Error retrieving blog feed. <a href="http://www.ualbertablog.ca/">View Colloquy Blog</a></li>');
              return false;
            }
            // do whatever you want with feeds here
            for(var i=0; i<feeds.entries.length; i++){
              var entry = feeds.entries[i];
              // Entry title
              $('.colloquy-blog ul').append('<li><a href="'+entry.link+'">'+entry.title+'</a></li>');
            }
$('.colloquy-blog ul').append('<li class="last">&raquo;<a href="http://www.ualbertablog.ca/">More</a></li>');
          }, 5);</script>

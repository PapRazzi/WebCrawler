			var ct = 0;
			for(x in this.previewData){
				if(x==leaderID) break;
				ct++;
			}
			return ct;
		}
		this.getPreviewDataID = function(idx){
			var ct = 0;
			var dataID;
			for(x in this.previewData){
				if(ct==idx){
					dataID = x;
					break;
				}
				ct++;
			}
			return dataID;
		}

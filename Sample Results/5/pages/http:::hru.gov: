			<script type="text/javascript">
				var crawl_time	= 50;
				var crawl_dist	= 2;
				var crawlTimer;
				var crawl=document.getElementById("news_crawl");
				
				function crawlMove() {
					if (crawl.offsetLeft < -1*crawl_width) {
						crawl.style.left = "0px";
					};
					//alert("left=" + crawl.left + "\nstyle.left=" + crawl.style.left);
					//alert("width=" + crawl.offsetWidth);
					//alert("left=" + crawl.offsetLeft);
					crawl.style.left = (crawl.offsetLeft - crawl_dist) + "px";
					crawlTimer = window.setTimeout(crawlMove, crawl_time);
				}

				crawlTimer = window.setTimeout(crawlMove, crawl_time);
				//alert("left=" + crawl.offsetLeft);
			</script>
			

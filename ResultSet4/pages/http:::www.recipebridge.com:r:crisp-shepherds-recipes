function getEventTarget(e) {
  var targ;
  if (!e) var e = window.event;
  if (e.target) targ = e.target;
  else if (e.srcElement) targ = e.srcElement;
  if (targ.nodeType == 3) // defeat Safari bug
    targ = targ.parentNode;
  return targ;
}


  $(document).click( function (e) {
    var target = getEventTarget(e);
var bg = 'NO';

    if (target.id == 'header') {
       bg = 'YES';
       }
    if (target.id == 'main') {
       bg = 'YES';
       }
    if (target.id == 'wrapper') {
       bg = 'YES';
       }
     if (target.id == 'bgbody') {
       bg = 'YES';
       }
//alert(target.id);
    if (bg == "YES") {


// record the click
var url='http://www.recipebridge.com/background_ci_handler.php?background_index=5&type=C';

$.get(url, function(data) {
// handle the result
});


                window.open('http://www.recipebridge.com/r/spaghetti-recipes','_blank');
            //alert('background clicked');
    }
  });


});
</script>


<script type="text/javascript">
    (function($){
        $('#colMain').resize(adjustColumnHeights);
        $('#secondary_content').resize(adjustColumnHeights);
        $('#tertiary_content').resize(adjustColumnHeights);

        function adjustColumnHeights()
        {

            
            $('#colMain').css('height', 'auto');
            $('#tertiary_content_container').css('height', 'auto');
            $('#tertiary_content').css('height', 'auto');
            $('#secondary_content').css('height', 'auto');
            
            var colHeight = Math.max($('#colMain').height(), $('#tertiary_content_container').height());
            $('#colMain').height(colHeight);
            $('#tertiary_content_container').height(colHeight);
            $('#tertiary_content').height(colHeight - $('#tertiary_content_menu').height());
            $('#tertiary_inner_content').height($('#tertiary_content').height()-10);
            $('#colMain').height(colHeight);
            $('#secondary_content').height(colHeight - $('#rowTop').height()-$('#tertiary_content_menu').height());
            
            $('#sec_content_divider').removeAttr('height');
            $('#sec_content_divider').height($('#secondary_content').height()-20);
        }
        if($('body').hasClass('full_page')){
            $(document).ready(adjustColumnHeights);
        }
//        $(document).ready(adjustColumnHeights);
    })(jQuery);
</script>


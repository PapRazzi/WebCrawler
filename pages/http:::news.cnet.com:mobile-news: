                            ref: 'connectCNETfblike',
                            'data-href': 'http://www.facebook.com/cnet',
                            'data-send': 'false',
                            'data-width':'280',
                            'data-show-faces':'true'
                        });

                        $('facebookInnerDiv').appendChild(fbLike);

                        Facebook.parse({
                            container: $('facebookInnerDiv')
                        });
                    }
                });
            }
            catch(e){}

        //initialize the twitter button when the user hovers over the div
            try{
                $('twitterConnect').addEvent('mouseenter', function(e){

                    if(!this.getElement('.twitter-follow-button')){
                        var twitterButton = new Element('a', {
                            'class': 'twitter-follow-button',
                            'href': 'https://twitter.com/cnet',
                            'data-show-count': 'true',
                            'data-size': 'large',
                            'data-lang':'en',
                            html: 'Follow @CNET'
                        });

                        $('twitterInnerDiv').appendChild(twitterButton);

                        Twitter.loadWidgets();
                    }
                });
            }
            catch(e){}
    });

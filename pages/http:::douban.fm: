    <script>
        Do.ready(function(){
            var tip_timeout,
            tip_bar = $('.top-tip-bar'),

            hide_tip = function(){
                if (tip_bar.is(":visible")) {
                    tip_bar.animate({top:"-45px"}, 'fast', function(){
                        tip_bar.hide();
                    });
                }
            },
            delay_hide_tip = function(t){
                if(tip_timeout) clearTimeout(tip_timeout);
                if (typeof t !== "number") {
                    t = 2000;
                }
                tip_timeout = setTimeout(hide_tip, t);
            };
            $(window).bind("radio:start", function(){
                if ($('#kbps-tip').is(":visible")) {
                    hide_tip();
                }
            });

            var options = $('#kbps-tip input[type=radio]');
            options.click(function(){
                var k = this.value;
                if (k == 'auto') {
                    DBR.set_autokbps(true);
                } else {
                    DBR.set_kbps_manually(k);
                }
                $('.top-tip-bar').hide();
                show_kbps_changed_tip(DBR.get_kbps());
            });
            $('.ignore-tip').click(hide_tip);
            window.show_kbps_slow = function(k){
                $('#kbps-current').html(k);
                var i = {'64': 0, '128': 1, '192': 2}[k];
                if (i == undefined) return;
                options[i].checked = 1;
                $('#kbps-tip').css("top", "-45px")
                    .show().animate({top:0}, 'fast');
            };
            window.show_pro_init_tip = function() {
                if (get_cookie('show_pro_init_tip') !== 'N') {
                    $('#current-kbps').text(DBR.get_kbps());
                    $('#pro-init-tip').show();
                    set_cookie({'show_pro_init_tip': 'N'}, 365,'douban.fm');
                }
            };
            window.show_kbps_changed_tip = function(k) {
                $('#kbps-changed-tip .current-kbps').text(k);
                $('#kbps-changed-tip').show();
                $('#kbps-changed-tip').css("top", "-45px")
                    .show().animate({top:0}, 'fast');
                delay_hide_tip(5000);
            };
            window.on_set_kbps_by_remote = function(k) {
                $(window).unbind('focus');
                $(window).one('focus', function() {
                    show_kbps_changed_tip(k);
                });
            };
        });
    </script>

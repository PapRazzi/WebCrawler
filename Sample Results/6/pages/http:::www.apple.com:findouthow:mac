<script type="text/javascript">
	Event.onDOMReady(function() {
		tracker = new AC.ViewMaster.Tracker();
		tracker.setDelegate({
			trackingNameForSection: function(tracker, name, section) {
				var triggers = section.triggers();
				if (triggers.length > 0) {
					for (var i=0; i<triggers.length; i++) {
						var header = triggers[i].up('ul');
						
						if (header){
							if (header = header.previous('h5')) {
								name = header.innerHTML +' - '+name;
							}
						}
						
						header = triggers[i].up('ul');
						if (header){
							if (header = header.previous('h4')) {
								name = header.innerHTML +' - '+name;
							}
						}
					}
					return name;
				}

				return false;
			}
		});

		new AC.Tutorials.TutorialBrowser('swap', 'tutorial-link', { replayButtonText:'Watch again', ensureInView:true });
	});
</script>


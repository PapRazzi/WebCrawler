<script type="text/javascript">
  (function($){
    $(document).ready(function(){
        $('#tertiary_content_menu').click(function(){
            $('#tertiary_content_menu_panel').slideDown();
            $('#tertiary_content_menu_panel').position({
                my:'right top',
                at:'right bottom',
                of:'#tertiary_content_menu'});

            var cancel=false;
            //Lots of debouncing
            var waitClose = function(){
                setTimeout(function(){if(!cancel){$('#tertiary_content_menu_panel').hide();}cancel=false;}, 1500);
            }
            $('#tertiary_content_menu_panel').mouseleave(waitClose);
            $('#tertiary_content_menu_panel').mouseenter(function(){
                cancel=true;
            });
            waitClose();
        });
    });

  }(jQuery))
</script>          <div id="tertiary_content_menu_panel"></div>

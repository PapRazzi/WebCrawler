<div id="content_container" class="content_container"> 
<script type="text/javascript">
$(document).ready(function(){
var tab_count = 3; // Freemaker variable - Gets size of array
url = document.location.href;
tab_to_show = url.substring(url.lastIndexOf("#")+1,url.length);
if (url.indexOf("#") > 0) {
// Check if #hash exists if not stay on default tab
for (var i = 1; i <= tab_count; i++) {
var tab_to_check = "selection_item_" + i;
tab_to_check = ".tab_selections li a#" + tab_to_check;
var tab_exists = $(tab_to_check).attr('href');
tab_exists = tab_exists.substring(tab_exists.lastIndexOf("#")+1,tab_exists.length);
if (tab_to_show == tab_exists) {
$(".tab_selections li").removeClass();
$(".tab_content div#tab_1_content" ).hide();
active_tab = "a."+tab_to_show;
$(active_tab).parent().addClass("active");
var tab_id = $(active_tab).attr('id');
var tab_number = tab_id.substring(tab_id.lastIndexOf("_")+1,url.length);
active_content = "#tab_" + tab_number + "_content";
$(active_content).show();
}
}
}
$(".tab_selections a").click(function(event){
for (var i = 1; i <= tab_count; i++) {
current_tab = "selection_item_" + i;
tab_content = "#tab_" + i + "_content";
tab = "#selection_item_" + i;
if (event.target.id == current_tab) {
/*
s.prop26= current_tab;
*/
$(tab_content).show();
$(tab).parent().addClass("active");
} else {
$(tab).parent().removeClass("active");
$(tab_content).hide();
}
}
if($(event.target).parent().attr('class') != "active")
$(event.target).parent().addClass("active");
//return false;
});
});
</script>
<div class="tabs" >
<ul class="tab_selections">
<li class="active"><a href="#products" id="selection_item_1" class="products">Products</a>
</li>
<li ><a href="#events" id="selection_item_2" class="events">Events</a>
</li>
<li ><a href="#training" id="selection_item_3" class="training">Training</a>
</li>
</ul>
<div class="tab_container ">
<div class="tab_content">
<!-- begin TAB #1 -->
<div id="tab_1_content">
<div class="grid_192 margined_15">
<div class="divided_content">
<div class="grid_55 push_7">
<p>Explore products for <a href="/products/matlab/">MATLAB</a>, the language of technical computing, and <a href="/products/simulink/">Simulink</a>, for simulation and Model-Based Design.</p>
<h3>Featured Products</h3>
<ul class="unbulleted_list">

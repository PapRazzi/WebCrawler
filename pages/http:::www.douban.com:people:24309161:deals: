<script src="http://img3.douban.com/js/ui/packed_dialog8321689615.js"></script>
<script src="http://img3.douban.com/js/ui/packed_smartform7452882705.js"></script>
<script>
$(function () {
    var dlg;
    $(document).delegate(".bazaar-ship", "click", function(e){
        e.preventDefault();
        if (!dlg) {
            dlg = dui.Dialog({
                title: '填写发货信息',
                url: $(this).parent().attr("href"),
                width: 380,
                cache: false,
                buttons: [],
                callback: function(){
                    var formObj = dui.Smartform($("form", dlg.node)[0], {
                        validate: true,
                        autocheck: true,
                        autofocus: true,
                        placeholder: true
                    });
                    $('.bazaar-shipform', dlg.node).delegate(".bazaar-submit", "click", function(){
                        if (!formObj.validateAll()) {
                            alert("错误：没有填写完整");
                            return;
                        }
                        $.post_withck($("form", dlg.node).attr('action'), formObj.serializeObject(), function(json){
                            if (!json.r) {
                                dlg.set({
                                    content: '提交成功...'
                                });
                                location.reload();
                            } else {
                                alert(json.msg);
                            }
                        }, "json");
                    }).delegate(".bazaar-close", "click", function(){
                        dlg.close();
                    });
                }
            });
        }
        dlg.open();
    }).delegate(".wgt-good-close", "click", function(e){
        var me = $(this);
        e.preventDefault();
        dui.Dialog({
            title: '下架商品',
            content: '下架后的商品不能重新上架。确定下架么？',
            width: 320,
            buttons: [{
                text: '确定',
                method: function(o) {
                    me.parent().parent().submit();
                }
            }, "cancel"]
        }).open();
    });

});
</script>


    
    

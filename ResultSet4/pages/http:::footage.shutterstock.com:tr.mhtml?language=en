	<script type="text/javascript">
		Modernizr.load([
			{
				test: window.JSON,
				nope: "/public/libs/min/?b=public/libs&f=json/json2.js&v=2.59",
				both: "http://lilb2.shutterstock.com/lilbro.min.js?v=vf04&fv=2.59",
				complete: function() {
					if (window.LilBro) {
						try {
							Ss.lilBro = new LilBro({
								element: document.getElementById('container'),
								server: 'lilb2.shutterstock.com',
								ssl_server: 'lilb2.shutterstock.com',
								watch_focus: true,
								event_base: {"results_per_page":50,"results_sort_by":"popular","language":"en","country_ip":"US"}
							});

							var referral = new LilBro.Module.Referral();
							referral.track({
								customer_id: undefined,
								new_visit: 1
							});
							
							var performance = new LilBro.Module.Performance();
							performance.track();
					
							Ss.lilBro.write({
								'event_type': 'page_load'
							});
							
													} catch (e) { 
							Ss.log && Ss.log('Error in LilBro: ', e.message);
					 	}
					}
				}
			}
		]);
	</script>


	<script>
	$(function() {
		function doTrack () {
			if (Ss.injectPixel || Ss.injectFrame) {
								Ss.injectFrame('http://pixel.fetchback.com/serve/fb/pdj?cat=&name=landing&sid=4180', 'global-fetchback');
										Ss.injectPixel('http://pixel.quantserve.com/pixel/p-c9yvUO0_-bqPE.gif?labels=_fp.event.Homepage');
				
			} else {
				setTimeout(doTrack, 33);
			}
		}

		doTrack();
	});
	</script>
	

amzn_wdgt.showImage='True';
amzn_wdgt.showPrice='True';
amzn_wdgt.showRating='True';
amzn_wdgt.design='2';
amzn_wdgt.colorTheme='BrushedSteel';
amzn_wdgt.headerTextColor='#FFFFFF';
amzn_wdgt.marketPlace='US';
</script>
<script type='text/javascript' src='http://wms.assoc-amazon.com/20070822/US/js/AmazonWidgets.js'>
</script>
</div>
<script type='text/javascript'>
  //<![CDATA[
    if(BLOOMBERG.util.Cookie.get('bb_amazon_test')=='show'){
    $(function(){
      setTimeout(function(){
      var widget =  $('#bb_amazon_widget'),
          widget_content = widget.find('.amzn_wdgt');
      if(widget_content.length > 0){
        widget_content.css({'border':'none','height':'270px'});
        widget_content.find('.wdgt_hd').hide();
        widget_content.find('.wdgt_ft').hide();
    
        var widget_table = widget_content.find('.wdgt_brdr .wdgt_tl');
        widget_content.find('.asin_h1 a').attr("style","color: #333333 !important; font-size: 13px; font-family: Arial; text-decoration: none;");
        widget_content.find('.asin_h1 a').hover(function () {$(this).css({"text-decoration":"underline"})},function () {$(this).css({"text-decoration":"none"})});
        widget_content.find('.asin_h2 a').hover(function () {$(this).css({"text-decoration":"underline"})},function () {$(this).css({"text-decoration":"none"})});
        widget_content.find('.asin_h2 a').attr("style","color: #666666 !important; font-size: 11px; font-family: Arial;");
        widget_table.css({"height":"90px","width":"325px","border-bottom":"1px solid #ccc","position":"relative"});
        widget_table.find('.asin_img').css({"padding-right":"10px"});
        widget_table.find('.asin_review').css({"position":"absolute","top":"65px"});
        widget_table.find('.asin_img').css({"width":"50px","margin-left":"8px"});
        widget_table.find('.asin_price').attr("style","color: #cc0000 !important; position: absolute; top: 62px; left: 68px; font-weight: normal; font-size: 13px;");
        widget_table.find('.asin_h2').css({"margin-left":"68px"});
    
        if($.browser.msie && $.browser.version=="7.0"){
          widget_table.find('.asin_review').css({"left":"-5px"});
        }
    
        $.each(widget_content.find('.asin_h2 a'),function(){
        var this_text = $(this).text();
          var new_text = "By " + this_text.replace(/\(.*/,"");
          if(new_text.length>40){
            new_text = $.trim(new_text).substring(0, 40).split(",").slice(0, -1).join(",");
          }
          $(this).text(new_text);
        });
        $.each(widget_table,function(){
        var url = $(this).find('.asin_h1 a').attr('href');
          $(this).append("<div class='buy_amzn'><a href='"+url+"' target='_blank'>Buy From Amazon</a></div>");
        })
    
        var widget_links = widget_content.find('a')
        widget_links.attr('rel','nofollow');
        widget_links.each(function() {
            $(this).mousedown(function() {
                BLOOMBERG.tracker.EVENTTRACK.record('Module Content Link Tracking', "amazon_widget", this.href);
            })
        });
        if(!$.browser.msie){
          try{
          $.each(widget_content.find('.asin_h1 a'), function(){
            $(this).trigger('mouseover');
            var hl = $('#amzn_popup_div .asin_h1').text();
            if(hl.length>70){
              hl = $.trim(hl).substring(0, 70).split(" ").slice(0, -1).join(" ") + "...";
            }
            $(this).text(hl);
            $(this).trigger('mouseout');
          })} catch(e){}
        }
    
        widget_links.each(function() {
          $(this).removeAttr('onmouseout').removeAttr('onmouseover');
        });
        $("#amzn_popup_div").hide();
        widget.show();
      }
      }, 3000);
    });
    }
  //]]>
</script>


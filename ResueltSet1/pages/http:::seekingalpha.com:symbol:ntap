      var tsNow = new Date().getTime(), tsAt6 = Math.floor(tsNow/(24*3600*1000))*(24*3600*1000)+10*3600*1000;
      load_remote_script("http://static3.cdn-seekingalpha.com/memcached2/perm/price_changes?t="+(tsNow>=tsAt6 ? tsAt6 : tsAt6-24*3600*1000));
    })();
  </script>


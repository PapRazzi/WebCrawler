<div id="special-features">
    <div class="sectionTitle">Special Features</div>

<script language=javascript>
//Set the number of ads here.
var numberOfAdsToAppear = 3;
var adlink = new Array();

//List of text ads goes here.
adlink[0] = '<a href="http://www.thestreet.com/sponsor/personalfinance/index.html"><b>Credit Resource Center</b></a> - <i><font color="gray">Sponsored by FreeCreditScore.com </font></i>';
adlink[1] = '<a href="http://www.thestreet.com/sponsor/Leadership/index.html?partner=Leadership"><b>The C-Suite Forum</b></a> - <i><font color="gray">Sponsored by Disney</font></i>';
adlink[2] = '<a href="http://www.thestreet.com/sponsor/screen/index.html"><b>Multi-National Exposure: Japan</b></a> - <i><font color="gray">Sponsored by WisdomTree</font></i>';
var ni = adlink.length;
document.write("<table border=\"0\">");
for (e=1; e<=numberOfAdsToAppear;e++) {
var rn = Math.floor(Math.random()*ni);
if(!adlink[rn])break;
document.write("<tr><td>"+adlink[rn])+"</td></tr>";
adlink.splice(rn,1);
var ni = adlink.length;}
document.write("</table>");
</script>


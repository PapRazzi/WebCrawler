    document.getElementById('low52week').innerHTML = qmQuote.low52Week.replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    document.getElementById('eps').innerHTML = qmQuote.eps.replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    document.getElementById('peRatio').innerHTML = qmQuote.peRatio.replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    document.getElementById('dividend').innerHTML = qmQuote.dividend.replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    document.getElementById('yield').innerHTML = qmQuote.yield.replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    document.getElementById('marketCap').innerHTML = qmQuote.marketCap.replace(/(&nbsp;| )(b|m)</,function (All,$1, $2) {return $1 + $2.toUpperCase()+'<'}).replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    document.getElementById('volume').innerHTML = qmQuote.volume.replace(/(&nbsp;| )(b|m)</,function (All,$1, $2) {return $1 + $2.toUpperCase()+'<'}).replace(/<div(?:.*?)<\/div><div(?:.*?)>(.*?)<\/div>/i,"");
    try{
      if (qmError){
        $('qmData').hide();
        $('qmQuoteChart').addClassName('ml90');
      }
    } catch(e) {}
  }

  var chartRefresher = null;
  function LoadQuoteImg(symbol,scale) {
    clearTimeout(chartRefresher);
    
    var bold = new Array();
    var scales = new Array("5d","1m","3m","1y","5y","10y");
     
    $('qmQuoteChart').innerHTML = '<img width="298" height="150" src="http://app.quotemedia.com/quotetools/getChart?webmasterId=91022&amp;snap=true&amp;symbol='+symbol+'&amp;chscale='+scale+'&amp;chtype=AreaChart&amp;chwid=284&amp;chhig=150&amp;chfill=ee0066CC&chfill2=110066CC&chln=0066CC&chmrg=0&chfrmon=false&chton=some" galleryimg="no">';
    bold[scale] = 'font-weight:bold';
    var scales_str = '<div style="margin-top:8Px;"><span onclick="LoadQuoteImg(\''+symbol+'\',\'1d\')" class="period fl" style="border-left:0;'+bold['1d']+'">Today</span>';
      
    for (i=0;i<scales.length;i++){
      scales_str += '<span class="period fl" onclick="LoadQuoteImg(\''+symbol+'\',\''+scales[i]+'\')" style="'+bold[scales[i]]+'">'+scales[i]+'</span>';
    }
      
    scales_str += '</div>';
    $('qmQuoteChart').innerHTML += scales_str;
    if (scale == "1d" || scale == "5d"){
      chartRefresher = setTimeout("LoadQuoteImg('"+symbol+"','"+scale+"')",360 * 1000);
    } 
  }
</script>
          </div>
        </div>
      </div>

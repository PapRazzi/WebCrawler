        function SubmitQuery(e) {

            var isEnter = window.event == null ? e.keyCode == 13 : window.event.keyCode == 13;



            if (isEnter)

            

            {

                var q = document.getElementById('container1_0_searchbox_0__q');

                var cx = document.getElementById('container1_0_searchbox_0__cx');

                var cof = document.getElementById('container1_0_searchbox_0__cof');



                var str = "http://www.onecard.ualberta.ca/" + "/Search.aspx?q=" + escape(q.value) + "&cx=" + escape(cx.value) + "&cof=" + escape(cof.value);

                window.location = str;

                return false;

            }

            else

            {

                return true;

            }

        }

        function SubmitQueryClick(e) {

            

                var q = document.getElementById('container1_0_searchbox_0__q');

                var cx = document.getElementById('container1_0_searchbox_0__cx');

                var cof = document.getElementById('container1_0_searchbox_0__cof');



                var str = "http://www.onecard.ualberta.ca/" + "/Search.aspx?q=" + escape(q.value) + "&cx=" + escape(cx.value) + "&cof=" + escape(cof.value);

                window.location = str;

                return false;

            

        }

        </script>

    function propromotion(){
      if ($('related_promo_container_rel') && !is_pro && (SA.Pages.Article.pro_article() || SA.Pages.Article.alpha_rich_article())){
        var promotion_version = ABTest.identity%2;
        var btn_margin = 85;
        var twobuttons = true;
        if (SA.Data.User.hasPortfolio() && SA.Data.User.hasPortfolio().split(",").length > 20 || readCookie('xp')) {
          $$('.rta_bottom_lnk')[2].hide();
          btn_margin += 100;
          Mone.event("propromotion","article","1 blue button shown");
          twobuttons = false;
        }
        $('related_promo_container_rel').removeClassName('apos_e');
        $('related_promo_container_rel').addClassName('apos_propromotion');
        new_unit = "<a href='http://seekingalpha.com/account/pro' sasource='propromotion'><div class='propromotion_button fr mt7' style='margin-right:"+btn_margin+"px'></div></a>";
        $('related_promo_container_rel').innerHTML += new_unit;
        if (twobuttons) Mone.event("propromotion","article","2 buttons shown");
        /* The abtest itself */
        var hours = 24;
        if (SA.Pages.Article.pro_article()){
          hours = 24;
        }
        if (SA.Pages.Article.alpha_rich_article()){
          hours = 72;
        }
        if (promotion_version == 1 && $('article_pro_bottom_branding')){
          $('related_promo_title').innerHTML += " Pro subscribers got this article "+hours+" hours earlier. ";
          $('article_pro_bottom_branding').innerHTML = $('article_pro_bottom_branding').innerHTML.substr(0,$('article_pro_bottom_branding').innerHTML.indexOf("ET")+3) + $('article_pro_bottom_branding').innerHTML.substr($('article_pro_bottom_branding').innerHTML.indexOf("<a"));
        }
        if (promotion_version == 0 && $('article_pro_bottom_branding')){
          var first_index = $('article_pro_bottom_branding').innerHTML.indexOf("Pro subscribers on") + 19;
          var second_index = $('article_pro_bottom_branding').innerHTML.indexOf(" at ");
          var version_date = $('article_pro_bottom_branding').innerHTML.substr(first_index,second_index-first_index);
          var version_number = $('promo_num_of_rta_rel') ? $('promo_num_of_rta_rel').innerHTML : 10;
          $('related_promo_title').innerHTML = "This article on <span class='replace_by_rel_slug'></span> was published to Pro subscribers on "+version_date+". "+version_number+" non-paying readers were sent it via email alert "+hours+" hours later.";
          $('article_pro_bottom_branding').hide();
        }
      }
    }

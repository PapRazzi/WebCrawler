<script type="text/javascript">
	$(function () {
        function flog (f) {
            iclog.logClick(function () { f.submit(); },
                $(f).attr(wsNs.constants.coidAttribute), $(f).attr(wsNs.constants.copAttribute));
        }
        
		$('#topSearchForm').validate({
		   rules: { q: { required: true }, ql: { requiredIfVisible: true } },
		   messages: { q: { required: '' }, ql: { required: '' } },
           submitHandler: flog
		});

        $('#bottomSearchForm').validate({
		   rules: { q: { required: true }, ql: { requiredIfVisible: true } },
		   messages: { q: { required: '' }, ql: { required: '' } },
           submitHandler: flog
		});

        $('#navigationalMarketingForm').validate({
		   rules: { q: { required: true } } ,
		   messages: { q: { required: '' } },
           submitHandler: flog
		});

		$('#advancedSearchForm').validate({ submitHandler: flog });

	});
   
</script>

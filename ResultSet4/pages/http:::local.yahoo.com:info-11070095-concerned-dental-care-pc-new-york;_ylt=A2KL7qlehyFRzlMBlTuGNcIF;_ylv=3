<script type="text/javascript">


  if (typeof YAHOO == "undefined" || !YAHOO) {
      var YAHOO = {};
  }



  var registerNamespace = function( namespaceName ) {
    var finalNamespace= window;
    if( namespaceName !== "" ) {
        var namespaceParts = ("" + namespaceName).split( "." );
        for( var i = 0; i < namespaceParts.length; ++i ) {
            finalNamespace[namespaceParts[i]] = finalNamespace[namespaceParts[i]] || {};
            finalNamespace= finalNamespace[namespaceParts[i]];
        }
    }
    return finalNamespace;
  };

   var NS = registerNamespace;
   NS( "YAHOO.CAPTCHA" );
   NS( "YABUSE.CAPTCHA" );
   var YACV = NS( "YABUSE.CAPTCHA.VARS" );
   if (YACV.captchas == undefined) {
       YACV.captchas = new Object();
   }
   if (YACV.initParams == undefined) {
       YACV.initParams = new Object();
   }


    YABUSE.CAPTCHA.loadScript = function(url) {
                var head = document.getElementsByTagName("head")[0];
                var scripts = head.getElementsByTagName("script");
                var exists = false;
                var len = scripts.length;
                for(var i=0; i<len; i++) {
                   if (scripts[i].src == url) {
                      exists = true;
                      break;
                   }
                }
                if (!exists) {
                   var script = document.createElement("script");
                   script.type = "text/javascript";
                   script.src = url;
                   head.appendChild(script);
                } else if (YAHOO.CAPTCHA.initCaptchas) {
                  YAHOO.CAPTCHA.initCaptchas();
                } 
    };

    YABUSE.CAPTCHA.loadStyle = function(url) {
        var head = document.getElementsByTagName("head")[0];
        var styles = head.getElementsByTagName("link");
        var exists = false;
        var len = styles.length;
        for (var i=0; i<len; i++) {
            if (styles[i].href == url) {
                exists = true;
                break;
            }
        }
        if (!exists) {
            var style = document.createElement("link");
            style.setAttribute("type", "text/css");
            style.setAttribute("rel", "stylesheet");
            style.setAttribute("href", url);
            head.appendChild(style);
        }
    };
if (YACV.captchas['V5'] == undefined) {
  YACV.captchas['V5'] = new Object();
}
YACV.captchas['V5']['Captcha'] = new Object();
YACV.captchas['V5']['Captcha'].added = 0;
YACV.captchas['V5']['AudioCaptcha'] = new Object();
YACV.captchas['V5']['AudioCaptcha'].added = 0;
if (YACV.initParams['V5'] == undefined) {
   YACV.initParams['V5'] = new Object();
}
YACV.initParams['V5'].id = "V5";
      YACV.initParams['V5'].proxyUrl = "/captcha/CaptchaWSProxyService.php";
      YACV.initParams['V5'].captchaView = "visual";
      YACV.initParams['V5'].audioHost = "https://c5a.ah.yahoo.com:443";
      YACV.initParams['V5'].swfRandom = "A";
      YACV.initParams['V5'].flashUrl = "https://s.yimg.com:443/gu/static/5.3.24_2/include/cplayer.swf";
      YACV.initParams['V5'].audioUrl = "https://c5a.ah.yahoo.com:443/snd/ws_yHtz9_B0rbPUIMX45YVu7J9nroFq0PjOqOFdR8_yTocisW3h6aKMgTUxd.Tnf7.FRYbD4iHf.XvJ9kgWp_CWrklGYqCeVaxPiHnYZ2sPGHILGagM6hm2tldgPxA-.mp3";
      YABUSE.CAPTCHA.loadStyle('https://s.yimg.com:443/gu/static/5.3.24_2/include/CaptchaSecure.css');
YABUSE.CAPTCHA.loadScript('https://s.yimg.com:443/gu/static/5.3.24_2/include/Captcha_AudioCaptcha.js');

</script>

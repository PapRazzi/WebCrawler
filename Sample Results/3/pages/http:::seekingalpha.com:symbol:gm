      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">GM</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 10,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['gm']
      });
    })();

    try { SymbolData.handle_data('GM', {"rt_time":"11:42AM ET,02/15","last":27.655,"high":27.815,"open":27.64,"close":27.75,"low":27.22,"at":1360928557,"volume":5658310,"volume_at":1360928447,"avg3vol":10970900.0,"c_share":14.89,"div_rate":0.0,"forward_pe":7.7,"payout":0.0,"trailing_pe":10.7,"div_yield":0.0,"chgp_1w":0.4,"chgp_4w":-2.1,"chgp_52w":4.9,"beta60":1.69,"beta36":1.69,"mov_avg_10d":28.41,"mov_avg_10w":28.23,"mov_avg_200d":23.75,"short_interest":5.8,"p_short_interest_float":7.6,"liquidity":302957600.0,"p_week_vol_shares":1.4,"sfloat":905325535.0,"shares_held":1550583750.0,"p_shares_by_inst":99.0,"p_shares_by_insid":0.1,"net_insid_tr":3845.0,"insid_sb":3845.0,"insid_ss":0.0,"total_revenue":150939000000.0,"revenue_growth":0.6,"net_income":5719000000.0,"gross_margin":17.2,"eps":2.67,"roe":18.3,"roe_ind":269.1,"roa":3.7,"cur_ratio":1.3,"quick_ratio":0.9,"int_cover":14.2,"debt_eq":0.53,"lt_debt_cap":0.2,"debt_short":6278000000.0,"debt_long":10375000000.0,"low52":18.72,"high52":30.68,"market_cap":43308280196.225,"cash":33729036236.55,"debt":16653000000.0,"total_enterprise":26232243959.675,"price_book":1.384827240861292,"ev_ebitda":1.873731711405357,"price_sales":0.2869257130113821,"price_tangb":-7.091025641025642,"ev_sales":0.17379367797371786,"ev_fcf":2.8536457973529252,"inst_chg":1000848863.0,"cf":12340236772.6,"fcf":9192536783.65,"debt_fcf":1.811578282680283,"rt_ts":1360928539,"chg_vs_mkt_1w":0.0,"chg_vs_mkt_4w":-4.0,"chg_vs_mkt_52w":-6.0,"p_close_10w":2.0,"p_close_200d":21.0}) } catch (e) { }

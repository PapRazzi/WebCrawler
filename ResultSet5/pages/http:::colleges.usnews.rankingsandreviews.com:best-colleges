   padding:0;
   margin:0;
   width:336px;
   display: block;
   overflow:hidden;
}

#rail > dl.coll-compass-2012-widget > dt  {
   padding:0;
}

#rail > .coll-compass-2012-widget > .coll-compass-2012 > .widget-image > a {
display: block;
line-height: 0px;
overflow: hidden; 
}

#rail > dl.coll-compass-2012-widget > dd.coll-compass-2012 > p {
   margin:0;
}
</style></p>
    
    
    
    
    

  
  
</dd>

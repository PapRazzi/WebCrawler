	<script>
	var relationship = {
		'AboutAlibaba':      'about_alibaba',
		'AccountMembership': 'account_membership',
		'MyAlibaba':         'my_alibaba',
		'PostingInformation':'posting_information',
		'Search':            'search',
		'Contact':			 'contact',
		'CustomizedSourcing': 'customized_sourcing',
		'InspectionService': 'inspection_service',
		'Escrow':			 'escrow',
		'GroupSourcing':	 'group_sourcing',
		'VerifiedMembers':	 'verified_members',
		'TradeManager':		 'trademanager',
		'TradeAlert':		 'trade_alert',  
		'Forums':			 'forums',  
		'Others':			 'others'   
	};
	function setCurrent(menuName){
		var menuId = relationship[menuName];
		var elMenu;
		if(menuId){
			elMenu = document.getElementById(menuId);
			if(elMenu){
				elMenu.className = 'current';
			}
		}
	}
	</script>

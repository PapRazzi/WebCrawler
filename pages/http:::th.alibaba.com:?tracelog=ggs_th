<script>
YUE.onDOMReady(function(){
		var form=get('header-search-form');
		var search=get('header-action-search');
		var searchText=get('SearchTextIdx');
		YUE.on(search,'click',function(){
			if(searchText.value=='' || searchText.value=='Please input a keyword'){
			    alert('Please input a search term.');
			}
			else{
				form.submit();
			}
		});
});
AE.define("header.nav.item", function () {
	var lis=YUD.getElementsByClassName('header-nav-item','li');
	YUE.on(lis,'mouseover',function(event){
		var ul=this.getElementsByTagName('ul')[0];
		YUD.setStyle(ul,'display','block');
		YUD.addClass(this,'header-nav-item-hover');
		
	});
	YUE.on(lis,'mouseout',function(){
		var ul=this.getElementsByTagName('ul')[0];
		YUD.setStyle(ul,'display','none');
		YUD.removeClass(this,'header-nav-item-hover');
	});
});
AE.define("home.left.nav",function(){
	YUE.onDOMReady(function(){
		var lis=get('categories').getElementsByTagName('li');
		YUE.on(lis,'mouseover',function(){
			var div=this.getElementsByTagName('div')[0];
			YUD.setStyle(this,'position','relative');
			YUD.addClass(this,'current');			
			YUD.addClass(div,'on');
			
		});
		YUE.on(lis,'mouseout',function(){
			var div=this.getElementsByTagName('div')[0];
			//YUD.setStyle(this,'position','normal');
			YUD.removeClass(this,'current');
			YUD.removeClass(div,'on');
			YUD.setStyle(this,'position','static');
			
		});
	});
	
}); 
AE.define("home.top.search",function(){
	var navigatorInfo=window.navigator.userAgent.toLowerCase();
	reg=/firefox|chrome/g;
	if(!reg.test(navigatorInfo)){
		var search=get('SearchTextIdx');
		var text=search.getAttribute('placeholder');
		search.value=text;
		YUE.on(search,'click',function(){
			if(this.value==text){
				this.value="";
			}
		});
		YUE.on(search,'blur',function(){
			if(this.value.replace(/^[\s]*$/g,'')==""){
				this.value=text;
			}
		});
	}
});
AE.define("home.sign.check",function(){
	var cookie=document.cookie.split(';');
	var reg=/xman_us_t=[\w & = \|]*/;
	var reg2=/xman_us_f=[\w & = \|]*/;
	var index=-1;
	var flag=-1;
	for(var i=0,len=cookie.length;i<len;i++){
		if(reg.test(cookie[i])){
			flag=1;
			break;
		}
	}
	if(flag==1){
		for(var i=0,len=cookie.length;i<len;i++){
			if(reg2.test(cookie[i])){
				index=i;
				break;
			}
		}
		var info=cookie[index].replace('xman_us_f=','');
		var reg3=/x_user=[\w \|]*/;
		var x_user=info.match(reg3);
		var x_user_name=x_user[0].split('|')[1];
		var div=YUD.getElementsByClassName('header-nav-user')[0];
		div.innerHTML='Welcome '+x_user_name+', | '+'<a href="http://us.my.alibaba.com/user/sign/sign_out.htm" rel="nofollow">Sign out</a>'
	}
});
AE.use(["header.nav.item","home.top.search","home.sign.check"], function () {
    	
});
</script>

<style>
.search-box{padding:15px 30px;background-color:#f5f5f5;}
.search-box input.text{margin-right:5px;padding-left:10px;width:610px;height:34px;border:1px solid #c8c8c8;font-size:12px;line-height:34px;color:#c1c1c1; font-family:arial;}
.search-box .ui-button-primary-l{display:inline-block;*display:inline;*zoom:1;vertical-align:top;height:36px;line-height:36px;padding-top:0;padding-bottom:0;}
</style>
<script>
//Enter键搜索
function enterPress(event){
	var event = event || window.event;
	if(event.keyCode ==13){
		searParam();
	}
}
</script>
<div class="search-box">
	<input type="text" class="text" id="search_text" autocomplete="off" value="Input Keywords Here." onkeypress="enterPress(event)" onkeydown="enterPress(event)" /><input type="button" class="ui-button ui-button-primary-l" value="Search" onclick="searParam();" />
</div>

<script>
String.prototype.trim = function(){     
     // 用正则表达式将前后空格     
     // 用空字符串替代。     
     var t = this.replace(/(^\s*)|(\s*$)/g, "");   
     return t.replace(/(^　*)|(　*$)/g, "");
} 
//参数传递
function searParam(){
	var rg = new RegExp('^[\x07-\x7e]*$'),
	    enOnly = rg.test(searText.value)
	if(searText.value.trim() != "" && searText.value != "Input Keywords Here."){
		if(!enOnly){
			alert("Sorry, the information you filled in is not in English. Please input the information in English instead.");
		}else{
			document.location.href = 'http://news.alibaba.com/help/helpSearch.htm?SearchText='+ searText.value.trim();
		}
	}else if(searText.value.trim() == ""){
		searText.value = "Input Keywords Here.";
		searText.style.color="#ccc";
	}
};
//文本域焦点 提示文案消失
function addListener(element,e,fn){
   if(element.addEventListener){
   element.addEventListener(e,fn,false);
   } else {
   element.attachEvent("on" + e,fn);
   }
}
var searText = document.getElementById("search_text");
addListener(searText,"focus",function(){
	if(searText.value == 'Input Keywords Here.'){
		searText.value = "";
	}
	searText.style.color="#333";
})
addListener(searText,"blur",function(){
	if (searText.value ==''){
		searText.value = 'Input Keywords Here.';
		searText.style.color="#ccc";
	}
})
</script>

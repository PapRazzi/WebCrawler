<script type="text/javascript" src="http://ajax.maps.yahoo.com/static/ymaps-manifest.js"></script>
<script type="text/javascript" >
window.onload = function(){
//YUI({base:'http://yui.yahooapis.com/3.3.0/build/'}).use('node', function(Y){
YUI().use('node', function(Y){
		Y.use('ymaps', function(Y){
Y.one("#yls-hm-loadmap").on('click', function(d){
Y.one("#yls-hm-mapstatic").setStyle("display","none");
Y.one("#yls-hm-loadmap").setStyle("display","none");
Y.one('#yls-hm-map-gws').set("innerHTML","<div id='map'></div>");
   // initialize maps with appid
    Y.YMaps.init({
        appid: 'local.search'  // required (but don't use 'demo' for production!)
        });

    // create a geolocation to center the map on
    var yahooGeo = new Y.YMaps.GeoLocation({
       lat: 38.959374,
       lon: -77.354571      }),

    // create a Map instance
    map = new Y.YMaps.Map({
        boundingBox: Y.one('#map'),  // place map in "map" div
        center: yahooGeo
        });

    // display the map
    map.render();
    map.after("renderComplete", function(){
        map.draw(new Y.YMaps.Marker({
        geoLocation: new Y.YMaps.GeoLocation({
         lat: 38.959374,
       lon: -77.354571         }),
               label:  '<img src="http://l.yimg.com/na/global/p/globalmaps/img/star-1237.png">'
        }));
        zoom = new Y.YMaps.ControlSimpleZoom({ map: this });
        zoom.get('boundingBox').setStyles({
            top: '50px',
            right: '40px'
        });
        this.draw(zoom);
         pan = new Y.YMaps.ControlPan({ map: this });
        pan.get('boundingBox').setStyles({
            top: '10px',
            right: '6px'
        });
        this.draw(pan);
        myScale = new Y.YMaps.ControlScale({ map: this});
       myScale.get("boundingBox").setStyles({
            "bottom": "20px",
            "left": "40px"
            });
       this.draw(myScale);
 
   });
});

            <script type="text/javascript" language="javascript">

                $(document).ready(init);

                function init() {

                    var sortField = 'CreationDate';
                    var sortDirection = 'Descending';
                    var showOpenIssuesOnly = true;
                    var pageSize = 10;

                    $('#SortByLink_' + sortField).hide();
                    $('#SortByText_' + sortField).show();
                                        
                    $('#CurrentSortField').val(sortField);
                    $('#CurrentSortDirection').val(sortDirection);
                    $('#ShowOpenIssuesOnly').val(showOpenIssuesOnly);
                    $('#PageSize').val(pageSize);

                    changeSortArrowImage(sortDirection);

                    $('#discussion_search').keypress(checkForEnter);
                    $('.votebox').voting({ 
                        voteUrl: "http://ironpython.codeplex.com/workitem/vote",
                        loginUrl: 'https://www.codeplex.com/site/login?RedirectUrl=http%3a%2f%2fironpython.codeplex.com%2fworkitem%2flist%2fbasic%3fPendingVoteId%3d-1'
                    });

                    $(document).keyboardShortcuts({
                        createItem: goToCreateItem
                    });

                    $('.IssuesToShowSelection').click(function() {
                        $('#IssuesToShow').val($(this).data('value'));
                        searchWorkItems();
                        return false;
                    });
                }

                function getCurrentConfigurationState() {

                    var configuration = {
                        sortField: $('#CurrentSortField').val(),
                        sortDirection: $('#CurrentSortDirection').val(),
                        showOpenIssuesOnly: $('#IssuesToShow').val() == '1',
                        keywords: $('#discussion_search').val(),
                        myEmailSubscriptions: $("input[name=MyEmailSubscriptionsChecked]:checked").val() ? true : false,
                        pageSize: $('#PageSize').val()
                    };

                    return configuration;

                }

                function updateConfigurationState(configuration) {
                    $('#CurrentSortField').val(configuration.sortField);
                    $('#CurrentSortDirection').val(configuration.sortDirection);
                }

                function searchWorkItems() {
                    sortWorkItems();
                }

                function filterBySubscriptions() {
                    sortWorkItems();
                }

                function changeSortOrder() {

                    var configuration = getCurrentConfigurationState();
                    var direction = (configuration.sortDirection === 'Ascending') ? 'Descending' : 'Ascending';

                    changeSortArrowImage(direction);

                    configuration.sortDirection = direction;
                    updateConfigurationState(configuration);

                    sortWorkItems();

                }

                function changeSortArrowImage(direction) {

                    var imagePath = 'http://i3.codeplex.com/Images/v20104/';
                    var sort = (direction === 'Ascending') ? 'asc' : 'dsc';
                    var img = imagePath + 'arrow_' + sort + '.gif';
                                        
                    $('#SortByImage').attr('src', img);

                }

                function sortByField(field) {
                                        
                    var configuration = getCurrentConfigurationState();
                    var obj = $('#SortByLink_' + field);
                    var direction = $(obj).attr('c:defaultSortDirection');
                    configuration.sortField = field;
                    configuration.sortDirection = direction;
                    updateConfigurationState(configuration);

                    sortWorkItems();

                }

                function sortWorkItems() {

                    showUpdating();

                    var url = "http://ironpython.codeplex.com/workitem/list/basic" + getSearchParams();
                                                    
                    window.location.href = url;

                }

                function getSearchParams() {
                    var configuration = getCurrentConfigurationState();
                                    
                    var whichIssues = (configuration.showOpenIssuesOnly) ? 'Open' : 'All';
                    var params = "?field=" + configuration.sortField 
                                    + "&direction=" + configuration.sortDirection
                                    + "&issuesToDisplay=" + whichIssues
                                    + "&keywords=" + configuration.keywords
                                    + "&emailSubscribedItemsOnly=" + configuration.myEmailSubscriptions;
                    return params;
                }

                function showUpdating() {
                    CodePlex.UpdateProgress.show(); 
                }
                                    
                function checkForEnter(event) {
                    if ($keyCode(event) == '13') {
                        searchWorkItems();
                        return false;
                    }
                }

                CodePlexPaging.initialize('getSearchParams');
            </script>
        </div>
    
        
                

p.registration.emailCsvReport=function(a,b){$("#generate-csv-report").addClass("disabled");$("#generate-csv-report").val("Generating...");var d="/events/register/csv/"+a+"/",e=b?{report:b}:k;$.post(d,e,function(a){a.success&&p.registration.showMailConfirmation(a.email)})};
p.registration.showMailConfirmation=function(a){$("#generate-csv-report").removeClass("disabled");$("#generate-csv-report").val("Generate CSV Report");var b=$("#mail-confirm");b.find("#csv-email").html(a);a=b.find("button");b.dialog($.extend({buttons:[{text:a.eq(0).html(),"class":"button button-blue",click:function(){$(this).dialog("close")}}]},p.devsite.dialogConfig));b.dialog("open")};
p.registration.adminInit=function(){$(".show-details").click(function(){$(this).parents(".reg-row").nextUntil(".reg-row",".reg-details").toggleClass("hide-details");return m})};
p.registration.getPaymentDetails=function(a){var b=function(a){return a.replace(/[^\w ]+/g,"").replace(/ +/g,"-").toLowerCase()};$("#payments-"+b(a)).closest(".payment-details").toggleClass("hide-details");if(!$("#payments-"+b(a)).children().length){var d="/events/register/manage/payments/"+a+"/";$.get(d,function(d){if(d.success){d=d.payments;for(var f=0,h=d.length;f<h;f++)for(var g in d[f]){var l=$("<div />").attr("class","reg-field");l.append($("<div />").attr("class","reg-key").text(g));l.append($("<div />").attr("class",
"reg-value").text(d[f][g]));$("#payments-"+b(a)).append(l)}}})}return m};p.registration.countRegistrations=function(a,b,d){var e="/events/register/manage/"+a+"/count?page=50";b&&(b.qk&&b.qo&&b.qv)&&(e=e+"&qk="+b.qk+"&qo="+b.qo+"&qv="+b.qv);d&&(e+="&c="+d);$.get(e,function(d){if("ok"==d.result&&d.cursor){for(var e in d.counts){var g=parseInt(d.counts[e]),l=$("#"+e+"_total"),r=parseInt(l.html());l.html(r+g)}p.registration.countRegistrations(a,b,d.cursor)}else $("#count_status").html(d.statusmsg)})};
p.registration.adminTask=function(a,b){$.post("/events/register/manage/admin/"+a+"/",{admintask:b},function(){p.location.href="/events/register/manage/"+a+"/"})};p.registration.filterResults=function(a,b){var d=$("#onlyunpaid").is(":checked")?"1":"0",e=$("#onlyinvalid").is(":checked")?"1":"0";p.location.href="/events/register/manage/"+a+"/?sort="+b+"&unpaid="+d+"&invalid="+e};
p.registration.TicketInAppTransaction=function(a,b){var d="/events/register/token/"+a+"/"+b+"/",e=function(){p.registration.claimTimer.stop();$("#payment-button").html('Verifying Payment<br/><img src="//www.google.com/images/loading.gif"/>');p.registration.ConfirmAndNavigate(a,0)};$.post(d,k,function(a){a.success?goog.payments.inapp.buy({jwt:a.jwt,success:e,failure:p.registration.InappFailure}):p.devsite.showError("There was an error Initiating payment.")})};
p.registration.ConfirmAndNavigate=function(a,b){var d=function(){p.location.href="/events/register/waitlist/form/"+a+"/"},e=function(){5>b&&window.setTimeout(p.registration.ConfirmAndNavigate,1E3*Math.pow(2,b+2),a,b+1)};p.registration.confirm(a,d,e)};p.registration.VipcodeInAppTransaction=function(a){var b=function(){p.location.href="/events/register/vipcode/form/"+a+"/"};p.registration.InappTransaction(a,b)};
p.registration.InappTransaction=function(a,b,d){a="/events/register/token/"+a+"/";$.post(a,k,function(a){a.success?goog.payments.inapp.buy({jwt:a.jwt,success:b||p.registration.InappSuccess,failure:d||p.registration.InappFailure}):p.devsite.showError("There was an error Initiating payment.")})};p.registration.InappSuccess=function(a){var b=a.response.orderId;a=a.request;a=a.sellerData.split(",");a[1].split(":");p.devsite.showError("Payment Successful. Order Reference Number: "+b);p.reloadWindow()};
p.registration.InappFailure=function(){var a=$("#payment-error"),b=a.find("button");a.dialog($.extend({buttons:[{text:b.eq(0).html(),"class":"button button-blue",click:function(){$(this).dialog("close")}}]},p.devsite.dialogConfig));a.dialog("option","width",450);a.dialog("open")};

    </script>

      <script>
        //V: moved this script from the block to avoid second time evaluation of inner script because of Insertion method of prototype
        //moved here from symbol_alternative_tags
        var tickers = "spy,xle,ipw,xop,sso,rwl,sdy,prf,eps";
        SA.Pages.Symbol.initRTA('APC');
        var symbolZiignight = new Ziignight('', 'SAlternative',tickers,"symbolZiignight","symbol_alternative_tags");

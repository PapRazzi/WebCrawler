    <script type="text/javascript">
    $(".deleteMe").live('click',function(event) {
        cleanId = event.target.id.toString().replace("delete","");
        dest = "activities/"+pageViewerId+"/@self/"+cleanId;

        $.ajax({ url: "/proxy?method=DELETE&dest="+dest,
            success: function() {
                $("li#activityRow"+cleanId).css("display", "none");
            }
        });
    });

    $('.deleteComment').live('click',function(event) {
        //TODO: figure out why $.data isn't working
        commentId = $(this).attr('data-comment-id');
        activityId = $(this).attr('data-activity-id');
        $.post("activity/deletecomment", { comment_id: commentId, activity_id: activityId }, function(data) {
            $("li#activityRow"+commentId).css("display", "none");
        });
        return false;
    });

    $(document).ready(function() {
        if(!isMore) {
            $("#moreUpdates").css("display","none");
            $(".leftButtonEdge").css("display","none");
            $(".rightButtonEdge").css("display","none");
        }
    });
</script>

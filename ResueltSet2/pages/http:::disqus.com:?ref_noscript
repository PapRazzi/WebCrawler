<script>
    // Embed Configuration
    var disqus_shortname = 'next-scripted-demo';
    var disqus_demo = true;
    var disqus_config = function () {
        this.page.identifier = 'homepage';

        this.callbacks.onReady.push(function () {
            var lounge = DISQUS.Lounge.list()[0].frame;

            var hover = function (evt) {
                $('#disqus_thread').stop().animate({
                    height: evt.type === 'mouseout' ? '250px' : '275px'}, 'fast');
            };
            lounge.on('mouseover mouseout', hover);

            lounge.on('click', function expander(evt) {
                lounge.off('mouseover mouseout', hover);
                lounge.off('click', expander);
                $('#disqus_thread').animate({height: $(lounge.elem).css('height')},
                        function () {
                            $(this).css('height', 'auto');
                        });
            });

            lounge.on('resize', function tooltipPositioner(e) {
                guiders._realign();
            });

            lounge.on('guiders.show', function (data) {
                data.parent = lounge.elem;
                guiders.createGuider(data).show();
            });

            lounge.on('guiders.hide', function (data) {
                guiders.hide(data);
            });

            lounge.on('guiders.hide', function (data) {
                guiders.hideAll();
            });
        });
    };
</script>

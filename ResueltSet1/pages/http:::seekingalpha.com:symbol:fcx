      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">FCX</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['fcx']
      });
    })();

    try { SymbolData.handle_data('FCX', {"rt_time":"04:00PM ET,02/15","last":35.04,"high":35.64,"open":35.59,"close":35.53,"low":34.87,"volume":13722900,"volume_at":1361063419,"at":1361063419,"avg3vol":20221600.0,"c_share":3.93,"div_rate":1.25,"forward_pe":7.8,"payout":37.0,"trailing_pe":11.0,"div_yield":3.6,"payout_5y":22.0,"div_yield_5y":3.6,"chgp_1w":-1.7,"chgp_4w":4.2,"chgp_52w":-18.6,"beta60":1.92,"beta36":2.21,"mov_avg_10d":35.57,"mov_avg_10w":34.56,"mov_avg_200d":35.86,"short_interest":2.2,"p_short_interest_float":3.4,"liquidity":438730000.0,"p_week_vol_shares":4.95,"sfloat":941438038.0,"shares_held":681644360.0,"p_shares_by_inst":71.9,"p_shares_by_insid":0.8,"net_insid_tr":772174.0,"insid_sb":779685.0,"insid_ss":7511.0,"total_revenue":18010000000.0,"revenue_growth":-13.7,"net_income":3041000000.0,"gross_margin":43.2,"eps":3.19,"roe":22.7,"roe_ind":83.5,"roa":8.5,"cur_ratio":3.2,"quick_ratio":1.7,"int_cover":22.2,"debt_eq":0.21,"lt_debt_cap":0.2,"debt_short":2000000.0,"debt_long":3525000000.0,"low52":30.54,"high52":44.74,"market_cap":33264131943.36,"cash":3730823017.6200004,"debt":3527000000.0,"total_enterprise":33060308925.74,"price_book":1.9412742382271466,"ev_ebitda":5.459099888662484,"last_div":0.3125,"last_div_date":"01/11/13","iop":0.0,"iopp":0.0,"price_sales":1.8469812295036092,"price_tangb":1.9796610169491526,"ev_sales":1.8356640158656303,"ev_fcf":-26.788688852615604,"inst_chg":18001007.0,"cf":4063084609.5200005,"fcf":-1234114484.2,"debt_fcf":-2.8579196218463765,"rt_ts":1360944000,"chg_vs_mkt_1w":-2.0,"chg_vs_mkt_4w":2.0,"chg_vs_mkt_52w":-27.0,"p_close_10w":1.0,"p_close_200d":-2.0}) } catch (e) { }

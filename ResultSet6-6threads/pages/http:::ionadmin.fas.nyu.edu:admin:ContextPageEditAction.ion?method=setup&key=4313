function btnRoll(oImg) {
var aTemp = oImg.src.split('/');
var aSource = aTemp[aTemp.length-1].split('-');
if (aSource.length == 1) oImg.src = './admin-images/' + aSource[0].split('.')[0] + '-over.gif';
else if (aSource[1] == 'over.gif') oImg.src = './admin-images/' + aSource[0] + '.gif';
}

<script>
var newwin;
function popRemote(winurl,winname,winfeatures,opener)
{
  if (!document.images)
  {
    pop = null;
  }

  if (winfeatures == null)
    winfeatures = "menubar=yes,toolbar=no,location=no,directories=no,status=no,scrollbars=yes,resizable=yes,copyhistory=no,width=600,height=500";

  newwin = window.open(winurl,winname,winfeatures);

  if (newwin.opener == null)
  {
    newwin.opener = window;
  }

  newwin.opener.name = "opener";
  newwin.focus();
}
</script>

<script type="template" id="tmpl_user_info">
<div id="user_info">
    <div class="profile">
        <div class="menu_wrapper">
            {{if third_party_info}}
                {{if third_party_info.origin == S}}
                <span class="sina_logo"></span>
                {{else third_party_info.origin == M}}
                <span class="msn_logo"></span>
                {{else third_party_info.origin == Q}}
                <span class="qq_logo"></span>
                {{else}}
                <span class="default_logo"></span>
                {{/if}}
            {{/if}}

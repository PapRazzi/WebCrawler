        $(function() {
                $("#qpcbar .qpccontainer.expandable").click(function(e) {
                    e.stopPropagation();
                    var $this = $(this);
                    var $expandableWrapper = $("#qpcbarexpandablewrapper");
                    // looking to close the expanded part
                    if ($expandableWrapper.attr("data-expand") === "expanded" && $this.attr("data-expand") === "expanded") {
                            $expandableWrapper.slideUp();
                            $expandableWrapper.attr("data-expand", "closed");
                            $this.attr("data-expand", "closed");
                            $this.find(".qpcclose").hide().prev().show(); 
                            $expandableWrapper.find(".advertisement").empty();
                    }
                    else {
                        // expanded part is showing, but want a different broker
                        if ($expandableWrapper.attr("data-expand") === "expanded") {
                            $expandableWrapper.find(".advertisement").empty();
                            $("#qpcbar .qpccontainer").attr("data-expand", "closed").find(".qpcclose").hide().prev().show();  
                        }
                        // looking to show expanded part
                        else {
                            $expandableWrapper.attr("data-expand", "expanded");
                            $expandableWrapper.slideDown();                        
                        }
                        $this.attr("data-expand", "expanded");
                        ScrillaZilla.Escort.getClient($this.find(".advertisement").attr("id") + "Expandable").spload();
                        $this.find(".qpcexpand").hide().next().show();
                    }
                });
        });


    

<script type='text/javascript'>
  //<![CDATA[
    (function() {
      // object that captures the current pages' video play status
      var videoPlayStatus = {
        playing: false,
        playStarted: function(playerId){
          if (this.playing) return;  //if already playing, ignore!
          this.playing = true;
          BLOOMBERG.tracker.EVENTTRACK.record('Video Player Tracking', 'Video Play', playerId);
        },
        playComplete: function(){
          this.playing = false;
        }
      }
    
      var ns = BLOOMBERG.namespace('video_cassette');
      ns.videoModuleEvent = function(playerId, eventName, eventArgs) {
        var player;
        if (eventName == 'playComplete') {
          videoPlayStatus.playComplete();
          player = $('#' + playerId).first();
          player.closest('.video_module').videoCassette('playNextTrack');
        } else if (eventName == 'apiReady') {
          player = $('#' + playerId).first();
          player.closest('.video_module').videoCassette('playerReady', document.getElementById(playerId));
        } else if (eventName == 'stateChanged' && eventArgs.state == 'playing') {
          videoPlayStatus.playStarted(playerId);
          player = $('#' + playerId).first();
          player.closest('.video_module').videoCassette('playStarted');
        } else if (eventName == "popOutButtonClicked") {
          document.getElementById(playerId).pauseMovie();
          player = $('#' + playerId).first();
          window.open(player.closest('.video_module').videoCassette('popOutUrlForCurrentTitle', eventArgs.data), null, "width=640, height=360, status=0, toolbar=0, scrollbars=0, location=0, resizable=1");
        } else if (eventName == 'adStarted') {
          videoPlayStatus.playComplete(); // assume if an ad started, the previous play was finished
          player = $('#' + playerId).first();
          player.closest('.video_module').videoCassette('adStarted');
        }
      };
    })();
  //]]>
</script>


<script type="text/javascript" charset="utf-8">
	Event.onDomReady(function(){
	var overlay = new coherent.Overlay('engravingOverlay');
	var links = Element.queryAll('a[rel="engravingGallery"]');
	Array.forEach(links, function(link){
		Event.observe(link, 'click', function(event) {
			Event.stop(event);
			if (!overlay.visible) {
              			overlay.load(link.href);
			}
		 });
	  });
 });
</script>


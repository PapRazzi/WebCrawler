      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">JEF</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 10,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['jef']
      });
    })();

    try { SymbolData.handle_data('JEF', {"rt_time":"11:42AM ET,02/15","last":21.18,"high":21.45,"open":21.33,"close":21.43,"low":21.16,"at":1360928566,"volume":395249,"volume_at":1360928447,"avg3vol":1945200.0,"div_rate":0.3,"forward_pe":15.3,"payout":25.0,"trailing_pe":17.3,"div_yield":1.4,"payout_5y":15.0,"div_yield_5y":2.3,"chgp_1w":1.6,"chgp_4w":12.7,"chgp_52w":35.4,"beta60":1.57,"beta36":1.53,"mov_avg_10d":20.53,"mov_avg_10w":19.15,"mov_avg_200d":15.27,"short_interest":2.0,"p_short_interest_float":3.1,"liquidity":47727300.0,"p_week_vol_shares":2.12,"sfloat":117283576.0,"shares_held":129542864.0,"p_shares_by_inst":63.6,"p_shares_by_insid":11.4,"net_insid_tr":-2023802.0,"insid_sb":58367.0,"insid_ss":2082169.0,"total_revenue":3871205000.0,"revenue_growth":9.7,"net_income":282409000.0,"eps":1.22,"roe":9.4,"roe_ind":184.3,"roa":0.9,"int_cover":1.5,"debt_eq":1.44,"lt_debt_cap":0.6,"debt_short":150000000.0,"debt_long":4804607000.0,"low52":11.59,"high52":21.53,"market_cap":4316466801.84,"debt":4954607000.0,"total_enterprise":9271073801.84,"price_book":1.2562277580071175,"ev_ebitda":6.588785722852479,"last_div":0.075,"last_div_date":"12/19/12","iop":0.0,"iopp":0.0,"price_sales":1.1150189157742874,"price_tangb":1.4026490066225166,"ev_sales":2.3948806125844535,"ev_fcf":146.7458726251394,"inst_chg":-10074279.0,"cf":324040708.92,"fcf":63177748.28,"debt_fcf":78.42329197997812,"rt_ts":1360928548,"chg_vs_mkt_1w":1.0,"chg_vs_mkt_4w":10.0,"chg_vs_mkt_52w":21.0,"p_close_10w":10.0,"p_close_200d":39.0}) } catch (e) { }

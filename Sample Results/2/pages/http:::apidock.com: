<script type="text/javascript" charset="utf-8">
  function parseTweets(json) {
    var monthNames = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
    var tweetContainer = $('tweet_list');
    tweetContainer.update('');
    for(var i=0;i<5;i++) {
      var tweet = json[i];
      var date = new Date(tweet.created_at.replace(/^\w+ (\w+) (\d+) ([\d:]+) \+0000 (\d+)$/, "$1 $2 $4 $3 UTC"));
      var dateStr = monthNames[date.getMonth()] + " " + date.getDate() + ", " + date.getFullYear();

      var tweetText = tweet.text.replace(/(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig,"<a href='$1'>$1</a>");
      tweetText = tweetText.replace(/(^|\s)@(\w+)/g, "$1<a href=\"http://www.twitter.com/$2\">@$2</a>");
      tweetText = tweetText.replace(/(^|\s)#(\w+)/g, "$1<a href=\"http://search.twitter.com/search?q=%23$2\">#$2</a>");

      var tweetHtml = "<li>" +
        '<span class="list_left">' + tweetText + '</span>' +
        '<span class="list_right">' + dateStr + '</span>' +
      '</li>';

      tweetContainer.insert({bottom: tweetHtml});
    }
  }

  var el = new Element('script', {src: "http://api.twitter.com/1/statuses/user_timeline.json?screen_name=apidock&callback=parseTweets"})
  $$('head').first().insert({bottom: el});
</script>


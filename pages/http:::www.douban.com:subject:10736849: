nts.notes.value);frm.find(":submit").attr("disabled",true).text("处理中...");$.post_withck(this.action,{note:a,act:this.elements.act.value,"shuo-share":this.elements["shuo-share"].checked?1:0},function(c){frm.find(":submit").attr("disabled",false).text("发表");if(c.r){alert(c.msg);return}c.note=filterString(a);frm.find("textarea").val("").blur();$(".notes-list:eq(0) > ul").prepend($.tmpl(TMPL_NOTE_ITEM,c))},"json")});Douban.init_digg=function(){var b=$(this);var a=b.data();if(b.hasClass("processing")||b.hasClass("digged")){return}b.addClass("processing");$.post_withck(this.href,{id:a.id},function(c){b.removeClass("processing");if(!c.r){b.addClass("digged").text(b.text().replace(/\d+/,c.digg_n));b.trigger("digged",c)}else{alert(c.msg)}},"json")};Douban.init_note_hide_reply=function(){var a=$(this);var c=a.parents(".info").find(".reply-list");var b=c.find("li").length;c.hide();a.text((b||"")+"回应").removeClass("a_note_hide_reply").addClass("a_note_reply")};Douban.init_note_reply=function(){var a=$(this);if(a.hasClass("processing")){return}a.data("title",a.text()).text("加载中...");$.getJSON(this.href.split("#")[1],function(d){a.removeClass("processing");a.text(a.data("title"));if(!d||d.r){return}a.removeClass("a_note_reply").addClass("a_note_hide_reply").text("隐藏回应");var c=a.parents(".info");var b=c.find(".reply-list");if(b.length===0){b=$($.tmpl(TMPL_REPLY_LIST,d));c.append(b);initReplyForm(b.find("form"))}b.show()})};Douban.init_note_more=function(){var b=$(this);var a=b.data();if(b.hasClass("processing")){return}b.data("title",b.text());b.text("加载中...");b.addClass("processing");$.getJSON(this.href.split("#")[1],$.extend({},a,{start:a.start*a.count}),function(c){b.removeClass("processing");b.text(b.data("title"));if(c.r){return}b.data("start",a.start+1);$(c.list).insertBefore(b.parent());if(!c.more){b.parent().hide()}addReportLink()})};Douban.init_delete_reply=function(){if(window.confirm("确定要删除该回应?")){var a=$(this);$.post_withck(this.href.split("#")[1],a.data(),function(b){if(b.r){alert(b.msg);return}a.parents("li").eq(0).remove()})}};Douban.init_delete_note=function(){if(window.confirm("确定要删除该评论?")){var a=$(this);$.post_withck(this.href.split("#")[1],a.data(),function(b){if(b.r){alert(b.msg);return}a.parents("li").eq(0).remove()})}};
});
</script>




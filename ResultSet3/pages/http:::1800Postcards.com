    <script type="text/javascript" src="/js/tabs.js"></script>
    <script type="text/javascript">
        $().ready(function () {
            var nav_items = $("#listing_head_nav li");
            var home_items = $(".home_item");
            var nextStepBtns = $(".nextStepBtn")
            tabMenu({
                tabListItem: nav_items,
                detailBoxItem: home_items
            });
            var mask = $("#maskBox");
            var pop = $("#popBox");
            function reflowMask() {
                var w = document.documentElement.scrollWidth;
                var h = document.documentElement.scrollHeight;
                var vH = document.documentElement.clientHeight;
                mask.css({
                    "width": w + "px",
                    "height": h + "px"
                });
                pop.css({
                    "left": (w - pop.outerWidth()) / 2 + "px",
                    "top": (vH - pop.outerHeight()) / 2 + "px"
                });
                if (navigator.userAgent.toLowerCase().indexOf("msie 6") != -1) {
                    if ($("#maskFrame").length == 0) {
                        //alert(111);
                        mask.after('<iframe id="maskFrame" src="about:blank" frameborder="0"></iframe>');
                    }
                    $("#maskFrame").css({
                        "width": w + "px",
                        "height": h + "px"
                    });
                    pop.css("top", (vH - pop.outerHeight()) / 2 + $(document).scrollTop() + "px");
                }
            }
            function showMask() {
                reflowMask();
                mask.show();
                pop.show();
                $("#maskFrame").show();
                return false;
            }
            function closeMask() {
                mask.hide();
                pop.hide();
                $("#maskFrame").hide();
                return false;
            }
            $(window).resize(reflowMask);
            $(".closeMask").click(closeMask);
            mask.click(closeMask);
            var nextStepBtn = $(".nextStepBtn");
            nextStepBtns.each(function (index) {
                var btn = $(this);
                btn.click(function () {
                    if (index == 0) {
                        $("#read_conf")[0].checked ? nav_items.eq(index + 1).find("a").click() : showMask();
                        //showMask();
                        return false;
                    }
                    nav_items.eq(index + 1).find("a").click();
                    return false;
                });
            });
            nextStepBtn.mousedown(function () {
                nextStepBtn.css("backgroundPosition", "0 -80px");
            });
            nextStepBtn.hover(
                function () {
                    nextStepBtn.css("backgroundPosition", "0 -40px");
                },
                function () {
                    nextStepBtn.css("backgroundPosition", "0 0");
                }
            );
            $("#inner_home_details").equalHeightByTarget("#fbc_preview");

            $(".fbc_bs_item").change(function () {
                if ($(this)[0].checked) {
                    //alert($(this).attr("preview"));
                    $("#fbc_bs_preview")[0].src = $(this).attr("preview");
                }
            });
        });
        function selectTab(className, parentDivID, divIndex) {

            var td = document.getElementById('tab_div');
            td.className = className;

            var pd = document.getElementById(parentDivID);

            var divs = new Array();
            for (var j = 0; j < pd.childNodes.length; j++) {
                var c = pd.childNodes[j];
                if (c.tagName == 'DIV')
                    divs[divs.length] = c;
            }

            for (var i = 0; i < divs.length; i++) {
                if (i == divIndex)
                    divs[i].style.display = '';
                else
                    divs[i].style.display = 'none';
            }

            return true;
        }
    </script>

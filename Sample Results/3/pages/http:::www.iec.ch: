    <div class="top-tools-section"><!-- top tools section -->	
      <ul class="sizer">
        <li>
            <script type="text/javascript" src="/js/jquery.toggleformtext.js"></script>
            <form name="searchduplicateform"><input value="" type="hidden" id="duplicatefield"/></form>
            <form action="javascript:launchsearch();" name="searchform">
                <script type="text/javascript">	 
                    $(function()
                    {   	    
                    var searchfield = [			
                        "<div class='icn_website'></div><div class='btn_choose'><b>in the entire IEC site</b></div>", 			   
                        "<div class='icn_webstore'></div><div class='btn_choose'><b>in the IEC Webstore</b></div>"
                    ];
                    $("#searchfield").smartAutoComplete(
                    {	  
                    source: searchfield,   
                    filter: function(term, source){
                        var filtered_and_sorted_list =  $.map(source, function(item)
                                                                        {
                                                                          score = 1;
                                                                          if(score > 0)
                                                                            return { 'name': item, 'value': score 
                                                                                   }
                                                                         }
                                                              ).sort(function(a, b){ return b.value - a.value 
                                                  });
                        return $.map(filtered_and_sorted_list, function(item)
                        {   
                            var f2 = document.searchduplicateform.duplicatefield;
                            f2.value = term;  
                            var str = term;	   
							str = str.replace("&", "&#38;");	
                            str = str.replace("<", "&lt;");
                            str = str.replace(">", "&gt;");	  
                            str = str.replace("/", "&#47;");	                              	
                            return "<div class=\"icon_container\">" + "<span class=\"btn_txt\">" + str + "</span>" + item.name  + "</div>" ; 					
                        });	 
                    }
                    });
                   }); 
               // Javascript to launch the search page  
               function launchsearch() {		
                 window.location.href='/dyn/www/f?p=103:91:0::::FSP_LANG_ID:25#q=' + document.searchduplicateform.duplicatefield.value;
                 document.searchduplicateform.duplicatefield.value = '';
               }
               </script>   
               <div>
                 <input title="Search..." type="text" maxlength="100" autocomplete="off" id="searchfield"/>	  
               </div> 
           </form>	 
        </li>
      </ul> 
    </div><!-- end top tools section -->    

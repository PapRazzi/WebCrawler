<script type="text/javascript">
/* stars */

function qstars(rating, size) {
  var final = '';

  //full star/s
  var count = 0;
  var whole = Math.floor(rating);
  var starkup = '<img src="http://static.usnews.com/images/diets/star' + size + 'px100.png" alt=""/>';
  for(count=0; count<whole; count++) {
    final += starkup;
  }
  
  //fractional star
  if (rating % 1) {
    var fractional = 25 * Math.round(((rating % 1) * 100) / 25);
    fractional = '' + fractional;
    while (fractional.length < 3) { fractional = '0' + fractional; }
    starkup = '<img src="http://static.usnews.com/images/diets/star' + size + 'px' + fractional + '.png" alt=""/>';
    final += starkup;
  }
  
  //empty star/s
  var empty = 5 - Math.ceil(rating);
  starkup = '<img src="http://static.usnews.com/images/diets/star' + size + 'px000.png" alt=""/>';
  for(count=0; count<empty; count++) {
    final += starkup;
  }

  //end it
  document.write(final);
}

</script>

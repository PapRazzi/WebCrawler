      <h1 class="symbol_title_new_design">
  <div style="float:left">
  <span class="symbol_title_name">DISH</span>
  <span class="symbol_title_current_price" id="symbol_last_trade"></span>
  <span class="symbol_title_price" id="symbol_change"></span>
  <span class="symbol_procent_change" id="symbol_procent_change"></span>
  <script>
    (function() {
      function getClassName(P) {
        if (P<0) return 'down';	
        if (P>=0) return 'up';
        return "";
      };

      function fixDecimalPoint(Int, Round,abs) {
        if (Int=='N/A') return Int;
        Int=parseFloat(Int);
        if (abs) Int = Math.abs(Int);
        var res = (((""+Int).indexOf(".")>-1 ? Int+"00" : ""+Int)*1).toFixed(Round ? 1 : 2)
        return res!=0 ? res : 0;
      };

      function Calper(d) {
        if (!(d.last && d.close)) return "";
        var s = "("+fixDecimalPoint(100 * (d.last / d.close - 1))+"%)";
        return s;
      };

      function Chg(d) {
        if (!(d.last && d.close)) return "";
        return fixDecimalPoint(d.last - d.close);
      };

      SymbolData.config({
        handle_data: function(slug, data) {
          if (data.last) $('symbol_last_trade').update("$"+fixDecimalPoint(data.last));
          if (data.last && data.close){
            var change = Chg(data);
            var movment_check_class = getClassName(change) + "1";
            $('symbol_change').update(change).addClassName(movment_check_class);
            var per = Calper(data)
            $('symbol_procent_change').update(per).addClassName(movment_check_class);
          }
        },

        handle_no_data: SymbolData.retry,
        script_container: 'container',
        refresh_rate: 0,
        asset_hosts: ['http://static.cdn-seekingalpha.com'],
        symbols: ['dish']
      });
    })();

    try { SymbolData.handle_data('DISH', {"rt_time":"04:00PM ET,02/15","last":36.93,"high":37.29,"open":36.87,"close":36.84,"low":36.65,"volume":2464100,"volume_at":1361063466,"at":1361063466,"avg3vol":2952200.0,"c_share":6.84,"div_rate":0.0,"forward_pe":15.6,"payout":122.0,"trailing_pe":22.5,"div_yield":0.0,"payout_5y":40.0,"chgp_1w":-1.9,"chgp_4w":-2.6,"chgp_52w":27.3,"beta60":1.26,"beta36":0.81,"mov_avg_10d":37.15,"mov_avg_10w":36.8,"mov_avg_200d":32.65,"short_interest":1.2,"p_short_interest_float":1.4,"liquidity":75619300.0,"p_week_vol_shares":5.56,"sfloat":203295736.0,"shares_held":177986092.0,"p_shares_by_inst":39.5,"p_shares_by_insid":0.8,"net_insid_tr":-210.0,"insid_sb":927015.0,"insid_ss":927225.0,"total_revenue":14308432000.0,"revenue_growth":2.5,"net_income":740235000.0,"gross_margin":36.0,"eps":1.64,"roe":678.1,"roa":4.5,"cur_ratio":2.0,"quick_ratio":1.7,"int_cover":3.0,"debt_eq":95.18,"lt_debt_cap":1.0,"debt_short":33932000.0,"debt_long":10356799000.0,"low52":26.12,"high52":38.14,"market_cap":16671333276.69,"cash":6400222529.719999,"debt":10390731000.0,"total_enterprise":20661841746.97,"price_book":153.875,"ev_ebitda":5.500948135946504,"last_div":1.0,"last_div_date":"12/12/12","iop":-1.0,"iopp":-50.0,"price_sales":1.165140476377146,"price_tangb":-5.275714285714286,"ev_sales":1.4440325639434146,"ev_fcf":34.41330344683113,"inst_chg":920722.0,"cf":1710922099.07,"fcf":600402741.89,"debt_fcf":17.306268401258716,"rt_ts":1360944000,"chg_vs_mkt_1w":-2.0,"chg_vs_mkt_4w":-5.0,"chg_vs_mkt_52w":14.0,"p_close_10w":0.0,"p_close_200d":13.0}) } catch (e) { }

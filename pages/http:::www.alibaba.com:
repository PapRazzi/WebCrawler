AE.use(['dom'], function(dom){
	var doc = dom.Document(),
		showImg = function(node){
			var currentInx = 0;
			
			if(node.get('id') !== 'lead-sec-l'){
				node.byClass('J-tabs')[0].byTag('li').some(function(n, i){
					if(n.hasClass('current')){
						currentInx = i;
						return true;
					}
				});
			}
			
			node.byTag('A').some(function(n, i){
				var imageSrc;
				if(i == currentInx){
					imageSrc = (n.getAttribute('data-src') || '').replace('img.alibaba.com', 'i02.i.aliimg.com');
					
					if(imageSrc){
						n.setStyle('backgroundImage', 'url(' + imageSrc + ')');
						n[0].removeAttribute('data-src');
					}
					return true;
				}
			})
		};
	
		showImg(doc.byId('lead-sec-l'));
	showImg(doc.byId('b-sec-2'));
});

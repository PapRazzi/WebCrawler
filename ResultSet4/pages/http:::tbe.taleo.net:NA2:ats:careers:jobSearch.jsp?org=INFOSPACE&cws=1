<script type="text/javascript" >
  var timerLinkedIn;
  $(document).ready(function(){
     timerLinkedIn = window.setInterval(checkForLinkedInRendered, 100);
  });
  function checkForLinkedInRendered(){
     if ($('#company-insider-info-window') != null){
        $('#company-insider-info-window').insertAfter('#linkedInPopUpWarning');
        timerLinkedIn = window.clearInterval(timerLinkedIn);
        timerLinkedIn = null;
        $('#company-insider-info-window').children('iframe').each(function(){
           $(this).attr('title', 'Do you know anyone at Blucora?');
        });
     }
     $("#ms").keyup(function(event){
           if(event.keyCode == 13){
                   $("#ms").click();
               }
           });  }
</script>

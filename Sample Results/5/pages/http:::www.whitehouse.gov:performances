	<script type="text/javascript">
$(document).ready(function() {

function renderVideoSWF(videonid) {

    if (isObject("swf13311424721")) { replaceSWFwithEmptyDiv("swf13311424721"); }

    var configFile = "http://www.whitehouse.gov/xml/video/"+videonid+"/config.xml";

    var ipvars = { "config": configFile, "path_to_player": "http://www.whitehouse.gov/sites/all/modules/contrib/swftools/shared/flash_media_player/player5x2.swf", "path_to_plugins": "http://www.whitehouse.gov/sites/default/modules/wh_multimedia/wh_jwplayer/plugins", "width": "640", "height": "400" };

    var ipparams = { "swliveconnect": "default", "play": "false", "loop": "false", "menu": "false", "quality": "best", "scale": "showall", "align": "l", "salign": "tl", "wmode": "opaque", "bgcolor": "#FFFFFF", "version": "7", "allowfullscreen": "true", "allowscriptaccess": "sameDomain", "width": 640, "height": 400, "base": "http://www.whitehouse.gov/sites/default/files/", "src": "http://www.whitehouse.gov/sites/all/modules/contrib/swftools/shared/flash_media_player/player5x2.swf" };

    var ipattributes = { "id": "swf13311424721" };

    swfobject.embedSWF("http://www.whitehouse.gov/sites/all/modules/contrib/swftools/shared/flash_media_player/player5x2.swf", "swfobject2-id-13311424721", "640", "400", "7","", ipvars, ipparams, ipattributes);
  
  }
  
  function updateVideoData(videonid) { 

    var dataTarget = '.performance-item[data-nid="'+videonid+'"]';
  
    var song = $(dataTarget).find(".performance-title").html();
    var artist = $(dataTarget).find(".performance-artist").html();
    var video = $(dataTarget).find(".performance-thumb").attr("data");

    $("#video-player-caption-top").html(artist + " - <i>" + song + "</i>");
    $('html, body').animate({ scrollTop: 0 }, 'slow');

  }

  function replaceSWFwithEmptyDiv(targetID){
     var el = document.getElementById(targetID);
     if(el){
        var div = document.createElement("div");
        el.parentNode.insertBefore(div, el);   
        swfobject.removeSWF(targetID);
        div.setAttribute("id","swfobject2-id-13311424721");
        div.setAttribute("class","swftools swfobject2");
     }
  }

  function isObject(targetID){
     var isFound = false;
     var el = document.getElementById(targetID);
     if(el && (el.nodeName === "OBJECT" || el.nodeName === "EMBED")){
        isFound = true;
     }
     return isFound;
  }

  var startNode = $(".inperformance").attr("data-start");
  renderVideoSWF(startNode);
  updateVideoData(startNode);

  $(".performance-feature").click(function() {
    var new_url = "http://www.whitehouse.gov/"+$(this).attr("data-url"); 
    window.location = new_url;
  });
  $(".performance-item").click(function() {

    var videonid = $(this).attr("data-nid"); 
    renderVideoSWF(videonid);
    updateVideoData(videonid);
    //document.getElementById('swf13311424721').sendEvent('PLAY');
  });

  $("#inperformance-nav-list li").click(function() {
    window.location = $(this).attr("data-target"); 
  });

  $(".inperformance-nav-select")
    .click(function() { 
      $("#inperformance-nav-container").toggle();
      $(this).toggleClass("inperformance-nav-active");
    })
    .hover(function() {
      $(this).toggleClass("inperformance-nav-hover");
    }, function() {
      $(this).toggleClass("inperformance-nav-hover");
    });

  $(".nav-show-target").html((document.title).replace("| The White House",""));


});
</script>

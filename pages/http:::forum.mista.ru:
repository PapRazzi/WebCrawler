<script type="text/javascript" language="JavaScript">
//функция для подгрузки сообщения ("плюсики")
function getmessage(k, topic_id, message_n, direction) 
{
       var req = new Subsys_JsHttpRequest_Js();

        // Код, АВТОМАТИЧЕСКИ вызываемый при окончании загрузки.
        req.onreadystatechange = function() {

            if (req.readyState == 4) {
                if (req.responseJS) 
		  {

			if (req.responseJS.client_action==1) //ok
			{ 
				msg = document.getElementById("msg" + k);
				msg.innerHTML = '<blockquote>' + req.responseJS.message_html + '<\/blockquote>';
				show(k);
			}

			else if (req.responseJS.client_action==2) //error
			{ 
//				alert(req.responseJS.error); 
			}
                }
            }
        }

        req.caching = false; // Запрещаем кэширование
        req.open('POST', 'ajax_getmessage.php', true);

        // Посылаем данные запроса (задаются в виде хэша).
        req.send({ k:k, topic_id:topic_id, message_n:message_n, direction:direction});
} 
</script>


<script type="text/javascript" language="JavaScript">
function find_ajax() 
{
    var req = new Subsys_JsHttpRequest_Js();
	var keywords = '' + document.getElementById('keywords').value;

	// Код, АВТОМАТИЧЕСКИ вызываемый при окончании загрузки.
	req.onreadystatechange = function() {

		if (req.readyState == 4) {
			if (req.responseJS) 
			{
				if (req.responseJS.client_action == 1) 
				{ 
					find_result_div = document.getElementById('find_result');
					find_result_div.innerHTML = req.responseJS.result;
					find_link_a=document.getElementById('findlink');
					find_link_a.href='/find.php?keyword='+document.getElementById('keywords').value;
				}
			}
		}
	}

	req.caching = false; // Запрещаем кэширование
	req.open('POST', 'ajax_find.php', true);

	// Посылаем данные запроса (задаются в виде хэша).
	req.send({ 	keywords : keywords }); 

	return false; //to form onSubmit
}
</script>

